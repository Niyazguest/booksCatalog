INSERT INTO BOOK VALUES(4007,203719,'\u0410\u043b\u0435\u043a\u0441\u0435\u0439 \u0410\u0440\u0445\u0430\u043d\u0433\u0435\u043b\u044c\u0441\u043a\u0438\u0439: C++Builder. \u0420\u0430\u0431\u043e\u0442\u0430 \u0441 \u0434\u043e\u043a\u0443\u043c\u0435\u043d\u0442\u0430\u043c\u0438 Excel','\u0410\u0440\u0445\u0430\u043d\u0433\u0435\u043b\u044c\u0441\u043a\u0438\u0439 \u0410\u043b\u0435\u043a\u0441\u0435\u0439 \u042f\u043a\u043e\u0432\u043b\u0435\u0432\u0438\u0447','','\u0411\u0438\u043d\u043e\u043c, 2010 \u0433.',835.0E0,'978-5-9518-0315-3','480 (\u041e\u0444\u0441\u0435\u0442)','\u0422\u0438\u043f \u043e\u0431\u043b\u043e\u0436\u043a\u0438: 7\u0411\u0446 - \u0442\u0432\u0435\u0440\u0434\u0430\u044f, \u0446\u0435\u043b\u043b\u043e\u0444\u0430\u043d\u0438\u0440\u043e\u0432\u0430\u043d\u043d\u0430\u044f (\u0438\u043b\u0438 \u043b\u0430\u043a\u0438\u0440\u043e\u0432\u0430\u043d\u043d\u0430\u044f) \u0418\u043b\u043b\u044e\u0441\u0442\u0440\u0430\u0446\u0438\u0438: \u0427\u0435\u0440\u043d\u043e-\u0431\u0435\u043b\u044b\u0435','742 \u0433','241x173x26 \u043c\u043c','\u041a\u043d\u0438\u0433\u0430 \u043f\u043e\u0441\u0432\u044f\u0449\u0435\u043d\u0430 \u0440\u0430\u0437\u043b\u0438\u0447\u043d\u044b\u043c \u0432\u0430\u0440\u0438\u0430\u043d\u0442\u0430\u043c \u0438\u0441\u043f\u043e\u043b\u044c\u0437\u043e\u0432\u0430\u043d\u0438\u044f Excel \u0432 \u043f\u0440\u0438\u043b\u043e\u0436\u0435\u043d\u0438\u044f\u0445 C++Builder. \u0418\u043d\u0442\u0435\u0433\u0440\u0430\u0446\u0438\u044f C++Builder \u0438 Excel \u043f\u043e\u0437\u0432\u043e\u043b\u044f\u0435\u0442 \u0432 \u043f\u043e\u043b\u043d\u043e\u0439 \u043c\u0435\u0440\u0435 \u0438\u0441\u043f\u043e\u043b\u044c\u0437\u043e\u0432\u0430\u0442\u044c \u0438\u0445 \u0441\u0438\u043b\u044c\u043d\u044b\u0435 \u0441\u0442\u043e\u0440\u043e\u043d\u044b. \u0421++Builder \u043e\u0431\u0435\u0441\u043f\u0435\u0447\u0438\u0432\u0430\u0435\u0442 \u0441\u043e\u0437\u0434\u0430\u043d\u0438\u0435 \u0443\u0434\u043e\u0431\u043d\u043