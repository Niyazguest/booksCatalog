INSERT INTO COMMENT VALUES(1572,'Divergence','2014-01-11','\u041a\u043d\u0438\u0433\u0430 \u043e\u0447\u0435\u043d\u044c \u043f\u043e\u043b\u0435\u0437\u043d\u0430\u044f, \u043d\u043e \u0447\u0438\u0442\u0430\u0435\u0442\u0441\u044f \u0440\u0435\u0430\u043b\u044c\u043d\u043e \u0442\u044f\u0436\u0435\u043b\u043e. \u0422\u043e \u0435\u0441\u0442\u044c, \u043f\u0440\u043e\u0441\u0442\u043e \u0447\u0438\u0442\u0430\u0442\u044c \u0438 \u0441\u0440\u0430\u0437\u0443 \u043f\u043e\u043d\u0438\u043c\u0430\u0442\u044c - \u043d\u0435 \u043f\u043e\u043b\u0443\u0447\u0438\u0442\u0441\u044f. \u041d\u0443\u0436\u043d\u043e \u0440\u0430\u0437\u0431\u0438\u0440\u0430\u0442\u044c\u0441\u044f \u0432 \u043a\u0430\u0436\u0434\u043e\u043c \u043f\u0440\u0438\u043c\u0435\u0440\u0435. \u0417\u0430 \u0441\u0447\u0435\u0442 \u0441\u0432\u043e\u0435\u0439 \u0441\u043b\u043e\u0436\u043d\u043e\u0441\u0442\u0438, \u043a\u043d\u0438\u0433\u0430 \u043c\u043e\u0436\u0435\u0442 \u043e\u0442\u0431\u0438\u0442\u044c \u0436\u0435\u043b\u0430\u043d\u0438\u0435 \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u0438\u0440\u043e\u0432\u0430\u0442\u044c (\u044d\u0442\u043e \u0437\u0430\u043c\u0435\u0447\u0430\u0435\u0442 \u0434\u0430\u0436\u0435 \u0441\u0430\u043c \u0430\u0432\u0442\u043e\u0440), \u043f\u043e\u044d\u0442\u043e\u043c\u0443 \u043d\u0435 \u0441\u0442\u043e\u0438\u0442 \u0437\u0430\u0446\u0438\u043a\u043b\u0438\u0432\u0430\u0442\u044c\u0441\u044f \u043d\u0430 \u043d\u0435\u0439 =) . \u041e\u0434\u043d\u0430\u043a\u043e, \u0434\u043b\u044f \u0442\u0435\u0445, \u043a\u0442\u043e \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u0438\u0440\u0443\u0435\u0442 \u043d\u0430 \u0421\u0438++ - \u043a\u043d\u0438\u0433\u0430 must have. \u0414\u041c\u041a-\u041f\u0440\u0435\u0441\u0441 - \u043f\u043b\u043e\u0445\u043e\u0439 \u0438\u0437\u0434\u0430\u0442\u0435\u043b\u044c. \u041a\u0443\u0447\u0430 \u0433\u043b\u0443\u043f\u044b\u0445 \u043e\u043f\u0435\u0447\u0430\u0442\u043e\u043a \u0432 \u0442\u0435\u043a\u0441\u0442\u0435. \u0411\u0443\u043c\u0430\u0433\u0430 \u043a\u0430\u0447\u0435\u0441\u0442\u0432\u0435\u043d\u043d\u0430\u044f, \u0431\u0443\u043a\u0432\u044b \u0447\u0435\u0442\u043a\u0438\u0435 - \u0445\u043e\u0442\u044c \u044d\u0442\u043e \u0445\u043e\u0440\u043e\u0448\u043e.',3792)
INSERT INTO COMMENT VALUES(1573,'Kassavetes','2015-01-01','\u041d\u0435\u0441\u043a\u043e\u043b\u044c\u043a\u043e \u0444\u043e\u0442\u043e \u043f\u0440\u0435\u0434\u0441\u0442\u0430\u0432\u043b\u0435\u043d\u043d\u043e\u0433\u043e \u0438\u0437\u0434\u0430\u043d\u0438\u044f \u041f\u043e\u043a\u0430\u0437\u0430\u0442\u044c \u0438\u0437\u043e\u0431\u0440\u0430\u0436\u0435\u043d\u0438\u044f \u0421\u043a\u0440\u044b\u0442\u044c \u0438\u0437\u043e\u0431\u0440\u0430\u0436\u0435\u043d\u0438\u044f',3792)
INSERT INTO COMMENT VALUES(1574,'\u0413\u0435\u0440\u0430\u0441\u0438\u043c\u043e\u0432 \u0420\u043e\u043c\u0430\u043d','2015-01-15','\u0416\u0434\u0430\u043b \u044d\u0442\u0443 \u043a\u043d\u0438\u0433\u0443 \u043e\u0447\u0435\u043d\u044c \u0434\u0430\u0432\u043d\u043e \u043f\u043e \u043f\u0440\u0438\u0447\u0438\u043d\u0435 \u043f\u043e\u043f\u0443\u043b\u044f\u0440\u0438\u0437\u0430\u0446\u0438\u0438 HTML5, \u0432 \u043b\u0438\u0446\u0435 \u043a\u043e\u0442\u043e\u0440\u043e\u0433\u043e \u044f \u0432\u0438\u0436\u0443 \u0431\u0443\u0434\u0443\u0449\u0435\u0435 \u043c\u043e\u0431\u0438\u043b\u044c\u043d\u044b\u0445 \u043f\u0440\u0438\u043b\u043e\u0436\u0435\u043d\u0438\u0439. \u0421\u043f\u0430\u0441\u0438\u0431\u043e \u041b\u0430\u0431\u0438\u0440\u0438\u043d\u0442\u0443 \u0437\u0430 \u043e\u0447\u0435\u043d\u044c \u0431\u044b\u0441\u0442\u0440\u0443\u044e \u0434\u043e\u0441\u0442\u0430\u0432\u043a\u0443. \u041d\u043e \u043a \u043a\u0430\u0447\u0435\u0441\u0442\u0432\u0443 \u043a\u043d\u0438\u0433\u0438 \u0443 \u043c\u0435\u043d\u044f \u043e\u0433\u0440\u043e\u043c\u043d\u044b\u0435 \u043f\u0440\u0435\u0442\u0435\u043d\u0437\u0438\u0438. \u0418 \u0441\u0430\u043c\u0430\u044f \u0433\u043b\u0430\u0432\u043d\u0430\u044f \u043f\u0440\u0435\u0442\u0435\u043d\u0437\u0438\u044f - \u043a\u0430\u0447\u0435\u0441\u0442\u0432\u043e \u0431\u0443\u043c\u0430\u0433\u0438. \u041d\u0438\u043a\u0430\u043a\u0438\u043c \u0421\u0430\u043d\u041f\u0438\u043d\u0430\u043c \u043d\u0430 \u043c\u043e\u0439 \u0432\u0437\u0433\u043b\u044f\u0434 \u043a\u0430\u0447\u0435\u0441\u0442\u0432\u043e \u043f\u0435\u0447\u0430\u0442\u0438 \u043d\u0435 \u0441\u043e\u043e\u0442\u0432\u0435\u0442\u0441\u0442\u0432\u0443\u0435\u0442. \u0421\u0442\u0440\u0430\u043d\u0438\u0446\u044b \u0431\u043b\u0435\u043a\u043b\u043e \u0441\u0435\u0440\u044b\u0435, \u0448\u0440\u0438\u0444\u0442 \u0442\u0443\u0441\u043a\u043b\u043e\u0432\u0430\u0442. \u0422\u0430\u043a\u043e\u0435 \u043e\u0449\u0443\u0449\u0435\u043d\u0438\u0435, \u0447\u0442\u043e \u043d\u0430\u043f\u0435\u0447\u0430\u0442\u0430\u043d\u043e \u0431\u044b\u043b\u043e \u0432 \u043f\u043e\u0434\u0432\u0430\u043b\u0435. \u041e\u0442\u0437\u044b\u0432 \u043d\u0430\u043f\u0438\u0441\u0430\u043b \u043d\u0435 \u0432 \u043e\u0431\u0438\u0434\u0443 \u0438\u0437\u0434\u0430\u0442\u0435\u043b\u044f, \u0430 \u0441 \u0446\u0435\u043b\u044c\u044e \u043f\u0440\u0435\u0434\u0443\u043f\u0440\u0435\u0434\u0438\u0442\u044c \u043f\u043e\u043a\u0443\u043f\u0430\u0442\u0435\u043b\u0435\u0439. \u041d\u0435 \u043f\u043e\u0441\u043a\u0443\u043f\u0438\u0442\u0435\u0441\u044c \u0438 \u0437\u0430\u043a\u0430\u0436\u0438\u0442\u0435 \u043d\u0430 amazon''\u0435 \u0430\u043d\u0433\u043b\u0438\u0439\u0441\u043a\u0438\u0439 \u0432\u0430\u0440\u0438\u0430\u043d\u0442. \u0412 \u043a\u0430\u0447\u0435\u0441\u0442\u0432\u0435 \u0431\u0443\u043c\u0430\u0433\u0438 \u0432\u044b\u0438\u0433\u0440\u0430\u0435\u0442\u0435, \u043a \u0442\u043e\u043c\u0443 \u0436\u0435 \u0438 \u044f\u0437\u044b\u043a \u0441\u0432\u043e\u0439 \u0443\u043b\u0443\u0447\u0448\u0438\u0442\u0435. \u041d\u0435\u0441\u043c\u043e\u0442\u0440\u044f \u043d\u0430 \u043d\u0435\u0433\u0430\u0442\u0438\u0432, \u043a\u043d\u0438\u0433\u0443 \u043e\u0441\u0442\u0430\u0432\u0438\u043b, \u0442\u0430\u043a \u043a\u0430\u043a \u0438\u0437\u043b\u043e\u0436\u0435\u043d\u0438\u0435 \u043c\u0430\u0442\u0435\u0440\u0438\u0430\u043b\u0430 \u0445\u043e\u0440\u043e\u0448\u0435\u0435. \u0414\u043b\u044f \u043d\u043e\u0432\u0438\u0447\u043a\u043e\u0432 \u0438\u043b\u0438 \u043d\u0435\u043c\u043d\u043e\u0433\u043e, \u043d\u043e \u0440\u0430\u0437\u0431\u0438\u0440\u0430\u044e\u0449\u0438\u0445\u0441\u044f \u043b\u044e\u0434\u0435\u0439 \u043f\u043e\u0434\u043e\u0439\u0434\u0435\u0442 \u0438\u0434\u0435\u0430\u043b\u044c\u043d\u043e! \u0421\u043a\u0440\u044b\u0442\u044c',3793)
INSERT INTO COMMENT VALUES(1575,'\u0422\u0440\u043e\u0444\u0438\u043c\u043e\u0432 \u041c\u0438\u0445\u0430\u0438\u043b','2013-01-02','\u041a\u0430\u043a \u043d\u0435 \u043b\u044e\u0431\u043b\u044e \u044f \u0440\u043e\u0441\u0441\u0438\u0439\u0441\u043a\u0438\u0445 \u0430\u0432\u0442\u043e\u0440\u043e\u0432 \u043e\u0431\u0443\u0447\u0430\u044e\u0449\u0435\u0439 \u043b\u0438\u0442\u0435\u0440\u0430\u0442\u0443\u0440\u044b. \u041e\u0441\u043e\u0431\u0435\u043d\u043d\u043e \u0434\u0438\u0437\u0430\u0439\u043d\u0430, \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u044f \u0438 \u043e\u043a\u043e\u043b\u043e\u043a\u043e\u043c\u043f\u044c\u044e\u0442\u0435\u0440\u043d\u044b\u0445 \u0442\u0435\u043c\u0430\u0442\u0438\u043a. \u041f\u0438\u0448\u0443\u0442, \u043a\u0430\u043a-\u0431\u0443\u0434\u0442\u043e \u043b\u0435\u043a\u0446\u0438\u044e \u0432 \u0412\u0423\u0417\u0435 \u0447\u0438\u0442\u0430\u044e\u0442 - \u0431\u0435\u0433\u043b\u043e, \u043c\u043d\u043e\u0433\u043e "\u0432\u043e\u0434\u044b" \u0438 \u043d\u0435\u043f\u043e\u043b\u0435\u0437\u043d\u043e. \u0414\u0430\u043d\u043d\u0430\u044f \u043a\u043d\u0438\u0433\u0430 \u0447\u0435\u043c "\u043f\u0440\u0435\u043a\u0440\u0430\u0441\u043d\u0430": \u0430\u0432\u0442\u043e\u0440 \u043f\u0438\u0448\u0435\u0442, \u0447\u0442\u043e \u0434\u043b\u044f \u0438\u0437\u0443\u0447\u0435\u043d\u0438\u044f php \u043d\u0443\u0436\u043d\u043e \u0437\u043d\u0430\u0442\u044c html \u0438 css. \u0418 \u043d\u0430\u0447\u0438\u043d\u0430\u0435\u0442 "\u0433\u0430\u043b\u043e\u043f\u043e\u043c \u043f\u043e \u0415\u0432\u0440\u043e\u043f\u0430\u043c" \u0438\u0445 "\u043e\u0441\u0432\u0435\u0449\u0430\u0442\u044c". \u0423\u0440\u044b\u0432\u043a\u0430\u043c\u0438 \u043f\u0440\u0435\u0434\u0441\u0442\u0430\u0432\u043b\u0435\u043d\u044b \u0442\u0435\u0433\u0438, \u043d\u0435 \u043f\u043e\u043d\u044f\u0442\u043d\u0430 \u0438\u0445 \u0438\u0437\u0431\u0438\u0440\u0430\u0442\u0435\u043b\u044c\u043d\u043e\u0441\u0442\u044c - \u0430\u0431\u0441\u043e\u043b\u044e\u0442\u043d\u043e \u0445\u0430\u043e\u0442\u0438\u0447\u043d\u043e \u0432\u044b\u0431\u0440\u0430\u043d\u043d\u044b\u0435. \u041f\u0440\u0438\u043c\u0435\u0440\u044b \u0438\u0441\u043f\u043e\u043b\u044c\u0437\u043e\u0432\u0430\u043d\u0438\u044f \u0441\u043e\u043e\u0442\u0432\u0435\u0442\u0441\u0442\u0432\u0443\u044e\u0449\u0438\u0435. \u0421\u043d\u0430\u0447\u0430\u043b\u0430 \u043f\u0440\u0435\u0434\u0441\u0442\u0430\u0432\u043b\u0435\u043d\u044b \u043b\u0438\u0441\u0442\u0438\u043d\u0433\u0438 \u043f\u0440\u0438\u043c\u0435\u0440\u043e\u0432 \u043a\u043e\u0434\u0430, \u0430 \u0432 \u0441\u0430\u043c\u043e\u043c \u043a\u043e\u043d\u0446\u0435 \u043e\u043f\u0438\u0441\u0430\u043d\u0438\u0435 \u0441\u0432\u043e\u0439\u0441\u0442\u0432 \u043e\u0431\u044a\u0435\u043a\u0442\u043e\u0432. \u041b\u043e\u0433\u0438\u0447\u043d\u0435\u0435 \u0431\u044b\u043b\u043e \u0431\u044b \u0434\u0435\u043b\u0430\u0442\u044c \u043d\u0430\u043e\u0431\u043e\u0440\u043e\u0442: \u0437\u043d\u0430\u043a\u043e\u043c\u0438\u0442\u044c \u0441\u043e \u0441\u0432\u043e\u0439\u0441\u0442\u0432\u0430\u043c\u0438 \u043e\u0431\u044a\u0435\u043a\u0442\u043e\u0432, \u0430 \u043f\u043e\u0442\u043e\u043c \u0443\u0436\u0435 \u043f\u043e\u043a\u0430\u0437\u044b\u0432\u0430\u0442\u044c \u0438\u0445 \u043d\u0430 \u043f\u0440\u0430\u043a\u0442\u0438\u043a\u0435. \u041f\u0440\u0438 \u0443\u0442\u0432\u0435\u0440\u0436\u0434\u0435\u043d\u0438\u0438, \u0447\u0442\u043e \u043d\u0435\u043e\u0431\u0445\u043e\u0434\u0438\u043c\u043e \u0437\u043d\u0430\u0442\u044c html \u0438 css, \u044f \u0441\u0447\u0438\u0442\u0430\u044e, \u043d\u0443\u0436\u043d\u043e \u043b\u0438\u0431\u043e \u043f\u043e\u043b\u043d\u043e\u0441\u0442\u044c\u044e \u043e\u0441\u0432\u0435\u0449\u0430\u0442\u044c \u0443\u043a\u0430\u0437\u0430\u043d\u043d\u044b\u0435 \u0442\u0435\u043c\u044b, \u043b\u0438\u0431\u043e \u043d\u0435 \u043f\u0438\u0441\u0430\u0442\u044c \u043e \u043d\u0438\u0445 \u0432\u043e\u0432\u0441\u0435 \u0438 \u043e\u0442\u0441\u044b\u043b\u0430\u0442\u044c \u043a \u0434\u0440\u0443\u0433\u0438\u043c \u0438\u0441\u0442\u043e\u0447\u043d\u0438\u043a\u0430\u043c. \u041d\u0435\u0437\u043d\u0430\u044e\u0449\u0438\u0439 \u0447\u0435\u043b\u043e\u0432\u0435\u043a \u0441\u043e\u0439\u0434\u0435\u0442 \u0441 \u0443\u043c\u0430. \u0421\u043a\u0440\u044b\u0442\u044c',3796)
INSERT INTO COMMENT VALUES(1576,'\u041d\u0430\u0440\u044b\u0448\u043a\u0438\u043d \u0421\u0435\u0440\u0433\u0435\u0439','2013-01-31','\u0424\u043e\u0442\u043e\u0433\u0440\u0430\u0444\u0438\u0438 \u043f\u0435\u0440\u0432\u044b\u0445 \u0441\u0442\u0440\u0430\u043d\u0438\u0446: \u041f\u043e\u043a\u0430\u0437\u0430\u0442\u044c \u0438\u0437\u043e\u0431\u0440\u0430\u0436\u0435\u043d\u0438\u044f \u0421\u043a\u0440\u044b\u0442\u044c \u0438\u0437\u043e\u0431\u0440\u0430\u0436\u0435\u043d\u0438\u044f',3796)
INSERT INTO COMMENT VALUES(1577,'\u041b\u0435\u0430\u043d','2015-01-03','\u0414\u043e\u0431\u0430\u0432\u043b\u044e \u0444\u043e\u0442\u043e\u0433\u0440\u0430\u0444\u0438\u0438 \u0441\u043e\u0434\u0435\u0440\u0436\u0430\u043d\u0438\u044f: \u041f\u043e\u043a\u0430\u0437\u0430\u0442\u044c \u0438\u0437\u043e\u0431\u0440\u0430\u0436\u0435\u043d\u0438\u044f \u0421\u043a\u0440\u044b\u0442\u044c \u0438\u0437\u043e\u0431\u0440\u0430\u0436\u0435\u043d\u0438\u044f',3804)
INSERT INTO COMMENT VALUES(1578,'Marina O.','2015-01-30','\u0414\u043e\u0431\u0430\u0432\u043b\u044e \u0444\u043e\u0442\u043e\u0433\u0440\u0430\u0444\u0438\u0438 \u043a\u043d\u0438\u0433\u0438. \u041f\u043e\u043a\u0430\u0437\u0430\u0442\u044c \u0438\u0437\u043e\u0431\u0440\u0430\u0436\u0435\u043d\u0438\u044f \u0421\u043a\u0440\u044b\u0442\u044c \u0438\u0437\u043e\u0431\u0440\u0430\u0436\u0435\u043d\u0438\u044f',3804)
INSERT INTO COMMENT VALUES(1579,'\u041a\u0438\u0441\u0435\u043b\u0435\u0432\u0430 \u0412\u0430\u043b\u0435\u043d\u0442\u0438\u043d\u0430','2013-01-19','\u0422\u043e\u043b\u0441\u0442\u0430\u044f \u043e\u0431\u043b\u043e\u0436\u043a\u0430, \u0433\u0430\u0437\u0435\u0442\u043d\u044b\u0435 \u0441\u0442\u0440\u0430\u043d\u0438\u0446\u044b, \u043a\u0430\u0447\u0435\u0441\u0442\u0432\u043e \u043f\u0435\u0447\u0430\u0442\u0438 \u0445\u043e\u0440\u043e\u0448\u0435\u0435. \u041a\u043d\u0438\u0433\u0430 \u043d\u0430\u043f\u0438\u0441\u0430\u043d\u0430 \u043a\u0430\u043d\u0434\u0438\u0434\u0430\u0442\u043e\u043c \u0444\u0438\u0437\u0438\u043a\u043e-\u043c\u0430\u0442\u0435\u043c\u0430\u0442\u0438\u0447\u0435\u0441\u043a\u0438\u0445 \u043d\u0430\u0443\u043a, \u0434\u043e\u0446\u0435\u043d\u0442\u043e\u043c, \u0437\u0430\u0432\u0435\u0434\u0443\u044e\u0449\u0438\u043c \u043a\u0430\u0444\u0435\u0434\u0440\u043e\u0439 \u0438\u043d\u0444\u043e\u0440\u043c\u0430\u0446\u0438\u043e\u043d\u043d\u044b\u0445 \u0442\u0435\u0445\u043d\u043e\u043b\u043e\u0433\u0438\u0439 \u0423\u0440\u0430\u043b\u044c\u0441\u043a\u043e\u0433\u043e \u0433\u043e\u0441\u0443\u0434\u0430\u0440\u0441\u0442\u0432\u0435\u043d\u043d\u043e\u0433\u043e \u0442\u0435\u0445\u043d\u0438\u0447\u0435\u0441\u043a\u043e\u0433\u043e \u0443\u043d\u0438\u0432\u0435\u0440\u0441\u0438\u0442\u0435\u0442\u0430. \u041a\u0443\u043f\u0438\u043b\u0438 \u0440\u0435\u0431\u0435\u043d\u043a\u0443 \u0434\u043b\u044f \u0438\u0437\u0443\u0447\u0435\u043d\u0438\u044f \u0421++. \u041f\u043e \u043f\u043e\u0432\u043e\u0434\u0443 \u043f\u043e\u043b\u0435\u0437\u043d\u043e\u0441\u0442\u0438 \u0441\u043e\u0434\u0435\u0440\u0436\u0430\u043d\u0438\u044f \u0438 \u0438\u0437\u043b\u043e\u0436\u0435\u043d\u0438\u044f \u0438\u043d\u0444\u043e\u0440\u043c\u0430\u0446\u0438\u0438, \u0441\u043a\u0430\u0437\u0430\u0442\u044c \u0435\u0449\u0435 \u0442\u0440\u0443\u0434\u043d\u043e, \u0442\u0430\u043a \u043a\u0430\u043a \u0442\u043e\u043b\u044c\u043a\u043e \u043d\u0430\u0447\u0438\u043d\u0430\u0435\u043c \u0438\u0437\u0443\u0447\u0435\u043d\u0438\u0435 \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u044f. \u041f\u043e\u043a\u0430\u0437\u0430\u0442\u044c \u0438\u0437\u043e\u0431\u0440\u0430\u0436\u0435\u043d\u0438\u044f \u0421\u043a\u0440\u044b\u0442\u044c \u0438\u0437\u043e\u0431\u0440\u0430\u0436\u0435\u043d\u0438\u044f',3808)
