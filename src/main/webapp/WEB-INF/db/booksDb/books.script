INSERT INTO COMMENT VALUES(1569,'PGovorun','2015-01-24','\u041a\u0430\u0447\u0435\u0441\u0442\u0432\u043e \u0431\u0443\u043c\u0430\u0433\u0438 - \u043e\u0442\u043b\u0438\u0447\u043d\u043e\u0435, \u043d\u043e \u0447\u0435\u0440\u043d\u043e-\u0431\u0435\u043b\u044b\u0435 \u0438\u043b\u043b\u044e\u0441\u0442\u0440\u0430\u0446\u0438\u0438. \u041a\u043d\u0438\u0433\u0443 \u043c\u043e\u0436\u043d\u043e \u0440\u0435\u043a\u043e\u043c\u0435\u043d\u0434\u043e\u0432\u0430\u0442\u044c \u043a\u0430\u043a \u043e\u0431\u0437\u043e\u0440\u043d\u044b\u0439 \u0443\u0447\u0435\u0431\u043d\u0438\u043a \u043f\u043e HTML5. \u041d\u0435\u043e\u0434\u043d\u043e\u043a\u0440\u0430\u0442\u043d\u043e \u043b\u043e\u0432\u0438\u043b\u0430 \u0441\u0435\u0431\u044f \u043d\u0430 \u043c\u044b\u0441\u043b\u0438, \u0447\u0442\u043e \u043a\u043d\u0438\u0433\u0435 \u043d\u0435 \u0445\u0432\u0430\u0442\u0430\u0435\u0442 \u0433\u043b\u0443\u0431\u0438\u043d\u044b \u0438\u0437\u043b\u043e\u0436\u0435\u043d\u0438\u044f - \u0441\u043b\u0438\u0448\u043a\u043e\u043c \u043f\u043e\u0432\u0435\u0440\u0445\u043d\u043e\u0441\u0442\u043d\u043e, \u0441\u043b\u0438\u0448\u043a\u043e\u043c \u043e\u0434\u043d\u043e\u0431\u043e\u043a\u043e. \u0412 \u043e\u0431\u0449\u0435\u043c, \u043d\u0435 \u0440\u0435\u043a\u043e\u043c\u0435\u043d\u0434\u0443\u044e \u0442\u0435\u043c, \u043a\u0442\u043e \u0443\u0436\u0435 \u0445\u043e\u0442\u044c \u043a\u0430\u043a-\u0442\u043e \u0437\u043d\u0430\u043a\u043e\u043c \u0441 \u044d\u0442\u043e\u0439 \u0442\u0435\u043c\u043e\u0439.',3785)
