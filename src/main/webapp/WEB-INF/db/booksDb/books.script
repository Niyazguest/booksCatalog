INSERT INTO BOOK VALUES(3319,470658,'\u0411\u0435\u0440\u0435\u0437\u0438\u043d, \u0411\u0435\u0440\u0435\u0437\u0438\u043d: C \u0438 C++. \u041d\u0430\u0447\u0430\u043b\u044c\u043d\u044b\u0439 \u043a\u0443\u0440\u0441','\u0411\u0435\u0440\u0435\u0437\u0438\u043d \u0411\u043e\u0440\u0438\u0441 \u0418\u0432\u0430\u043d\u043e\u0432\u0438\u0447, \u0411\u0435\u0440\u0435\u0437\u0438\u043d \u0421\u0435\u0440\u0433\u0435\u0439 \u0411\u043e\u0440\u0438\u0441\u043e\u0432\u0438\u0447','\u0413\u043e\u043b\u0443\u0431\u0435\u0432 \u041e. \u0410.','\u0414\u0438\u0430\u043b\u043e\u0433-\u041c\u0418\u0424\u0418, 2014 \u0433.',318.0E0,'5-86404-075-4','280 (\u0413\u0430\u0437\u0435\u0442\u043d\u0430\u044f)','\u0422\u0438\u043f \u043e\u0431\u043b\u043e\u0436\u043a\u0438: \u043e\u0431\u043b - \u043c\u044f\u0433\u043a\u0438\u0439 \u043f\u0435\u0440\u0435\u043f\u043b\u0435\u0442 (\u043a\u0440\u0435\u043f\u043b\u0435\u043d\u0438\u0435 \u0441\u043a\u0440\u0435\u043f\u043a\u043e\u0439 \u0438\u043b\u0438 \u043a\u043b\u0435\u0435\u043c) \u0418\u043b\u043b\u044e\u0441\u0442\u0440\u0430\u0446\u0438\u0438: \u0427\u0435\u0440\u043d\u043e-\u0431\u0435\u043b\u044b\u0435','192 \u0433','200x142x12 \u043c\u043c','\u0410\u043d\u043d\u043e\u0442\u0430\u0446\u0438\u044f \u043a \u043a\u043d\u0438\u0433\u0435 "C \u0438 C++. \u041d\u0430\u0447\u0430\u043b\u044c\u043d\u044b\u0439 \u043a\u0443\u0440\u0441" \u041a\u043d\u0438\u0433\u0430 \u044f\u0432\u043b\u044f\u0435\u0442\u0441\u044f \u0443\u0447\u0435\u0431\u043d\u044b\u043c \u043f\u043e\u0441\u043e\u0431\u0438\u0435\u043c \u043f\u043e \u044f\u0437\u044b\u043a\u0430\u043c \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u044f \u0421 \u0438 \u0421++. \u041e\u043d\u0430 \u043c\u043e\u0436\u0435\u0442 \u0431\u044b\u0442\u044c \u0438\u0441\u043f\u043e\u043b\u044c\u0437\u043e\u0432\u0430\u043d\u0430 \u0434\u043b\u044f \u0438\u0437\u0443\u0447\u0435\u043d\u0438\u044f \u044f\u0437\u044b\u043a\u0430 \u0421, \u043a\u0430\u