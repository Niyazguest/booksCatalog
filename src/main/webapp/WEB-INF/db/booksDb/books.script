INSERT INTO COMMENT VALUES(1445,'\u0427\u0438\u0442\u0430\u0442\u0435\u043b\u044c \u0423\u0432\u043b\u0435\u0447\u0435\u043d\u043d\u044b\u0439','2015-01-05','\u0412 \u043a\u043d\u0438\u0433\u0435 \u043e\u043f\u0438\u0441\u044b\u0432\u0430\u0435\u0442\u0441\u044f \u0441\u043e\u0437\u0434\u0430\u043d\u0438\u0435 \u0432\u0435\u0431-\u043f\u0440\u0438\u043b\u043e\u0436\u0435\u043d\u0438\u044f \u043f\u0440\u0438\u043c\u0438\u0442\u0438\u0432\u043d\u043e\u0433\u043e TO-DO \u043b\u0438\u0441\u0442\u0430 \u0441 \u0438\u0441\u043f\u043e\u043b\u044c\u0437\u043e\u0432\u0430\u043d\u0438\u0435\u043c \u0441\u043e\u0432\u0440\u0435\u043c\u0435\u043d\u043d\u044b\u0445 \u0442\u0435\u0445\u043d\u043e\u043b\u043e\u0433\u0438\u0439 \u0438 \u043c\u0438\u043d\u0438-\u043f\u0440\u0438\u043b\u043e\u0436\u0435\u043d\u0438\u044f \u0434\u043b\u044f \u0437\u043d\u0430\u043a\u043e\u043c\u0441\u0442\u0432\u0430 \u0441 JS. \u041a\u0430\u0436\u0434\u0430\u044f \u0442\u0435\u0445\u043d\u043e\u043b\u043e\u0433\u0438\u044f \u0437\u0430\u0442\u0440\u043e\u043d\u0443\u0442\u0430 \u043e\u0447\u0435\u043d\u044c \u043f\u043e\u0432\u0435\u0440\u0445\u043d\u043e\u0441\u0442\u043d\u043e, \u0432\u044b\u0434\u0435\u0440\u043d\u0443\u0442\u044b \u043a\u0443\u0441\u043a\u0438 \u0442\u043e\u043b\u044c\u043a\u043e \u0434\u043b\u044f \u0440\u0435\u0448\u0435\u043d\u0438\u044f \u043f\u043e\u0441\u0442\u0430\u0432\u043b\u0435\u043d\u043d\u043e\u0439 \u0437\u0430\u0434\u0430\u0447\u0438. \u0413\u043b\u0443\u0431\u043e\u043a\u0438\u0445 \u0437\u043d\u0430\u043d\u0438\u0439 \u043f\u043e \u0442\u0435\u043c\u0435, \u043a\u0430\u043a \u043d\u0430\u043f\u0438\u0441\u0430\u043d\u043e \u0432 \u043e\u043f\u0438\u0441\u0430\u043d\u0438\u0438, \u043f\u043e\u0441\u043b\u0435 \u043f\u0440\u043e\u0447\u0442\u0435\u043d\u0438\u044f \u0432\u044b \u044f\u0432\u043d\u043e \u043d\u0435 \u043f\u043e\u043b\u0443\u0447\u0438\u0442\u0435, \u043f\u0440\u043e\u0441\u0442\u043e \u043e\u0437\u043d\u0430\u043a\u043e\u043c\u0438\u0442\u0435\u0441\u044c \u0441 \u0430\u043a\u0442\u0443\u0430\u043b\u044c\u043d\u044b\u043c\u0438 \u0442\u0435\u0445\u043d\u043e\u043b\u043e\u0433\u0438\u044f\u043c\u0438 (Git, HTML5, CSS3, JS, jQuery, JSON, Node.js, Express.js, MongoDB/Redis), \u043f\u043e\u0434\u0447\u0435\u0440\u043a\u0438\u0432\u0430\u044e "\u043e\u0437\u043d\u0430\u043a\u043e\u043c\u0438\u0442\u0435\u0441\u044c", \u0430 \u043d\u0435 \u0431\u0443\u0434\u0435\u0442\u0435 \u0443\u0432\u0435\u0440\u0435\u043d\u043d\u043e \u0438\u0441\u043f\u043e\u043b\u044c\u0437\u043e\u0432\u0430\u0442\u044c \u0432\u0441\u0451 \u0432\u044b\u0448\u0435\u043e\u043f\u0438\u0441\u0430\u043d\u043d\u043e\u0435. \u041a\u043d\u0438\u0433\u0430 \u043c\u043e\u0436\u0435\u0442 \u0442\u043e\u043b\u044c\u043a\u043e \u043f\u043e\u0434\u0442\u043e\u043b\u043a\u043d\u0443\u0442\u044c \u043d\u0430 \u0434\u0430\u043b\u044c\u043d\u0435\u0439\u0448\u0435\u0435 \u0438\u0437\u0443\u0447\u0435\u043d\u0438\u0435 \u043f\u0435\u0440\u0435\u0447\u0438\u0441\u043b\u0435\u043d\u043d\u043e\u0433\u043e. \u041b\u044e\u0434\u044f\u043c \u0441 \u043e\u043f\u044b\u0442\u043e\u043c \u0434\u0430\u043d\u043d\u0430\u044f \u043a\u043d\u0438\u0433\u0430 \u0432\u0440\u044f\u0434 \u043b\u0438 \u0431\u0443\u0434\u0435\u0442 \u043f\u043e\u043b\u0435\u0437\u043d\u0430, \u0443\u0436 \u043e\u0447\u0435\u043d\u044c \u043e\u043d\u0430 \u0434\u043b\u044f \u043d\u043e\u0432\u0438\u0447\u043a\u043e\u0432. \u0421\u043a\u0440\u044b\u0442\u044c',3336)
INSERT INTO COMMENT VALUES(1446,'*** \u041d\u0430\u0442\u0430\u043b\u0438\u044f','2015-01-19','\u041a\u043d\u0438\u0433\u0430 \u043d\u0430 \u0440\u0435\u0434\u043a\u043e\u0441\u0442\u044c \u043f\u043e\u043b\u0435\u0437\u043d\u0430\u044f \u0432 \u0432\u043e\u043f\u0440\u043e\u0441\u0430\u0445 \u043e\u0440\u0433\u0430\u043d\u0438\u0437\u0430\u0446\u0438\u0438 \u0440\u0430\u0431\u043e\u0442 \u043f\u043e \u0432\u043d\u0435\u0434\u0440\u0435\u043d\u0438\u044e (\u043f\u0440\u0438\u0447\u0435\u043c, \u044d\u0442\u043e \u043d\u0435 \u043e\u0431\u044f\u0437\u0430\u0442\u0435\u043b\u044c\u043d\u043e \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u044b 1\u0421, \u044d\u0442\u043e \u043c\u043e\u0433\u0443\u0442 \u0431\u044b\u0442\u044c \u043b\u044e\u0431\u044b\u0435 \u0434\u0440\u0443\u0433\u0438\u0435 \u043a\u0440\u0443\u043f\u043d\u044b\u0435 \u043f\u0440\u043e\u0435\u043a\u0442\u044b, \u043a\u043e\u0442\u043e\u0440\u044b\u043c\u0438 \u0437\u0430\u043d\u0438\u043c\u0430\u0435\u0442\u0441\u044f \u0431\u043e\u043b\u044c\u0448\u0435, \u0447\u0435\u043c 1-2 \u0447\u0435\u043b\u043e\u0432\u0435\u043a\u0430). \u0410 \u0434\u043b\u044f \u0432\u043d\u0435\u0434\u0440\u0435\u043d\u0438\u044f \u043f\u0440\u043e\u0434\u0443\u043a\u0442\u043e\u0432 1\u0421 \u2013 \u044d\u0442\u043e \u043f\u0440\u044f\u043c\u043e \u0433\u043e\u0442\u043e\u0432\u044b\u0439 \u043f\u0440\u0430\u043a\u0442\u0438\u0447\u0435\u0441\u043a\u0438\u0439 \u043d\u0430\u043f\u0440\u0430\u0432\u043b\u0435\u043d\u043d\u044b\u0439 \u0430\u043b\u0433\u043e\u0440\u0438\u0442\u043c \u0434\u0435\u0439\u0441\u0442\u0432\u0438\u0439. \u041b\u044e\u0431\u043e\u0439, \u043a\u0442\u043e \u0437\u0430\u043d\u0438\u043c\u0430\u0435\u0442\u0441\u044f \u0432\u043d\u0435\u0434\u0440\u0435\u043d\u0438\u0435\u043c \u0447\u0435\u0433\u043e-\u043b\u0438\u0431\u043e \u043a\u0440\u0443\u043f\u043d\u043e\u0433\u043e, \u0432 \u043f\u0440\u0438\u043d\u0446\u0438\u043f\u0435 \u0434\u043b\u044f \u0441\u0435\u0431\u044f \u0432 \u0433\u043e\u043b\u043e\u0432\u0435 \u0434\u0435\u0440\u0436\u0438\u0442 \u00ab\u043a\u0430\u0440\u0442\u0438\u043d\u043a\u0443\u00bb, \u043a\u0430\u043a \u043e\u043d \u044d\u0442\u043e \u0431\u0443\u0434\u0435\u0442 \u0434\u0435\u043b\u0430\u0442\u044c. \u041d\u043e, \u043a\u0430\u043a \u0438 \u0432\u043e \u043c\u043d\u043e\u0433\u0438\u0445 \u0432\u043e\u043f\u0440\u043e\u0441\u0430\u0445, \u043c\u044b \u0447\u0430\u0441\u0442\u043e \u0438\u0437\u043e\u0431\u0440\u0435\u0442\u0430\u0435\u043c \u0432\u0435\u043b\u043e\u0441\u0438\u043f\u0435\u0434\u044b \u0438 \u043d\u0430\u0441\u0442\u0443\u043f\u0430\u0435\u043c \u043d\u0430 \u0433\u0440\u0430\u0431\u043b\u0438, \u043d\u0430 \u043a\u043e\u0442\u043e\u0440\u044b\u0445 \u0443\u0436\u0435 \u043f\u043e\u0442\u0440\u0435\u043d\u0438\u0440\u043e\u0432\u0430\u043b\u0438\u0441\u044c \u0434\u0440\u0443\u0433\u0438\u0435. \u0420\u0435\u043a\u043e\u043c\u0435\u043d\u0434\u0443\u044e \u043f\u0440\u043e\u0447\u0438\u0442\u0430\u0442\u044c \u0435\u0435 \u0438 \u0442\u0435\u043c, \u043a\u0442\u043e \u043f\u043b\u0430\u043d\u0438\u0440\u0443\u0435\u0442 \u0432\u043d\u0435\u0434\u0440\u044f\u0442\u044c 1\u0421, \u043d\u0435\u0437\u0430\u0432\u0438\u0441\u0438\u043c\u043e \u043e\u0442 \u0442\u043e\u0433\u043e, \u0441\u0432\u043e\u0438\u043c\u0438 \u0441\u0438\u043b\u0430\u043c\u0438 \u044d\u0442\u043e \u0431\u0443\u0434\u0435\u0442 \u043f\u0440\u043e\u0438\u0437\u0432\u043e\u0434\u0438\u0442\u044c\u0441\u044f, \u0438\u043b\u0438 \u0441\u0438\u043b\u0430\u043c\u0438 \u0441\u0442\u043e\u0440\u043e\u043d\u043d\u0435\u0439 \u043e\u0440\u0433\u0430\u043d\u0438\u0437\u0430\u0446\u0438\u0438, \u0438 \u0442\u0435\u043c, \u043a\u0442\u043e \u0432\u043d\u0435\u0434\u0440\u044f\u0435\u0442 \u0434\u0440\u0443\u0433\u0438\u0435 \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u043d\u044b\u0435 \u0438 \u043f\u0440\u043e\u0434\u0443\u043a\u0442\u044b. \u041d\u0435\u043a\u043e\u0442\u043e\u0440\u044b\u0435 \u0432\u043e\u043f\u0440\u043e\u0441\u044b, \u043e\u043f\u0438\u0441\u0430\u043d\u043d\u044b\u0435 \u0432 \u0434\u0430\u043d\u043d\u043e\u0439 \u043a\u043d\u0438\u0433\u0435 (\u043d\u0430\u043f\u0440\u0438\u043c\u0435\u0440, \u043f\u043e\u0441\u0442\u0430\u043d\u043e\u0432\u043a\u0430 \u0446\u0435\u043b\u0435\u0439, \u043e\u0440\u0433\u0430\u043d\u0438\u0437\u0430\u0446\u0438\u044f \u044d\u0444\u0444\u0435\u043a\u0442\u0438\u0432\u043d\u043e\u0439 \u043a\u043e\u043c\u0430\u043d\u0434\u044b), \u0434\u0430\u044e\u0442 \u043f\u0438\u0449\u0443 \u0434\u043b\u044f \u0440\u0430\u0437\u043c\u044b\u0448\u043b\u0435\u043d\u0438\u044f \u043e\u0431 \u043e\u0440\u0433\u0430\u043d\u0438\u0437\u0430\u0446\u0438\u0438 \u0440\u0430\u0431\u043e\u0442 \u0438 \u0432 \u043f\u0440\u043e\u0447\u0438\u0445 \u043e\u0431\u043b\u0430\u0441\u0442\u044f\u0445, \u043f\u043e\u044d\u0442\u043e\u043c\u0443 \u043a\u043d\u0438\u0433\u0430 \u043f\u043e\u043b\u0435\u0437\u043d\u0430 \u0440\u0443\u043a\u043e\u0432\u043e\u0434\u0438\u0442\u0435\u043b\u044f\u043c \u0441\u0440\u0435\u0434\u043d\u0435\u0433\u043e \u0438 \u0432\u044b\u0441\u0448\u0435\u0433\u043e \u0443\u0440\u043e\u0432\u043d\u044f. \u0421\u043a\u0440\u044b\u0442\u044c',3337)
INSERT INTO COMMENT VALUES(1447,'adeliya','2015-01-03','\u041a\u043d\u0438\u0433\u0430 \u043f\u043e\u043b\u0435\u0437\u043d\u0430 \u0442\u0435\u043c, \u0447\u0442\u043e node.js \u0440\u0430\u0437\u0431\u0438\u0440\u0430\u0435\u0442\u0441\u044f \u0441 \u043e\u0441\u043d\u043e\u0432. \u041a\u0430\u0440\u043a\u0430\u0441\u044b \u0434\u0430\u044e\u0442\u0441\u044f \u043f\u043e\u0437\u0436\u0435. \u0420\u0435\u043a\u043e\u043c\u0435\u043d\u0434\u0443\u044e \u043a \u043f\u0440\u043e\u0447\u0442\u0435\u043d\u0438\u044e',3339)
INSERT INTO COMMENT VALUES(1448,'\u0415\u0434\u0438\u0445\u0430\u043d\u043e\u0432\u0430 \u0410\u043b\u0438\u0441\u0430','2014-01-17','\u0424\u043e\u0442\u043e \u0432\u043d\u0435\u0448\u043d\u0435\u0433\u043e \u0432\u0438\u0434\u0430 \u0438 \u0441\u0442\u0440\u0430\u043d\u0438\u0446: \u041f\u043e\u043a\u0430\u0437\u0430\u0442\u044c \u0438\u0437\u043e\u0431\u0440\u0430\u0436\u0435\u043d\u0438\u044f \u0421\u043a\u0440\u044b\u0442\u044c \u0438\u0437\u043e\u0431\u0440\u0430\u0436\u0435\u043d\u0438\u044f',3339)
INSERT INTO COMMENT VALUES(1449,'\u041d\u043e\u0440\u0435\u043d\u043a\u043e \u0411\u043e\u0433\u0434\u0430\u043d','2015-01-26','\u0417\u0430\u043c\u0435\u0447\u0430\u0442\u0435\u043b\u044c\u043d\u043e\u0435 \u0432\u0432\u0435\u0434\u0435\u043d\u0438\u0435 \u0432 \u0443\u0434\u0438\u0432\u0438\u0442\u0435\u043b\u044c\u043d\u044b\u0439 \u043c\u0438\u0440 Common Lisp. \u041d\u0435 \u0441\u043c\u043e\u0442\u0440\u044f \u043d\u0430 \u0432\u043e\u0437\u0440\u0430\u0441\u0442 (\u0441\u0442\u0430\u043d\u0434\u0430\u0440\u0442 \u0443\u0442\u0432\u0435\u0440\u0436\u0434\u0451\u043d \u0432 1994), \u044d\u0442\u043e\u0442 \u044f\u0437\u044b\u043a \u0438 \u043f\u043e \u0441\u0435\u0439 \u0434\u0435\u043d\u044c \u0434\u0438\u043d\u0430\u043c\u0438\u0447\u043d\u043e \u0440\u0430\u0437\u0432\u0438\u0432\u0430\u0435\u0442\u0441\u044f \u0431\u043b\u0430\u0433\u043e\u0434\u0430\u0440\u044f \u0443\u0434\u0438\u0432\u0438\u0442\u0435\u043b\u044c\u043d\u043e \u0433\u0438\u0431\u043a\u043e\u0439 \u0441\u0438\u0441\u0442\u0435\u043c\u0435 \u0440\u0435\u0444\u043b\u0435\u043a\u0441\u0438\u0438 \u0438 \u043c\u0435\u0442\u0430\u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u044f. \u041a\u043d\u0438\u0433\u0430 \u041f\u0438\u0442\u0435\u0440\u0430 \u0421\u0430\u0439\u0431\u0435\u043b\u044f \u043e\u0442\u043d\u044e\u0434\u044c \u043d\u0435 \u0437\u0430\u043d\u0443\u0434\u043d\u044b\u0439 \u0443\u0447\u0435\u0431\u043d\u0438\u043a, \u044d\u0442\u043e \u043b\u0451\u0433\u043a\u0430\u044f \u0438 \u0437\u0430\u043d\u0438\u043c\u0430\u0442\u0435\u043b\u044c\u043d\u0430\u044f \u043f\u0440\u043e\u0433\u0443\u043b\u043a\u0430 \u043f\u043e \u0441\u0430\u043c\u043e\u043c\u0443 \u0431\u043e\u0433\u0430\u0442\u043e\u043c\u0443 \u044f\u0437\u044b\u043a\u0443 \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u044f \u0438\u0437 \u043a\u043e\u0433\u0434\u0430 \u043b\u0438\u0431\u043e \u0441\u043e\u0437\u0434\u0430\u043d\u043d\u044b\u0445. \u041d\u0430\u0437\u0432\u0430\u043d\u0438\u0435 \u043a\u043d\u0438\u0433\u0438 \u043f\u0440\u0430\u0432\u0434\u0438\u0432\u043e, \u0442\u0430\u043a \u043a\u0430\u043a \u041f\u0438\u0442\u0435\u0440 \u0440\u0430\u0441\u0441\u043c\u0430\u0442\u0440\u0438\u0432\u0430\u0435\u0442 \u0434\u0435\u0439\u0441\u0442\u0432\u0438\u0442\u0435\u043b\u044c\u043d\u043e \u043f\u0440\u0430\u043a\u0442\u0438\u0447\u0435\u0441\u043a\u0438\u0435 \u0430\u0441\u043f\u0435\u043a\u0442\u044b \u0440\u0430\u0431\u043e\u0442\u044b \u0441 \u044f\u0437\u044b\u043a\u043e\u043c: \u0432\u0437\u0430\u0438\u043c\u043e\u0434\u0435\u0439\u0441\u0442\u0432\u0438\u0435 \u0441 \u0440\u0435\u0430\u043b\u044c\u043d\u044b\u043c \u043c\u0438\u0440\u043e\u043c, \u043d\u0430\u043f\u0438\u0441\u0430\u043d\u0438\u0435 \u0438\u043d\u0442\u0435\u0440\u043d\u0435\u0442 \u0441\u0435\u0440\u0432\u0435\u0440\u043e\u0432, \u0447\u0442\u0435\u043d\u0438\u0435 \u0438 \u0437\u0430\u043f\u0438\u0441\u044c \u0431\u0438\u043d\u0430\u0440\u043d\u044b\u0445 \u0444\u0430\u0439\u043b\u043e\u0432, \u0444\u043e\u0440\u043c\u0430\u0442\u0438\u0440\u043e\u0432\u0430\u043d\u043d\u044b\u0439 \u0432\u044b\u0432\u043e\u0434. \u041a\u0440\u043e\u043c\u0435 \u043f\u0440\u0430\u043a\u0442\u0438\u0447\u0435\u0441\u043a\u0438\u0445 \u0432\u0435\u0449\u0435\u0439 \u0432 \u043a\u043d\u0438\u0433\u0435 \u043f\u043e\u0434\u0440\u043e\u0431\u043d\u043e \u0438 \u0434\u043e\u0441\u0442\u0443\u043f\u043d\u044b\u043c \u044f\u0437\u044b\u043a\u043e\u043c \u0438\u0437\u043b\u043e\u0436\u0435\u043d\u044b \u0432\u0441\u0435 \u0441\u043b\u043e\u0436\u043d\u044b\u0435 \u043c\u043e\u043c\u0435\u043d\u0442\u044b Common Lisp, \u0432\u043a\u043b\u044e\u0447\u0430\u044f \u0443\u043d\u0438\u043a\u0430\u043b\u044c\u043d\u0443\u044e \u0441\u0438\u0441\u0442\u0435\u043c\u0443 "\u0443\u0441\u043b\u043e\u0432\u0438\u0439" (\u0430\u043d\u0430\u043b\u043e\u0433 "\u0438\u0441\u043a\u043b\u044e\u0447\u0435\u043d\u0438\u0439" \u0432 \u0434\u0440\u0443\u0433\u0438\u0445 \u044f\u0437\u044b\u043a\u0430\u0445), \u043c\u043e\u0433\u0443\u0449\u0435\u0441\u0442\u0432\u0435\u043d\u043d\u0443\u044e \u043c\u0430\u043a\u0440\u043e\u0441\u0438\u0441\u0442\u0435\u043c\u0443, \u043a\u043e\u0442\u043e\u0440\u0430\u044f \u0434\u0435\u043b\u0430\u0435\u0442 \u0438\u0437 Lisp \u0433\u0438\u0431\u043a\u0438\u0439 "\u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u0438\u0440\u0443\u0435\u043c\u044b\u0439 \u044f\u0437\u044b\u043a \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u044f", \u043e\u0441\u043e\u0431\u0435\u043d\u043d\u043e\u0441\u0442\u0438 \u0444\u043e\u0440\u043c\u0430\u0442\u0438\u0440\u043e\u0432\u0430\u043d\u043d\u043e\u0433\u043e \u0432\u044b\u0432\u043e\u0434\u0430 \u0441 \u043f\u043e\u043c\u043e\u0449\u044c\u044e \u043a\u043e\u043c\u0430\u043d\u0434\u044b format. \u0411\u0435\u0437\u0443\u0441\u043b\u043e\u0432\u043d\u043e \u044d\u0442\u0430 \u043a\u043d\u0438\u0433\u0430 \u044f\u0432\u043b\u044f\u0435\u0442\u0441\u044f \u0448\u0435\u0434\u0435\u0432\u0440\u043e\u043c \u0432 \u0436\u0430\u043d\u0440\u0435 \u0440\u0443\u043a\u043e\u0432\u043e\u0434\u0441\u0442\u0432 \u043f\u043e \u044f\u0437\u044b\u043a\u0430\u043c \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u044f, \u043a\u0430\u043a \u0438 \u0441\u0430\u043c \u044f\u0437\u044b\u043a, \u043a\u043e\u0442\u043e\u0440\u044b\u0439 \u043e\u043d\u0430 \u043e\u043f\u0438\u0441\u044b\u0432\u0430\u0435\u0442. \u041a\u0430\u043a \u043e\u0434\u0438\u043d \u0438\u0437 \u0441\u0442\u043e\u0440\u043e\u043d\u043d\u0438\u043a\u043e\u0432 Common Lisp, \u0438\u0441\u043f\u043e\u043b\u044c\u0437\u043e\u0432\u0430\u0432\u0448\u0438\u0445 \u0435\u0433\u043e \u0432 \u0440\u0435\u0430\u043b\u044c\u043d\u044b\u0445 \u043f\u0440\u043e\u0435\u043a\u0442\u0430\u0445, \u043c\u043e\u0433\u0443 \u0441\u043a\u0430\u0437\u0430\u0442\u044c, \u0447\u0442\u043e \u044d\u0442\u043e \u0441\u0430\u043c\u0430\u044f \u043f\u043e\u043b\u0435\u0437\u043d\u0430\u044f \u043a\u043d\u0438\u0433\u0430 \u0438\u0437 \u0432\u0441\u0435\u0445 \u0440\u0430\u043d\u0435\u0435 \u0432\u0441\u0442\u0440\u0435\u0447\u0430\u0432\u0448\u0438\u0445\u0441\u044f \u043c\u043e\u043d\u043e\u0433\u0440\u0430\u0444\u0438\u0439, \u0434\u0430\u0436\u0435 \u043a\u043d\u0438\u0433\u0438 \u041f\u043e\u043b\u0430 \u0413\u0440\u044d\u043c\u0430 (ANSI Common Lisp \u0438 On Lisp) \u043d\u0435 \u043c\u043e\u0433\u0443\u0442 \u0441\u043e\u0440\u0435\u0432\u043d\u043e\u0432\u0430\u0442\u044c\u0441\u044f \u0441 \u043d\u0435\u0439. \u041a\u0440\u043e\u043c\u0435 \u0442\u043e\u0433\u043e \u0441\u0435\u0433\u043e\u0434\u043d\u044f \u0438\u0437\u0434\u0430\u043d\u0438\u0435 \u043d\u0430 \u0440\u0443\u0441\u0441\u043a\u043e\u043c \u044f\u0437\u044b\u043a\u0435 \u043f\u0440\u043e\u0434\u0430\u0451\u0442\u0441\u044f \u043f\u043e \u0434\u043e\u0441\u0442\u0443\u043f\u043d\u043e\u0439 \u0446\u0435\u043d\u0435, \u0437\u0430\u0440\u0443\u0431\u0435\u0436\u043d\u043e\u0435 \u0438\u0437\u0434\u0430\u043d\u0438\u0435 \u0431\u044b\u043b\u043e \u0442\u0440\u0443\u0434\u043d\u043e \u0434\u043e\u0441\u0442\u0430\u0442\u044c \u0438 \u043e\u043d\u043e \u0438\u043c\u0435\u043b\u043e \u0432\u044b\u0441\u043e\u043a\u0443\u044e \u0446\u0435\u043d\u0443 \u0434\u043b\u044f \u043e\u0442\u0435\u0447\u0435\u0441\u0442\u0432\u0435\u043d\u043d\u043e\u0433\u043e \u0447\u0438\u0442\u0430\u0442\u0435\u043b\u044f. \u041d\u0430\u0441\u043b\u0430\u0436\u0434\u0430\u0435\u043c\u0441\u044f \u0445\u043e\u0440\u043e\u0448\u0435\u0439 \u043a\u043d\u0438\u0433\u043e\u0439, \u044f \u0441\u0432\u043e\u0439 \u044d\u043a\u0437\u0435\u043c\u043f\u043b\u044f\u0440 \u0443\u0436\u0435 \u0437\u0430\u043a\u0430\u0437\u0430\u043b. :-) \u0421\u043a\u0440\u044b\u0442\u044c',3342)
INSERT INTO COMMENT VALUES(1450,'\u041c\u043e\u0436\u0430\u0439\u0441\u043a\u0430\u044f \u041d\u0430\u0442\u0430\u043b\u044c\u044f \u0412\u0430\u043b\u0435\u0440\u044c\u0435\u0432\u043d\u0430','2014-01-05','\u041a\u043d\u0438\u0433\u0430 \u0445\u043e\u0440\u043e\u0448\u0430\u044f,\u043d\u043e \u0442\u043e\u043b\u044c\u043a\u043e \u0434\u043b\u044f \u0441\u0442\u0430\u0440\u0442\u0430 \u0432 \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u0438 \u043d\u0430 PHP \u0438 \u0434\u043b\u044f \u0438\u0437\u0443\u0447\u0435\u043d\u0438\u044f \u0435\u0433\u043e \u0430\u0437\u043e\u0432. \u0425\u043e\u0440\u043e\u0448\u0430 \u0442\u0430\u043a\u0436\u0435 \u0434\u043b\u044f \u0442\u043e\u0433\u043e, \u0447\u0442\u043e\u0431\u044b \u0431\u044b\u0441\u0442\u0440\u043e "\u0432\u0441\u043f\u043e\u043c\u043d\u0438\u0442\u044c" \u044f\u0437\u044b\u043a. \u041f\u043e \u0445\u043e\u0434\u0443 \u043a\u043d\u0438\u0438\u0433\u0438 \u0440\u0430\u0437\u0440\u0430\u0431\u0430\u0442\u044b\u0432\u0430\u0435\u0442\u0441\u044f \u043d\u0435\u0431\u043e\u043b\u044c\u0448\u043e\u0439 \u043f\u0440\u043e\u0435\u043a\u0442. \u041a\u043d\u0438\u0433\u0430 \u043d\u0430\u043f\u0438\u0441\u0430\u043d\u0430 \u043f\u043e\u043d\u044f\u0442\u043d\u044b\u043c, \u043f\u0440\u043e\u0441\u0442\u044b\u043c \u044f\u0437\u044b\u043a\u043e\u043c, \u0441 \u043f\u0440\u0438\u043c\u0435\u0440\u0430\u043c\u0438.\u0418\u0441\u0445\u043e\u0434\u043d\u0438\u043a\u0438 \u043a \u043a\u043d\u0438\u0433\u0435 \u043c\u043e\u0436\u043d\u043e \u0441\u043a\u0430\u0447\u0430\u0442\u044c \u0432 \u0418\u043d\u0442\u0435\u0440\u043d\u0435\u0442\u0435. \u041d\u0430 \u043f\u043e\u043b\u043d\u043e\u0446\u0435\u043d\u043d\u043e\u0435 \u0440\u0443\u043a\u043e\u0432\u043e\u0434\u0441\u0442\u0432\u043e \u043f\u043e \u044f\u0437\u044b\u043a\u0443 \u043d\u0435 \u043f\u0440\u0435\u0442\u0435\u043d\u0434\u0443\u0435\u0442.',3344)
INSERT INTO COMMENT VALUES(1451,'\u0415\u0434\u0438\u0445\u0430\u043d\u043e\u0432\u0430 \u0410\u043b\u0438\u0441\u0430','2014-01-09','\u0424\u043e\u0442\u043e \u0432\u043d\u0435\u0448\u043d\u0435\u0433\u043e \u0432\u0438\u0434\u0430 \u0438 \u0441\u0442\u0440\u0430\u043d\u0438\u0446: \u041f\u043e\u043a\u0430\u0437\u0430\u0442\u044c \u0438\u0437\u043e\u0431\u0440\u0430\u0436\u0435\u043d\u0438\u044f \u0421\u043a\u0440\u044b\u0442\u044c \u0438\u0437\u043e\u0431\u0440\u0430\u0436\u0435\u043d\u0438\u044f',3344)
INSERT INTO COMMENT VALUES(1452,'\u0415\u0434\u0438\u0445\u0430\u043d\u043e\u0432\u0430 \u0410\u043b\u0438\u0441\u0430','2014-01-09','\u0424\u043e\u0442\u043e \u0432\u043d\u0435\u0448\u043d\u0435\u0433\u043e \u0432\u0438\u0434\u0430 \u0438 \u0441\u0442\u0440\u0430\u043d\u0438\u0446: \u041f\u043e\u043a\u0430\u0437\u0430\u0442\u044c \u0438\u0437\u043e\u0431\u0440\u0430\u0436\u0435\u043d\u0438\u044f \u0421\u043a\u0440\u044b\u0442\u044c \u0438\u0437\u043e\u0431\u0440\u0430\u0436\u0435\u043d\u0438\u044f',3345)
INSERT INTO COMMENT VALUES(1453,'\u041f\u0443\u0437\u0438\u043a\u043e\u0432 \u0414\u043c\u0438\u0442\u0440\u0438\u0439','2014-01-25','\u041a\u043d\u0438\u0433\u0430 \u043e\u0442\u043b\u0438\u0447\u043d\u0430\u044f, \u043d\u043e \u043a\u0430\u0447\u0435\u0441\u0442\u0432\u043e \u043f\u0435\u0440\u0435\u0432\u043e\u0434\u0430 \u043e\u0442\u043a\u0440\u043e\u0432\u0435\u043d\u043d\u043e \u0441\u043b\u0430\u0431\u043e\u0435. \u041f\u0435\u0440\u0435\u0432\u043e\u0434\u0438\u043b \u0447\u0435\u043b\u043e\u0432\u0435\u043a \u044f\u0432\u043d\u043e \u0434\u0430\u043b\u0435\u043a\u0438\u0439 \u043e\u0442 \u043f\u0440\u0435\u0434\u043c\u0435\u0442\u043d\u043e\u0439 \u043e\u0431\u043b\u0430\u0441\u0442\u0438. \u041f\u0435\u0440\u0438\u043e\u0434\u0438\u0447\u0435\u0441\u043a\u0438 \u0432\u043e\u0437\u043d\u0438\u043a\u0430\u0435\u0442 \u043e\u0449\u0443\u0449\u0435\u043d\u0438\u0435, \u0447\u0442\u043e \u0447\u0438\u0442\u0430\u0435\u0448\u044c \u0442\u0435\u043a\u0441\u0442 \u043f\u043e\u0434\u0441\u0442\u0440\u043e\u0447\u043d\u0438\u043a\u0430. \u0415\u0441\u043b\u0438 \u0412\u044b \u0443\u0436\u0435 \u0438\u043c\u0435\u0435\u0442\u0435 \u043e\u043f\u044b\u0442 \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u044f, \u0437\u043d\u0430\u0435\u0442\u0435 \u043e\u0441\u043d\u043e\u0432\u043d\u044b\u0435 \u0442\u0435\u0440\u043c\u0438\u043d\u044b \u0438 \u0432\u0438\u0434\u0435\u043b\u0438 \u0438\u0445 \u043d\u0430 \u0430\u043d\u0433\u043b\u0438\u0439\u0441\u043a\u043e\u043c, \u0442\u043e \u043f\u0440\u043e\u0431\u043b\u0435\u043c \u043d\u0435 \u0432\u043e\u0437\u043d\u0438\u043a\u043d\u0435\u0442. \u0410 \u0432\u043e\u0442 \u043d\u0430\u0447\u0438\u043d\u0430\u044e\u0449\u0435\u0433\u043e \u044d\u0442\u043e \u0441\u043e\u0431\u044a\u0435\u0442 \u0441 \u0442\u043e\u043b\u043a\u0443. \u0414\u043b\u044f \u043f\u0440\u0438\u043c\u0435\u0440\u0430: "... \u043c\u0435\u0442\u043e\u0434\u044b \u043c\u043e\u0433\u0443\u0442 \u0438\u043c\u0435\u0442\u044c \u0441\u043f\u0438\u0441\u043a\u0438 \u0430\u0440\u0433\u0443\u043c\u0435\u043d\u0442\u043e\u0432 \u0434\u043b\u0438\u043d\u043e\u0439 \u0441 \u043f\u0435\u0440\u0435\u043c\u0435\u043d\u043d\u0443\u044e ..." \u0441\u0442\u0440. 215 . \u0420\u0435\u0447\u044c \u0448\u043b\u0430 \u043e \u0441\u043f\u0438\u0441\u043a\u0435 \u0430\u0440\u0433\u0443\u043c\u0435\u043d\u0442\u043e\u0432 \u043f\u0435\u0440\u0435\u043c\u0435\u043d\u043d\u043e\u0439 \u0434\u043b\u0438\u043d\u044b. \u0418 \u0435\u0449\u0435 \u0446\u0430\u0440\u0430\u043f\u0430\u0435\u0442 \u0433\u043b\u0430\u0437 "\u043d\u043e\u043b\u0435\u0432\u043e\u0439". \u0412\u0441\u0435 \u043f\u0440\u0438\u043c\u0435\u0440\u044b \u043a\u043e\u0434\u0430 \u043e\u0444\u043e\u0440\u043c\u043b\u0435\u043d\u044b \u0441 \u0432\u044b\u043a\u043b\u044e\u0447\u043a\u043e\u0439 \u0432\u043b\u0435\u0432\u043e - \u0440\u0430\u0437\u0431\u0438\u0440\u0430\u0442\u044c \u0442\u0430\u043a\u043e\u0439 \u043a\u043e\u0434 \u0442\u044f\u0436\u0435\u043b\u043e\u0432\u0430\u0442\u043e. \u0420\u0435\u0437\u044e\u043c\u0438\u0440\u0443\u044e: \u043d\u0435\u0441\u043c\u043e\u0442\u0440\u044f \u043d\u0430 \u043d\u0435\u0431\u043e\u043b\u044c\u0448\u0438\u0435 \u043d\u0435\u0434\u043e\u0447\u0435\u0442\u044b \u043a\u043d\u0438\u0433\u0430 \u043e\u0442\u043b\u0438\u0447\u043d\u0430\u044f, \u043c\u0430\u0442\u0435\u0440\u0438\u0430\u043b \u0438\u0437\u043b\u043e\u0436\u0435\u043d \u043e\u0447\u0435\u043d\u044c \u0432\u043d\u044f\u0442\u043d\u043e \u0438 \u043d\u0435\u043f\u0440\u0438\u043d\u0443\u0436\u0434\u0435\u043d\u043d\u043e, \u043d\u043e\u0440\u043c\u0430\u043b\u044c\u043d\u044b\u043c \u0447\u0435\u043b\u043e\u0432\u0435\u0447\u0435\u0441\u043a\u0438\u043c \u044f\u0437\u044b\u043a\u043e\u043c, \u0431\u0435\u0437 \u043b\u0438\u0448\u043d\u0435\u0439 \u0430\u043a\u0430\u0434\u0435\u043c\u0438\u0447\u043d\u043e\u0441\u0442\u0438. \u0421\u043a\u0440\u044b\u0442\u044c',3345)
INSERT INTO COMMENT VALUES(1454,'\u0414\u0432\u0443\u0436\u0438\u043b\u043e\u0432 \u041f\u0430\u0432\u0435\u043b','2015-01-11','\u041e\u0434\u043d\u043e\u0437\u043d\u0430\u0447\u043d\u043e \u043d\u0435 \u0434\u043b\u044f \u043d\u043e\u0432\u0438\u0447\u043a\u043e\u0432. \u041a\u0430\u043a \u0441\u043f\u0440\u0430\u0432\u043e\u0447\u043d\u0438\u043a - \u0434\u0430, \u043a\u043d\u0438\u0433\u0430 \u0445\u043e\u0440\u043e\u0448\u0430\u044f, \u0431\u043e\u043b\u044c\u0448\u043e\u0439 \u043e\u0431\u044a\u0451\u043c \u0442\u0435\u043c \u0438 \u0442.\u0434., \u043d\u043e \u043a\u0430\u043a \u043f\u0435\u0440\u0432\u0430\u044f \u043a\u043d\u0438\u0433\u0430 \u043f\u043e \u0434\u0436\u0430\u0432\u0435 - \u043d\u0435\u0442. \u041e\u0431\u044a\u044f\u0441\u043d\u0435\u043d\u0438\u044f \u043c\u043d\u043e\u0433\u0438\u0445 \u043c\u043e\u043c\u0435\u043d\u0442\u043e\u0432 \u043f\u043e\u0434\u0440\u0430\u0437\u0443\u043c\u0435\u0432\u0430\u044e\u0442 \u043d\u0430\u043b\u0438\u0447\u0438\u0435 \u0443\u0436\u0435 \u0445\u043e\u0442\u044f \u0431\u044b \u0431\u0430\u0437\u043e\u0432\u044b\u0445 \u0437\u043d\u0430\u043d\u0438\u0439.',3345)
INSERT INTO COMMENT VALUES(1455,'\u0415\u0434\u0438\u0445\u0430\u043d\u043e\u0432\u0430 \u0410\u043b\u0438\u0441\u0430','2014-01-10','\u0424\u043e\u0442\u043e \u0432\u043d\u0435\u0448\u043d\u0435\u0433\u043e \u0432\u0438\u0434\u0430 \u0438 \u0441\u0442\u0440\u0430\u043d\u0438\u0446: \u041f\u043e\u043a\u0430\u0437\u0430\u0442\u044c \u0438\u0437\u043e\u0431\u0440\u0430\u0436\u0435\u043d\u0438\u044f \u0421\u043a\u0440\u044b\u0442\u044c \u0438\u0437\u043e\u0431\u0440\u0430\u0436\u0435\u043d\u0438\u044f',3346)
INSERT INTO COMMENT VALUES(1456,'kshen','2015-01-21','\u0412 \u043b\u043e\u0437\u0443\u043d\u0433\u0430\u0445 \u043d\u0430 \u043e\u0431\u043b\u043e\u0436\u043a\u0435 \u0430\u0432\u0442\u043e\u0440 \u043d\u0435\u043c\u043d\u043e\u0433\u043e \u043f\u043e\u0441\u043a\u0440\u043e\u043c\u043d\u0438\u0447\u0430\u043b. \u041d\u0443\u0436\u043d\u043e \u0431\u044b\u043b\u043e \u043d\u0430\u043f\u0438\u0441\u0430\u0442\u044c "...\u0434\u043b\u044f \u0441\u0442\u0443\u0434\u0435\u043d\u0442\u043e\u0432 \u0424\u0418\u0417\u041c\u0410\u0422\u0410...". \u041f\u0435\u0440\u0432\u044b\u0435 \u043d\u0435\u043f\u0440\u0438\u044f\u0437\u043d\u0435\u043d\u043d\u044b\u0435 \u043c\u044b\u0441\u043b\u0438 \u0443 \u043c\u0435\u043d\u044f \u043c\u0435\u043b\u044c\u043a\u043d\u0443\u043b\u0438 \u043d\u0430 \u0441\u0442\u0440.24, \u043a\u043e\u0433\u0434\u0430 \u044f \u043f\u044b\u0442\u0430\u043b\u0441\u044f \u043e\u0441\u043c\u044b\u0441\u043b\u0438\u0442\u044c \u0444\u0440\u0430\u0437\u0443: "\u041e\u043f\u0435\u0440\u0430\u0442\u043e\u0440\u044b \u0438\u043d\u043a\u0440\u0435\u043c\u0435\u043d\u0442\u0430 \u0438 \u0434\u0435\u043a\u0440\u0435\u043c\u0435\u043d\u0442\u0430 \u043c\u043e\u0433\u0443\u0442 \u0438\u0441\u043f\u043e\u043b\u044c\u0437\u043e\u0432\u0430\u0442\u044c\u0441\u044f \u0432 \u043f\u0440\u0435\u0434\u0444\u0438\u043a\u0441\u043d\u043e\u0439 \u0438 \u043f\u043e\u0441\u0442\u0444\u0438\u043a\u0441\u043d\u043e\u0439 \u0444\u043e\u0440\u043c\u0430\u0445. \u0412 \u043f\u0440\u0435\u0434\u0444\u0438\u043a\u0441\u043d\u043e\u0439 \u0444\u043e\u0440\u043c\u0435 \u043e\u043f\u0435\u0440\u0430\u0442\u043e\u0440 \u0443\u043a\u0430\u0437\u044b\u0432\u0430\u0435\u0442\u0441\u044f \u043f\u0435\u0440\u0435\u0434 \u043e\u043f\u0435\u0440\u0430\u043d\u0434\u043e\u043c, \u0430 \u0432 \u043f\u043e\u0441\u0442\u0444\u0438\u043a\u0441\u043d\u043e\u0439 \u0444\u043e\u0440\u043c\u0435 - \u043f\u043e\u0441\u043b\u0435 \u043d\u0435\u0433\u043e." \u041d\u043e \u0432\u043e\u0437\u043d\u0435\u043d\u0430\u0432\u0438\u0434\u0435\u043b \u0430\u0432\u0442\u043e\u0440\u0430 \u044f \u043d\u0430 \u043f\u0435\u0440\u0432\u044b\u0445 \u043f\u0440\u0438\u043c\u0435\u0440\u0430\u0445 \u0432 \u043a\u043e\u043d\u0446\u0435 \u0433\u043b\u0430\u0432\u044b. \u0428\u043a\u043e\u043b\u0430 \u0431\u044b\u043b\u0430 \u0437\u0430\u043a\u043e\u043d\u0447\u0435\u043d\u0430 15 \u043b\u0435\u0442 \u043d\u0430\u0437\u0430\u0434, \u0438 \u044f \u043d\u0435 \u043f\u043e\u043c\u043d\u044e, \u0447\u0442\u043e \u0442\u0430\u043a\u043e\u0435 \u043b\u043e\u0433\u0430\u0440\u0438\u0444\u043c \u0438\u043b\u0438 \u0444\u0430\u043a\u0442\u043e\u0440\u0438\u0430\u043b, \u0430 \u043f\u0440\u043e \u0430\u0440\u043a\u0441\u0438\u043d\u0443\u0441 \u0442\u043e\u043b\u044c\u043a\u043e \u0441\u043b\u044b\u0448\u0430\u043b, \u0447\u0442\u043e \u0442\u0430\u043a\u043e\u0439 \u0435\u0441\u0442\u044c. \u042f \u043d\u0435 \u043c\u043e\u0433\u0443 \u0434\u0430\u0436\u0435 \u043f\u043e\u043d\u044f\u0442\u044c \u0444\u043e\u0440\u043c\u0443\u043b\u0443 z=|z|exp(i\u0444), \u043a\u0443\u0434\u0430 \u0443\u0436 \u0440\u0430\u0437\u043e\u0431\u0440\u0430\u0442\u044c \u043a\u043e\u0434... \u0418 \u043f\u043e\u0441\u043b\u0435\u0434\u043d\u0435\u0435 - \u0430\u0432\u0442\u043e\u0440 \u0438 \u0441\u043b\u043e\u0432\u043e\u043c \u043d\u0435 \u043e\u0431\u043c\u043e\u043b\u0432\u0438\u043b\u0441\u044f, \u043a\u0430\u043a \u0438 \u043a\u0443\u0434\u0430 \u0437\u0430\u043f\u0438\u0441\u044b\u0432\u0430\u0442\u044c \u0432\u0441\u0451 \u0442\u043e, \u0447\u0442\u043e \u043e\u043d \u043f\u0440\u0435\u043f\u043e\u0434\u043d\u043e\u0441\u0438\u0442. \u041f\u0440\u043e\u0441\u0442\u043e \u043a\u043e\u0434 \u0431\u0435\u0437 \u0443\u043a\u0430\u0437\u0430\u043d\u0438\u044f \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c, \u043d\u0435 \u0433\u043e\u0432\u043e\u0440\u044f \u0443\u0436\u0435 \u043e\u0431 \u043e\u043f\u0438\u0441\u0430\u043d\u0438\u0438 \u043a\u0430\u043a \u043f\u0440\u043e\u0432\u0435\u0440\u0438\u0442\u044c \u044d\u0442\u043e\u0442 \u043a\u043e\u0434. \u0422.\u0435. \u043f\u0440\u0435\u0434\u043f\u043e\u043b\u0430\u0433\u0430\u0435\u0442\u0441\u044f, \u0447\u0442\u043e \u0447\u0438\u0442\u0430\u0442\u0435\u043b\u044c \u0443\u0436\u0435 \u0434\u043e\u043b\u0436\u0435\u043d \u0441\u0430\u043c \u0432\u0441\u0451 \u044d\u0442\u043e \u0437\u043d\u0430\u0442\u044c, \u0445\u043e\u0442\u044f \u0432 \u043e\u043f\u0438\u0441\u0430\u043d\u0438\u0438 \u043d\u0430\u043f\u0438\u0441\u0430\u043d\u043e: "\u043f\u0440\u0435\u0434\u043d\u0430\u0437\u043d\u0430\u0447\u0435\u043d\u043d\u043e\u0433\u043e \u0434\u043b\u044f \u0438\u0437\u0443\u0447\u0435\u043d\u0438\u044f \u044f\u0437\u044b\u043a\u0430 \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u044f \u0421++ \u0441 \u043d\u0443\u043b\u044f." \u0412 \u043e\u0431\u0449\u0435\u043c, \u043a\u043d\u0438\u0433\u0430 \u043d\u0430\u043f\u0438\u0441\u0430\u043d\u0430 \u0432\u0443\u0437\u043e\u0432\u0441\u043a\u0438\u043c \u044f\u0437\u044b\u043a\u043e\u043c, \u0430 \u0435\u0441\u043b\u0438 \u0432\u044b \u0435\u0449\u0451 \u0438 \u043d\u0435 \u0441\u0438\u043b\u044c\u043d\u044b \u0432 \u0430\u043b\u0433\u0435\u0431\u0440\u0435 \u0438 \u0444\u0438\u0437\u0438\u043a\u0435, \u043d\u0435 \u0441\u043e\u0432\u0435\u0442\u0443\u044e \u043f\u043e\u043a\u0443\u043f\u0430\u0442\u044c \u044d\u0442\u0443 \u043a\u043d\u0438\u0433\u0443. \u0421\u043a\u0440\u044b\u0442\u044c',3346)
INSERT INTO COMMENT VALUES(1457,'\u0428\u0430\u0440\u0438\u043f\u043e\u0432 \u041c\u0430\u0440\u0430\u0442','2015-01-23','\u0425\u043e\u0440\u043e\u0448\u0430\u044f \u0438 \u0441\u043e\u0432\u0440\u0435\u043c\u0435\u043d\u043d\u0430\u044f \u043a\u043d\u0438\u0433\u0430 \u0434\u043b\u044f \u043d\u0430\u0447\u0438\u043d\u0430\u044e\u0449\u0438\u0445 \u0432 \u0438\u0437\u0443\u0447\u0435\u043d\u0438\u0438 \u0421++. \u041a\u0430\u0447\u0435\u0441\u0442\u0432\u043e \u0431\u0443\u043c\u0430\u0433\u0438 \u043e\u0442\u043b\u0438\u0447\u043d\u043e\u0435 \u043c\u0430\u0442\u0435\u0440\u0438\u0430\u043b \u043f\u043e\u0434\u0430\u043d \u0434\u043e\u0441\u0442\u0430\u0442\u043e\u0447\u043d\u043e \u0445\u043e\u0440\u043e\u0448\u043e. \u041a\u043d\u0438\u0433\u0430 \u043f\u043e\u043b\u044c\u0437\u0443\u0435\u0442\u0441\u044f \u043f\u043e\u043f\u0443\u043b\u044f\u0440\u043d\u043e\u0441\u0442\u044c\u044e \u0443 \u0447\u0438\u0442\u0430\u0442\u0435\u043b\u0435\u0439!',3346)
