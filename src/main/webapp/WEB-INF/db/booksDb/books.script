INSERT INTO COMMENT VALUES(1797,'\u0422\u0430\u043b\u0430\u043d\u0442\u043e\u0432 \u0421\u0435\u0440\u0433\u0435\u0439 \u0412\u043b\u0430\u0434\u0438\u043c\u0438\u0440\u043e\u0432\u0438\u0447','2008-01-31','\u042d\u0442\u0430 \u043a\u043d\u0438\u0433\u0430, \u043d\u0430\u043f\u0438\u0441\u0430\u043d\u043d\u0430\u044f \u0431\u043e\u043b\u0435\u0435 10 \u043b\u0435\u0442, \u0434\u0430\u043b\u0430 \u0442\u043e\u043b\u0447\u043e\u043a \u043a \u0440\u0430\u0437\u0432\u0438\u0442\u0438\u044e \u043d\u043e\u0432\u043e\u0433\u043e \u043d\u0430\u043f\u0440\u0430\u0432\u043b\u0435\u043d\u0438\u044f \u0432 \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u043d\u043e\u0439 \u0438\u043d\u0436\u0435\u043d\u0435\u0440\u0438\u0438. \u0421 \u0434\u0430\u0432\u043d\u0438\u0445 \u043f\u043e\u0440 \u0441\u0438\u0441\u0442\u0435\u043c\u043d\u044b\u0435 \u0430\u0440\u0445\u0438\u0442\u0435\u043a\u0442\u043e\u0440\u044b \u0438 \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u0438\u0441\u0442\u044b \u0438\u0441\u043f\u043e\u043b\u044c\u0437\u043e\u0432\u0430\u043b\u0438 \u0443\u0441\u0442\u043e\u0439\u0447\u0438\u0432\u044b\u0435 \u0438 \u0437\u0430\u0440\u0435\u043a\u043e\u043c\u0435\u043d\u0434\u043e\u0432\u0430\u0432\u0448\u0438\u0435 \u0441\u0435\u0431\u044f \u043f\u0440\u0438\u0435\u043c\u044b \u043f\u0440\u043e\u0435\u043a\u0442\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u044f \u0441\u043b\u043e\u0436\u043d\u044b\u0445 \u0441\u0438\u0441\u0442\u0435\u043c, \u043e\u0434\u043d\u0430\u043a\u043e \u043b\u0438\u0448\u044c \u0441 \u043f\u043e\u044f\u0432\u043b\u0435\u043d\u0438\u0435\u043c \u043a\u043d\u0438\u0433\u0438 "\u043f\u0430\u0442\u0435\u0440\u043d\u044b \u043f\u0440\u043e\u0435\u043a\u0442\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u044f" \u043f\u0440\u043e\u0438\u0437\u043e\u0448\u043b\u043e \u043a\u0430\u0447\u0435\u0441\u0442\u0432\u0435\u043d\u043d\u043e\u0435 \u043f\u0435\u0440\u0435\u043e\u0441\u043c\u044b\u0441\u043b\u0435\u043d\u0438\u0435 \u043d\u0435\u043e\u0431\u0445\u043e\u0434\u0438\u043c\u043e\u0441\u0442\u0438 \u0438\u0441\u043f\u043e\u043b\u044c\u0437\u043e\u0432\u0430\u043d\u0438\u044f \u0448\u0430\u0431\u043b\u043e\u043d\u043e\u0432. \u041f\u043e\u0441\u043b\u0435 \u044d\u0442\u043e\u0439 \u043a\u043d\u0438\u0433\u0438 \u0432\u044b\u0448\u043b\u043e \u043c\u043d\u043e\u0436\u0435\u0441\u0442\u0432\u043e \u0438\u0437\u0434\u0430\u043d\u0438\u0439 \u0441\u043e \u0441\u0445\u043e\u0436\u0438\u043c\u0438 \u043d\u0430\u0437\u0432\u0430\u043d\u0438\u044f\u043c\u0438, \u0430\u0432\u0442\u043e\u0440\u044b \u043d\u0430\u0447\u0430\u043b\u0438 \u043f\u0440\u0435\u0434\u043b\u0430\u0433\u0430\u0442\u044c \u043d\u043e\u0432\u044b\u0435 \u043f\u0430\u0442\u0442\u0435\u0440\u043d\u044b \u0438 \u043a\u043e\u043d\u043a\u0440\u0435\u0442\u0438\u0437\u0438\u0440\u043e\u0432\u0430\u0442\u044c \u0438\u0445 \u043f\u0440\u0438\u043c\u0435\u043d\u0435\u043d\u0438\u0435 \u0434\u043b\u044f \u0443\u0437\u043a\u0438\u0445 \u043f\u0440\u043e\u0431\u043b\u0435\u043c\u043d\u044b\u0445 \u043e\u0431\u043b\u0430\u0441\u0442\u0435\u0439. \u0412 \u043a\u043d\u0438\u0433\u0435 \u0434\u0430\u043d\u043e \u0441\u0438\u0441\u0442\u0435\u043c\u0430\u0442\u0438\u0447\u0435\u0441\u043a\u043e\u0435 \u043e\u043f\u0438\u0441\u0430\u043d\u0438\u0435 \u043e\u0441\u043d\u043e\u0432\u043d\u044b\u0445 \u043f\u0430\u0442\u0442\u0435\u0440\u043d\u043e\u0432, \u0438\u0445 \u043f\u0440\u0438\u043c\u0435\u043d\u0438\u043c\u043e\u0441\u0442\u044c, \u0440\u0435\u0430\u043b\u0438\u0437\u0430\u0446\u0438\u044f, \u0434\u043e\u0441\u0442\u043e\u0438\u043d\u0441\u0442\u0432\u0430 \u0438 \u043d\u0435\u0434\u043e\u0441\u0442\u0430\u0442\u043a\u0438. \u041a\u0430\u0436\u0434\u044b\u0439 \u043f\u0430\u0442\u0442\u0435\u0440\u043d \u0438\u043c\u0435\u0435\u0442 \u043e\u0431\u0440\u0430\u0437\u043d\u043e\u0435 \u043d\u0430\u0437\u0432\u0430\u043d\u0438\u0435, \u0441\u043f\u043e\u0441\u043e\u0431\u0441\u0442\u0432\u0443\u044e\u0449\u0435\u0435 \u0441\u043a\u043e\u0440\u0435\u0439\u0448\u0435\u043c\u0443 \u0437\u0430\u043f\u043e\u043c\u0438\u043d\u0430\u043d\u0438\u044e. \u0412 \u043a\u043e\u043d\u0446\u0435 \u0438\u043c\u0435\u0435\u0442\u0441\u044f \u0443\u0434\u043e\u0431\u043d\u044b\u0439 \u043a\u0430\u0442\u0430\u043b\u043e\u0433 \u0441 \u043a\u0440\u0430\u0442\u043a\u0438\u043c \u043e\u043f\u0438\u0441\u0430\u043d\u0438\u0435\u043c \u0432\u0441\u0435\u0445 \u0448\u0430\u0431\u043b\u043e\u043d\u043e\u0432. \u0418\u0437\u0443\u0447\u0435\u043d\u0438\u0435 \u0438 \u0438\u0441\u043f\u043e\u043b\u044c\u0437\u043e\u0432\u0430\u043d\u0438\u0435 \u043f\u0430\u0442\u0442\u0435\u0440\u043d\u043e\u0432 \u043f\u0440\u0438 \u0440\u0430\u0437\u0440\u0430\u0431\u043e\u0442\u043a\u0435 \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u043d\u044b\u0445 \u0441\u0438\u0441\u0442\u0435\u043c \u043f\u043e\u0437\u0432\u043e\u043b\u044f\u0435\u0442 \u043f\u0440\u043e\u0435\u043a\u0442\u0438\u0440\u043e\u0432\u0449\u0438\u043a\u0443 \u043f\u0435\u0440\u0435\u0439\u0442\u0438 \u043d\u0430 \u0438\u043d\u043e\u0439, \u0431\u043e\u043b\u0435\u0435 \u0432\u044b\u0441\u043e\u043a\u0438\u0439 \u0443\u0440\u043e\u0432\u0435\u043d\u044c, \u0440\u0430\u0437\u0440\u0430\u0431\u043e\u0442\u043a\u0438 \u043f\u0440\u043e\u0435\u043a\u0442\u0430. \u0422\u0435\u043f\u0435\u0440\u044c \u0430\u0440\u0445\u0438\u0442\u0435\u043a\u0442\u043e\u0440 \u0438 \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u0438\u0441\u0442 \u043c\u043e\u0433\u0443\u0442 \u043e\u043f\u0435\u0440\u0438\u0440\u043e\u0432\u0430\u0442\u044c \u043e\u0431\u0440\u0430\u0437\u043d\u044b\u043c\u0438 \u043d\u0430\u0437\u0432\u0430\u043d\u0438\u044f\u043c\u0438 \u043f\u0430\u0442\u0442\u0435\u0440\u043d\u043e\u0432 \u0438 \u043e\u0431\u0449\u0430\u0442\u044c\u0441\u044f \u043d\u0430 \u043e\u0434\u043d\u043e\u043c \u044f\u0437\u044b\u043a\u0435. \u0421\u043a\u0440\u044b\u0442\u044c',4045)
INSERT INTO COMMENT VALUES(1798,'\u0412\u043b\u0430\u0434\u0438\u043c\u0438\u0440','2015-01-25','must have \u043c\u0435\u0442\u043e\u0434\u0438\u0447\u043d\u043e, \u043f\u0440\u043e\u0441\u0442\u043e, \u043f\u043e\u0434\u0440\u043e\u0431\u043d\u043e, \u043d\u0430 \u043f\u0440\u0438\u043c\u0435\u0440\u0430\u0445, \u043d\u0430 \u043f\u0430\u043b\u044c\u0446\u0430\u0445',4045)
INSERT INTO COMMENT VALUES(1799,'\u0420\u044b\u0436\u0435\u043d\u044c\u043a\u0438\u0439','2011-01-08','\u0421\u0442\u0440\u0430\u043d\u0438\u0446\u044b \u0438\u0437 \u0438\u0437\u0434\u0430\u043d\u0438\u044f: \u041f\u043e\u043a\u0430\u0437\u0430\u0442\u044c \u0438\u0437\u043e\u0431\u0440\u0430\u0436\u0435\u043d\u0438\u044f \u0421\u043a\u0440\u044b\u0442\u044c \u0438\u0437\u043e\u0431\u0440\u0430\u0436\u0435\u043d\u0438\u044f',4048)
INSERT INTO COMMENT VALUES(1800,'mariya.bikeeva','2013-01-13','\u041f\u0440\u0438\u043e\u0431\u0440\u0435\u043b\u0430 \u044d\u0442\u0443 \u043a\u043d\u0438\u0433\u0443 \u0434\u043b\u044f \u0438\u0437\u0443\u0447\u0435\u043d\u0438\u044f web-\u0434\u0438\u0437\u0430\u0439\u043d\u0430. \u041f\u0440\u043e\u0441\u043c\u043e\u0442\u0440\u0435\u043b\u0430 \u0441\u043e\u0434\u0435\u0440\u0436\u0430\u043d\u0438\u0435 - \u043e\u0431\u0448\u0438\u0440\u043d\u043e\u0435. \u041f\u0440\u043e\u0447\u0438\u0442\u0430\u043b\u0430 \u043f\u0430\u0440\u0443 \u0441\u0442\u0440\u0430\u043d\u0438\u0446 - \u043d\u0430\u043f\u0438\u0441\u0430\u043d\u043e \u043f\u043e\u043d\u044f\u0442\u043d\u043e \u0434\u0430\u0436\u0435 \u0434\u043b\u044f \u0434\u0438\u043b\u0435\u0442\u0430\u043d\u0442\u0430. \u0414\u0443\u043c\u0430\u044e, \u043a\u043d\u0438\u0433\u0430 \u043e\u043f\u0440\u0430\u0432\u0434\u0430\u0435\u0442 \u043e\u0436\u0438\u0434\u0430\u043d\u0438\u044f \u0438 \u043f\u043e\u043c\u043e\u0436\u0435\u0442 \u043c\u043d\u0435, \u043d\u0430\u0447\u0438\u043d\u0430\u044e\u0449\u0435\u043c\u0443 web-\u0434\u0438\u0437\u0430\u0439\u043d\u0435\u0440\u0443, \u0432 \u0438\u0437\u0443\u0447\u0435\u043d\u0438\u0438 \u044d\u0442\u043e\u0433\u043e \u0441\u043b\u043e\u0436\u043d\u043e\u0433\u043e \u0434\u0435\u043b\u0430. \u0420\u0430\u0437\u043c\u0435\u0440\u044b \u043a\u043d\u0438\u0433\u0438: 16,5*23,5*1,5 \u0441\u043c. \u041e\u0431\u043b\u043e\u0436\u043a\u0430 \u043f\u043b\u043e\u0442\u043d\u0430\u044f, \u0433\u0438\u0431\u043a\u0430\u044f. \u0421\u0442\u0440\u0430\u043d\u0438\u0446\u044b \u0442\u043e\u043d\u043a\u0438\u0435, \u0441\u0435\u0440\u043e\u0432\u0430\u0442\u044b\u0435. \u0422\u0435\u043a\u0441\u0442 \u043d\u0435 \u0441\u043b\u0438\u0448\u043a\u043e\u043c \u043c\u0435\u043b\u043a\u0438\u0439, \u0447\u0438\u0442\u0430\u0442\u044c \u0443\u0434\u043e\u0431\u043d\u043e. \u0418\u043c\u0435\u0435\u0442\u0441\u044f \u0430\u043b\u0444\u0430\u0432\u0438\u0442\u043d\u044b\u0439 \u0443\u043a\u0430\u0437\u0430\u0442\u0435\u043b\u044c, \u0447\u0442\u043e \u043e\u0447\u0435\u043d\u044c \u0443\u0434\u043e\u0431\u043d\u043e. \u0427\u0442\u043e \u0435\u0449\u0435 \u043f\u043e\u0440\u0430\u0434\u043e\u0432\u0430\u043b\u043e: - \u0432 \u043f\u0440\u0438\u043b\u043e\u0436\u0435\u043d\u0438\u0435 \u0435\u0441\u0442\u044c \u0442\u0430\u0431\u043b\u0438\u0446\u0430 \u0446\u0432\u0435\u0442\u043e\u0432 HTML; - \u0441\u043f\u0438\u0441\u043e\u043a \u0441\u043f\u0435\u0446\u0438\u0430\u043b\u044c\u043d\u044b\u0445 \u0441\u0438\u043c\u0432\u043e\u043b\u043e\u0432 HTML; - \u043e\u043f\u0443\u0431\u043b\u0438\u043a\u043e\u0432\u0430\u043d\u044b \u043f\u043e\u043b\u0435\u0437\u043d\u044b\u0435 \u0441\u0441\u044b\u043b\u043a\u0438 \u043d\u0430 \u0440\u0435\u0441\u0443\u0440\u0441\u044b, \u043f\u043e\u0441\u0432\u044f\u0449\u0435\u043d\u043d\u044b\u0435 \u043f\u0440\u043e\u0435\u043a\u0442\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u044e \u0438 web-\u0434\u0438\u0437\u0430\u0439\u043d\u0443. \u0412 \u043e\u0431\u0449\u0435\u043c, \u043a\u043d\u0438\u0433\u0430 \u043e\u0447\u0435\u043d\u044c \u043f\u043e\u043b\u0435\u0437\u043d\u0430\u044f, \u044f \u0434\u043e\u0432\u043e\u043b\u044c\u043d\u0430 \u043f\u0440\u0438\u043e\u0431\u0440\u0435\u0442\u0435\u043d\u0438\u0435\u043c! \u0421\u043a\u0440\u044b\u0442\u044c',4050)
INSERT INTO COMMENT VALUES(1801,'\u042f\u043b\u0438\u043d\u0430','2011-01-17','\u0421\u0442\u0440\u0430\u043d\u0438\u0446\u044b \u0438\u0437 \u043a\u043d\u0438\u0433\u0438: \u041f\u043e\u043a\u0430\u0437\u0430\u0442\u044c \u0438\u0437\u043e\u0431\u0440\u0430\u0436\u0435\u043d\u0438\u044f \u0421\u043a\u0440\u044b\u0442\u044c \u0438\u0437\u043e\u0431\u0440\u0430\u0436\u0435\u043d\u0438\u044f',4050)
