INSERT INTO BOOK VALUES(3899,280161,'\u041a\u0438\u0440\u0441\u0442\u0435\u043d, \u0418\u0440\u0438\u043d\u0433\u0435\u0440, \u041a\u044e\u043d: \u041f\u043e\u0441\u0442\u0440\u0435\u043b\u044f\u0446\u0438\u043e\u043d\u043d\u0430\u044f \u0421\u0423\u0411\u0414 Cache 5. \u041e\u0431\u044a\u0435\u043a\u0442\u043d\u043e-\u043e\u0440\u0438\u0435\u043d\u0442\u0438\u0440\u043e\u0432\u0430\u043d\u043d\u0430\u044f \u0440\u0430\u0437\u0440\u0430\u0431\u043e\u0442\u043a\u0430 \u043f\u0440\u0438\u043b\u043e\u0436\u0435\u043d\u0438\u0439 (+CD)','\u041a\u0438\u0440\u0441\u0442\u0435\u043d \u0412\u043e\u043b\u044c\u0444\u0433\u0430\u043d\u0433, \u0418\u0440\u0438\u043d\u0433\u0435\u0440 \u041c\u0438\u0445\u0430\u044d\u043b\u044c, \u041a\u044e\u043d \u041c\u0430\u0442\u0438\u0430\u0441\u00a0 \u0432\u0441\u0435, \u0420\u0435\u0440\u0438\u0433 \u0411\u0435\u0440\u043d\u0445\u0430\u0440\u0434\u00a0 \u0441\u043a\u0440\u044b\u0442\u044c','\u0410\u0440\u0438\u0441\u0442\u043e\u0432 \u041a., \u041c\u0430\u0441\u043b\u043e\u0432 \u0410.','\u0411\u0438\u043d\u043e\u043c, 2011 \u0433.',599.0E0,'978-5-9518-0445-7','416 (\u041e\u0444\u0441\u0435\u0442)','\u0422\u0438\u043f \u043e\u0431\u043b\u043e\u0436\u043a\u0438: 7\u0411 - \u0442\u0432\u0435\u0440\u0434\u0430\u044f (\u043f\u043b\u043e\u0442\u043d\u0430\u044f \u0431\u0443\u043c\u0430\u0433\u0430 \u0438\u043b\u0438 \u043a\u0430\u0440\u0442\u043e\u043d) \u0418\u043b\u043b\u044e\u0441\u0442\u0440\u0430\u0446\u0438\u0438: \u0427\u0435\u0440\u043d\u043e-\u0431\u0435\u043b\u044b\u0435','668 \u0433','243x175x22 \u043c\u043c','\u0426\u0435\u043b\u044c \u043d\u0430\u0441\u0442\u043e\u044f\u0449\u0435\u0439 \u043a\u043d\u0438\u0433\u0438 - \u043d\u0435 \u0442\u043e\u043b\u044c\u043a\u043e \u0440\u0430\u0441\u0441\u043c\u043e\u0442\u0440\u0435\u0442\u044c \u043f\u043e\u0441\u0442\u0440\u0435\u043b\u044f\u0446\u0438\u043e\u043d\u043d\u044b\u0439 \u043f\u043e\u0434\u0445\u043e\u0434 Cache \u0441 \u043f\u043e\u0437\u0438\u0446\u0438\u0438 \u0442\u0435\u043e\u0440\u0438\u0438, \u043d\u043e \u0438 \u0434\u0430\u0442\u044c \u0432\u043a\u0443\u0441\u0438\u0442\u044c \u0440\u0430\u0437\u0440\u0430\u0431\u043e\u0442\u0447\u0438\u043a\u0443 \u043f\u0440\u0438\u043b\u043e\u0436\u0435\u043d\u0438\u0439 \u0435\u0435 \u043f\u043b\u043e\u0434\u044b. \u0428\u0430\u0433 \u0437\u0430 \u0448\u0430\u0433\u043e\u043c \u0430\u0432\u0442\u043e\u0440\u044b \u0441\u043e\u043f\u0440\u043e\u0432\u043e\u0436\u0434\u0430\u044e\u0442 \u0447\u0438\u0442\u0430\u0442\u0435\u043b\u044f \u043d\u0430 \u0442\u0435\u0440\u043d\u0438\u0441\u0442\u043e\u043c \u043f\u0443\u0442\u0438 \u0440\u0430\u0437\u0440\u0430\u0431\u043e\u0442\u043a\u0438 \u043f\u0435\u0440\u0432\u044b\u0445 \u043e\u0431\u044a\u0435\u043a\u0442\u043d\u043e-\u043e\u0440\u0438\u0435\u043d\u0442\u0438\u0440\u043e\u0432\u0430\u043d\u043d\u044b\u0445 \u043f\u0440\u0438\u043b\u043e\u0436\u0435\u043d\u0438\u0439 - \u043e\u0442 \u043f\u0440\u043e\u0441\u0442\u0435\u0439\u0448\u0435\u0439 CSP-\u0441\u0442\u0440\u0430\u043d\u0438\u0446\u044b, \u0441\u0432\u044f\u0437\u0430\u043d\u043d\u043e\u0439 \u0441 \u0431\u0430\u0437\u043e\u0439 \u0434\u0430\u043d\u043d\u044b\u0445, \u0434\u043e \u043f\u0440\u043e\u0442\u043e\u0442\u0438\u043f\u0430 \u043f\u0440\u0438\u043b\u043e\u0436\u0435\u043d\u0438\u044f \u0432 \u043e\u0431\u043b\u0430\u0441\u0442\u0438 \u044d\u043b\u0435\u043a\u0442\u0440\u043e\u043d\u043d\u043e\u0439 \u043a\u043e\u043c\u043c\u0435\u0440\u0446\u0438\u0438. Cache, \u043f\u043e\u0441\u0442\u0440\u0435\u043b\u044f\u0446\u0438\u043e\u043d\u043d\u0430\u044f \u0421\u0423\u0411\u0414 \u043a\u043e\u0440\u043f\u043e\u0440\u0430\u0446\u0438\u0438 InterSystems, \u043f\u043e\u0437\u0432\u043e\u043b\u044f\u0435\u0442 \u043f\u0440\u0435\u043e\u0434\u043e\u043b\u0435\u0442\u044c \u0438\u0437\u0432\u0435\u0441\u0442\u043d\u044b\u0435 \u043e\u0433\u0440\u0430\u043d\u0438\u0447\u0435\u043d\u0438\u044f \u0440\u0435\u043b\u044f\u0446\u0438\u043e\u043d\u043d\u044b\u0445 \u0438 \u043e\u0431\u044a\u043