INSERT INTO BOOK VALUES(3846,317332,'\u0412\u0438\u043a\u0442\u043e\u0440 \u041f\u0435\u0442\u0438\u043d: \u0421\u0430\u0439\u0442 \u043d\u0430 AJAX \u043f\u043e\u0434 \u043a\u043b\u044e\u0447. \u0413\u043e\u0442\u043e\u0432\u043e\u0435 \u0440\u0435\u0448\u0435\u043d\u0438\u0435 \u0434\u043b\u044f \u0438\u043d\u0442\u0435\u0440\u043d\u0435\u0442-\u043c\u0430\u0433\u0430\u0437\u0438\u043d\u0430','\u041f\u0435\u0442\u0438\u043d \u0412\u0438\u043a\u0442\u043e\u0440 \u0410\u043b\u0435\u043a\u0441\u0430\u043d\u0434\u0440\u043e\u0432\u0438\u0447','\u041a\u043e\u043d\u0434\u0443\u043a\u043e\u0432\u0430 \u0415\u043a\u0430\u0442\u0435\u0440\u0438\u043d\u0430','BHV, 2012 \u0433.',413.0E0,'978-5-9775-0769-1','448 (\u0413\u0430\u0437\u0435\u0442\u043d\u0430\u044f)','\u0422\u0438\u043f \u043e\u0431\u043b\u043e\u0436\u043a\u0438: \u043e\u0431\u043b - \u043c\u044f\u0433\u043a\u0438\u0439 \u043f\u0435\u0440\u0435\u043f\u043b\u0435\u0442 (\u043a\u0440\u0435\u043f\u043b\u0435\u043d\u0438\u0435 \u0441\u043a\u0440\u0435\u043f\u043a\u043e\u0439 \u0438\u043b\u0438 \u043a\u043b\u0435\u0435\u043c) \u0418\u043b\u043b\u044e\u0441\u0442\u0440\u0430\u0446\u0438\u0438: \u0427\u0435\u0440\u043d\u043e-\u0431\u0435\u043b\u044b\u0435','402 \u0433','233x165x23 \u043c\u043c','\u0410\u043d\u043d\u043e\u0442\u0430\u0446\u0438\u044f \u043a \u043a\u043d\u0438\u0433\u0435 "\u0421\u0430\u0439\u0442 \u043d\u0430 AJAX \u043f\u043e\u0434 \u043a\u043b\u044e\u0447. \u0413\u043e\u0442\u043e\u0432\u043e\u0435 \u0440\u0435\u0448\u0435\u043d\u0438\u0435 \u0434\u043b\u044f \u0438\u043d\u0442\u0435\u0440\u043d\u0435\u0442-\u043c\u0430\u0433\u0430\u0437\u0438\u043d\u0430" \u041e\u043f\u0438\u0441\u0430\u043d\u0430 \u0440\u0430\u0437\u0440\u0430\u0431\u043e\u0442\u043a\u0430 \u0432\u044b\u0441\u043e\u043a\u043e \u0438\u043d\u0442\u0435\u0440\u0430\u043a\u0442\u0438\u0432\u043d\u044b\u0445 Web-\u0441\u0430\u0439\u0442\u043e\u0432, \u043e\u0441\u043d\u043e\u0432\u0430\u043d\u043d\u044b\u0445 \u043d\u0430 \u043f\u0435\u0440\u0435\u0434\u043e\u0432\u043e\u0439 \u0442\u0435\u0445\u043d\u043e\u043b\u043e\u0433\u0438\u0438 AJAX, \u0440\u0430\u0431\u043e\u0442\u0430\u044e\u0449\u0438\u0445 \u0431\u0435\u0437 \u043f\u0435\u0440\u0435\u0437\u0430\u0433\u0440\u0443\u0437\u043a\u0438 \u0441\u0442\u0440\u0430\u043d\u0438\u0446 \u0438 \u043e\u0431\u043b\u0430\u0434\u0430\u044e\u0449\u0438\u0445 \u0444\u0443\u043d\u043a\u0446\u0438\u043e\u043d\u0430\u043b\u044c\u043d\u043e\u0441\u0442\u044c\u044e \u043d\u0430\u0441\u0442\u043e\u043b\u044c\u043d\u044b\u0445 \u043f\u0440\u0438\u043b\u043e\u0436\u0435\u043d\u0438\u0439. \u041e\u0431\u0443\u0447\u0435\u043d\u0438\u0435 \u043f\u043e\u0441\u0442\u0440\u043e\u0435\u043d\u043e \u043d\u0430 \u0441\u043a\u0432\u043e\u0437\u043d\u043e\u043c \u043f\u0440\u0438\u043c\u0435\u0440\u0435 \u0441\u043e\u0437\u0434\u0430\u043d\u0438\u044f \u0441 \u043d\u0443\u043b\u044f \u0433\u043e\u0442\u043e\u0432\u043e\u0433\u043e \u0440\u0435\u0448\u0435\u043d\u0438\u044f: \u0438\u043d\u0442\u0435\u0440\u043d\u0435\u0442-\u043c\u0430\u0433\u0430\u0437\u0438\u043d\u0430, \u0430 \u0442\u0430\u043a\u0436\u0435 \u0441\u0438\u0441\u0442\u0435\u043c\u044b \u0435\u0433\u043e \u0430\u0434\u043c\u0438\u043d\u0438\u0441\u0442\u0440\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u044f. \u0412\u043e \u0432\u0442\u043e\u0440\u043e\u043c \u0438\u0437\u0434\u0430\u043d\u0438\u0438 \u0440\u0430\u0441\u0441\u043c\u043e\u0442\u0440\u0435\u043d \u043d\u043e\u0432\u044b\u0439 \u0432\u0430\u0440\u0438\u0430\u043d\u0442 \u0438\u043d\u0442\u0435\u0440\u043d\u0435\u0442-\u043c\u0430\u0433\u0430\u0437\u0438\u043d\u0430 \u0441 \u0440\u0430\u0441\u0448\u0438\u0440\u0435\u043d\u043d\u044b\u043c \u0444\u0443\u043d\u043a\u0446\u0438\u043e\u043d\u0430\u043b\u043e\u043c, \u043f\u043e\u0437\u0432\u043e\u043b\u044f\u044e\u0449\u0438\u043c \u043a\u043e\u043d\u0442\u0440\u043e\u043b\u0438\u0440\u043e\u0432\u0430\u0442\u044c \u043d\u0430\u043b\u0438\u0447\u0438\u0435 \u0438 \u0437\u0430\u0433\u0440\u0443\u0437\u043a