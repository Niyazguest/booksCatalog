INSERT INTO BOOK VALUES(3433,411117,'\u0411\u044c\u0435\u0440\u043d \u0421\u0442\u0440\u0430\u0443\u0441\u0442\u0440\u0443\u043f: \u0414\u0438\u0437\u0430\u0439\u043d \u0438 \u044d\u0432\u043e\u043b\u044e\u0446\u0438\u044f \u044f\u0437\u044b\u043a\u0430 \u0421++','\u0421\u0442\u0440\u0430\u0443\u0441\u0442\u0440\u0443\u043f \u0411\u044c\u0435\u0440\u043d','\u041c\u043e\u0432\u0447\u0430\u043d \u0414. \u0410.','\u0414\u041c\u041a-\u041f\u0440\u0435\u0441\u0441, 2014 \u0433.',864.0E0,'978-5-94074-994-3','448 (\u041e\u0444\u0441\u0435\u0442)','\u0422\u0438\u043f \u043e\u0431\u043b\u043e\u0436\u043a\u0438: \u043e\u0431\u043b - \u043c\u044f\u0433\u043a\u0438\u0439 \u043f\u0435\u0440\u0435\u043f\u043b\u0435\u0442 (\u043a\u0440\u0435\u043f\u043b\u0435\u043d\u0438\u0435 \u0441\u043a\u0440\u0435\u043f\u043a\u043e\u0439 \u0438\u043b\u0438 \u043a\u043b\u0435\u0435\u043c) \u0418\u043b\u043b\u044e\u0441\u0442\u0440\u0430\u0446\u0438\u0438: \u0411\u0435\u0437 \u0438\u043b\u043b\u044e\u0441\u0442\u0440\u0430\u0446\u0438\u0439','478 \u0433','200x140x23 \u043c\u043c','\u0410\u043d\u043d\u043e\u0442\u0430\u0446\u0438\u044f \u043a \u043a\u043d\u0438\u0433\u0435 "\u0414\u0438\u0437\u0430\u0439\u043d \u0438 \u044d\u0432\u043e\u043b\u044e\u0446\u0438\u044f \u044f\u0437\u044b\u043a\u0430 \u0421++" \u0412 \u043a\u043d\u0438\u0433\u0435, \u043d\u0430\u043f\u0438\u0441\u0430\u043d\u043d\u043e\u0439 \u0441\u043e\u0437\u0434\u0430\u0442\u0435\u043b\u0435\u043c \u044f\u0437\u044b\u043a\u0430 C++ \u0411\u044c\u0435\u0440\u043d\u043e\u043c \u0421\u0442\u0440\u0430\u0443\u0441\u0442\u0440\u0443\u043f\u043e\u043c, \u043f\u0440\u0435\u0434\u0441\u0442\u0430\u0432\u043b\u0435\u043d\u043e \u043e\u043f\u0438\u0441\u0430\u043d\u0438\u0435 \u043f\u0440\u043e\u0446\u0435\u0441\u0441\u0430 \u043f\u0440\u043e\u0435\u043a\u0442\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u044f \u0438 \u0440\u0430\u0437\u0440\u0430\u0431\u043e\u0442\u043a\u0438 \u044f\u0437\u044b\u043a\u0430 \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u044f C++. \u0417\u0434\u0435\u0441\u044c \u0438\u0437\u043b\u043e\u0436\u0435\u043d\u044b \u0446\u0435\u043b\u0438, \u043f\u0440\u0438\u043d\u0446\u0438\u043f\u044b \u0438 \u043f\u0440\u0430\u043a\u0442\u0438\u0447\u0435\u0441\u043a\u0438\u0435 \u043e\u0433\u0440\u0430\u043d\u0438\u0447\u0435\u043d\u0438\u044f, \u043d\u0430\u043b\u043e\u0436\u0438\u0432\u0448\u0438\u0435 \u043e\u0442\u043f\u0435\u0447\u0430\u0442\u043e\u043a \u043d\u0430 \u0441\u0442\u0440\u0443\u043a\u0442\u0443\u0440\u0443 \u0438 \u043e\u0431\u043b\u0438\u043a C++, \u043e\u0431\u0441\u0443\u0436\u0434\u0435\u043d \u0434\u0438\u0437\u0430\u0439\u043d \u043d\u0435\u0434\u0430\u0432\u043d\u043e \u0434\u043e\u0431\u0430\u0432\u043b\u0435\u043d\u043d\u044b\u0445 \u0432 \u044f\u0437\u044b\u043a \u0441\u0440\u0435\u0434\u0441\u0442\u0432: \u0448\u0430\u0431\u043b\u043e\u043d\u043e\u0432, \u0438\u0441\u043a\u043b\u044e\u0447\u0435\u043d\u0438\u0439, \u0438\u0434\u0435\u043d\u0442\u0438\u0444\u0438\u043a\u0430\u0446\u0438\u0438 \u0442\u0438\u043f\u0430 \u0432\u043e \u0432\u0440\u0435\u043c\u044f \u0438\u0441\u043f\u043e\u043b\u043d\u0435\u043d\u0438\u044f \u0438 \u043f\u0440\u043e\u0441\u0442\u0440\u0430\u043d\u0441\u0442\u0432 \u0438\u043c\u0435\u043d. \u0410\u0432\u0442\u043e\u0440 \u0430\u043d\u0430\u043b\u0438\u0437\u0438\u0440\u0443\u0435\u0442 \u0440\u0435\u0448\u0435\u043d\u0438\u044f, \u043f\u0440\u0438\u043d\u044f\u0442\u044b\u0435 \u0432 \u0445\u043e\u0434\u0435 \u0440\u0430\u0431\u043e\u0442\u044b \u043d\u0430\u0434 \u044f\u0437\u044b\u043a\u043e\u043c, \u0438 \u0434\u0435\u043c\u043e\u043d\u0441\u0442\u0440\u0438\u0440\u0443\u0435\u0442, \u043a\u0430\u043a \u043f\u0440\u0430\u0432\u0438\u043b\u044c\u043d\u043e \u043f\u0440\u0438\u043c\u0435\u043d\u044f\u0442\u044c "\u0440\u0435\u0430\u043b\u044c\u043d\u044b\u0439 \u043e\u0431\u044a\u0435\u043a\u0442\u043d\u043e-\u043e\u0440\u0438\u0435\u043d\u0442\u0438\u0440\u043e\u0432\u0430\u043d\u043d\