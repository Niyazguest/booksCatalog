INSERT INTO COMMENT VALUES(1436,'\u041c\u043e\u0448\u043a\u043e\u0432 \u0415\u0432\u0433\u0435\u043d\u0438\u0439 \u0412\u0430\u0441\u0438\u043b\u044c\u0435\u0432\u0438\u0447','2015-01-11','\u043f\u0440\u0435\u0434\u043b\u0430\u0433\u0430\u044e \u0441\u0442\u0440\u0430\u043d\u0438\u0446\u044b \u043a\u043d\u0438\u0433\u0438 \u041f\u043e\u043a\u0430\u0437\u0430\u0442\u044c \u0438\u0437\u043e\u0431\u0440\u0430\u0436\u0435\u043d\u0438\u044f \u0421\u043a\u0440\u044b\u0442\u044c \u0438\u0437\u043e\u0431\u0440\u0430\u0436\u0435\u043d\u0438\u044f',3320)
INSERT INTO COMMENT VALUES(1437,'\u0418\u0432\u0430\u043d \u042f\u0433\u043e\u0434\u0437\u0438\u043d\u0441\u043a\u0438\u0439','2015-01-18','\u041d\u0430 \u043c\u043e\u0439 \u0432\u0437\u0433\u043b\u044f\u0434 \u043b\u0443\u0447\u0448\u0430\u044f \u0438 \u0447\u0443\u0442\u044c \u043b\u0438 \u043d\u0435 \u0435\u0434\u0438\u043d\u0441\u0442\u0432\u0435\u043d\u043d\u0430\u044f \u043a\u043d\u0438\u0433\u0430 \u043d\u0430 \u0434\u043e\u0441\u0442\u0443\u043f\u043d\u043e\u043c \u0434\u043b\u044f \u043d\u0430\u0447\u0438\u043d\u0430\u044e\u0449\u0438\u0445 \u044f\u0437\u044b\u043a\u0435 \u043f\u043e \u043c\u0438\u043a\u0440\u043e\u043a\u043e\u043d\u0442\u0440\u043e\u043b\u043b\u0435\u0440\u0430\u043c \u0441\u0435\u043c\u0435\u0439\u0441\u0442\u0432\u0430 MCS-51 (8051). \u0412 \u044d\u0442\u043e\u043c \u0442\u043e \u0435\u0451 \u0438 \u043f\u0440\u0435\u043b\u0435\u0441\u0442\u044c.... \u0414\u0435\u043b\u043e \u0432 \u0442\u043e\u043c \u0447\u0442\u043e \u043f\u043e \u043a\u043e\u043d\u0442\u0440\u043e\u043b\u043b\u0435\u0440\u0430\u043c PIC \u0438\u043b\u0438 AVR \u043c\u043d\u043e\u0433\u043e \u043b\u0438\u0442\u0435\u0440\u0430\u0442\u0443\u0440\u044b \u0430 \u043f\u043e MCS-51 \u043d\u0430\u043c\u043d\u043e\u0433\u043e \u043c\u0435\u043d\u044c\u0448\u0435...\u043d\u043e \u0441\u0430\u043c\u0438 \u043f\u043e \u0441\u0435\u0431\u0435 \u043c\u0438\u043a\u0440\u043e\u043a\u043e\u043d\u0442\u0440\u043e\u043b\u043b\u0435\u0440\u044b MCS-51 \u043e\u0447\u0435\u043d\u044c \u0445\u043e\u0440\u043e\u0448\u0438 \u0434\u043b\u044f \u043e\u0431\u0443\u0447\u0435\u043d\u0438\u044f, \u0438 \u0432 \u043f\u0440\u0438\u043d\u0446\u0438\u043f\u0435 \u043e\u043d\u0438 \u043e\u0447\u0435\u043d\u044c \u043d\u0435 \u043f\u043b\u043e\u0445\u0438.... \u0412 \u043e\u0431\u0449\u0435\u043c \u044f \u0441\u0447\u0438\u0442\u0430\u044e, \u0447\u0442\u043e \u043e\u0447\u0435\u043d\u044c \u0445\u043e\u0440\u043e\u0448\u0430\u044f \u043a\u043d\u0438\u0433\u0430...\u0440\u0435\u043a\u043e\u043c\u0435\u