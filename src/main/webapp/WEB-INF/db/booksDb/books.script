INSERT INTO COMMENT VALUES(1660,'bstrx','2011-01-18','\u041f\u0443\u0441\u0442\u044c \u0432\u0430\u0441 \u043d\u0435 \u043f\u0443\u0433\u0430\u0435\u0442 \u0441\u0442\u043e\u043b\u044c \u0431\u0440\u043e\u0441\u043a\u0438\u0439 \u0434\u0438\u0437\u0430\u0439\u043d \u043e\u0431\u043b\u043e\u0436\u043a\u0438 \u0438 \u0438\u043b\u043b\u044e\u0441\u0442\u0440\u0430\u0446\u0438\u0439, \u043a\u043d\u0438\u0433\u0430 \u044f\u0432\u043b\u044f\u0435\u0442\u0441\u044f \u043b\u0443\u0447\u0448\u0435\u0439 \u0438\u0437 \u043c\u043d\u0435 \u0438\u0437\u0432\u0435\u0441\u0442\u043d\u044b\u0445 \u0442\u0440\u0443\u0434\u043e\u0432 \u0434\u043b\u044f \u0442\u0435\u0445, \u043a\u0442\u043e \u0440\u0435\u0448\u0438\u043b \u043e\u0441\u0432\u043e\u0438\u0442\u044c \u041f\u0430\u0442\u0442\u0435\u0440\u043d\u044b \u043f\u0440\u043e\u0435\u043a\u0442\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u044f. \u0412 \u043e\u0442\u043b\u0438\u0447\u0438\u0435 \u043e\u0442 \u0432\u0441\u0435\u043c \u0438\u0437\u0432\u0435\u0441\u0442\u043d\u043e\u0433\u043e \u0442\u0440\u0443\u0434\u0430 "\u0431\u0430\u043d\u0434\u044b \u0447\u0435\u0442\u044b\u0440\u0451\u0445", \u0434\u0430\u043d\u043d\u0430\u044f \u043a\u043d\u0438\u0433\u0430 \u0434\u0430\u0451\u0442 \u043c\u0430\u043a\u0441\u0438\u043c\u0430\u043b\u044c\u043d\u043e \u0443\u043f\u0440\u043e\u0449\u0451\u043d\u043d\u043e\u0435 \u0438 \u043f\u043e\u0434\u0440\u043e\u0431\u043d\u043e\u0435 \u0440\u0430\u0437\u044a\u044f\u0441\u043d\u0435\u043d\u0438\u0435 \u043f\u0430\u0442\u0442\u0435\u0440\u043d\u0430\u043c. \u0412 \u043a\u0430\u0436\u0434\u043e\u0439 \u0433\u043b\u0430\u0432\u0435 \u0441\u043e\u0434\u0435\u0440\u0436\u0430\u0442\u044c\u0441\u044f \u0437\u0430\u0434\u0430\u043d\u0438\u044f, \u043a\u043e\u0442\u043e\u0440\u044b\u0435 \u0441\u0440\u0430\u0437\u0443 \u0436\u0435 \u0434\u0430\u044e\u0442 \u043f\u043e\u043d\u044f\u0442\u044c, \u0443\u0441\u0432\u043e\u0435\u043d \u043b\u0438 \u043c\u0430\u0442\u0435\u0440\u0438\u0430\u043b \u043f\u043e\u0441\u043b\u0435\u0434\u043d\u0438\u0445 \u0441\u0442\u0440\u0430\u043d\u0438\u0446 \u0438\u043b\u0438 \u043e\u043d\u0438 \u0431\u044b\u043b\u0438 \u043f\u0440\u043e\u0447\u0438\u0442\u0430\u043d\u044b "\u0432 \u043f\u0443\u0441\u0442\u0443\u044e". \u0412 \u043a\u043e\u043d\u0446\u0435 \u0433\u043b\u0430\u0432 \u043a \u0437\u0430\u0434\u0430\u043d\u0438\u044f\u043c \u0434\u0430\u044e\u0442\u0441\u044f \u043e\u0442\u0432\u0435\u0442\u044b. \u041f\u0440\u0438\u043c\u0435\u0440\u044b \u0432 \u043a\u043d\u0438\u0433\u0435 \u043d\u0430\u043f\u0438\u0441\u0430\u043d\u044b \u043d\u0430 Java, \u043d\u043e \u043a\u043d\u0438\u0433\u0430 \u043e\u0431\u044f\u0437\u0430\u0442\u0435\u043b\u044c\u043d\u0430 \u043a \u043f\u0440\u043e\u0447\u0442\u0435\u043d\u0438\u044e \u0432\u043d\u0435 \u0437\u0430\u0432\u0438\u0441\u0438\u043c\u043e\u0441\u0442\u0438 \u043e\u0442 \u043f\u0440\u0435\u0434\u043f\u043e\u0447\u0438\u0442\u0430\u0435\u043c\u043e\u0433\u043e \u044f\u0437\u044b\u043a\u0430. \u0414\u043b\u044f \u0438\u0441\u043a\u0443\u0448\u0451\u043d\u043d\u044b\u0445 \u0432 \u043f\u0430\u0442\u0442\u0435\u0440\u043d\u0430\u0445 \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u0438\u0441\u0442\u043e\u0432 \u043c\u0430\u0442\u0435\u0440\u0438\u0430\u043b \u043f\u043e\u043a\u0430\u0436\u0435\u0442\u0441\u044f \u0438\u0437\u043b\u0438\u0448\u043d\u0438 \u0440\u0430\u0437\u0436\u0451\u0432\u0430\u043d\u043d\u044b\u043c. \u0421\u043a\u0440\u044b\u0442\u044c',3907)
INSERT INTO COMMENT VALUES(1661,'Sasha','2012-01-26','\u042d\u0442\u0430 \u043a\u043d\u0438\u0433\u0430 \u043d\u0435\u0437\u0430\u043c\u0435\u043d\u0438\u043c\u0430 \u0434\u043b\u044f \u0442\u0435\u0445, \u043a\u0442\u043e \u0442\u043e\u043b\u044c\u043a\u043e \u043d\u0430\u0447\u0438\u043d\u0430\u0435\u0442 \u043e\u0441\u0432\u0430\u0438\u0432\u0430\u0442\u044c \u041f\u0430\u0442\u0442\u0435\u0440\u043d\u044b \u043f\u0440\u043e\u0435\u043a\u0442\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u044f. \u0415\u0441\u0442\u044c, \u043a\u043e\u043d\u0435\u0447\u043d\u043e, \u0434\u0440\u0443\u0433\u0430\u044f \u0437\u0430\u043c\u0435\u0447\u0430\u0442\u0435\u043b\u044c\u043d\u0430\u044f \u043a\u043d\u0438\u0433\u0430, \u043a\u043e\u0442\u043e\u0440\u0430\u044f \u0443\u0436\u0435 \u0441\u0442\u0430\u043b\u0430 \u043a\u043b\u0430\u0441\u0441\u0438\u043a\u043e\u0439 - "\u041f\u0440\u0438\u0435\u043c\u044b \u043e\u0431\u044a\u0435\u043a\u0442\u043d\u043e-\u043e\u0440\u0438\u0435\u043d\u0442\u0438\u0440\u043e\u0432\u0430\u043d\u043d\u043e\u0433\u043e \u043f\u0440\u043e\u0435\u043a\u0442\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u044f. \u041f\u0430\u0442\u0442\u0435\u0440\u043d\u044b \u043f\u0440\u043e\u0435\u043a\u0442\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u044f" (http://www.labirint.ru/books/87603/), \u043d\u043e \u043e\u043d\u0430 \u043c\u043e\u0436\u0435\u0442 \u043f\u043e\u043a\u0430\u0437\u0430\u0442\u044c\u0441\u044f \u0441\u043b\u043e\u0436\u043d\u043e\u0439 \u0434\u043b\u044f \u043d\u0435\u043e\u043f\u044b\u0442\u043d\u043e\u0433\u043e \u0440\u0430\u0437\u0440\u0430\u0431\u043e\u0442\u0447\u0438\u043a\u0430. \u0422\u0430\u043a \u0447\u0442\u043e \u043b\u0443\u0447\u0448\u0435 \u043d\u0430\u0447\u0430\u0442\u044c \u0441 \u0434\u0430\u043d\u043d\u043e\u0433\u043e \u0438\u0437\u0434\u0430\u043d\u0438\u044f. \u0421\u0441\u044b\u043b\u043a\u0438 \u043f\u043e \u0442\u0435\u043c\u0435: \u041f\u0440\u0438\u0435\u043c\u044b \u043e\u0431\u044a\u0435\u043a\u0442\u043d\u043e-\u043e