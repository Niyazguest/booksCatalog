INSERT INTO BOOK VALUES(3495,369712,'\u041c\u0435\u043b\u0430\u043d\u0441\u043e\u043d, \u041d\u043e\u0440\u0434\u0438\u043d, \u041b\u0443\u0438\u0441\u0438: \u041f\u0440\u043e\u0444\u0435\u0441\u0441\u0438\u043e\u043d\u0430\u043b\u044c\u043d\u0430\u044f \u0440\u0430\u0437\u0440\u0430\u0431\u043e\u0442\u043a\u0430 \u0441\u0430\u0439\u0442\u043e\u0432 \u043d\u0430 Drupal 7','\u041c\u0435\u043b\u0430\u043d\u0441\u043e\u043d \u0411., \u041d\u043e\u0440\u0434\u0438\u043d \u0414., \u041b\u0443\u0438\u0441\u0438 \u0416.','\u0421\u0435\u0440\u0433\u0438\u0435\u043d\u043a\u043e \u042e.','\u041f\u0438\u0442\u0435\u0440, 2013 \u0433.',647.0E0,'978-5-4461-0054-5','688 (\u041e\u0444\u0441\u0435\u0442)','\u0422\u0438\u043f \u043e\u0431\u043b\u043e\u0436\u043a\u0438: \u043e\u0431\u043b - \u043c\u044f\u0433\u043a\u0438\u0439 \u043f\u0435\u0440\u0435\u043f\u043b\u0435\u0442 (\u043a\u0440\u0435\u043f\u043b\u0435\u043d\u0438\u0435 \u0441\u043a\u0440\u0435\u043f\u043a\u043e\u0439 \u0438\u043b\u0438 \u043a\u043b\u0435\u0435\u043c) \u041e\u0444\u043e\u0440\u043c\u043b\u0435\u043d\u0438\u0435: \u0427\u0430\u0441\u0442\u0438\u0447\u043d\u0430\u044f \u043b\u0430\u043a\u0438\u0440\u043e\u0432\u043a\u0430 \u0418\u043b\u043b\u044e\u0441\u0442\u0440\u0430\u0446\u0438\u0438: \u0427\u0435\u0440\u043d\u043e-\u0431\u0435\u043b\u044b\u0435','786 \u0433','235x165x30 \u043c\u043c','\u0410\u043d\u043d\u043e\u0442\u0430\u0446\u0438\u044f \u043a \u043a\u043d\u0438\u0433\u0435 "\u041f\u0440\u043e\u0444\u0435\u0441\u0441\u0438\u043e\u043d\u0430\u043b\u044c\u043d\u0430\u044f \u0440\u0430\u0437\u0440\u0430\u0431\u043e\u0442\u043a\u0430 \u0441\u0430\u0439\u0442\u043e\u0432 \u043d\u0430 Drupal 7" \u042d\u0442\u0430 \u043a\u043d\u0438\u0433\u0430 \u043f\u0440\u0435\u0434\u0441\u0442\u0430\u0432\u043b\u044f\u0435\u0442 \u0441\u043e\u0431\u043e\u0439 \u043d\u0430\u0438\u0431\u043e\u043b\u0435\u0435 \u043f\u043e\u043b\u043d\u043e\u0435 \u0440\u0443\u043a\u043e\u0432\u043e\u0434\u0441\u0442\u0432\u043e \u043f\u043e