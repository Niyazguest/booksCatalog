INSERT INTO BOOK VALUES(3985,235739,'\u0413\u0435\u0440\u0431\u0435\u0440\u0442 \u0428\u0438\u043b\u0434\u0442: \u0421++ \u0434\u043b\u044f \u043d\u0430\u0447\u0438\u043d\u0430\u044e\u0449\u0438\u0445','\u0428\u0438\u043b\u0434\u0442 \u0413\u0435\u0440\u0431\u0435\u0440\u0442','\u0417\u0430\u0445\u0430\u0440\u043e\u0432\u0430 \u041b.','\u042d\u043a\u043e\u043c, 2010 \u0433.',975.0E0,'978-5-9790-0127-2','640 (\u041e\u0444\u0441\u0435\u0442)','\u0422\u0438\u043f \u043e\u0431\u043b\u043e\u0436\u043a\u0438: \u043e\u0431\u043b - \u043c\u044f\u0433\u043a\u0438\u0439 \u043f\u0435\u0440\u0435\u043f\u043b\u0435\u0442 (\u043a\u0440\u0435\u043f\u043b\u0435\u043d\u0438\u0435 \u0441\u043a\u0440\u0435\u043f\u043a\u043e\u0439 \u0438\u043b\u0438 \u043a\u043b\u0435\u0435\u043c) \u0418\u043b\u043b\u044e\u0441\u0442\u0440\u0430\u0446\u0438\u0438: \u0427\u0435\u0440\u043d\u043e-\u0431\u0435\u043b\u044b\u0435','774 \u0433','228x162x28 \u043c\u043c','\u0410\u043d\u043d\u043e\u0442\u0430\u0446\u0438\u044f \u043a \u043a\u043d\u0438\u0433\u0435 "\u0421++ \u0434\u043b\u044f \u043d\u0430\u0447\u0438\u043d\u0430\u044e\u0449\u0438\u0445" \u041f\u0440\u0435\u0434\u043b\u0430\u0433\u0430\u0435\u043c\u0430\u044f \u043a\u043d\u0438\u0433\u0430 \u043f\u0440\u0435\u0434\u0441\u0442\u0430\u0432\u043b\u044f\u0435\u0442 \u0441\u043e\u0431\u043e\u0439 \u0441\u0430\u043c\u043e\u0443\u0447\u0438\u0442\u0435\u043b\u044c, \u043a\u043e\u0442\u043e\u0440\u044b\u0439 \u0448\u0430\u0433 \u0437\u0430 \u0448\u0430\u0433\u043e\u043c \u0440\u0430\u0441\u0441\u043a\u0430\u0436\u0435\u0442 \u043e \u0432\u0441\u0435\u0445 \u043e\u0441\u043d\u043e\u0432\u043d\u044b\u0445 \u043f\u043e\u043d\u044f\u0442\u0438\u044f\u0445 \u044f\u0437\u044b\u043a\u0430 \u0421++, \u0432\u043a\u043b\u044e\u0447\u0430\u044f \u043f\u0435\u0440\u0435\u043c\u0435\u043d\u043d\u044b\u0435, \u0438\u043d\u0441\u0442\u0440\u0443\u043a\u0446\u0438\u0438 \u0443\u043f\u0440\u0430\u0432\u043b\u0435\u043d\u0438\u044f, \u0444\u0443\u043d\u043a\u0446\u0438\u0438, \u0442\u0438\u043f\u044b \u0438 \u043c\u0430\u0441\u0441\u0438\u0432\u044b \u0434\u0430\u043d\u043d\u044b\u0445, \u043a\u043b\u0430\u0441\u0441\u044b \u0438 \u043e\u0431\u044a\u0435\u043a\u0442\u044b. \u041a\u0440\u043e\u043c\u0435 \u0442\u043e\u0433\u043e, \u0432\u044b \u0443\u0437\u043d\u0430\u0435\u0442\u0435 \u043e \u043f\u0435\u0440\u0435\u0433\u0440\u0443\u0437\u043a\u0430\u0445, \u0443\u043f\u0440\u0430\u0432\u043b\u0435\u043d\u0438\u0438 \u0438\u0441\u043a\u043b\u044e\u0447\u0435\u043d\u0438\u044f\u043c\u0438, \u043d\u0430\u0441\u043b\u0435\u0434\u043e\u0432\u0430\u043d\u0438\u0438, \u0432\u0438\u0440\u0442\u0443\u0430\u043b\u044c\u043d\u044b\u0445 \u0444\u0443\u043d\u043a\u0446\u0438\u044f\u0445, \u043f\u043e\u043b\u0438\u043c\u043e\u0440\u0444\u0438\u0437\u043c\u0435, \u0432\u0432\u043e\u0434\u0435/\u0432\u044b\u0432\u043e\u0434\u0435. \u0420\u0430\u0441\u0441\u043c\u043e\u0442\u0440\u0435\u043d\u044b \u0438 \u0431\u043e\u043b\u0435\u0435 \u0441\u043b\u043e\u0436\u043d\u044b\u0435 \u0441\u0440\u0435\u0434\u0441\u0442\u0432\u0430 \u044f\u0437\u044b\u043a\u0430, \u0442\u0430\u043a\u0438\u0435 \u043a\u0430\u043a \u0448\u0430\u0431\u043b\u043e\u043d\u044b \u0438 \u043f\u0440\u043e\u0441\u0442\u0440\u0430\u043d\u0441\u0442\u0432\u0430 \u0438\u043c\u0435\u043d. \u041d\u0430\u0447\u0438\u043d\u0430\u0439\u0442\u0435 \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u0438\u0440\u043e\u0432\u0430\u0442\u044c \u043f\u0440\u044f\u043c\u043e \u0441\u0435\u0439\u0447\u0430\u0441 - \u0434\u0430\u043d\u043d\u0430\u044f \u043a\u043d\u0438\u0433\u0430 \u043f\u043e\u043c\u043e\u0436\u0435\u0442 \u0432\u0430\u043c \u0432 \u044d\u0442\u043e\u0439 \u0440\u0430\u0431\u043e\u0442\u0435. \u0412 \u043a\u043d\u0438\u0433\u0435 \u043e\u043f\u0438\u0441\u0430\u043d\u044b: \u0422\u0438\u043f\u044b \u0434\u0430\u043d\u043d\u044b\u0445 \u0438 \u043e\u043f\u0435\u0440\u0430\u0442\u043e\u0440\u044b \u0438 \u0423\u043f\u0440\u0430\u0432\u043b\u044f\u044e\u0449\u0438\u0435 \u043a\u043e\u043d\u0441\u0442\u0440\u0443\u043a\u0446\u0438\u0438 \u041a\u043b\u0430\u0441\u0441\u044b, \u043e\u0431\u044a\u0435\u043a\u0442\u044b \u0438 \u0448\u0430\u0431\u043b\u043e\u043d\u044b \u0438 \u0418\u043d\u0442\u0435\u0440\u0444\u0435\u0439\u0441\u044b, \u043c\u0430\u0441\u0441\u0438\u0432\u044b \u0438 \u043c\u0435\u0442\u043e\u0434\u044b \u041f\u0435\u0440\u0435\u0447\u0438\u0441\u043b\u0435\u043d\u0438\u044f \u0438 \u0441\u0442\u0440\u0443\u043a\u0442\u0443\u0440\u044b \u0438 \u041f\u0435\u0440\u0435\u0433\u0440\u0443\u0437\u043a\u0430 \u043e\u043f\u0435\u0440\u0430\u0442\u043e\u0440\u043e\u0432 \u041d\u0430\u0441\u043b\u0435\u0434\u043e\u0432\u0430\u043d\u0438\u0435 \u0438 \u041e\u0431\u0440\u0430\u0431\u043e\u0442\u043a\u0430 \u0438\u0441\u043a\u043b\u044e\u0447\u0435\u043d\u0438\u0439 \u0414\u0435\u043b\u0435\u0433\u0430\u0442\u044b, \u0441\u0432\u043e\u0439\u0441\u0442\u0432\u0430, \u0441\u043e\u0431\u044b\u0442\u0438\u044f \u0438 \u0418\u043d\u0434\u0435\u043a\u0441\u0430\u0442\u043e\u0440\u044b \u0438 \u0430\u0442\u0440\u0438\u0431\u0443\u0442\u044b \u0423\u043a\u0430\u0437\u0430\u0442\u0435\u043b\u0438 \u0438 \u043f\u043e\u043b\u0438\u043c\u043e\u0440\u0444\u0438\u0437\u043c \u0438 \u041c\u043d\u043e\u0433\u043e\u043f\u043e\u0442\u043e\u0447\u043d\u043e\u0435 \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u0435 \u041e\u0431\u043e\u0431\u0449\u0435\u043d\u043d\u044b\u0435 \u0442\u0438\u043f\u044b \u0438 \u041e\u0431\u043d\u0443\u043b\u044f\u0435\u043c\u044b\u0435 \u0442\u0438\u043f\u044b \u0410\u043d\u043e\u043d\u0438\u043c\u043d\u044b\u0435 \u043c\u0435\u0442\u043e\u0434\u044b \u0438 \u041a\u043b\u0430\u0441\u0441\u044b \u043a\u043e\u043b\u043b\u0435\u043a\u0446\u0438\u0439 \u041a\u043b\u0430\u0441\u0441\u044b \u0432\u0432\u043e\u0434\u0430/\u0432\u044b\u0432\u043e\u0434\u0430 \u0438 \u0420\u0430\u0431\u043e\u0442\u0430 \u0432 \u0441\u0435\u0442\u0438 \u0412\u0438\u0440\u0442\u0443\u0430\u043b\u044c\u043d\u044b\