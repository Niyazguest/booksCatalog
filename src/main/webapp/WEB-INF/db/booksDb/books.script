INSERT INTO BOOK VALUES(3438,408450,'\u0420\u043e\u0431\u0435\u0440\u0442 \u041a\u0430\u0431\u0430\u043a\u043e\u0432: R \u0432 \u0434\u0435\u0439\u0441\u0442\u0432\u0438\u0438. \u0410\u043d\u0430\u043b\u0438\u0437 \u0438 \u0432\u0438\u0437\u0443\u0430\u043b\u0438\u0437\u0430\u0446\u0438\u044f \u0434\u0430\u043d\u043d\u044b\u0445 \u043d\u0430 \u044f\u0437\u044b\u043a\u0435 R','\u041a\u0430\u0431\u0430\u043a\u043e\u0432 \u0420\u043e\u0431\u0435\u0440\u0442 \u0418.','\u041c\u043e\u0432\u0447\u0430\u043d \u0414. \u0410.','\u0414\u041c\u041a-\u041f\u0440\u0435\u0441\u0441, 2014 \u0433.',1610.0E0,'978-5-94074-912-7\u00a0 ','580 (\u041e\u0444\u0441\u0435\u0442)','\u0422\u0438\u043f \u043e\u0431\u043b\u043e\u0436\u043a\u0438: \u043e\u0431\u043b - \u043c\u044f\u0433\u043a\u0438\u0439 \u043f\u0435\u0440\u0435\u043f\u043b\u0435\u0442 (\u043a\u0440\u0435\u043f\u043b\u0435\u043d\u0438\u0435 \u0441\u043a\u0440\u0435\u043f\u043a\u043e\u0439 \u0438\u043b\u0438 \u043a\u043b\u0435\u0435\u043c) \u0418\u043b\u043b\u044e\u0441\u0442\u0440\u0430\u0446\u0438\u0438: \u0427\u0435\u0440\u043d\u043e-\u0431\u0435\u043b\u044b\u0435','630 \u0433','205x140x30 \u043c\u043c','\u0410\u043d\u043d\u043e\u0442\u0430\u0446\u0438\u044f \u043a \u043a\u043d\u0438\u0433\u0435 "R \u0432 \u0434\u0435\u0439\u0441\u0442\u0432\u0438\u0438. \u0410\u043d\u0430\u043b\u0438\u0437 \u0438 \u0432\u0438\u0437\u0443\u0430\u043b\u0438\u0437\u0430\u0446\u0438\u044f \u0434\u0430\u043d\u043d\u044b\u0445 \u043d\u0430 \u044f\u0437\u044b\u043a\u0435 R" R - \u044d\u0442\u043e \u043c\u043e\u0449\u043d\u044b\u0439 \u044f\u0437\u044b\u043a \u0434\u043b\u044f \u0441\u0442\u0430\u0442\u0438\u0441\u0442\u0438\u0447\u0435\u0441\u043a\u0438\u0445 \u0432\u044b\u0447\u0438\u0441\u043b\u0435\u043d\u0438\u0439 \u0438 \u0433\u0440\u0430\u0444\u0438\u043a\u0438, \u043a\u043e\u0442\u043e\u0440\u044b\u0439 \u043c\u043e\u0436\u0435\u0442 \u0441\u043f\u0440\u0430\u0432\u0438\u0442\u0441\u044f \u043f\u043e\u0438\u0441\u0442\u0438\u043d\u0435 \u044