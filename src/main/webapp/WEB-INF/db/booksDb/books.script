INSERT INTO COMMENT VALUES(1509,'\u041a\u043d\u0443\u0442\u043e\u0432\u0430 \u0422\u0430\u0442\u044c\u044f\u043d\u0430','2014-01-07','\u041d\u0430 \u0441\u0430\u043c\u043e\u0439 \u043a\u043d\u0438\u0433\u0435 \u043d\u0430\u043f\u0438\u0441\u0430\u043d\u043e, \u0447\u0442\u043e \u043f\u0440\u0438\u0441\u0443\u0442\u0441\u0442\u0432\u0443\u0435\u0442 \u0434\u0438\u0441\u043a, \u043d\u043e \u0432 \u043d\u0430\u043b\u0438\u0447\u0438\u0438 \u0435\u0433\u043e , \u043a \u0441\u043e\u0436\u0430\u043b\u0435\u043d\u0438\u044e, \u043d\u0435\u0442.',3430)
INSERT INTO COMMENT VALUES(1510,'\u041f\u0430\u0440\u0445\u043e\u043c\u0435\u043d\u043a\u043e \u0420\u0443\u0441\u043b\u0430\u043d','2015-01-01','\u041a\u043d\u0438\u0433\u0430 \u0441\u043b\u043e\u0436\u043d\u0430\u044f, \u043d\u0435 \u0434\u043b\u044f \u043d\u043e\u0432\u0438\u0447\u043a\u043e\u0432. \u041c\u0430\u0442\u0435\u0440\u0438\u0430\u043b \u0438\u0437\u043b\u0430\u0433\u0430\u0435\u0442\u0441\u044f "\u0441\u0443\u0445\u0438\u043c" \u044f\u0437\u044b\u043a\u043e\u043c \u0443\u0447\u0435\u0431\u043d\u0438\u043a\u0430 \u043c\u0430\u0442\u0435\u043c\u0430\u0442\u0438\u043a\u0438. \u041c\u043d\u043e\u0433\u0438\u0435 \u043d\u044e\u0430\u043d\u0441\u044b \u043e\u043f\u0443\u0441\u043a\u0430\u044e\u0442\u0441\u044f, \u043f\u043e\u044f\u0441\u043d\u0435\u043d\u0438\u044f, \u0437\u0430\u0447\u0430\u0441\u0442\u0443\u044e, \u043f\u0440\u0438\u0432\u043e\u0434\u044f\u0442\u0441\u044f \u0432 \u0432\u0438\u0434\u0435 \u043c\u0430\u0442\u0435\u043c\u0430\u0442\u0438\u0447\u0435\u0441\u043a\u0438\u0445 \u0432\u044b\u0440\u0430\u0436\u0435\u043d\u0438\u0439, \u0445\u043e\u0442\u044f \u043d\u0438\u0447\u0442\u043e \u043d\u0435 \u043c\u0435\u0448\u0430\u043b\u043e \u0442\u043e\u0436\u0435 \u0441\u0430\u043c\u043e\u0435 \u0432\u044b\u0440\u0430\u0437\u0438\u0442\u044c \u0441\u043b\u043e\u0432\u0430\u043c\u0438. \u041c\u043d\u043e\u0433\u0438\u0435 \u043f\u0440\u0438\u043c\u0435\u0440\u044b \u0435\u0449\u0451 \u0437\u0430\u043f\u0443\u0442\u0430\u043d\u043d\u0435\u0435 \u0438 \u0441\u043b\u043e\u0436\u043d\u0435\u0435, \u0447\u0435\u043c \u043e\u0431\u044a\u044f\u0441\u043d\u0435\u043d\u0438\u044f \u0440\u0430\u0431\u043e\u0442\u044b \u0430\u043b\u0433\u043e\u0440\u0438\u0442\u043c\u043e\u0432. \u0412 \u043e\u0431\u0449\u0435\u043c, \u0442\u0435, \u043a\u0442\u043e \u043d\u043e\u0440\u043c\u0430\u043b\u044c\u043d\u043e \u0441\u043c\u043e\u0436\u0435\u0442 \u0432\u043e\u0441\u043f\u0440\u0438\u043d\u044f\u0442\u044c \u0442\u0430\u043a\u043e\u0439 \u043c\u0430\u0442\u0435\u0440\u0438\u0430\u043b, \u0441\u043a\u043e\u0440\u0435\u0435 \u0432\u0441\u0435\u0433\u043e \u0432\u0441\u0451 \u044d\u0442\u043e \u0443\u0436\u0435 \u0437\u043d\u0430\u044e\u0442, \u0430 \u043a\u0442\u043e \u043d\u0435 \u0437\u043d\u0430\u0435\u0442, \u043f\u043e\u0439\u043c\u0451\u0442 \u0441 \u0432\u0435\u043b\u0438\u043a\u0438\u043c \u0442\u0440\u0443\u0434\u043e\u043c...',3430)
INSERT INTO COMMENT VALUES(1511,'Divergence','2014-01-11','\u0412 \u043a\u043d\u0438\u0433\u0435 \u043e\u043f\u0438\u0441\u0430\u043d\u0430 \u0438\u0441\u0442\u043e\u0440\u0438\u044f \u0441\u043e\u0437\u0434\u0430\u043d\u0438\u044f \u0421++. \u041a\u043d\u0438\u0433\u0430 \u043f\u043e\u043c\u043e\u0433\u0430\u0435\u0442 \u043b\u0443\u0447\u0448\u0435 \u043f\u043e\u043d\u044f\u0442\u044c \u043d\u0435\u043a\u043e\u0442\u043e\u0440\u044b\u0435 \u043c\u0435\u043b\u043a\u0438\u0435, \u043d\u043e \u043e\u0447\u0435\u043d\u044c \u0432\u0430\u0436\u043d\u044b\u0439 \u0430\u0441\u043f\u0435\u043a\u0442\u044b \u044f\u0437\u044b\u043a\u0430 \u0438 \u0447\u0438\u0442\u0430\u0435\u0442\u0441\u044f \u043b\u0435\u0433\u043a\u043e (\u0434\u043b\u044f \u0442\u0435\u0445\u043d\u0438\u0447\u0435\u0441\u043a\u043e\u0439 \u043b\u0438\u0442\u0435\u0440\u0430\u0442\u0443\u0440\u044b). \u0412 \u043f\u0440\u043e\u0446\u0435\u0441\u0441\u0435 \u043f\u0440\u043e\u0447\u0442\u0435\u043d\u0438\u044f, \u0443 \u043c\u0435\u043d\u044f \u0432\u044b\u0440\u0430\u0431\u043e\u0442\u0430\u043b\u043e\u0441\u044c \u0433\u043b\u0443\u0431\u043e\u043a\u043e\u0435 \u0443\u0432\u0430\u0436\u0435\u043d\u0438\u0435 \u043a \u0411\u044c\u0435\u0440\u043d\u0443 \u0421\u0442\u0440\u0430\u0443\u0441\u0442\u0440\u0443\u043f\u0443, \u043d\u0430\u0441\u0442\u043e\u043b\u044c\u043a\u043e \u044d\u0442\u043e\u0442 \u0447\u0435\u043b\u043e\u0432\u0435\u043a \u043f\u043e\u0441\u043b\u0435\u0434\u043e\u0432\u0430\u0442\u0435\u043b\u0435\u043d, \u0434\u0435\u043c\u043e\u043a\u0440\u0430\u0442\u0438\u0447\u0435\u043d \u0438 \u043c\u0443\u0434\u0440. \u041f\u0440\u0438\u0448\u043b\u043e \u043f\u043e\u043d\u0438\u043c\u0430\u043d\u0438\u0435, \u0447\u0442\u043e \u0443 \u044f\u0437\u044b\u043a\u0430 \u0435\u0441\u0442\u044c \u043d\u0435\u043a\u043e\u0442\u043e\u0440\u044b\u0435 \u043f\u0440\u043e\u0431\u043b\u0435\u043c\u044b, \u043d\u043e \u0438\u0437\u0431\u0430\u0432\u0438\u0442\u044c\u0441\u044f \u043e\u0442 \u043d\u0438\u0445 \u043f\u043e\u043a\u0430 \u043d\u0435 \u043f\u043e\u043b\u0443\u0447\u0430\u0435\u0442\u0441\u044f \u043f\u043e \u0440\u044f\u0434\u0443 \u0432\u0430\u0436\u043d\u044b\u0445 \u043f\u0440\u0438\u0447\u0438\u043d, \u043e \u043a\u043e\u0442\u043e\u0440\u044b\u0445 \u043c\u0430\u043b\u043e \u043a\u0442\u043e \u0437\u0430\u0434\u0443\u043c\u044b\u0432\u0430\u0435\u0442\u0441\u044f. \u041a\u043d\u0438\u0433\u0430 \u043e\u0442\u043b\u0438\u0447\u043d\u0430\u044f, \u0441\u043e\u0432\u0435\u0442\u0443\u044e \u043f\u0440\u043e\u0447\u0438\u0442\u0430\u0442\u044c \u0432\u0441\u0435\u043c, \u043a\u0442\u043e \u0438\u043d\u0442\u0435\u0440\u0435\u0441\u0443\u0435\u0442\u0441\u044f \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u0435\u043c \u043d\u0430 \u0421\u0438++. \u0415\u0441\u0442\u044c \u043f\u0440\u0435\u0442\u0435\u043d\u0437\u0438\u0438 \u0442\u043e\u043b\u044c\u043a\u043e \u043a \u0438\u0437\u0434\u0430\u0442\u0435\u043b\u044e \u0414\u041c\u041a-\u043f\u0440\u0435\u0441\u0441: \u043a\u0430\u043a \u043e\u0431\u044b\u0447\u043d\u043e \u043c\u043d\u043e\u0433\u043e \u043e\u043f\u0435\u0447\u0430\u0442\u043e\u043a, \u043e\u043d\u0438 \u043d\u0435 \u0432\u043b\u0438\u044f\u044e\u0442 \u043d\u0430 \u043f\u043e\u043d\u0438\u043c\u0430\u043d\u0438\u0435 \u0442\u0435\u043a\u0441\u0442\u0430, \u043d\u043e \u0440\u0430\u0437\u0434\u0440\u0430\u0436\u0430\u044e\u0442 \u0438 \u0440\u0430\u0441\u0441\u0442\u0440\u0430\u0438\u0432\u0430\u044e\u0442; \u0442\u0430\u043a \u0436\u0435 \u0443 \u043a\u043d\u0438\u0433\u0438 \u043d\u0435\u0443\u0434\u0430\u0447\u043d\u044b\u0439 \u043f\u0435\u0440\u0435\u043f\u043b\u0435\u0442 \u0438 \u0447\u0442\u043e\u0431\u044b \u043f\u0440\u043e\u0447\u0435\u0441\u0442\u044c \u0431\u0443\u043a\u0432\u044b \u043d\u0430 \u043f\u0440\u0430\u0432\u043e\u043c \u043a\u0440\u0430\u0435 \u043b\u0435\u0432\u043e\u0439 \u0441\u0442\u0440\u0430\u043d\u0438\u0446\u0435, \u043f\u0440\u0438\u0445\u043e\u0434\u0438\u0442\u0441\u044f \u043a\u043d\u0438\u0433\u0443 \u0440\u0430\u0441\u043a\u0440\u044b\u0432\u0430\u0442\u044c \u0441\u043b\u0438\u0448\u043a\u043e\u043c \u0441\u0438\u043b\u044c\u043d\u043e, \u0447\u0442\u043e \u043a \u043a\u043e\u043d\u0446\u0443 \u0447\u0442\u0435\u043d\u0438\u044f \u043c\u043e\u0436\u0435\u0442 \u0443\u043d\u0438\u0447\u0442\u043e\u0436\u0438\u0442\u044c \u043a\u043b\u0435\u0435\u0432\u043e\u0439 \u043f\u0435\u0440\u0435\u043f\u043b\u0435\u0442. \u0411\u0443\u043c\u0430\u0433\u0430 \u043a\u0430\u0447\u0435\u0441\u0442\u0432\u0435\u043d\u043d\u0430\u044f, \u0431\u0435\u043b\u0430\u044f, \u0431\u0443\u043a\u0432\u044b \u043c\u0435\u043b\u043a\u043e\u0432\u0430\u0442\u044b, \u043d\u043e \u0447\u0438\u0442\u0430\u0435\u0442\u0441\u044f \u0434\u043e\u0441\u0442\u0430\u0442\u043e\u0447\u043d\u043e \u043a\u043e\u043c\u0444\u043e\u0440\u0442\u043d\u043e. \u0410\u0432\u0442\u043e\u0440\u0443 \u0432\u0441\u0435 10 \u0437\u0432\u0435\u0437\u0434, \u0438\u0437\u0434\u0430\u0442\u0435\u043b\u044e 5 \u0437\u0432\u0435\u0437\u0434 (\u0433\u043b\u0443\u043f\u044b\u0435 \u043e\u043f\u0435\u0447\u0430\u0442\u043a\u0438 \u0443\u0436\u0435 \u043d\u0435 \u0432 \u043f\u0435\u0440\u0432\u043e\u0439 \u043a\u043d\u0438\u0433\u0435 \u043e\u0442 \u043d\u0438\u0445, \u0447\u0438\u0442\u0430\u0442\u044c \u043a\u043d\u0438\u0433\u0443 \u0432 \u0442\u0430\u043a\u043e\u043c \u0444\u043e\u0440\u043c\u0430\u0442\u0435 \u0438 \u043f\u0435\u0440\u0435\u043f\u043b\u0435\u0442\u0435 \u043d\u0435\u0443\u0434\u043e\u0431\u043d\u043e). \u0421\u043a\u0440\u044b\u0442\u044c',3433)
INSERT INTO COMMENT VALUES(1512,'mkam','2013-01-22','\u041e\u0434\u0438\u043d \u0438\u0437 \u043a\u0430\u043d\u043e\u043d\u0438\u0447\u0435\u0441\u043a\u0438\u0445 \u0442\u0440\u0443\u0434\u043e\u0432 \u043f\u043e \u044f\u0437\u044b\u043a\u0443 \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u044f \u0421++. \u0422\u0440\u0435\u0431\u0443\u0435\u0442 \u0432\u043d\u0438\u043c\u0430\u0442\u0435\u043b\u044c\u043d\u043e\u0433\u043e \u0438 \u0432\u0434\u0443\u043c\u0447\u0438\u0432\u043e\u0433\u043e \u0432\u0447\u0438\u0442\u044b\u0432\u0430\u043d\u0438\u044f. \u0411\u043b\u0430\u0433\u043e\u0434\u0430\u0440\u044f \u0442\u043e\u043c\u0443, \u0447\u0442\u043e \u0430\u0432\u0442\u043e\u0440 \u0441\u043c\u043e\u0433 \u043f\u0435\u0440\u0435\u0434\u0430\u0442\u044c \u043d\u0435 \u043f\u0440\u043e\u0441\u0442\u043e \u0440\u0435\u0448\u0435\u043d\u0438\u044f \u043a\u043e\u043d\u043a\u0440\u0435\u0442\u043d\u044b\u0445 \u0437\u0430\u0434\u0430\u0447, \u0430 \u043e\u043f\u0438\u0441\u0430\u0442\u044c \u043c\u0435\u0442\u043e\u0434\u0438\u043a\u0443 \u0440\u0435\u0448\u0435\u043d\u0438\u044f \u0437\u0430\u0434\u0430\u0447 \u0441 \u0438\u0441\u043f\u043e\u043b\u044c\u0437\u043e\u0432\u0430\u043d\u0438\u0435\u043c \u044f\u0437\u044b\u043a\u0430 \u043d\u0430 \u0431\u043e\u043b\u0435\u0435 \u0430\u0431\u0441\u0442\u0440\u0430\u043a\u0442\u043d\u043e\u043c \u0443\u0440\u043e\u0432\u043d\u0435, \u043a\u043d\u0438\u0433\u0430 \u043d\u0435 \u043f\u043e\u0442\u0435\u0440\u044f\u043b\u0430 \u0430\u043a\u0442\u0443\u0430\u043b\u044c\u043d\u043e\u0441\u0442\u0438 \u043f\u043e\u0447\u0442\u0438 \u0437\u0430 \u0434\u0432\u0430\u0434\u0446\u0430\u0442\u0438 \u043b\u0435\u0442\u043d\u044e\u044e \u0438\u0441\u0442\u043e\u0440\u0438\u044e. \u041e\u0431\u044f\u0437\u0430\u0442\u0435\u043b\u044c\u043d\u043e \u043a \u043f\u0440\u043e\u0447\u0442\u0435\u043d\u0438\u044e \u043a\u0430\u043a \u043b\u044e\u0434\u044f\u043c \u0442\u043e\u043b\u044c\u043a\u043e \u0437\u0430\u043a\u043e\u043d\u0447\u0438\u0432\u0448\u0438\u043c \u0447\u0438\u0442\u0430\u0442\u044c \u0443\u0447\u0435\u0431\u043d\u0438\u043a \u043d\u0430\u0447\u0430\u043b\u044c\u043d\u043e\u0433\u043e \u0443\u0440\u043e\u0432\u043d\u044f \u043f\u043e \u0434\u0430\u043d\u043d\u043e\u043c\u0443 \u044f\u0437\u044b\u043a\u0443, \u0442\u0430\u043a \u0438 \u0431\u043e\u043b\u0435\u0435 \u043e\u043f\u044b\u0442\u043d\u044b\u043c \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u0438\u0441\u0442\u0430\u043c, \u0438\u0441\u043f\u043e\u043b\u044c\u0437\u0443\u044e\u0449\u0438\u043c \u0432 \u0441\u0432\u043e\u0439 \u0440\u0430\u0431\u043e\u0442\u0435 C++. \u0421\u043a\u0440\u044b\u0442\u044c',3434)
INSERT INTO COMMENT VALUES(1513,'\u0422\u0443\u043b\u0435\u043f\u043e\u0432 \u0414\u0430\u043c\u0438\u0440','2014-01-24','\u041e\u0442\u043b\u0438\u0447\u043d\u0430\u044f \u043a\u043d\u0438\u0433\u0430 \u0434\u043b\u044f \u0442\u0435\u0445, \u043a\u0442\u043e \u0445\u043e\u0447\u0435\u0442 \u0440\u0430\u0437\u043e\u0431\u0440\u0430\u0442\u044c\u0441\u044f \u0438\u0437\u043d\u0443\u0442\u0440\u0438 \u043a\u0430\u043a \u0440\u0430\u0431\u043e\u0442\u0430\u0435\u0442 CLR. \u0412\u043d\u0430\u0447\u0430\u043b\u0435 \u0440\u0430\u0441\u0441\u043c\u0430\u0442\u0440\u0438\u0432\u0430\u0435\u0442\u0441\u044f \u043e\u0447\u0435\u043d\u044c \u043c\u043d\u043e\u0433\u043e \u043f\u043e\u043b\u0435\u0437\u043d\u044b\u0445 \u0438\u043d\u0441\u0442\u0440\u0443\u043c\u0435\u043d\u0442\u043e\u0432 \u0434\u043b\u044f \u043f\u0440\u043e\u0444\u0438\u043b\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u044f \u0438 \u043e\u043f\u0442\u0438\u043c\u0438\u0437\u0430\u0446\u0438\u0438 \u043f\u0440\u0438\u043b\u043e\u0436\u0435\u043d\u0438\u0439 (\u043a\u0430\u043a \u0432\u0441\u0442\u0440\u043e\u0435\u043d\u043d\u044b\u0435 \u0432 windows, \u0442\u0430\u043a \u0438 \u0441\u0442\u043e\u0440\u043e\u043d\u043d\u0438\u0435). \u041f\u043e\u0441\u043b\u0435 \u0440\u0430\u0441\u0441\u043a\u0430\u0437\u044b\u0432\u0430\u0435\u0442\u0441\u044f \u043e \u0432\u043d\u0443\u0442\u0440\u0435\u043d\u043d\u0435\u043c \u0443\u0441\u0442\u0440\u043e\u0439\u0441\u0442\u0432\u0435 \u0442\u0438\u043f\u043e\u0432 \u0432 .Net, \u043e\u0447\u0435\u043d\u044c \u043c\u043d\u043e\u0433\u043e \u043f\u0440\u0438\u043c\u0435\u0440\u043e\u0432 \u043a\u043e\u0434\u0430, MSIL, \u0435\u0441\u0442\u044c \u043f\u0440\u0438\u043c\u0435\u0440\u044b \u043d\u0430 \u0430\u0441\u0441\u0435\u043c\u0431\u043b\u0435\u0440\u0435. \u0412 \u043e\u0431\u044b\u0447\u043d\u044b\u0445 \u043a\u043d\u0438\u0433\u0430\u0445 \u043d\u0435 \u0440\u0430\u0441\u0441\u043a\u0430\u0437\u044b\u0432\u0430\u044e\u0442 \u043d\u0430\u0441\u0442\u043e\u043b\u044c\u043a\u043e \u0433\u043b\u0443\u0431\u043e\u043a\u043e \u043e \u0432\u043d\u0443\u0442\u0440\u0435\u043d\u043d\u0435\u043c \u0443\u0441\u0442\u0440\u043e\u0439\u0441\u0442\u0432\u0435 \u043f\u043b\u0430\u0442\u0444\u043e\u0440\u043c\u044b .Net. \u041f\u043e\u0441\u043b\u0435 \u043e\u0447\u0435\u043d\u044c \u043f\u043e\u0434\u0440\u043e\u0431\u043d\u043e \u0440\u0430\u0441\u0441\u043c\u0430\u0442\u0440\u0438\u0432\u0430\u0435\u0442\u0441\u044f \u0443\u0441\u0442\u0440\u043e\u0439\u0441\u0442\u0432\u043e Garbage Collector''\u0430. \u0422\u0430\u043a\u0436\u0435 \u0432 \u043a\u043d\u0438\u0433\u0435 \u0435\u0441\u0442\u044c \u0441\u043e\u0432\u0435\u0442\u044b \u0434\u043b\u044f \u043e\u043f\u0442\u0438\u043c\u0438\u0437\u0430\u0446\u0438\u0438 web \u043f\u0440\u0438\u043b\u043e\u0436\u0435\u043d\u0438\u0439, \u043e\u043f\u0442\u0438\u043c\u0438\u0437\u0430\u0446\u0438\u0438 \u0430\u043b\u0433\u043e\u0440\u0438\u0442\u043c\u043e\u0432, \u0441\u043e\u0432\u0435\u0442\u044b \u043f\u043e \u0432\u044b\u0431\u043e\u0440\u0443 \u043f\u043e\u0434\u0445\u043e\u0434\u044f\u0449\u0438\u0445 \u0434\u043b\u044f \u0440\u0430\u0437\u043b\u0438\u0447\u043d\u044b\u0445 \u0437\u0430\u0434\u0430\u0447 \u043a\u043e\u043b\u043b\u0435\u043a\u0446\u0438\u0439. \u0412\u0441\u044f \u043a\u043d\u0438\u0433\u0430 \u0441\u043e\u043f\u0440\u043e\u0432\u043e\u0436\u0434\u0430\u0435\u0442\u0441\u044f \u0431\u043e\u043b\u044c\u0448\u0438\u043c \u043a\u043e\u043b-\u043c \u043f\u0440\u0438\u043c\u0435\u0440\u043e\u0432, \u0434\u0438\u0430\u0433\u0440\u0430\u043c\u043c (\u0443\u043f\u0440\u043e\u0449\u0430\u044e\u0449\u0438\u0445 \u043f\u043e\u043d\u0438\u043c\u0430\u043d\u0438\u0435). \u0415\u0441\u043b\u0438 \u0432\u044b \u0445\u043e\u0442\u0438\u0442\u0435 \u043f\u043e\u043d\u044f\u0442\u044c \u043a\u0430\u043a \u0438\u0437\u043d\u0443\u0442\u0440\u0438 \u0440\u0430\u0431\u043e\u0442\u0430\u0435\u0442 \u043f\u043b\u0430\u0442\u0444\u043e\u0440\u043c\u0430 .Net \u0438 CLR - \u043a\u043d\u0438\u0433\u0430 \u0434\u043b\u044f \u0432\u0430\u0441. \u0421\u043a\u0440\u044b\u0442\u044c',3437)
INSERT INTO COMMENT VALUES(1514,'adeliya','2015-01-03','\u0423\u043d\u0438\u043a\u0430\u043b\u044c\u043d\u044b\u0439 \u043c\u0430\u0442\u0435\u0440\u0438\u0430\u043b. \u041e\u0447\u0435\u043d\u044c \u043f\u043e\u043b\u0435\u0437\u0435\u043d \u0442\u0435\u043c, \u043a\u0442\u043e \u0432\u0441\u0435\u0440\u044c\u0435\u0437 \u043e\u0437\u0430\u0431\u043e\u0447\u0435\u043d \u0445\u0430\u0439-\u043f\u0435\u0440\u0444\u043e\u043c\u0430\u043d\u0441\u043e\u043c \u0432 .NET',3437)
INSERT INTO COMMENT VALUES(1515,'arawacus','2015-01-22','\u041b\u0443\u0447\u0448\u0430\u044f \u043a\u043d\u0438\u0433\u0430 \u043f\u0440\u043e \u044f\u0437\u044b\u043a R \u043d\u0430 \u0440\u0443\u0441\u0441\u043a\u043e\u043c \u044f\u0437\u044b\u043a\u0435. \u041f\u043e\u0434\u0440\u043e\u0431\u043d\u0430\u044f \u0438 \u043e\u0441\u043d\u043e\u0432\u0430\u0442\u0435\u043b\u044c\u043d\u0430\u044f \u0441 \u043f\u0440\u0438\u043c\u0435\u0440\u0430\u043c\u0438. \u0428\u0430\u0433 \u0437\u0430 \u0448\u0430\u0433\u043e\u043c \u043f\u043e\u043c\u043e\u0436\u0435\u0442 \u043e\u0441\u0432\u043e\u0438\u0442\u044c \u044d\u0442\u043e\u0442 \u043d\u0435\u0437\u0430\u043c\u0435\u043d\u0438\u043c\u044b\u0439 \u0432 \u0441\u043e\u0432\u0440\u0435\u043c\u0435\u043d\u043d\u043e\u043c \u043d\u0430\u0443\u0447\u043d\u043e\u043c \u043c\u0438\u0440\u0435 \u0441\u0442\u0430\u0442\u0438\u0441\u0442\u0438\u0447\u0435\u0441\u043a\u0438\u0439 \u0438\u043d\u0441\u0442\u0440\u0443\u043c\u0435\u043d\u0442. \u0412\u043c\u0435\u0441\u0442\u0435 \u0441 \u041d\u0430\u0433\u043b\u044f\u0434\u043d\u043e\u0439 \u0441\u0442\u0430\u0442\u0438\u0441\u0442\u0438\u043a\u043e\u0439 - \u0437\u0430\u043c\u0435\u0447\u0430\u0442\u0435\u043b\u044c\u043d\u044b\u0439 \u0443\u0447\u0435\u0431\u043d\u0438\u043a. \u0422\u043e\u0432\u0430\u0440\u044b \u043f\u043e \u0442\u0435\u043c\u0435: "\u041d\u0430\u0433\u043b\u044f\u0434\u043d\u0430\u044f \u0441\u0442\u0430\u0442\u0438\u0441\u0442\u0438\u043a\u0430. \u0418\u0441\u043f\u043e\u043b\u044c\u0437\u0443\u0435\u043c R!" \u041f\u0435\u0442\u0440\u043e\u0432, \u0428\u0438\u043f\u0443\u043d\u043e\u0432, \u0411\u0430\u043b\u0434\u0438\u043d, \u0412\u043e\u043b\u043a\u043e\u0432\u0430, \u041a\u043e\u0440\u043e\u0431\u0435\u0439\u043d\u0438\u043a\u043e\u0432, \u041d\u0430\u0437\u0430\u0440\u043e\u0432\u0430, \u0421\u0443\u0444\u0438\u044f\u043d\u043e\u0432 \u041f\u043e\u043a\u0430\u0437\u0430\u0442\u044c \u0438\u0437\u043e\u0431\u0440\u0430\u0436\u0435\u043d\u0438\u044f \u0421\u043a\u0440\u044b\u0442\u044c \u0438\u0437\u043e\u0431\u0440\u0430\u0436\u0435\u043d\u0438\u044f',3438)
