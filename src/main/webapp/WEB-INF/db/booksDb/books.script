INSERT INTO BOOK VALUES(3483,374110,'\u0422\u0435\u0440\u0435\u0437\u0430 \u041d\u0435\u0439\u043b: \u041c\u043e\u0431\u0438\u043b\u044c\u043d\u0430\u044f \u0440\u0430\u0437\u0440\u0430\u0431\u043e\u0442\u043a\u0430. \u0413\u0430\u043b\u0435\u0440\u0435\u044f \u0448\u0430\u0431\u043b\u043e\u043d\u043e\u0432','\u041d\u0435\u0439\u043b \u0422\u0435\u0440\u0435\u0437\u0430','\u041a\u0430\u043b\u044f\u0435\u0432\u0430 \u0415.','\u041f\u0438\u0442\u0435\u0440, 2013 \u0433.',520.0E0,'978-5-496-00154-0','208 (\u041e\u0444\u0441\u0435\u0442)','\u0422\u0438\u043f \u043e\u0431\u043b\u043e\u0436\u043a\u0438: \u043e\u0431\u043b - \u043c\u044f\u0433\u043a\u0438\u0439 \u043f\u0435\u0440\u0435\u043f\u043b\u0435\u0442 (\u043a\u0440\u0435\u043f\u043b\u0435\u043d\u0438\u0435 \u0441\u043a\u0440\u0435\u043f\u043a\u043e\u0439 \u0438\u043b\u0438 \u043a\u043b\u0435\u0435\u043c) \u0418\u043b\u043b\u044e\u0441\u0442\u0440\u0430\u0446\u0438\u0438: \u0427\u0435\u0440\u043d\u043e-\u0431\u0435\u043b\u044b\u0435 + \u0446\u0432\u0435\u0442\u043d\u044b\u0435','314 \u0433','235x165x11 \u043c\u043c','\u0410\u043d\u043d\u043e\u0442\u0430\u0446\u0438\u044f \u043a \u043a\u043d\u0438\u0433\u0435 "\u041c\u043e\u0431\u0438\u043b\u044c\u043d\u0430\u044f \u0440\u0430\u0437\u0440\u0430\u0431\u043e\u0442\u043a\u0430. \u0413\u0430\u043b\u0435\u0440\u0435\u044f \u0448\u0430\u0431\u043b\u043e\u043d\u043e\u0432" \u041a\u043e\u0433\u0434\u0430 \u0432\u0430\u043c \u043f\u043e\u0442\u0440\u0435\u0431\u0443\u0435\u0442\u0441\u044f \u0441\u0440\u043e\u0447\u043d\u043e \u0441\u043e\u0437\u0434\u0430\u0442\u044c \u0445\u043e\u0440\u043e\u0448\u043e \u0441\u043f\u0440\u043e\u0435\u043a\u0442\u0438\u0440\u043e\u0432\u0430\u043d\u043d\u043e\u0435, \u043b\u0435\u0433\u043a\u043e\u0435 \u0432 \u043d\u0430\u0432\u0438\u0433\u0430\u0446\u0438\u0438 \u043c\u043e\u0431\u0438\u043b\u044c\u043d\u043e\u0435 \u043f\u0440\u0438\u043b\u043e\u0436\u0435\u043d\u0438\u0435, \u0432\u0440\u0435\u043c\u0435\u043d\u0438, \u0447\u0442\u043e\u