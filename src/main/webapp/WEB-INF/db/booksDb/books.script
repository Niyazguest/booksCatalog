INSERT INTO BOOK VALUES(3821,336202,'\u0414\u0436\u0438\u043d \u0411\u0430\u043a\u043b\u0438\u043d: \u041f\u0440\u043e\u0444\u0435\u0441\u0441\u0438\u043e\u043d\u0430\u043b\u044c\u043d\u043e\u0435 \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u0435 \u043f\u0440\u0438\u043b\u043e\u0436\u0435\u043d\u0438\u0439 \u0434\u043b\u044f iPhone \u0438 iPad','\u0411\u0430\u043a\u043b\u0438\u043d \u0414\u0436\u0438\u043d','\u041e\u0431\u0440\u0443\u0447\u0435\u0432 \u0412.','\u042d\u043a\u0441\u043c\u043e, 2012 \u0433.',1230.0E0,'978-5-699-55828-5','672 (\u041e\u0444\u0441\u0435\u0442)','\u0422\u0438\u043f \u043e\u0431\u043b\u043e\u0436\u043a\u0438: 7\u0411 - \u0442\u0432\u0435\u0440\u0434\u0430\u044f (\u043f\u043b\u043e\u0442\u043d\u0430\u044f \u0431\u0443\u043c\u0430\u0433\u0430 \u0438\u043b\u0438 \u043a\u0430\u0440\u0442\u043e\u043d) \u041e\u0444\u043e\u0440\u043c\u043b\u0435\u043d\u0438\u0435: \u0427\u0430\u0441\u0442\u0438\u0447\u043d\u0430\u044f \u043b\u0430\u043a\u0438\u0440\u043e\u0432\u043a\u0430 \u0418\u043b\u043b\u044e\u0441\u0442\u0440\u0430\u0446\u0438\u0438: \u0427\u0435\u0440\u043d\u043e-\u0431\u0435\u043b\u044b\u0435','948 \u0433','240x170x35 \u043c\u043c','\u0410\u043d\u043d\u043e\u0442\u0430\u0446\u0438\u044f \u043a \u043a\u043d\u0438\u0433\u0435 "\u041f\u0440\u043e\u0444\u0435\u0441\u0441\u0438\u043e\u043d\u0430\u043b\u044c\u043d\u043e\u0435 \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u0435 \u043f\u0440\u0438\u043b\u043e\u0436\u0435\u043d\u0438\u0439 \u0434\u043b\u044f iPhone \u0438 iPad" \u0421\u043e\u0437\u0434\u0430\u0432\u0430\u0442\u044c \u043d\u043e\u0432\u044b\u0435 \u043f\u0440\u0438\u043b\u043e\u0436\u0435\u043d\u0438\u044f \u043f\u043e\u0434 iPhone SDK \u0438 \u043f\u0435\u0440\u0435\u043d\u043e\u0441\u0438\u0442\u044c \u043d\u0430 \u044d\u0442\u0443 \u043f\u043b\u0430\u0442\u0444\u043e\u0440\u043c\u0443 \u0443\u0436\u0435 \u0441\u0443\u0449\u0435\u0441\u0442\