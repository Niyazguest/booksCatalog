INSERT INTO COMMENT VALUES(1723,'\u041a\u043b\u0438\u043c\u0430\u0448\u0435\u0432\u0441\u043a\u0438\u0439 \u0410\u043b\u0435\u043a\u0441\u0430\u043d\u0434\u0440 \u0421\u0442\u0430\u043d\u0438\u0441\u043b\u0430\u0432\u043e\u0432\u0438\u0447','2011-01-16','\u041d\u0435 \u0441\u043e\u0432\u0441\u0435\u043c \u0441\u043e\u043e\u0442\u0432\u0435\u0442\u0441\u0442\u0432\u0443\u0435\u0442 \u043d\u0430\u0437\u0432\u0430\u043d\u0438\u044e. \u0410 \u0438\u043c\u0435\u043d\u043d\u043e \u0432 \u0447\u0430\u0441\u0442\u0438 "\u041e\u0441\u0432\u043e\u0439 \u0441\u0430\u043c\u043e\u0441\u0442\u043e\u044f\u0442\u0435\u043b\u044c\u043d\u043e". \u041a\u043d\u0438\u0433\u0430 \u0441\u043a\u043e\u0440\u0435\u0435 \u0432\u0441\u0435\u0433\u043e \u043f\u0440\u0435\u0434\u043d\u0430\u0437\u043d\u0430\u0447\u0435\u043d\u0430 \u0434\u043b\u044f \u0441\u043f\u0435\u0446\u0438\u0430\u043b\u0438\u0441\u0442\u0430 \u0437\u043d\u0430\u044e\u0449\u0435\u0433\u043e \u043e\u0441\u043d\u043e\u0432\u044b \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u044f. \u041e\u0442\u0441\u0443\u0442\u0441\u0442\u0432\u0438\u0435 \u043f\u043e\u0434\u0440\u043e\u0431\u043d\u044b\u0445 \u043e\u0431\u044a\u044f\u0441\u043d\u0435\u043d\u0438\u0439 \u043a\u043e\u043c\u043c\u0430\u043d\u0434 \u043e\u0441\u043d\u043e\u0432\u043d\u043e\u0439 \u043d\u0435\u0434\u043e\u0441\u0442\u0430\u0442\u043e\u043a. \u041f\u0440\u0438\u0445\u043e\u0434\u0438\u0442\u0441\u044f \u0438\u0441\u043a\u0430\u0442\u044c \u0438\u043d\u0444\u043e\u0440\u043c\u0430\u0446\u0438\u044e \u0432 \u0434\u0440\u0443\u0433\u0438\u0445 \u0438\u0441\u0442\u043e\u0447\u043d\u0438\u043a\u0430\u0445.',3968)
INSERT INTO COMMENT VALUES(1724,'\u041a\u0430\u0440\u043f\u0435\u0447\u0435\u043d\u043a\u043e \u0414\u0435\u043d\u0438\u0441','2015-01-23','\u041d\u0435\u043f\u043b\u043e\u0445\u0430\u044f \u043a\u043d\u0438\u0433\u0430 \u0434\u043b\u044f \u043d\u0430\u0447\u0438\u043d\u0430\u044e\u0449\u0438\u0445 \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u0438\u0441\u0442\u043e\u0432, \u0445\u043e\u0440\u043e\u0448\u0438\u0435 \u043f\u0440\u0438\u043c\u0435\u0440\u044b \u0441 \u043e\u0431\u043e\u0441\u043d\u043e\u0432\u0430\u043d\u0438\u044f\u043c\u0438.',3971)
INSERT INTO COMMENT VALUES(1725,'\u0421\u0438\u0434\u0435\u043b\u044c\u043d\u0438\u043a\u043e\u0432 \u0412\u0430\u0441\u0438\u043b\u0438\u0439','2014-01-04','\u041f\u043e \u043c\u0435\u0440\u0435 \u043f\u0440\u043e\u0447\u0442\u0435\u043d\u0438\u044f \u044d\u0442\u043e\u0439 \u043a\u043d\u0438\u0433\u0438 \u0441\u043e\u0437\u0434\u0430\u0451\u0442\u0441\u044f \u0432\u043f\u0435\u0447\u0430\u0442\u043b\u0435\u043d\u0438\u0435, \u0447\u0442\u043e \u0441\u043e\u0437\u0434\u0430\u0442\u0435\u043b\u0438 \u0431\u0430\u043d\u043d\u0435\u0440\u043e\u0432-\u0432\u044b\u043c\u043e\u0433\u0430\u0442\u0435\u043b\u0435\u0439 \u0443\u0447\u0438\u043b\u0438\u0441\u044c \u043f\u043e \u043d\u0435\u0439. \u041f\u0440\u0438 \u044d\u0442\u043e\u043c \u0446\u0435\u043b\u044c \u0430\u0432\u0442\u043e\u0440\u0430, \u043d\u0430 \u043c\u043e\u0439 \u0432\u0437\u0433\u043b\u044f\u0434, \u043f\u0440\u043e\u0441\u0442\u043e \u0440\u0430\u0441\u0441\u043a\u0430\u0437\u0430\u0442\u044c \u043e \u0440\u0435\u0434\u043a\u043e \u0438\u0441\u043f\u043e\u043b\u044c\u0437\u0443\u0435\u043c\u044b\u0445 \u0432\u043e\u0437\u043c\u043e\u0436\u043d\u043e\u0441\u0442\u044f\u0445 Windows. \u041c\u043d\u0435 \u043b\u0438\u0447\u043d\u043e \u0431\u043e\u043b\u044c\u0448\u0435 \u0432\u0441\u0435\u0433\u043e \u043f\u043e\u043d\u0440\u0430\u0432\u0438\u043b\u0430\u0441\u044c \u0447\u0430\u0441\u0442\u044c \u043e \u0441\u0435\u0442\u0435\u0432\u044b\u0445 \u0432\u043e\u0437\u043c\u043e\u0436\u043d\u043e\u0441\u0442\u044f\u0445. \u041e\u043f\u0438\u0441\u0430\u043d\u0438\u0435 \u043f\u043e\u043d\u044f\u0442\u043d\u043e\u0435 \u0441 \u043f\u0440\u0438\u043c\u0435\u0440\u0430\u043c\u0438 \u0438 \u043e\u0431\u044a\u044f\u0441\u043d\u0435\u043d\u0438\u044f\u043c\u0438. \u041c\u0438\u043d\u0438\u043c\u0443\u043c \u0442\u0435\u043e\u0440\u0438\u0438 \u0438 \u043c\u0430\u043a\u0441\u0438\u043c\u0443\u043c \u043f\u043e\u043b\u044c\u0437\u044b.',3971)
INSERT INTO COMMENT VALUES(1726,'\u041c\u0438\u0445\u0430\u043b\u044c\u0447\u0443\u043a \u0415\u043a\u0430\u0442\u0435\u0440\u0438\u043d\u0430','2015-01-01','\u041a\u043d\u0438\u0433\u0430 \u043f\u043e\u043d\u0440\u0430\u0432\u0438\u043b\u0430\u0441\u044c. \u0412\u043e\u0437\u043c\u043e\u0436\u043d\u043e, \u043e\u043d\u0430 \u0434\u0435\u0439\u0441\u0442\u0432\u0438\u0442\u0435\u043b\u044c\u043d\u043e \u043d\u0435 \u043f\u043e\u0434\u0445\u043e\u0434\u0438\u0442 \u0434\u043b\u044f \u043e\u0431\u0443\u0447\u0435\u043d\u0438\u044f \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u044e \u0441 \u0442\u043e\u0447\u043a\u0438 \u0437\u0440\u0435\u043d\u0438\u044f \u0444\u0443\u043d\u0434\u0430\u043c\u0435\u043d\u0442\u0430\u043b\u044c\u043d\u043e\u0433\u043e \u043f\u043e\u0434\u0445\u043e\u0434\u0430, \u043d\u043e \u0443\u043d\u0438\u043a\u0430\u043b\u044c\u043d\u0430 \u043f\u0440\u0438\u043c\u0435\u0440\u0430\u043c\u0438 \u0438 \u043b\u0435\u0433\u043a\u043e\u0441\u0442\u044c\u044e \u0438\u0437\u043b\u043e\u0436\u0435\u043d\u0438\u044f. \u0410\u0432\u0442\u043e\u0440 \u043f\u0438\u0441\u0430\u043b \u043d\u0435 \u0443\u0447\u0435\u0431\u043d\u0438\u043a, \u0430 \u0441\u0432\u043e\u0439 \u0430\u0432\u0442\u043e\u0440\u0441\u043a\u0438\u0439 \u0432\u0437\u0433\u043b\u044f\u0434 \u043d\u0430 \u0442\u0430\u043a\u043e\u0435 \u044f\u0432\u043b\u0435\u043d\u0438\u0435, \u043a\u0430\u043a \u044f\u0437\u044b\u043a \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u044f Delphi. \u0421\u0430\u043c\u043e \u0438\u0437\u0434\u0430\u043d\u0438\u0435 \u0441\u0434\u0435\u043b\u0430\u043d\u043e \u0434\u0435\u0448\u0435\u0432\u043e. \u0413\u0430\u0437\u0435\u0442\u043d\u0430\u044f \u0431\u0443\u043c\u0430\u0433\u0430, \u043a\u043b\u0435\u0435\u043d\u044b\u0439 \u043f\u0435\u0440\u0435\u043f\u043b\u0435\u0442, \u043c\u044f\u0433\u043a\u0430\u044f \u043e\u0431\u043b\u043e\u0436\u043a\u0430. \u041a \u043a\u043d\u0438\u0433\u0435 \u043f\u0440\u0438\u043b\u0430\u0433\u0430\u0435\u0442\u0441\u044f CD \u0441 \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u0430\u043c\u0438 \u0438 \u0434\u043e\u043f\u043e\u043b\u043d\u0438\u0442\u0435\u043b\u044c\u043d\u044b\u043c\u0438 \u043c\u0430\u0442\u0435\u0440\u0438\u0430\u043b\u0430\u043c\u0438. \u041f\u0440\u0438\u043b\u0430\u0433\u0430\u044e \u0440\u0435\u0430\u043b\u044c\u043d\u044b\u0435 \u0444\u043e\u0442\u043e\u0433\u0440\u0430\u0444\u0438\u0438 \u0440\u0435\u0430\u043b\u044c\u043d\u043e\u0439 \u043a\u043d\u0438\u0433\u0438, \u0430 \u043d\u0435 \u0441\u043a\u0430\u043d\u044b. \u041f\u043e\u043a\u0430\u0437\u0430\u0442\u044c \u0438\u0437\u043e\u0431\u0440\u0430\u0436\u0435\u043d\u0438\u044f \u0421\u043a\u0440\u044b\u0442\u044c \u0438\u0437\u043e\u0431\u0440\u0430\u0436\u0435\u043d\u0438\u044f',3973)
INSERT INTO COMMENT VALUES(1727,'\u043b\u044b\u043b\u043e\u0432 \u044e\u0440\u0438\u0439','2015-01-07','\u043a\u0430\u043a \u0443\u0447\u0435\u0431\u043d\u0438\u043a \u0438 "\u0431\u0438\u0431\u043b\u0438\u044f" \u0432\u0440\u044f\u0434 \u043b\u0438 \u043f\u043e\u0434\u043e\u0439\u0434\u0435\u0442 \u043d\u0430\u0447\u0438\u043d\u0430\u044e\u0449\u0438\u043c, \u0438\u0431\u043e \u043c\u043d\u043e\u0433\u0438\u0435 \u043f\u0440\u0438\u043c\u0435\u0440\u044b \u044d\u0442\u043e \u0445\u043e\u0440\u043e\u0448\u043e, \u043d\u043e \u043e\u0431\u044a\u044f\u0441\u043d\u0435\u043d\u0438\u0435 \u0447\u0430\u0441\u0442\u043e \u043f\u043e\u0432\u0435\u0440\u0445\u043d\u043e\u0441\u0442\u043d\u043e\u0435, \u0430\u0432\u0442\u043e\u0440 \u0431\u0435\u0437 \u0441\u043e\u043c\u043d\u0435\u043d\u0438\u044f \u0437\u043d\u0430\u0435\u0442 \u043f\u0440\u0435\u0434\u043c\u0435\u0442 ,\u043d\u043e \u044d\u0442\u043e \u0431\u0430\u0437\u044b \u0434\u043b\u044f \u043f\u043e\u043d\u0438\u043c\u0430\u043d\u0438\u044f \u0447\u0438\u0442\u0430\u044e\u0449\u0438\u0445 \u043d\u0435 \u0434\u0430\u0435\u0442, \u043c\u043d\u043e\u0433\u043e "\u0445\u0438\u0442\u0440\u043e\u0441\u0442\u0435\u0439" \u0440\u0430\u0437\u043d\u044b\u0445 \u0440\u0430\u0431\u043e\u0442\u044b \u0441 \u043a\u043e\u043c\u043f\u043e\u043d\u0435\u043d\u0442\u0430\u043c\u0438, \u043f\u0440\u0430\u043a\u0442\u0438\u0447\u0435\u0441\u043a\u0438 \u0432\u0440\u044f\u0434 \u043b\u0438 \u043a\u043e\u043c\u0443 \u043f\u043e\u043d\u0430\u0434\u043e\u0431\u044f\u0442\u0441\u044f...\u043a\u043d\u0438\u0433\u0430 \u0441\u043a\u043e\u0440\u0435\u0435 \u0434\u043b\u044f \u0442\u0435\u0445 \u043a\u0442\u043e \u0443\u0436\u0435 \u0437\u043d\u0430\u0435\u0442 \u043f\u0440\u0435\u0434\u043c\u0435\u0442 \u0438 \u043c\u043e\u0436\u0435\u0442 \u0431\u044b\u0442\u044c \u043a\u0430\u043a \u0440\u0430\u0437 "\u0445\u0438\u0442\u0440\u043e\u0441\u0442\u0435\u0439 " \u043e\u0442 \u0441\u043a\u0443\u043a\u0438 \u0440\u0430\u0437\u0432\u0435 \u043f\u043e\u043d\u0430\u0431\u0440\u0430\u0442\u044c\u0441\u044f \u041e\u0447\u0435\u043d\u044c \u043f\u043e\u0434\u0443\u043c\u0430\u0439\u0442\u0435 \u043f\u0440\u0435\u0436\u0434\u0435 \u0447\u0435\u043c \u043f\u043e\u043a\u0443\u043f\u0430\u0442\u044c, \u0437\u0430\u043c\u0443\u0447\u0430\u0435\u0442\u0435\u0441\u044c "\u0433\u0443\u0433\u043b\u0438\u0442\u044c" \u0440\u0430\u0437\u0431\u0438\u0440\u0430\u044f \u043f\u0440\u0438\u043c\u0435\u0440\u044b \u0421\u043a\u0440\u044b\u0442\u044c',3973)
INSERT INTO COMMENT VALUES(1728,'\u0420\u044b\u0436\u0435\u043d\u044c\u043a\u0438\u0439','2011-01-13','\u0421\u0442\u0440\u0430\u043d\u0438\u0446\u044b \u0438\u0437 \u043a\u043d\u0438\u0433\u0438: \u041f\u043e\u043a\u0430\u0437\u0430\u0442\u044c \u0438\u0437\u043e\u0431\u0440\u0430\u0436\u0435\u043d\u0438\u044f \u0421\u043a\u0440\u044b\u0442\u044c \u0438\u0437\u043e\u0431\u0440\u0430\u0436\u0435\u043d\u0438\u044f',3973)
INSERT INTO COMMENT VALUES(1729,'\u042f\u043b\u0438\u043d\u0430','2011-01-15','\u0421\u0442\u0440\u0430\u043d\u0438\u0446\u044b \u0438\u0437 \u043a\u043d\u0438\u0433\u0438, \u0441\u043e\u0434\u0435\u0440\u0436\u0430\u043d\u0438\u0435: \u041f\u043e\u043a\u0430\u0437\u0430\u0442\u044c \u0438\u0437\u043e\u0431\u0440\u0430\u0436\u0435\u043d\u0438\u044f \u0421\u043a\u0440\u044b\u0442\u044c \u0438\u0437\u043e\u0431\u0440\u0430\u0436\u0435\u043d\u0438\u044f',3974)
INSERT INTO COMMENT VALUES(1730,'\u0413\u0430\u043b\u0443\u0437\u0438\u043d \u0421\u0442\u0430\u043d\u0438\u0441\u043b\u0430\u0432','2012-01-21','\u041a\u043d\u0438\u0433\u0430 "\u0421\u043e\u0432\u0440\u0435\u043c\u0435\u043d\u043d\u043e\u0435 \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u0435 \u0441 \u043d\u0443\u043b\u044f! (+CD)" \u043f\u0440\u0435\u0434\u0441\u0442\u0430\u0432\u043b\u044f\u0435\u0442 \u0441\u043e\u0431\u043e\u0439 \u043f\u043b\u043e\u0445\u043e \u0441\u0432\u044f\u0437\u0430\u043d\u043d\u044b\u0439 \u043d\u0430\u0431\u043e\u0440 \u0438\u0437: -\u043d\u0435\u0434\u043e\u0440\u0430\u0431\u043e\u0442\u0430\u043d\u043d\u043e\u0433\u043e \u0441\u0430\u043c\u043e\u0443\u0447\u0438\u0442\u0435\u043b\u044f(\u0433\u043b\u0430\u0432\u04301"\u041d\u0435\u0444\u043e\u0440\u043c\u0430\u043b\u044c\u043d\u043e\u0435 \u0432\u0432\u0435\u0434\u0435\u043d\u0438\u0435"),\u0432 \u043d\u0435\u0439 \u043d\u0435\u0442 \u043d\u0438 \u043e\u0434\u043d\u043e\u0433\u043e \u043f\u0440\u0438\u043c\u0435\u0440\u0430 \u0434\u043e\u0432\u0435\u0434\u0435\u043d\u043d\u043e\u0433\u043e \u0434\u043e \u043a\u043e\u043d\u0446\u0430,\u0447\u0442\u043e \u0432\u0430\u0436\u043d\u043e \u0434\u043b\u044f \u043d\u0430\u0447\u0430\u043b\u044c\u043d\u043e\u0433\u043e \u044d\u0442\u0430\u043f\u0430 \u0438\u0437\u0443\u0447\u0435\u043d\u0438\u044f \u044f\u0437\u044b\u043a\u0430.\u041d\u0435 \u043f\u043e\u043a\u0430\u0437\u0430\u043d\u044b \u0441\u043f\u043e\u0441\u043e\u0431\u044b \u0437\u0430\u043f\u0443\u0441\u043a\u0430 \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c,\u0441\u043f\u043e\u0441\u043e\u0431\u044b \u0437\u0430\u0434\u0430\u043d\u0438\u044f \u0438\u0441\u0445\u043e\u0434\u043d\u044b\u0445 \u0437\u043d\u0430\u0447\u0435\u043d\u0438\u0439 \u043f\u0430\u0440\u0430\u043c\u0435\u0442\u0440\u043e\u0432 \u043f\u0440\u043e\u0446\u0435\u0434\u0443\u0440.\u041d\u0435\u0442 \u043f\u0440\u0430\u043a\u0442\u0438\u043a\u0438 \u0440\u0430\u0437\u0440\u0430\u0431\u043e\u0442\u043a\u0438 \u0437\u0430\u043a\u043e\u043d\u0447\u0435\u043d\u043d\u044b\u0445 \u043c\u043e\u0434\u0443\u043b\u0435\u0439, \u0430 \u0431\u0435\u0437 \u044d\u0442\u043e\u0433\u043e \u043d\u0435 \u043d\u0430\u0443\u0447\u0438\u0448\u044c. -\u0413\u043b\u0430\u0432\u0430 2.\u0422\u0435\u043e\u0440\u0438\u044f \u044f\u0437\u044b\u043a\u0430, \u0430 \u043a\u0430\u043a \u0435\u0451 \u043f\u0440\u0438\u043c\u0435\u043d\u0438\u0442\u044c \u0432 \u043a\u043e\u043d\u043a\u0440\u0435\u0442\u043d\u044b\u0445 \u0441\u043b\u0443\u0447\u0430\u044f\u0445 \u043d\u0435\u044f\u0441\u043d\u043e.\u0412\u0441\u044f \u0442\u0435\u043e\u0440\u0438\u044f \u0435\u0441\u0442\u044c \u0438 \u0432 CD. -\u0413\u043b\u0430\u0432\u04303."\u041f\u0440\u0430\u043a\u0442\u0438\u043a\u0443\u043c".\u041f\u043e\u043b\u043d\u043e\u0435 \u0440\u0430\u0437\u043e\u0447\u0430\u0440\u043e\u0432\u0430\u043d\u0438\u0435.\u042d\u0442\u0443 \u0433\u043b\u0430\u0432\u0443 \u0441\u043b\u0435\u0434\u043e\u0432\u0430\u043b\u043e \u043d\u0430\u0437\u0432\u0430\u0442\u044c: "\u0421\u0431\u043e\u0440\u043d\u0438\u043a \u0437\u0430\u0434\u0430\u0447 \u0431\u0435\u0437 \u0440\u0435\u0448\u0435\u043d\u0438\u044f". \u041d\u0435 \u043d\u0430\u0443\u0447\u0438\u0432 \u0447\u0438\u0442\u0430\u0442\u0435\u043b\u044f \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u0438\u0440\u043e\u0432\u0430\u0442\u044c \u043d\u0430 \u041a\u041f \u0435\u043c\u0443 \u043f\u0440\u0435\u0434\u043b\u0430\u0433\u0430\u044e\u0442 \u0440\u0435\u0448\u0430\u0442\u044c \u043c\u0430\u0442. \u0437\u0430\u0434\u0430\u0447\u0438, \u0432\u043a\u043b\u044e\u0447\u0430\u044f "\u0413\u0440\u0430\u0444\u044b". \u041a\u043d\u0438\u0433\u0430 \u043c\u043d\u043e\u0433\u043e \u0431\u044b \u0432\u044b\u0438\u0433\u0440\u0430\u043b\u0430, \u0435\u0441\u043b\u0438 \u0431\u044b \u0432 \u0413\u043b\u0430\u0432\u04351 \u0431\u044b\u043b\u0438 \u043f\u0440\u0438\u0432\u0435\u0434\u0435\u043d\u044b \u0441 \u0434\u0435\u0441\u044f\u0442\u043e\u043a \u0440\u0430\u0431\u043e\u0447\u0438\u0445 \u043c\u043e\u0434\u0443\u043b\u0435\u0439 \u0441 \u043f\u043e\u0434\u0440\u043e\u0431\u043d\u044b\u043c \u043e\u043f\u0438\u0441\u0430\u043d\u0438\u0435\u043c \u0438\u0445 \u0441\u043e\u0441\u0442\u0430\u0432\u043b\u0435\u043d\u0438\u044f, \u0440\u0430\u0437\u043c\u0435\u0449\u0435\u043d\u0438\u044f \u0432 \u0440\u0430\u0431\u043e\u0447\u0438\u0445 \u043f\u0430\u043f\u043a\u0430\u0445, \u0441\u043f\u043e\u0441\u043e\u0431\u0430\u0445 \u0437\u0430\u043f\u0443\u0441\u043a\u0430, \u0441\u043e\u0445\u0440\u0430\u043d\u0435\u043d\u0438\u044f \u0440\u0435\u0437\u0443\u043b\u044c\u0442\u0430\u0442\u043e\u0432,\u0438\u043c\u043f\u043e\u0440\u0442\u0430 \u0432 \u0434\u0440\u0443\u0433\u0438\u0435 \u0434\u043e\u043a\u0443\u043c\u0435\u043d\u0442\u044b, \u0442.\u0435.\u0431\u044b\u043b\u0430 \u0431\u044b \u0445\u043e\u0440\u043e\u0448\u0438\u043c \u0441\u0430\u043c\u043e\u0443\u0447\u0438\u0442\u0435\u043b\u0435\u043c. \u041e\u043f\u0438\u0441\u0430\u0442\u0435\u043b\u044c\u043d\u044b\u0439 \u0442\u0430\u043b\u0430\u043d\u0442 \u0430\u0432\u0442\u043e\u0440\u0430 \u043e\u0447\u0435\u0432\u0438\u0434\u0435\u043d. \u0422\u043e, \u0447\u0442\u043e \u043e\u043d \u0445\u043e\u0440\u043e\u0448\u043e \u0437\u043d\u0430\u0435\u0442, \u043e\u043d \u0447\u0435\u0442\u043a\u043e \u0438\u0437\u043b\u0430\u0433\u0430\u0435\u0442.\u041d\u043e \u0432 \u041a\u041f \u043f\u0440\u0438\u043d\u0446\u0438\u043f\u0438\u0430\u043b\u044c\u043d\u043e \u043d\u043e\u0432\u044b\u0439 \u043f\u043e\u0434\u0445\u043e\u0434 \u043a \u043d\u0430\u043f\u0438\u0441\u0430\u043d\u0438\u044e \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c.\u0422\u0435\u043a\u0441\u0442\u044b \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c \u0430\u0432\u0442\u043e\u0440\u0430 \u0438 \u041d.\u0412\u0438\u0440\u0442\u0430 (\u0441\u043c. \u043f\u0440\u0438\u043c\u0435\u0440\u044b \u0438\u0437 \u0435\u0433\u043e \u043a\u043d\u0438\u0433\u0438)\u0441\u0438\u043b\u044c\u043d\u043e \u043e\u0442\u043b\u0438\u0447\u0430\u044e\u0442\u0441\u044f. \u0423\u0441\u043f\u0435\u0445\u043e\u0432 \u0430\u0432\u0442\u043e\u0440\u0443 \u043f\u043e \u0434\u043e\u0440\u0430\u0431\u043e\u0442\u043a\u0430\u043c \u043a\u043d\u0438\u0433\u0438! \u0421\u043a\u0440\u044b\u0442\u044c',3977)
INSERT INTO COMMENT VALUES(1731,'\u041f\u043e\u043d\u043e\u043c\u0430\u0440\u0435\u0432 \u041e\u043b\u0435\u0433','2011-01-13','\u0412 \u0446\u0435\u043b\u043e\u043c \u043a\u043d\u0438\u0433\u0430 \u0445\u043e\u0440\u043e\u0448\u0430\u044f. \u041f\u043e\u0441\u043b\u0435\u0434\u043e\u0432\u0430\u0442\u0435\u043b\u044c\u043d\u043e \u0438\u0437\u043b\u043e\u0436\u0435\u043d \u043c\u0430\u0442\u0435\u0440\u0438\u0430\u043b \u043e \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u0438 \u043d\u0430 \u044f\u0437\u044b\u043a\u0435 Visual Basic Application \u0438 \u043f\u0440\u0438\u043c\u0435\u043d\u0435\u043d\u0438\u0438 \u0435\u0433\u043e \u0432 \u043e\u0444\u0438\u0441\u043d\u044b\u0445 \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u0430\u0445: Word \u0438 Excel. \u041a\u043d\u0438\u0433\u0430 \u0431\u0443\u0434\u0435\u0442 \u043f\u043e\u043b\u0435\u0437\u043d\u0430 \u043a\u0430\u043a \u043d\u0430\u0447\u0438\u043d\u0430\u044e\u0449\u0438\u043c \u043f\u043e\u043b\u044c\u0437\u043e\u0432\u0430\u0442\u0435\u043b\u044f\u043c( \u0434\u0430\u0436\u0435 \u043d\u0435 \u0437\u043d\u0430\u044e\u0449\u0438\u043c \u043e\u0441\u043d\u043e\u0432 \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u044f), \u0442\u0430\u043a \u0438 \u043e\u043f\u044b\u0442\u043d\u044b\u043c \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u0438\u0441\u0442\u0430\u043c. \u0414\u043e\u0441\u0442\u0430\u0442\u043e\u0447\u043d\u043e\u0435 \u043a\u043e\u043b\u0438\u0447\u0435\u0441\u0442\u0432\u043e \u043f\u0440\u0438\u043c\u0435\u0440\u043e\u0432, \u0441\u043a\u0440\u0438\u043d\u0448\u043e\u0442\u044b \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c.',3980)
INSERT INTO COMMENT VALUES(1732,'urgench','2012-01-18','\u041d\u0435\u0434\u043e\u0440\u043e\u0433\u043e \u043e\u0446\u0435\u043d\u0438\u043b\u0438 \u0441\u0435\u0439 \u043f\u0440\u0435\u043a\u0440\u0430\u0441\u043d\u044b\u0439 \u0442\u0440\u0443\u0434!',3980)
INSERT INTO COMMENT VALUES(1733,'\u0412\u0430\u0432\u0443\u043b\u0438\u043d\u0430 \u0418\u043d\u043d\u0430','2011-01-16','\u0421\u043b\u043e\u0436\u043d\u0430\u044f. \u043d\u043e \u043f\u043e\u043b\u0435\u0437\u043d\u0430\u044f',3981)
INSERT INTO COMMENT VALUES(1734,'\u0414\u0430\u0432\u043b\u0435\u0442\u0448\u0438\u043d\u0430 \u042d\u043b\u044c\u0437\u0430','2011-01-07','\u041a\u0443\u043f\u0438\u0442\u044c \u044d\u0442\u0443 \u043a\u043d\u0438\u0433\u0443 \u043f\u043e\u0441\u043e\u0432\u0435\u0442\u043e\u0432\u0430\u043b \u043c\u043e\u0435\u043c\u0443 \u0441\u044b\u043d\u0443 \u043f\u0440\u0435\u043f\u043e\u0434\u0430\u0432\u0430\u0442\u0435\u043b\u044c \u0438\u0437 \u0443\u043d\u0438\u0432\u0435\u0440\u0441\u0438\u0442\u0435\u0442\u0430, \u0441\u043a\u0430\u0437\u0430\u0432 \u043f\u0440\u0438 \u044d\u0442\u043e\u043c: "\u041f\u043e\u0447\u0438\u0442\u0430\u0439 \u0435\u0435 \u0438 \u0442\u044b \u0431\u044b\u0441\u0442\u0440\u043e \u043e\u0441\u0432\u043e\u0438\u0448\u044c \u043c\u0430\u0442\u0435\u0440\u0438\u0430\u043b \u0441\u0430\u043c\u043e\u0441\u0442\u043e\u044f\u0442\u0435\u043b\u044c\u043d\u043e. \u042d\u0442\u043e \u043b\u0443\u0447\u0448\u0430\u044f \u043a\u043d\u0438\u0433\u0430 \u043f\u043e \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u044e". \u041f\u0440\u0435\u043f\u043e\u0434\u0430\u0432\u0430\u0442\u0435\u043b\u044c \u043e\u043a\u0430\u0437\u0430\u043b\u0441\u044f \u043f\u0440\u0430\u0432: \u0441\u044b\u043d, \u0435\u0449\u0435 \u043d\u043e\u0432\u0438\u0447\u043e\u043a \u0432 \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u0438, \u0431\u044b\u0441\u0442\u0440\u043e \u0441\u0442\u0430\u043b \u043e\u0441\u0432\u0430\u0438\u0432\u0430\u0442\u044c \u044f\u0437\u044b\u043a \u041e\u041e\u041f \u0432 \u0421++ \u0438 \u0443\u0436\u0435 \u043f\u0438\u0448\u0435\u0442 \u043f\u0440\u043e\u0441\u0442\u0435\u043d\u044c\u043a\u0438\u0435 \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u044b. \u041f\u043e \u0435\u0433\u043e \u0441\u043b\u043e\u0432\u0430\u043c, \u043a\u043d\u0438\u0433\u0430 \u0447\u0438\u0442\u0430\u0435\u0442\u0441\u044f \u043e\u0447\u0435\u043d\u044c \u043b\u0435\u0433\u043a\u043e. \u041c\u0430\u0442\u0435\u0440\u0438\u0430\u043b, \u0434\u0430\u043d\u043d\u044b\u0439 \u0432 \u043d\u0435\u0439, \u043d\u0435 \u043d\u0430\u0439\u0442\u0438 \u0432 \u0438\u043d\u0442\u0435\u0440\u043d\u0435\u0442\u0435 \u0438 \u043f\u0440\u0435\u043f\u043e\u0434\u0430\u0432\u0430\u0442\u0435\u043b\u044c \u043d\u0435 \u0432 \u0441\u043e\u0441\u0442\u043e\u044f\u043d\u0438\u0438 \u0434\u0430\u0442\u044c \u044d\u0442\u043e\u0442 \u043c\u0430\u0442\u0435\u0440\u0438\u0430\u043b \u0443\u0447\u0435\u043d\u0438\u043a\u0443 \u0437\u0430 \u043a\u043e\u0440\u043e\u0442\u043a\u043e\u0435 \u0432\u0440\u0435\u043c\u044f \u0437\u0430\u043d\u044f\u0442\u0438\u044f. \u0421\u0435\u0439\u0447\u0430\u0441 \u043a\u043d\u0438\u0433\u0430 \u0420.\u041b\u0430\u0444\u043e\u0440\u0435 \u0441\u0442\u0430\u043b\u0430 \u0443 \u043d\u0430\u0441 \u043d\u0430\u0441\u0442\u043e\u043b\u044c\u043d\u043e\u0439 \u043a\u043d\u0438\u0433\u043e\u0439 \u043f\u043e \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u044e. \u0412\u0441\u0435\u043c \u0441\u043e\u0432\u0435\u0442\u0443\u044e! \u041a\u0440\u043e\u043c\u0435 \u0442\u043e\u0433\u043e, \u044f \u0431\u044b\u043b\u0430 \u043f\u0440\u0438\u044f\u0442\u043d\u043e \u0443\u0434\u0438\u0432\u043b\u0435\u043d\u0430 \u043e\u0442\u043b\u0438\u0447\u043d\u044b\u043c \u043a\u0430\u0447\u0435\u0441\u0442\u0432\u043e\u043c \u043f\u0435\u0447\u0430\u0442\u0438 \u0438 \u043e\u0444\u043e\u0440\u043c\u043b\u0435\u043d\u0438\u044f. \u0421\u043a\u0440\u044b\u0442\u044c',3982)
INSERT INTO COMMENT VALUES(1735,'\u041b\u044b\u0436\u0438\u043d \u0418\u0432\u0430\u043d','2012-01-06','\u041a\u043d\u0438\u0433\u0443 \u043f\u0440\u0438\u043e\u0431\u0440\u0435\u043b, \u043f\u0440\u043e\u0447\u0438\u0442\u0430\u0432 \u043e\u0442\u0437\u044b\u0432\u044b \u0434\u0440\u0443\u0433\u0438\u0445 \u043f\u043e\u043a\u0443\u043f\u0430\u0442\u0435\u043b\u0435\u0439. \u0421\u043e\u043c\u043d\u0435\u0432\u0430\u043b\u0441\u044f \u043f\u0440\u0438 \u043f\u043e\u043a\u0443\u043f\u043a\u0435, \u0442\u0430\u043a \u043a\u0430\u043a \u043a\u043d\u0438\u0433\u0430 \u043d\u0435 \u0438\u0437 \u0434\u0435\u0448\u0435\u0432\u044b\u0445, \u043d\u043e \u043e\u043d\u0430 \u0442\u043e\u0433\u043e \u0441\u0442\u043e\u0438\u0442! \u041d\u0438\u0441\u043a\u043e\u043b\u044c\u043a\u043e \u043d\u0435 \u043f\u043e\u0436\u0430\u043b\u0435\u043b, \u043a\u0443\u043f\u0438\u0432 \u0435\u0451. \u041c\u0430\u0442\u0435\u0440\u0438\u0430\u043b \u0447\u0438\u0442\u0430\u0435\u0442\u0441\u044f \u043b\u0435\u0433\u043a\u043e, \u0435\u0441\u0442\u044c \u043c\u0430\u0441\u0441\u0430 \u0432\u043e\u043f\u0440\u043e\u0441\u043e\u0432 \u0438 \u0443\u043f\u0440\u0430\u0436\u043d\u0435\u043d\u0438\u0439 \u0432 \u043a\u043e\u043d\u0446\u0435 \u043a\u0430\u0436\u0434\u043e\u0439 \u0433\u043b\u0430\u0432\u044b. \u0412 \u043f\u0435\u0440\u0432\u044b\u0445 \u0433\u043b\u0430\u0432\u0430\u0445 \u043e\u043f\u0438\u0441\u044b\u0432\u0430\u044e\u0442\u0441\u044f \u043e\u0441\u043d\u043e\u0432\u044b \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u044f \u043d\u0430 \u0421++, \u0437\u0430\u0442\u0435\u043c \u0442\u0435\u043c\u044b \u0443\u0441\u043b\u043e\u0436\u043d\u044f\u044e\u0442\u0441\u044f. \u042f \u043d\u0435 \u043d\u043e\u0432\u0438\u0447\u043e\u043a \u0432 \u0421++, \u043d\u043e \u0443\u0436\u0435, \u043d\u0430\u0447\u0438\u043d\u0430\u044f \u0441 5 \u0433\u043b\u0430\u0432\u044b (\u0430 \u0432\u0441\u0435\u0433\u043e \u0438\u0445 16), \u043d\u0430\u0447\u0430\u043b \u043d\u0430\u0445\u043e\u0434\u0438\u0442\u044c \u043c\u043d\u043e\u0433\u043e \u043d\u043e\u0432\u043e\u0433\u043e. \u0412\u0441\u0435 \u043b\u0438\u0441\u0442\u0438\u043d\u0433\u0438 \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c \u043d\u0430\u043f\u0438\u0441\u0430\u043d\u044b \u0441 \u0443\u0447\u0435\u0442\u043e\u043c \u043f\u043e\u0441\u043b\u0435\u0434\u043d\u0435\u0433\u043e \u0441\u0442\u0430\u043d\u0434\u0430\u0440\u0442\u0430 \u0421++. \u041d\u0435 \u0432\u0441\u0442\u0440\u0435\u0447\u0430\u043b \u0431\u043e\u043b\u044c\u0448\u0435 \u043a\u043d\u0438\u0433, \u0432 \u043a\u043e\u0442\u043e\u0440\u044b\u0445 \u043d\u0430\u0440\u044f\u0434\u0443 \u0441 \u0430\u0437\u0430\u043c\u0438 \u044f\u0437\u044b\u043a\u0430 \u043e\u043f\u0438\u0441\u0430\u043d\u044b \u0442\u0430\u043a\u0438\u0435 \u0432\u0430\u0436\u043d\u044b\u0435 \u043c\u043e\u043c\u0435\u043d\u0442\u044b, \u043a\u0430\u043a "\u0428\u0430\u0431\u043b\u043e\u043d\u044b \u0438 \u0438\u0441\u043a\u043b\u044e\u0447\u0435\u043d\u0438\u044f", "\u0421\u0442\u0430\u043d\u0434\u0430\u0440\u0442\u043d\u0430\u044f \u0431\u0438\u0431\u043b\u0438\u043e\u0442\u0435\u043a\u0430 \u0448\u0430\u0431\u043b\u043e\u043d\u043e\u0432(STL)", "\u041f\u0435\u0440\u0435\u0433\u0440\u0443\u0437\u043a\u0430 \u043e\u043f\u0435\u0440\u0430\u0446\u0438\u0439". \u0414\u043b\u044f \u0430\u0431\u0441\u043e\u043b\u044e\u0442\u043d\u043e\u0433\u043e \u043d\u043e\u0432\u0438\u0447\u043a\u0430 \u0432 \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u0438 \u043a\u043d\u0438\u0433\u0430 \u0431\u0443\u0434\u0435\u0442 \u0441\u043b\u043e\u0436\u043d\u043e\u0439, \u043d\u0443\u0436\u043d\u043e \u0438\u043c\u0435\u0442\u044c \u0445\u043e\u0442\u044f \u0431\u044b \u0431\u0430\u0437\u043e\u0432\u043e\u0435 \u043f\u0440\u0435\u0434\u0441\u0442\u0430\u0432\u043b\u0435\u043d\u0438\u0435 \u043e \u0442\u043e\u043c, \u043a\u0430\u043a \u0441\u043e\u0437\u0434\u0430\u044e\u0442\u0441\u044f \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u044b. \u0421\u043e\u0432\u0435\u0442\u0443\u044e \u043a\u0443\u043f\u0438\u0442\u044c \u0432\u0441\u0435\u043c, \u043a\u0442\u043e \u0438\u0437\u0443\u0447\u0430\u0435\u0442 \u0421++! \u041a\u043d\u0438\u0433\u0430 \u0434\u0435\u0439\u0441\u0442\u0432\u0438\u0442\u0435\u043b\u044c\u043d\u043e \u043f\u043e\u043b\u0435\u0437\u043d\u0430\u044f \u0438 \u043d\u0443\u0436\u043d\u0430\u044f. \u0421\u043a\u0440\u044b\u0442\u044c',3982)
INSERT INTO COMMENT VALUES(1736,'konst216','2011-01-17','\u041e\u0447\u0435\u043d\u044c \u0434\u043e\u043b\u0433\u043e \u0438\u0441\u043a\u0430\u043b \u043a\u043d\u0438\u0433\u0443 \u0432 \u043a\u043e\u0442\u043e\u0440\u043e\u0439 \u043f\u0440\u043e\u0441\u0442\u044b\u043c \u044f\u0437\u044b\u043a\u043e\u043c \u0440\u0430\u0441\u0441\u043a\u0430\u0437\u044b\u0432\u0430\u043b\u043e\u0441\u044c \u043a\u0430\u043a \u043f\u0438\u0441\u0430\u0442\u044c \u043e\u0431\u044a\u0435\u043a\u0442\u043d\u043e-\u043e\u0440\u0438\u0435\u043d\u0442\u0438\u0440\u043e\u0432\u0430\u043d\u043d\u044b\u0439 \u043a\u043e\u0434 \u0432 \u0421++. \u0412\u043e \u0432\u0441\u0435\u0445 \u043a\u043d\u0438\u0433\u0430\u0445, \u043a\u043e\u0442\u043e\u0440\u044b\u0435 \u044f \u0447\u0438\u0442\u0430\u043b \u0434\u043e \u044d\u0442\u043e\u0433\u043e, \u043f\u0440\u043e\u0441\u0442\u043e \u043e\u043f\u0438\u0441\u044b\u0432\u0430\u043b\u0441\u044f \u0441\u0430\u043c \u044f\u0437\u044b\u043a \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u044f \u0441 \u0435\u0433\u043e \u0432\u043e\u0437\u043c\u043e\u0436\u043d\u043e\u0441\u0442\u044f\u043c\u0438 \u0438 \u043d\u0435\u0434\u043e\u0441\u0442\u0430\u0442\u043a\u0430\u043c\u0438, \u0430 \u0432\u043e\u0442 \u043a\u0430\u043a \u043f\u0438\u0441\u0430\u0442\u044c \u043d\u0435\u043f\u043e\u0441\u0440\u0435\u0434\u0441\u0442\u0432\u0435\u043d\u043d\u043e \u0432 \u0441\u0442\u0438\u043b\u0435 \u041e\u041e\u041f \u0442\u043e\u043b\u043a\u043e\u043c \u043d\u0438\u0433\u0434\u0435 \u043d\u0435 \u0433\u043e\u0432\u043e\u0440\u0438\u0442\u044c\u0441\u044f, \u0442\u0430\u043a \u0438 \u0441\u0432\u0430\u043b\u0438\u0432\u0430\u0435\u0448\u044c\u0441\u044f \u0432 \u043f\u0440\u043e\u0446\u0435\u0434\u0443\u0440\u043d\u044b\u0439 \u0441\u0442\u0438\u043b\u044c. \u0412\u0441\u044f \u0441\u0442\u0440\u0443\u043a\u0442\u0443\u0440\u0430 \u0434\u0430\u043d\u043d\u043e\u0439 \u043a\u043d\u0438\u0433\u0438 \u0438 \u0432\u0441\u0435 \u043f\u0440\u0438\u043c\u0435\u0440\u044b \u043f\u043e\u0441\u0442\u0440\u043e\u0435\u043d\u044b \u0442\u0430\u043a\u0438\u043c \u043e\u0431\u0440\u0430\u0437\u043e\u043c, \u0447\u0442\u043e\u0431\u044b \u0441\u0440\u0430\u0437\u0443 \u043d\u0430\u0443\u0447\u0438\u0442\u044c \u043f\u0438\u0441\u0430\u0442\u044c \u0432 \u0441\u0442\u0438\u043b\u0435 \u041e\u041e\u041f. \u041a\u043d\u0438\u0433\u0430 \u043e\u0447\u0435\u043d\u044c \u043f\u043e\u043d\u0440\u0430\u0432\u0438\u043b\u0430\u0441\u044c, \u0441\u0432\u043e\u0435\u0439 \u043b\u0430\u043a\u043e\u043d\u0438\u0447\u043d\u043e\u0441\u0442\u044c\u044e \u0438 \u043d\u0430\u0433\u043b\u044f\u0434\u043d\u043e\u0441\u0442\u044c\u044e. \u0421\u043e\u0432\u0435\u0442\u0443\u044e \u0432\u0441\u0435\u043c \u043a\u0442\u043e \u0445\u043e\u0447\u0435\u0442 \u043d\u0430\u0443\u0447\u0438\u0442\u044c\u0441\u044f \u043f\u0438\u0441\u0430\u0442\u044c \u041e\u041e\u041f \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u044b, \u043d\u0430 \u043c\u043e\u0449\u043d\u043e\u043c \u0438 \u0433\u0438\u0431\u043a\u043e\u043c \u0421++. \u0421\u043a\u0440\u044b\u0442\u044c',3982)
INSERT INTO COMMENT VALUES(1737,'\u041a\u043b\u0438\u043c\u0430\u0448\u0435\u0432\u0441\u043a\u0438\u0439 \u0410\u043b\u0435\u043a\u0441\u0430\u043d\u0434\u0440 \u0421\u0442\u0430\u043d\u0438\u0441\u043b\u0430\u0432\u043e\u0432\u0438\u0447','2011-01-20','\u041e\u0442\u043b\u0438\u0447\u043d\u0430\u044f \u043a\u043d\u0438\u0433\u0430. \u0414\u0435\u0439\u0441\u0442\u0432\u0438\u0442\u0435\u043b\u044c\u043d\u043e \u0434\u043b\u044f \u043d\u0430\u0447\u0438\u043d\u0430\u044e\u0449\u0438\u0445 \u0438 \u0448\u0430\u0433 \u0437\u0430 \u0448\u0430\u0433\u043e\u043c. \u0421\u044b\u043d (\u043f\u043e\u0434\u0440\u043e\u0441\u0442\u043e\u043a 15 \u043b\u0435\u0442) \u043e\u0446\u0435\u043d\u0438\u043b \u043b\u0435\u0433\u043a\u043e\u0441\u0442\u044c \u043f\u043e\u043d\u0438\u043c\u0430\u043d\u0438\u044f \u0438 \u043f\u043e\u0434\u0440\u043e\u0431\u043d\u044b\u0435 \u043e\u0431\u044a\u044f\u0441\u043d\u0435\u043d\u0438\u044f.',3985)
INSERT INTO COMMENT VALUES(1738,'sanchezzko','2011-01-14','\u0425\u043e\u0440\u043e\u0448\u0435\u0435 \u0438\u0437\u0434\u0430\u043d\u0438\u0435. \u0411\u0435\u043b\u0430\u044f, \u0447\u0443\u0442\u044c \u043f\u0440\u043e\u0441\u0432\u0435\u0447\u0438\u0432\u0430\u044e\u0449\u0430\u044f \u0431\u0443\u043c\u0430\u0433\u0430. \u0425\u043e\u0440\u043e\u0448\u0430\u044f \u043a\u043d\u0438\u0433\u0430. \u0421\u0430\u043c\u043e\u0435 \u043e\u043d\u043e \u0434\u043b\u044f \u043d\u0430\u0447\u0438\u043d\u0430\u044e\u0449\u0438\u0445. \u041f\u0430\u0440\u0430 \u0444\u043e\u0442\u043e\u043a: \u041f\u043e\u043a\u0430\u0437\u0430\u0442\u044c \u0438\u0437\u043e\u0431\u0440\u0430\u0436\u0435\u043d\u0438\u044f \u0421\u043a\u0440\u044b\u0442\u044c \u0438\u0437\u043e\u0431\u0440\u0430\u0436\u0435\u043d\u0438\u044f',3985)
INSERT INTO COMMENT VALUES(1739,'hyena-hyena','2014-01-26','\u0410\u0432\u0442\u043e\u0440 \u0441 \u043f\u0435\u0440\u0432\u044b\u0445 \u0441\u0442\u0440\u0430\u043d\u0438\u0446 \u043f\u0440\u0438\u0441\u0442\u0443\u043f\u0430\u0435\u0442 \u043a \u0434\u0435\u043b\u0443, \u043d\u0435 \u0442\u0435\u0440\u044f\u044f \u0432\u0440\u0435\u043c\u0435\u043d\u0438 \u043d\u0430 \u043f\u0440\u0435\u0434\u044b\u0441\u0442\u043e\u0440\u0438\u0438 \u0438 \u043a\u0430\u043a\u0438\u0435-\u0442\u043e \u0440\u0430\u0437\u043c\u044b\u0448\u043b\u0438\u0442\u0435\u043b\u044c\u043d\u044b\u0435 \u0434\u0435\u0431\u0440\u0438. \u0422\u0430\u043a\u043e\u0439 \u043f\u043e\u0434\u0445\u043e\u0434 \u043f\u043e\u0437\u0432\u043e\u043b\u044f\u0435\u0442 \u0447\u0435\u043b\u043e\u0432\u0435\u043a\u0443 \u0441\u0440\u0430\u0437\u0443 \u043f\u043e\u0433\u0440\u0443\u0437\u0438\u0442\u044c\u0441\u044f \u0432 \u0438\u0437\u0443\u0447\u0435\u043d\u0438\u0435 \u043c\u0430\u0442\u0435\u0440\u0438\u043b\u0430. \u0412\u0441\u044f \u043a\u043d\u0438\u0433\u0430 \u0440\u0430\u0437\u0431\u0438\u0442\u0430 \u043d\u0430 12 \u043c\u043e\u0434\u0443\u043b\u0435\u0439 \u0441 \u043f\u043e\u0441\u043b\u0435\u0434\u0443\u044e\u0449\u0438\u043c \u0443\u0441\u043b\u043e\u0436\u043d\u0435\u043d\u0438\u0435\u043c \u043f\u0440\u043e\u0439\u0434\u0435\u043d\u043d\u043e\u0433\u043e. \u0412 \u0442\u0435\u0447\u0435\u043d\u0438\u0435 \u043a\u0430\u0436\u0434\u043e\u0433\u043e \u043c\u043e\u0434\u0443\u043b\u044f \u0447\u0438\u0442\u0430\u0442\u0435\u043b\u044e \u043f\u0440\u0435\u0434\u043b\u0430\u0433\u0430\u0435\u0442\u0441\u044f \u043e\u0442\u0432\u0435\u0442\u0438\u0442\u044c \u043d\u0430 \u0440\u044f\u0434 \u0432\u043e\u043f\u0440\u043e\u0441\u043e\u0432 \u043f\u043e \u043d\u0435\u0441\u043a\u043e\u043b\u044c\u043a\u0438\u043c \u043f\u0440\u043e\u0439\u0434\u0435\u043d\u043d\u044b\u043c \u043f\u0430\u0440\u0430\u0433\u0440\u0430\u0444\u0430\u043c, \u0447\u0442\u043e \u043f\u043e\u043c\u043e\u0433\u0430\u0435\u0442 \u0447\u0435\u043b\u043e\u0432\u0435\u043a\u0443 \u0441\u0430\u043c\u043e\u043c\u0443 \u0441\u0435\u0431\u044f \u043a\u043e\u043d\u0442\u0440\u043e\u043b\u0438\u0440\u043e\u0432\u0430\u0442\u044c \u0432 \u0443\u0441\u0432\u043e\u0435\u043d\u0438\u0438 \u0438 \u043f\u043e\u043d\u0438\u043c\u0430\u043d\u0438\u0438 \u043c\u0430\u0442\u0435\u0440\u0438\u0430\u043b\u0430. \u041f\u043e \u043e\u043a\u043e\u043d\u0447\u0430\u043d\u0438\u0438 \u043c\u043e\u0434\u0443\u043b\u044f \u043d\u0443\u0436\u043d\u043e \u043e\u0442\u0432\u0435\u0442\u0438\u0442\u044c \u043d\u0430 \u0432\u043e\u043f\u0440\u043e\u0441\u044b \u0438 \u0440\u0435\u0448\u0438\u0442\u044c \u0437\u0430\u0434\u0430\u0447\u0438. \u0422\u0430\u043a\u0438\u043c \u043e\u0431\u0440\u0430\u0437\u043e\u043c, \u0443 \u043d\u043e\u0432\u0438\u0447\u043a\u0430 \u0444\u043e\u0440\u043c\u0438\u0440\u0443\u0435\u0442\u0441\u044f \u0434\u043e\u0441\u0442\u0430\u0442\u043e\u0447\u043d\u043e \u043f\u0440\u043e\u0447\u043d\u044b\u0439 \u0444\u0443\u043d\u0434\u0430\u043c\u0435\u043d\u0442, \u043d\u0435 \u043f\u0440\u043e\u0438\u0441\u0445\u043e\u0434\u0438\u0442 \u043f\u0435\u0440\u0435\u0438\u0437\u0431\u044b\u0442\u043a\u0430 \u043d\u0435\u043d\u0443\u0436\u043d\u043e\u0439 \u0438\u043d\u0444\u043e\u0440\u043c\u0430\u0446\u0438\u0435\u0439. \u0410\u0432\u0442\u043e\u0440 \u0442\u0430\u043a\u0436\u0435 \u0447\u0435\u0442\u043a\u043e \u043f\u043e\u044f\u0441\u043d\u044f\u0435\u0442 \u0438 \u043f\u043e\u043a\u0430\u0437\u044b\u0432\u0430\u0435\u0442, \u0434\u043b\u044f \u0447\u0435\u0433\u043e \u043d\u0443\u0436\u0435\u043d \u0437\u0434\u0435\u0441\u044c \u0438\u043c\u0435\u043d\u043d\u043e \u044d\u0442\u043e\u0442 \u043c\u0435\u0442\u043e\u0434, \u0430 \u043d\u0435 \u043a\u0430\u043a\u043e\u0439-\u043b\u0438\u0431\u043e \u0434\u0440\u0443\u0433\u043e\u0439. \u0412 \u043a\u043e\u043d\u0446\u0435 \u043a\u043d\u0438\u0433\u0438 \u0438\u043c\u0435\u0435\u0442\u0441\u044f \u043f\u0440\u0438\u043b\u043e\u0436\u0435\u043d\u0438\u0435, \u0433\u0434\u0435 \u0435\u0441\u0442\u044c \u043e\u0442\u0432\u0435\u0442\u044b (\u0434\u043b\u044f \u0441\u0430\u043c\u043e\u043f\u0440\u043e\u0432\u0435\u0440\u043a\u0438), \u043d\u044e\u0430\u043d\u0441\u044b, \u0432\u044b\u043d\u0435\u0441\u0435\u043d\u043d\u044b\u0435 \u0438\u0437 \u043c\u043e\u0434\u0443\u043b\u0435\u0439, \u0438 \u0440\u0430\u0431\u043e\u0442\u0430 \u0441\u043e \u0441\u0442\u0430\u0440\u044b\u043c \u043a\u043e\u043c\u043f\u0438\u043b\u044f\u0442\u043e\u0440\u043e\u043c. \u0422\u043e\u043b\u044c\u043a\u043e, \u0441\u0432\u0435\u0440\u044f\u044f\u0441\u044c \u0441 \u043e\u0442\u0432\u0435\u0442\u0430\u043c\u0438, \u043d\u0430\u0434\u043e \u043f\u043e\u043d\u0438\u043c\u0430\u0442\u044c, \u0447\u0442\u043e \u0412\u0430\u0448 \u043e\u0442\u0432\u0435\u0442 \u043c\u043e\u0436\u0435\u0442 \u043e\u0442\u043b\u0438\u0447\u0430\u0442\u044c\u0441\u044f \u043f\u043e \u043a\u043e\u0434\u0443 \u043e\u0442 \u0430\u0432\u0442\u043e\u0440\u0441\u043a\u043e\u0433\u043e, \u043d\u043e \u0432\u044b\u043f\u043e\u043b\u043d\u044f\u0442\u044c \u043d\u0435\u043e\u0431\u0445\u043e\u0434\u0438\u043c\u044b\u0435 \u0434\u0435\u0439\u0441\u0442\u0432\u0438\u044f. \u042d\u0442\u043e \u043d\u0435 \u043a\u0440\u0438\u043c\u0438\u043d\u0430\u043b. \u041f\u0440\u043e\u0441\u0442\u043e \u043e\u0442\u0432\u0435\u0442\u044b \u043e\u0442 \u0430\u0432\u0442\u043e\u0440\u0430 \u0431\u043e\u043b\u0435\u0435 \u043b\u0430\u043a\u043e\u043d\u0438\u0447\u043d\u044b. \u041e\u0442 \u0441\u0435\u0431\u044f \u0434\u043e\u0431\u0430\u0432\u043b\u044e \u0435\u0449\u0451, \u0447\u0442\u043e \u043f\u0440\u043e\u0431\u043e\u0432\u0430\u043b \u0438\u0437\u0443\u0447\u0430\u0442\u044c \u0421+ \u043f\u043e \u0434\u0440\u0443\u0433\u043e\u0439 \u043a\u043d\u0438\u0436\u043a\u0435. \u0418 \u0431\u0440\u043e\u0441\u0438\u043b. \u0411\u044b\u043b\u043e \u043c\u043d\u043e\u0433\u043e \u043b\u0438\u0448\u043d\u0435\u0439 \u0438\u043d\u0444\u043e\u0440\u043c\u0430\u0446\u0438\u0438 \u043f\u043e \u0445\u043e\u0434\u0443 \u043e\u0431\u044a\u044f\u0441\u043d\u0435\u043d\u0438\u044f \u043c\u0430\u0442\u0435\u0440\u0438\u0430\u043b\u0430, \u0447\u0442\u043e \u043c\u0435\u0448\u0430\u043b\u043e \u0443\u0441\u0432\u043e\u0435\u043d\u0438\u044e. \u041f\u043e\u0442\u043e\u043c \u0440\u0435\u0448\u0438\u043b \u043a\u0443\u043f\u0438\u0442\u044c \u044d\u0442\u0443, \u0438 \u043e\u043d\u0430 \u043c\u043d\u0435 \u043f\u0440\u0438\u0448\u043b\u0430\u0441\u044c \u043f\u043e \u0434\u0443\u0448\u0435. \u0412\u0441\u0451 \u0436\u0435 \u043c\u0435\u0442\u043e\u0434 \u043f\u043e\u0434\u0430\u0447\u0438 \u043c\u0430\u0442\u0435\u0440\u0438\u0430\u043b\u0430 \u0438\u0433\u0440\u0430\u0435\u0442 \u0431\u043e\u043b\u044c\u0448\u0443\u044e \u0440\u043e\u043b\u044c. \u0421\u043a\u0440\u044b\u0442\u044c',3985)
INSERT INTO COMMENT VALUES(1740,'dvpublic','2011-01-29','\u041d\u0435\u043f\u043b\u043e\u0445\u0430\u044f \u043a\u043d\u0438\u0436\u043a\u0430. \u041f\u043e\u0434\u043e\u0439\u0434\u0435\u0442 \u0442\u0435\u043c, \u043a\u0442\u043e \u0445\u043e\u0447\u0435\u0442 \u043e\u0437\u043d\u0430\u043a\u043e\u043c\u0438\u0442\u044c\u0441\u044f \u0441 \u0430\u0437\u0430\u043c\u0438 \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u044f \u043d\u0430 Silverlight \u0438 \u043f\u043e\u043d\u044f\u0442\u044c, \u0447\u0442\u043e \u0441\u043e\u0431\u0441\u0442\u0432\u0435\u043d\u043d\u043e \u044d\u0442\u0430 \u0442\u0435\u0445\u043d\u043e\u043b\u043e\u0433\u0438\u044f \u0432 \u0441\u0435\u0431\u044f \u0432\u043a\u043b\u044e\u0447\u0430\u0435\u0442. \u041d\u0435\u0434\u043e\u0441\u0442\u0430\u0442\u043a\u043e\u043c, \u043d\u0430 \u043c\u043e\u0439 \u0432\u0437\u0433\u043b\u044f\u0434, \u044f\u0432\u043b\u044f\u0435\u0442\u0441\u044f \u043e\u0442\u0441\u0443\u0442\u0441\u0442\u0432\u0438\u0435 \u0432\u043d\u044f\u0442\u043d\u043e\u0433\u043e \u043e\u0431\u0437\u043e\u0440\u0430 \u043e\u0433\u0440\u0430\u043d\u0438\u0447\u0435\u043d\u0438\u0439 \u0434\u0430\u043d\u043d\u043e\u0439 \u0442\u0435\u0445\u043d\u043e\u043b\u043e\u0433\u0438\u0438. \u0415\u0441\u043b\u0438 \u0432\u044b \u0437\u0430\u0434\u0443\u043c\u044b\u0432\u0430\u0435\u0442\u0435\u0441\u044c \u043e \u0440\u0430\u0437\u0440\u0430\u0431\u043e\u0442\u043a\u0435 RIA \u043f\u0440\u0438\u043b\u043e\u0436\u0435\u043d\u0438\u044f \u043d\u0430 SL \u0442\u043e \u0432\u0430\u043c \u043f\u043e\u043b\u0435\u0437\u043d\u043e \u0437\u043d\u0430\u0442\u044c, \u0441 \u043a\u0430\u043a\u0438\u043c\u0438 \u043f\u043e\u0434\u0432\u043e\u0434\u043d\u044b\u043c\u0438 \u043a\u0430\u043c\u043d\u044f\u043c\u0438 \u043f\u0440\u0438\u0434\u0435\u0442\u0441\u044f \u0441\u0442\u043e\u043b\u043a\u043d\u0443\u0442\u044c\u0441\u044f. \u0410 \u0442\u0430\u043a\u0438\u0445 \u0432\u0435\u0434\u044c \u043f\u0440\u0435\u0434\u043e\u0441\u0442\u0430\u0442\u043e\u0447\u043d\u043e... \u0412 SL \u043d\u0435\u0442 DataSet? \u041e\u0431 \u044d\u0442\u043e\u043c \u043d\u0438 \u0441\u043b\u043e\u0432\u0430. \u0422\u0435\u0445\u043d\u043e\u043b\u043e\u0433\u0438\u044f, \u0440\u0430\u0441\u0441\u0447\u0438\u0442\u0430\u043d\u043d\u0430\u044f \u043d\u0430 \u0440\u0430\u0437\u0440\u0430\u0431\u043e\u0442\u043a\u0443 \u043a\u043e\u0440\u043f\u043e\u0440\u0430\u0442\u0438\u0432\u043d\u044b\u0445 RIA \u043f\u0440\u0438\u043b\u043e\u0436\u0435\u043d\u0438\u0439, \u043d\u0435 \u043f\u0440\u0435\u0434\u043e\u0441\u0442\u0430\u0432\u043b\u044f\u0435\u0442 \u0441\u043f\u043e\u0441\u043e\u0431 \u043b\u043e\u0433\u0438\u043d \u0437\u0430\u043b\u043e\u0433\u0438\u043d\u0435\u043d\u043d\u043e\u0433\u043e \u043f\u043e\u043b\u044c\u0437\u043e\u0432\u0430\u0442\u0435\u043b\u044f? \u0422\u043e\u0436\u0435 \u043d\u0435 \u0443\u043f\u043e\u043c\u0438\u043d\u0430\u0435\u0442\u0441\u044f... \u0414\u0430, \u044d\u0442\u043e \u0447\u0430\u0441\u0442\u043d\u043e\u0441\u0442\u0438, \u043d\u043e \u0447\u0430\u0441\u0442\u043d\u043e\u0441\u0442\u0438 \u043a\u0440\u0430\u0439\u043d\u0435 \u0432\u0430\u0436\u043d\u044b\u0435. \u0420\u0430\u0437\u0434\u0435\u043b \u043a\u043d\u0438\u0433\u0438, \u043e\u0431\u0440\u0430\u0449\u0430\u044e\u0449\u0438\u0439 \u0432\u043d\u0438\u043c\u0430\u043d\u0438\u0435 \u043d\u0430 \u043f\u043e\u0434\u043e\u0431\u043d\u044b\u0435 \u043f\u0440\u043e\u0431\u043b\u0435\u043c\u044b, \u0431\u044b\u043b \u0431\u044b \u043e\u0447\u0435\u043d\u044c \u043a\u0441\u0442\u0430\u0442\u0438. \u041d\u0435 \u043f\u043e\u0432\u0440\u0435\u0434\u0438\u043b\u0430 \u0431\u044b \u0438 \u0433\u043b\u0430\u0432\u0430 \u043e \u043a\u0440\u043e\u0441\u0441\u043f\u043b\u0430\u0442\u0444\u043e\u0440\u043c\u0435\u043d\u043d\u043e\u0441\u0442\u0438 - \u043f\u0440\u043e Moonlight \u043d\u0438\u0447\u0435\u0433\u043e \u043d\u0435\u0442. \u0421\u043a\u0440\u044b\u0442\u044c',3986)
INSERT INTO COMMENT VALUES(1741,'\u041c\u0438\u0441\u0441\u0438\u0441 \u0411\u043e\u043d\u0434','2010-01-01','\u0414\u0430\u043d\u043d\u0430\u044f \u043a\u043d\u0438\u0433\u0430 \u0431\u0443\u0434\u0435\u0442 \u043f\u043e\u043b\u0435\u0437\u043d\u0430 \u0442\u0435\u043c, \u043a\u0442\u043e \u0432\u043f\u0435\u0440\u0432\u044b\u0435 \u0441\u0442\u043e\u043b\u043a\u043d\u0443\u043b\u0441\u044f \u0441 Silverlight 4 \u0438 \u0437\u0430\u0445\u043e\u0442\u0435\u043b \u0443\u0437\u043d\u0430\u0442\u044c \u043e \u043d\u0435\u043c \u043f\u043e\u0434\u0440\u043e\u0431\u043d\u0435\u0435. \u0410\u0432\u0442\u043e\u0440 \u0438\u0437\u043b\u0430\u0433\u0430\u0435\u0442 \u043c\u0430\u0442\u0435\u0440\u0438\u0430\u043b \u043e\u0447\u0435\u043d\u044c \u0434\u043e\u0441\u0442\u0443\u043f\u043d\u044b\u043c \u0438 \u043f\u043e\u043d\u044f\u0442\u043d\u044b\u043c \u044f\u0437\u044b\u043a\u043e\u043c. \u0412 \u043a\u043d\u0438\u0433\u0435 \u043c\u043d\u043e\u0433\u043e \u0432\u043d\u0438\u043c\u0430\u043d\u0438\u044f \u0443\u0434\u0435\u043b\u044f\u0435\u0442\u0441\u044f \u0441\u043e\u0437\u0434\u0430\u043d\u0438\u044e \u043f\u0440\u0438\u043b\u043e\u0436\u0435\u043d\u0438\u0439 \u0441\u0440\u0435\u0434\u0441\u0442\u0432\u0430\u043c\u0438 Visual Studio, \u0430 \u0432\u043e\u0442 \u043e\u043f\u0438\u0441\u0430\u043d\u0438\u0435 \u0440\u0435\u0434\u0430\u043a\u0442\u043e\u0440\u0430 Blend \u0437\u0434\u0435\u0441\u044c \u043e\u043f\u0443\u0449\u0435\u043d\u043e. \u0418\u0437\u0443\u0447\u0435\u043d\u0438\u0435 Silverlight 4 \u043d\u0430\u0447\u0438\u043d\u0430\u0435\u0442\u0441\u044f \u0441\u043e \u0437\u043d\u0430\u043a\u043e\u043c\u0441\u0442\u0432\u043e\u043c \u0441 \u043d\u043e\u0432\u044b\u043c\u0438 \u0432\u043e\u0437\u043c\u043e\u0436\u043d\u043e\u0441\u0442\u044f\u043c\u0438 \u044d\u0442\u043e\u0439 \u0442\u0435\u0445\u043d\u043e\u043b\u043e\u0433\u0438\u0438, \u0434\u0430\u043b\u0435\u0435 \u0430\u0432\u0442\u043e\u0440 \u043f\u0435\u0440\u0435\u0445\u043e\u0434\u0438\u0442 \u043a \u0430\u0440\u0445\u0438\u0442\u0435\u043a\u0442\u0443\u0440\u0435, \u0437\u0430\u0442\u0435\u043c \u043a \u0440\u0430\u0431\u043e\u0442\u0435 \u0441 \u044d\u043b\u0435\u043c\u0435\u043d\u0442\u0430\u043c\u0438 \u0443\u043f\u0440\u0430\u0432\u043b\u0435\u043d\u0438\u044f. \u0411\u043e\u043b\u044c\u0448\u0430\u044f \u0447\u0430\u0441\u0442\u044c \u043c\u0430\u0442\u0435\u0440\u0438\u0430\u043b\u043e\u0432 \u043a\u043d\u0438\u0433\u0438 \u043f\u043e\u0441\u0432\u044f\u0449\u0435\u043d\u0430 \u043a\u043e\u0440\u043f\u043e\u0440\u0430\u0442\u0438\u0432\u043d\u044b\u043c \u043f\u0440\u0438\u043b\u043e\u0436\u0435\u043d\u0438\u044f\u043c, \u043d\u043e \u0442\u0430\u043a\u0436\u0435 \u043f\u0440\u0438\u0441\u0443\u0442\u0441\u0442\u0432\u0443\u044e\u0442 \u0441\u0432\u0435\u0434\u0435\u043d\u0438\u044f \u043e \u0432\u043e\u0437\u043c\u043e\u0436\u043d\u043e\u0441\u0442\u0438, \u043a\u043e\u0442\u043e\u0440\u044b\u0435 \u0441\u0432\u044f\u0437\u0430\u043d\u044b \u0441 \u0433\u0440\u0430\u0444\u0438\u043a\u043e\u0439 \u0438 \u0438\u043d\u0444\u043e\u0440\u043c\u0430\u0446\u0438\u0435\u0439. \u041e\u0431\u0437\u043e\u0440 \u0430\u0432\u0442\u043e\u0440 \u0437\u0430\u043a\u0430\u043d\u0447\u0438\u0432\u0430\u0435\u0442 \u0432\u043e\u043f\u0440\u043e\u0441\u0430\u043c\u0438 \u043e\u0442\u043b\u0430\u0434\u043a\u0438, \u0442\u0435\u0441\u0442\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u044f \u0438 \u043f\u043e\u0432\u044b\u0448\u0435\u043d\u0438\u044f \u043f\u0440\u043e\u0438\u0437\u0432\u043e\u0434\u0438\u0442\u0435\u043b\u044c\u043d\u043e\u0441\u0442\u0438 Silverlight \u2013 \u043f\u0440\u0438\u043b\u043e\u0436\u0435\u043d\u0438\u0439. \u0422\u0430\u043a\u0438\u043c \u043e\u0431\u0440\u0430\u0437\u043e\u043c, \u0432 \u0434\u0430\u043d\u043d\u043e\u0439 \u043a\u043d\u0438\u0433\u0435 \u043c\u043e\u0436\u043d\u043e \u043d\u0430\u0439\u0442\u0438 \u0432\u0441\u044e \u0438\u043d\u0444\u043e\u0440\u043c\u0430\u0446\u0438\u044e, \u043a\u043e\u0442\u043e\u0440\u0430\u044f \u0431\u0443\u0434\u0435\u0442 \u043d\u0435\u043e\u0431\u0445\u043e\u0434\u0438\u043c\u0430 \u0440\u0430\u0437\u0440\u0430\u0431\u043e\u0442\u0447\u0438\u043a\u0430\u043c, \u0447\u0442\u043e\u0431\u044b \u043f\u0440\u0438\u0441\u0442\u0443\u043f\u0438\u0442\u044c \u043a \u0441\u043e\u0437\u0434\u0430\u043d\u0438\u044e \u0434\u0430\u0436\u0435 \u0441\u0430\u043c\u044b\u0445 \u0441\u043b\u043e\u0436\u043d\u044b\u0445 \u043f\u0440\u0438\u043b\u043e\u0436\u0435\u043d\u0438\u0439. \u0414\u043b\u044f \u0442\u0435\u0445, \u043a\u0442\u043e \u0437\u043d\u0430\u043a\u043e\u043c \u0441 \u043e\u0444\u0438\u0446\u0438\u0430\u043b\u044c\u043d\u043e\u0439 \u0434\u043e\u043a\u0443\u043c\u0435\u043d\u0442\u0430\u0446\u0438\u0435\u0439 \u043e Silverligth4 & WCF RIA Services, \u043a\u043d\u0438\u0433\u0430 \u043d\u043e\u0432\u043e\u0439 \u0438\u043d\u0444\u043e\u0440\u043c\u0430\u0446\u0438\u0438 \u043d\u0435 \u043f\u0440\u0435\u0434\u043e\u0441\u0442\u0430\u0432\u0438\u0442. \u0421\u043a\u0440\u044b\u0442\u044c',3986)
