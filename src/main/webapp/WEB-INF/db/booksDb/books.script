INSERT INTO BOOK VALUES(3429,412985,'\u0427\u0430\u0440\u043b\u044c\u0437 \u041f\u0435\u0442\u0446\u043e\u043b\u044c\u0434: \u041f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u0435 \u0434\u043b\u044f Microsoft Windows 8. 6-\u0435 \u0438\u0437\u0434\u0430\u043d\u0438\u0435','\u041f\u0435\u0442\u0446\u043e\u043b\u044c\u0434 \u0427\u0430\u0440\u043b\u044c\u0437','\u0421\u0435\u0440\u0433\u0438\u0435\u043d\u043a\u043e \u042e.','\u041f\u0438\u0442\u0435\u0440, 2014 \u0433.',1324.0E0,'978-5-496-00863-1','1008 (\u041e\u0444\u0441\u0435\u0442)','\u0422\u0438\u043f \u043e\u0431\u043b\u043e\u0436\u043a\u0438: 7\u0411 - \u0442\u0432\u0435\u0440\u0434\u0430\u044f (\u043f\u043b\u043e\u0442\u043d\u0430\u044f \u0431\u0443\u043c\u0430\u0433\u0430 \u0438\u043b\u0438 \u043a\u0430\u0440\u0442\u043e\u043d) \u041e\u0444\u043e\u0440\u043c\u043b\u0435\u043d\u0438\u0435: \u0422\u0438\u0441\u043d\u0435\u043d\u0438\u0435 \u0441\u0435\u0440\u0435\u0431\u0440\u043e\u043c \u0418\u043b\u043b\u044e\u0441\u0442\u0440\u0430\u0446\u0438\u0438: \u0427\u0435\u0440\u043d\u043e-\u0431\u0435\u043b\u044b\u0435','1246 \u0433','241x177x46 \u043c\u043c','\u0410\u043d\u043d\u043e\u0442\u0430\u0446\u0438\u044f \u043a \u043a\u043d\u0438\u0433\u0435 "\u041f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u0435 \u0434\u043b\u044f Microsoft Windows 8. 6-\u0435 \u0438\u0437\u0434\u0430\u043d\u0438\u0435" \u0428\u0435\u0441\u0442\u043e\u0435 \u0438\u0437\u0434\u0430\u043d\u0438\u0435 \u044d\u0442\u043e\u0439 \u043b\u0435\u0433\u0435\u043d\u0434\u0430\u0440\u043d\u043e\u0439 \u043a\u043d\u0438\u0433\u0438 \u043f\u0440\u0438\u0448\u043b\u043e\u0441\u044c \u0436\u0434\u0430\u0442\u044c \u043f\u043e\u0447\u0442\u0438 15 \u043b\u0435\u0442! \u0412 \u0441\u0432\u043e\u0435\u043c \u043d\u043e\u0432\u043e\u043c \u0442\u0440\u0443\u0434\u0435 \u0427\u0430\u0440\u043b\u044c\u0437 \u041f\u0435\u0442\u0446\u043e\u043b\u044c\u0434, \u0438\u0437\u0432\u0435\u0441\u0442\u043d\u044b\u0439 \u0430\u0432\u0442\u043e\u0440 \u0438 \u043e\u0434\u0438\u043d \u0438\u0437 \u043f\u0438\u043e\u043d\u0435\u0440\u043e\u0432 Windows-\u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u044f, \u0440\u0430\u0441\u0441\u043a\u0430\u0437\u044b\u0432\u0430\u0435\u0442 \u043e \u0440\u0430\u0437\u0440\u0430\u0431\u043e\u0442\u043a\u0435 \u043f\u0440\u0438\u043b\u043e\u0436\u0435\u043d\u0438\u0439 \u0434\u043b\u044f Windows Store \u0441 \u0438\u0441\u043f\u043e\u043b\u044c\u0437\u043e\u0432\u0430\u043d\u0438\u0435\u043c C# \u0438 XAML \u0432 \u0441\u0440\u0435\u0434\u0435 Windows Runtime. \u041f\u0435\u0440\u0432\u0430\u044f \u0447\u0430\u0441\u0442\u044c \u043a\u043d\u0438\u0433\u0438 "\u041e\u0441\u043d\u043e\u0432\u044b" \u043f\u043e\u0441\u0432\u044f\u0449\u0435\u043d\u0430 \u043f\u0440\u0438\u0435\u043c\u0430\u043c, \u043a\u043e\u0442\u043e\u0440\u044b\u0435 \u043e\u0441\u043e\u0431\u0435\u043d\u043d\u043e \u0432\u0430\u0436\u043d\u044b \u0434\u043b\u044f \u0431\u043e\u043b\u044c\u0448\u0438\u043d\u0441\u0442\u0432\u0430 \u0440\u044f\u0434\u043e\u0432\u044b\u0445 \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u0438\u0441\u0442\u043e\u0432: \u0441\u0431\u043e\u0440\u043a\u0435 \u0433\u043e\u0442\u043e\u0432\u044b\u0445 \u044d\u043b\u0435\u043c\u0435\u043d\u0442\u043e\u0432 \u0443\u043f\u0440\u0430\u0432\u043b\u0435\u043d\u0438\u044f \u0432 \u043f\u0440\u0438\u043b\u043e\u0436\u0435\u043d\u0438\u0438 \u0438 \u0438\u0445 \u0441\u0432\u044f\u0437\u044b\u0432\u0430\u043d\u0438\u044e \u0441 \u043a\u043e\u0434\u043e\u043c \u0438 \u0434\u0430\u043d\u043d\u044b\u043c\u0438. \u0417\u0434\u0435\u0441\u044c \u043f\u043e\u043b\u043d\u043e\u0441\u0442\u044c\u044e \u043e\u0431\u044a\u044f\u0441\u043d\u044f\u0435\u0442\u0441\u044f \u0432\u0435\u0441\u044c \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u043d\u044b\u0439 \u043a\u043e\u0434 \u0438 \u0440\u0430\u0437\u043c\u0435\u0442\u043a\u0430, \u0433\u0435\u0