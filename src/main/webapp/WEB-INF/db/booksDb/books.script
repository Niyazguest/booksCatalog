INSERT INTO COMMENT VALUES(1743,'\u0417\u043e\u043b\u043e\u0442\u0430\u044f \u0440\u044b\u0431\u043a\u0430','2011-01-07','\u041a\u043d\u0438\u0433\u0430 \u0447\u0443\u0442\u044c \u0443\u0432\u0435\u043b\u0438\u0447\u0435\u043d\u043d\u043e\u0433\u043e \u0444\u043e\u0440\u043c\u0430\u0442\u0430,\u0432 \u043c\u044f\u0433\u043a\u043e\u0439 \u043e\u0431\u043b\u043e\u0436\u043a\u0435,\u043b\u0435\u0433\u043a\u0430\u044f.\u0411\u0443\u043c\u0430\u0433\u0430 \u0432\u043d\u0443\u0442\u0440\u0438 \u043d\u0435\u043f\u043b\u043e\u0445\u043e\u0433\u043e \u043a\u0430\u0447\u0435\u0441\u0442\u0432\u0430.\u041f\u0440\u0438\u043a\u043b\u0430\u0434\u044b\u0432\u0430\u044e \u043f\u043e\u0434\u0440\u043e\u0431\u043d\u043e\u0435 \u043e\u0433\u043b\u0430\u0432\u043b\u0435\u043d\u0438\u0435 \u0438 \u0441\u0442\u0440\u0430\u043d\u0438\u0446\u044b \u0438\u0437 \u043a\u043d\u0438\u0433\u0438. \u041f\u043e\u043a\u0430\u0437\u0430\u0442\u044c \u0438\u0437\u043e\u0431\u0440\u0430\u0436\u0435\u043d\u0438\u044f \u0421\u043a\u0440\u044b\u0442\u044c \u0438\u0437\u043e\u0431\u0440\u0430\u0436\u0435\u043d\u0438\u044f',3987)
INSERT INTO COMMENT VALUES(1744,'\u041e\u043a\u0441\u0430\u043d\u0447\u0438\u043a','2011-01-10','\u0423\u0436\u0435 \u0432\u044b\u0448\u043b\u0430 \u0432 \u0441\u0432\u0435\u0442 \u043f\u043b\u0430\u0442\u0444\u043e\u0440\u043c\u0430 \u0432\u0435\u0440\u0441\u0438\u0438 2.3, \u0432 \u0442\u043e \u0432\u0440\u0435\u043c\u044f \u043a\u0430\u043a \u043f\u0440\u0438\u043c\u0435\u0440\u044b \u0432 \u043a\u043d\u0438\u0433\u0435 \u043e\u0441\u043d\u043e\u0432\u0430\u043d\u044b \u043d\u0430 \u0432\u0435\u0440\u0441\u0438\u0438 1.1, \u0447\u0442\u043e \u0443\u0436\u0435 \u0433\u043e\u0432\u043e\u0440\u0438\u0442 \u043e\u0431 "\u0430\u043a\u0442\u0443\u0430\u043b\u044c\u043d\u043e\u0441\u0442\u0438" \u0438\u043d\u0444\u043e\u0440\u043c\u0430\u0446\u0438\u0438 \u0432 \u043a\u043d\u0438\u0433\u0435. \u041a\u0430\u043a \u0447\u0435\u043b\u043e\u0432\u0435\u043a, \u043a\u043e\u0442\u043e\u0440\u043e\u043c\u0443 \u043f\u0440\u0438\u0448\u043b\u043e\u0441\u044c \u0432 \u0441\u0436\u0430\u0442\u044b\u0435 \u0441\u0440\u043e\u043a\u0438 \u043e\u0441\u0432\u0430\u0438\u0432\u0430\u0442\u044c \u043d\u043e\u0432\u0443\u044e \u043f\u043b\u0430\u0442\u0444\u043e\u0440\u043c\u0443, \u043c\u043e\u0433\u0443 \u0441\u043a\u0430\u0437\u0430\u0442\u044c, \u0447\u0442\u043e \u0434\u0430\u043d\u043d\u0443\u044e \u043a\u043d\u0438\u0433\u0443 \u043c\u043e\u0436\u043d\u043e \u043f\u043e\u0441\u043e\u0432\u0435\u0442\u043e\u0432\u0430\u0442\u044c \u043f\u043e\u0447\u0438\u0442\u0430\u0442\u044c \u043d\u0430\u0447\u0438\u043d\u0430\u044e\u0449\u0438\u043c, \u043a\u043e\u0433\u0434\u0430 \u043d\u0435\u0442 \u0434\u043e\u0441\u0442\u0443\u043f\u0430 \u0432\u043e \u0432\u0441\u0435\u043c\u0438\u0440\u043d\u0443\u044e \u043f\u0430\u0443\u0442\u0438\u043d\u0443 (\u0432 \u043c\u0435\u0442\u0440\u043e, \u0432 \u044d\u043b\u0435\u043a\u0442\u0440\u0438\u0447\u043a\u0435) \u0438\u043b\u0438 \u043e\u0447\u0435\u043d\u044c \u043f\u043b\u043e\u0445\u043e \u0441 \u0430\u043d\u0433\u043b\u0438\u0439\u0441\u043a\u0438\u043c \u044f\u0437\u044b\u043a\u043e\u043c.',3988)
INSERT INTO COMMENT VALUES(1745,'\u0413\u043e\u0440\u0448\u0435\u043d\u0438\u043d \u041c\u0430\u043a\u0441\u0438\u043c','2010-01-13','\u041f\u043e\u0445\u043e\u0436\u0435 \u043d\u0430 \u041f\u0440\u043e\u043c\u0442\u043e\u0432\u0441\u043a\u0438\u0439 \u043f\u0435\u0440\u0435\u0432\u043e\u0434 \u0441 \u043f\u043e\u0441\u043b\u0435\u0434\u0443\u044e\u0449\u0435\u0439 \u043b\u0438\u0442\u0435\u0440\u0430\u0442\u0443\u0440\u043d\u043e\u0439 \u043f\u0440\u0430\u0432\u043a\u043e\u0439. \u041f\u043e\u043b\u043d\u043e\u0435 \u043e\u0442\u0441\u0443\u0442\u0441\u0442\u0432\u0438\u0435 \u0442\u0435\u0445\u043d\u0438\u0447\u0435\u0441\u043a\u043e\u0439 \u0430\u0434\u0430\u043f\u0442\u0430\u0446\u0438\u0438 \u043f\u0435\u0440\u0435\u0432\u043e\u0434\u0430, \u0432\u043e\u0437\u043c\u043e\u0436\u043d\u043e \u0432 \u043e\u0440\u0438\u0433\u0438\u043d\u0430\u043b\u0435 \u043a\u043d\u0438\u0433\u0430 \u0445\u043e\u0440\u043e\u0448\u0430, \u043d\u043e \u0440\u0443\u0441\u0441\u043a\u0438\u0439 \u0432\u0430\u0440\u0438\u0430\u043d\u0442 \u043a \u043f\u0440\u043e\u0447\u0442\u0435\u043d\u0438\u044e \u043d\u0435 \u0440\u0435\u043a\u043e\u043c\u0435\u043d\u0434\u0443\u0435\u0442\u0441\u044f. \u0415\u0441\u043b\u0438 \u043d\u0443\u0436\u043d\u044b \u0441\u043e\u0432\u0435\u0442\u044b \u043d\u0430 \u0440\u0443\u0441\u0441\u043a\u043e\u043c - \u0432\u044b\u0431\u0438\u0440\u0430\u0439\u0442\u0435 \u0441\u0442\u0430\u0442\u044c\u0438 \u0438 \u0444\u043e\u0440\u0443\u043c\u044b, \u043f\u0440\u0438\u043b\u0438\u0447\u043d\u044b\u0445 \u043a\u043d\u0438\u0433 \u043f\u043e Android`\u0443 \u043d\u0430 \u0440\u0443\u0441\u0441\u043a\u043e\u043c \u043d\u0435\u0442.',3988)
INSERT INTO COMMENT VALUES(1746,'\u0421\u043e\u0440\u043e\u043a\u0438\u043d\u0430 \u041b\u044e\u0434\u0432\u0438\u0433\u0430','2010-01-01','\u041d\u0430 \u043c\u043e\u0439 \u0432\u0437\u0433\u043b\u044f\u0434, \u043a\u043d\u0438\u0433\u0430 \u044f\u0432\u043b\u044f\u0435\u0442\u0441\u044f \u043b\u043e\u0433\u0438\u0447\u0435\u0441\u043a\u0438\u043c \u043f\u0440\u043e\u0434\u043e\u043b\u0436\u0435\u043d\u0438\u0435\u043c \u043a\u043d\u0438\u0433\u0438 \u0420\u043e\u0431\u0435\u0440\u0442\u0430 \u041c\u0430\u0440\u0442\u0438\u043d\u0430 "\u0411\u044b\u0441\u0442\u0440\u0430\u044f \u0440\u0430\u0437\u0440\u0430\u0431\u043e\u0442\u043a\u0430 \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c". \u0412 \u043d\u0435\u0439, \u0442\u0430\u043a\u0436\u0435 \u043a\u0430\u043a \u0438 \u0432 \u043f\u0440\u0435\u0434\u0448\u0435\u0441\u0442\u0432\u0435\u043d\u043d\u0438\u0446\u0435, \u0430\u0432\u0442\u043e\u0440 \u0434\u0435\u043b\u0438\u0442\u0441\u044f \u043f\u0440\u0438\u043d\u0446\u0438\u043f\u0430\u043c\u0438 \u043d\u0430\u043f\u0438\u0441\u0430\u043d\u0438\u044f \u0445\u043e\u0440\u043e\u0448\u0435\u0433\u043e, \u0443\u0441\u0442\u043e\u0439\u0447\u0438\u0432\u043e\u0433\u043e \u043a \u0438\u0437\u043c\u0435\u043d\u0435\u043d\u0438\u044f\u043c, \u043a\u043e\u0434\u0430. \u041f\u0440\u0438\u043c\u0435\u0440\u044b \u0432 \u043a\u043d\u0438\u0433\u0435 \u043f\u0440\u0435\u0434\u0441\u0442\u0430\u0432\u043b\u0435\u043d\u044b \u043d\u0430 \u044f\u0437\u044b\u043a\u0435 Java. \u041f\u0435\u0440\u0435\u0432\u043e\u0434 \u043c\u043e\u0433\u0443 \u043d\u0430\u0437\u0432\u0430\u0442\u044c \u043e\u0442\u043b\u0438\u0447\u043d\u044b\u043c, \u043a\u0430\u0447\u0435\u0441\u0442\u0432\u043e \u043f\u0435\u0447\u0430\u0442\u0438 \u0442\u043e\u0436\u0435 \u043d\u0430 \u0443\u0440\u043e\u0432\u043d\u0435. \u0412\u043e\u043e\u0431\u0449\u0435 \u0432\u0441\u0435 \u043a\u043d\u0438\u0433\u0438 \u0420\u043e\u0431\u0435\u0440\u0442\u0430 \u041c\u0430\u0440\u0442\u0438\u043d\u0430 \u043c\u043e\u0436\u043d\u043e \u0441\u043c\u0435\u043b\u043e \u0441\u0442\u0430\u0432\u0438\u0442\u044c \u0432 \u043e\u0434\u0438\u043d \u0440\u044f\u0434 \u0441 \u043a\u043d\u0438\u0433\u0430\u043c\u0438 \u041c\u0430\u0440\u0442\u0438\u043d\u0430 \u0424\u0430\u0443\u043b\u0435\u0440\u0430. \u0420\u0435\u043a\u043e\u043c\u0435\u043d\u0434\u0443\u044e \u043a \u043f\u0440\u043e\u0447\u0442\u0435\u043d\u0438\u044e, \u0446\u0435\u043d\u043d\u0430\u044f \u043a\u043d\u0438\u0433\u0430. \u041a\u0440\u0430\u0442\u043a\u043e\u0435 \u043e\u0433\u043b\u0430\u0432\u043b\u0435\u043d\u0438\u0435: \u0413\u043b\u0430\u0432\u0430 1. \u0427\u0438\u0441\u0442\u044b\u0439 \u043a\u043e\u0434 \u0413\u043b\u0430\u0432\u0430 2. \u0421\u043e\u0434\u0435\u0440\u0436\u0430\u0442\u0435\u043b\u044c\u043d\u044b\u0435 \u0438\u043c\u0435\u043d\u0430 \u0413\u043b\u0430\u0432\u0430 3. \u0424\u0443\u043d\u043a\u0446\u0438\u0438 \u0413\u043b\u0430\u0432\u0430 4. \u041a\u043e\u043c\u043c\u0435\u043d\u0442\u0430\u0440\u0438\u0438 \u0413\u043b\u0430\u0432\u0430 5. \u0424\u043e\u0440\u043c\u0430\u0442\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u0435 \u0413\u043b\u0430\u0432\u0430 6. \u041e\u0431\u044a\u0435\u043a\u0442\u044b \u0438 \u0441\u0442\u0440\u0443\u043a\u0442\u0443\u0440\u044b \u0434\u0430\u043d\u043d\u044b\u0445 \u0413\u043b\u0430\u0432\u0430 7. \u041e\u0431\u0440\u0430\u0431\u043e\u0442\u043a\u0430 \u043e\u0448\u0438\u0431\u043e\u043a \u0413\u043b\u0430\u0432\u0430 8. \u0413\u0440\u0430\u043d\u0438\u0446\u044b \u0413\u043b\u0430\u0432\u0430 9. \u041c\u043e\u0434\u0443\u043b\u044c\u043d\u044b\u0435 \u0442\u0435\u0441\u0442\u044b \u0413\u043b\u0430\u0432\u0430 10. \u041a\u043b\u0430\u0441\u0441\u044b \u0413\u043b\u0430\u0432\u0430 11. \u0421\u0438\u0441\u0442\u0435\u043c\u044b \u0413\u043b\u0430\u0432\u0430 12. \u0424\u043e\u0440\u043c\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u0435 \u0430\u0440\u0445\u0438\u0442\u0435\u043a\u0442\u0443\u0440\u044b \u0413\u043b\u0430\u0432\u0430 13. \u041c\u043d\u043e\u0433\u043e\u043f\u043e\u0442\u043e\u0447\u043d\u043e\u0441\u0442\u044c \u0413\u043b\u0430\u0432\u0430 14. \u041f\u043e\u0441\u043b\u0435\u0434\u043e\u0432\u0430\u0442\u0435\u043b\u044c\u043d\u043e\u0435 \u043e\u0447\u0438\u0449\u0435\u043d\u0438\u0435 \u0413\u043b\u0430\u0432\u0430 15. \u0412\u043d\u0443\u0442\u0440\u0435\u043d\u043d\u0435\u0435 \u0441\u0442\u0440\u043e\u0435\u043d\u0438\u0435 JUnit \u0413\u043b\u0430\u0432\u0430 16. \u041f\u0435\u0440\u0435\u0440\u0430\u0431\u043e\u0442\u043a\u0430 SerialDate \u0413\u043b\u0430\u0432\u0430 17. \u0417\u0430\u043f\u0430\u0445\u0438 \u0438 \u044d\u0432\u0440\u0438\u0441\u0442\u0438\u0447\u0435\u0441\u043a\u0438\u0435 \u043f\u0440\u0430\u0432\u0438\u043b\u0430 \u041f\u0440\u0438\u043b\u043e\u0436\u0435\u043d\u0438\u0435 \u0410. \u041c\u043d\u043e\u0433\u043e\u043f\u043e\u0442\u043e\u0447\u043d\u043e\u0441\u0442\u044c II \u0421\u043a\u0440\u044b\u0442\u044c',3989)
