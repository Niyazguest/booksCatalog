INSERT INTO COMMENT VALUES(1593,'GERBER','2013-01-13','\u041d\u0430\u0447\u0438\u0442\u0430\u043b\u0441\u044f \u043e\u0442\u0437\u044b\u0432\u043e\u0432 \u0438 \u043a\u0443\u043f\u0438\u043b \u0434\u0430\u043d\u043d\u0443\u044e \u043a\u043d\u0438\u0433\u0443. \u041c\u044f\u0433\u043a\u043e \u0441\u043a\u0430\u0437\u0430\u0442\u044c \u0442\u0435\u043f\u0435\u0440\u044c \u0443 \u043c\u0435\u043d\u044f \u0434\u043e\u043c\u0430 \u043a\u0443\u0441\u043e\u043a \u043c\u0443\u043a\u0443\u043b\u0430\u0442\u0443\u0440\u044b \u0437\u0430 800\u0440. \u0412\u043e\u0437\u043c\u043e\u0436\u043d\u043e \u044d\u0442\u0430 \u043a\u043d\u0438\u0433\u0430 \u0438 \u0441\u043f\u043e\u0441\u043e\u0431\u043d\u0430 \u043d\u0430\u0443\u0447\u0438\u0442\u044c \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u044e \u043d\u0430 Java, \u043d\u043e \u0441\u043a\u043e\u0440\u0435\u0435 \u0432\u0441\u0435\u0433\u043e \u0442\u043e\u043b\u044c\u043a\u043e \u043b\u044e\u0434\u0435\u0439 \u0441 \u0437\u0430\u043f\u0430\u0434\u0430 \u0438\u043b\u0438 \u0441 \u0430\u043d\u0430\u043b\u043e\u0433\u0438\u0447\u043d\u044b\u043c \u043c\u044b\u0448\u043b\u0435\u043d\u0438\u0435\u043c. \u0421\u0447\u0438\u0442\u0430\u044e, \u0447\u0442\u043e \u043f\u0438\u0441\u0430\u0442\u044c \u0431\u043e\u043b\u0435\u0435 \u043c\u0435\u043d\u0435\u0435 \u0442\u0435\u0445\u043d\u0438\u0447\u0438\u0441\u043a\u0443\u044e \u043b\u0438\u0442\u0435\u0440\u0430\u0442\u0443\u0440\u0443 \u043d\u0430 \u043e\u0441\u043d\u043e\u0432\u0435 \u043f\u0441\u0438\u0445\u043e\u043b\u043e\u0433\u0438\u0447\u0435\u0441\u043a\u043e\u0433\u043e \u0432\u043d\u0443\u0448\u0435\u043d\u0438\u044f \u0432 \u043d\u0430\u0448\u0435\u0439 \u0441\u0442\u0440\u0430\u043d\u0435 \u043d\u0435 \u043f\u0440\u0438\u0436\u0438\u0432\u0435\u0442\u0441\u044f. \u0410\u0432\u0442\u043e\u0440\u044b \u043a\u043d\u0438\u0433\u0438 \u043e\u0431\u0449\u0430\u044e\u0442\u0441\u044f \u0441 \u0447\u0438\u0442\u0430\u0442\u0435\u043b\u0435\u043c \u0432 \u043c\u0430\u043d\u0435\u0440\u0435 \u0431\u0435\u0441\u0435\u0434\u044b \u0441 \u0440\u0435\u0431\u0435\u043d\u043a\u043e\u043c \u0441 \u043d\u0430\u0440\u0443\u0448\u0435\u043d\u0438\u0435\u043c \u043f\u0441\u0438\u0445\u0438\u043a\u0438 \u0438 \u0432\u043d\u0438\u043c\u0430\u043d\u0438\u044f. \u041e\u0434\u043d\u0438\u043c \u0441\u043b\u043e\u0432\u043e\u043c - \u0436\u0430\u043b\u0435\u044e \u043e \u0434\u0430\u043d\u043d\u043e\u0439 \u043f\u043e\u043a\u0443\u043f\u043a\u0435. \u041c\u043d\u0435 \u0430\u0436 \u0432\u0441\u0435 \u0436\u0435\u043b\u0430\u043d\u0438\u0435 \u043a \u0438\u0437\u0443\u0447\u0435\u043d\u0438\u044e Java \u043e\u0442\u0431\u0438\u043b\u0438(( \u0421\u043a\u0440\u044b\u0442\u044c',3837)
INSERT INTO COMMENT VALUES(1594,'MiCCim','2013-01-18','\u0417\u043d\u0430\u043a\u043e\u043c\u0441\u0442\u0432\u043e \u0441 \u044d\u0442\u043e\u0439 \u043a\u043d\u0438\u0433\u043e\u0439 \u043d\u0430\u0447\u0430\u043b\u043e\u0441\u044c \u0435\u0449\u0435 \u0441 \u044d\u043b\u0435\u043a\u0442\u0440\u043e\u043d\u043d\u043e\u0433\u043e \u0432\u0430\u0440\u0438\u0430\u043d\u0442\u0430. \u041d\u043e \u0432 \u044d\u043b\u0435\u043a\u0442\u0440\u043e\u043d\u043d\u043e\u043c \u0432\u0430\u0440\u0438\u0430\u043d\u0442\u0435, \u0447\u0438\u0442\u0430\u0442\u044c \u043d\u0435 \u043e\u0447\u0435\u043d\u044c \u0443\u0434\u043e\u0431\u043d\u043e, \u0442\u0435\u043c \u0431\u043e\u043b\u0435\u0435 \u0442\u0430\u043a\u0438\u0435 \u0432\u0435\u0449\u0438 \u0441\u0432\u044f\u0437\u0430\u043d\u043d\u044b\u0435 \u0441 \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u0435\u043c. \u042f \u043d\u0435 \u0437\u043d\u0430\u044e \u043a\u0430\u043a \u043b\u044e\u0434\u044f\u043c \u043c\u043e\u0436\u0435\u0442 \u043d\u0435 \u043d\u0440\u0430\u0432\u0438\u0442\u044c\u0441\u044f \u0434\u0430\u043d\u043d\u0430\u044f \u043a\u043d\u0438\u0433\u0430, \u043d\u0430 \u043c\u043e\u0439 \u0432\u0437\u0433\u043b\u044f\u0434, \u043e\u0431\u0443\u0447\u0435\u043d\u0438\u0435 \u0434\u043e\u043b\u0436\u043d\u043e \u0431\u044b\u0442\u044c \u043d\u0435 \u0441\u043a\u0443\u0447\u043d\u044b\u043c. \u041f\u043e \u0441\u0440\u0430\u0432\u043d\u0435\u043d\u0438\u044e \u0441 \u0434\u0440\u0443\u0433\u0438\u043c\u0438 \u0443\u0447\u0435\u0431\u043d\u0438\u043a\u0430\u043c\u0438, \u0432 \u043a\u043e\u0442\u043e\u0440\u044b\u0445 \u043c\u043d\u043e\u0436\u0435\u0441\u0442\u0432\u043e \u0441\u0443\u0445\u0438\u0445 \u0441\u0442\u0430\u0442\u0435\u0439, \u044d\u0442\u0430 \u0434\u0435\u0439\u0441\u0442\u0432\u0438\u0442\u0435\u043b\u044c\u043d\u043e \u0434\u0430\u0435\u0442 \u0442\u043e\u043b\u043a\u043e\u0432\u044b\u0435 \u043f\u043e\u044f\u0441\u043d\u0435\u043d\u0438\u044f \u0438 \u0437\u043d\u0430\u043d\u0438\u044f. \u0418 \u044f\u0432\u043b\u044f\u0435\u0442\u0441\u044f \u043d\u0430\u0438\u0431\u043e\u043b\u0435\u0435 \u043b\u0435\u0433\u043a\u043e\u0439. \u042d\u0442\u043e \u0442\u0430\u043a \u0441\u043a\u0430\u0437\u0430\u0442\u044c \u043f\u0435\u0440\u0432\u044b\u0439 \u044d\u0442\u0430\u043f. \u041f\u043e\u0441\u043b\u0435 \u043f\u0440\u043e\u0447\u0442\u0435\u043d\u0438\u044f, \u0438 \u0423\u042f\u0421\u041d\u0415\u041d\u0418\u042f \u043c\u0430\u0442\u0435\u0440\u0438\u0430\u043b\u0430 \u044d\u0442\u043e\u0439 \u043a\u043d\u0438\u0433\u0438, \u043c\u043e\u0436\u043d\u043e \u043f\u0435\u0440\u0435\u0445\u043e\u0434\u0438\u0442\u044c \u043d\u0430 \u0434\u0440\u0443\u0433\u043e\u0439 \u0443\u0440\u043e\u0432\u0435\u043d\u044c. \u0427\u0438\u0442\u0430\u0442\u044c \u0434\u0440\u0443\u0433\u0438\u0435 \u043c\u0430\u0442\u0435\u0440\u0438\u0430\u043b\u044b \u043f\u043e Java \u0412\u0430\u043c \u0441\u0442\u0430\u043d\u0435\u0442 \u0432 \u0440\u0430\u0437\u044b \u043b\u0435\u0433\u0447\u0435. 10 \u0438\u0437 10. \u0421\u043e\u0432\u0435\u0442\u0443\u044e. \u041a\u0430\u0447\u0435\u0441\u0442\u0432\u043e \u043f\u0435\u0447\u0430\u0442\u0438 \u0443 \u0438\u0437\u0434\u0430\u0442\u0435\u043b\u044c\u0441\u0442\u0432\u0430 \u043e\u0442\u043b\u0438\u0447\u043d\u043e\u0435. \u041a\u0430\u0447\u0435\u0441\u0442\u0432\u043e \u043c\u0430\u0442\u0435\u0440\u0438\u0430\u043b\u0430 \u0442\u043e\u0436\u0435. \u0421\u043a\u0440\u044b\u0442\u044c',3837)
INSERT INTO COMMENT VALUES(1595,'\u0412\u0435\u043b\u044c\u044f\u043d\u043e\u0432\u0430 \u041f\u043e\u043b\u0438\u043d\u0430','2013-01-07','\u041d\u0430\u0434\u043e \u043d\u0430\u0447\u0430\u0442\u044c \u0441 \u0442\u043e\u0433\u043e, \u0447\u0442\u043e \u0432\u0441\u044f \u0441\u0435\u0440\u0438\u044f Head First \u043e\u0447\u0435\u043d\u044c \u043b\u0435\u0433\u043a\u0430 \u0432 \u0432\u043e\u0441\u043f\u0440\u0438\u044f\u0442\u0438\u0438 \u043c\u0430\u0442\u0435\u0440\u0438\u0430\u043b\u0430. \u041e\u0442\u043b\u0438\u0447\u043d\u044b\u0435 \u043a\u043d\u0438\u0433\u0438, \u0432 \u043a\u043e\u0442\u043e\u0440\u044b\u0445 \u0438\u043d\u0444\u043e\u0440\u043c\u0430\u0446\u0438\u044f \u043f\u043e\u0434\u0430\u0435\u0442\u0441\u044f \u0432 \u0432\u0438\u0434\u0435 \u043f\u043e\u043b\u044c\u0437\u043e\u0432\u0430\u0442\u0435\u043b\u044c\u0441\u043a\u043e\u0433\u043e \u0438\u043d\u0442\u0435\u0440\u0444\u0435\u0439\u0441\u0430, \u0442.\u0435 \u0434\u043e\u043f\u043e\u043b\u043d\u044f\u0435\u0442\u0441\u044f \u043c\u043d\u043e\u0436\u0435\u0441\u0442\u0432\u043e\u043c \u043a\u0430\u0440\u0442\u0438\u043d\u043e\u043a \u0438 \u043f\u0440\u0438\u043c\u0435\u0440\u043e\u0432. \u041a\u0430\u0441\u0430\u0435\u043c\u043e \u043a\u043d\u0438\u0433\u0438 \u043f\u043e Java - \u043f\u043e\u0434\u043e\u0439\u0434\u0435\u0442 \u0434\u0430\u0436\u0435 \u0434\u043b\u044f \u0430\u0431\u0441\u043e\u043b\u044e\u0442\u043d\u044b\u0445 \u043d\u043e\u0432\u0438\u0447\u043a\u043e\u0432. \u0412\u0441\u0435 \u0440\u0430\u0441\u0441\u043c\u0430\u0442\u0440\u0438\u0432\u0430\u0435\u0442\u0441\u044f \u0441 \u0441\u0430\u043c\u044b\u0445 \u0430\u0437\u043e\u0432. (\u0443\u043f\u0440\u0430\u0432\u043b\u0435\u043d\u0438\u0435 \u0441 \u043f\u043e\u043c\u043e\u0449\u044c\u044e \u043a\u043e\u043c\u0430\u043d\u0434\u043d\u043e\u0439 \u0441\u0442\u0440\u043e\u043a\u0438, \u0443\u0441\u0442\u0430\u043d\u043e\u0432\u043a\u0430 \u0438 \u0442\u0434), \u0442\u0430\u043a \u0438 \u043d\u0435 \u043d\u043e\u0432\u