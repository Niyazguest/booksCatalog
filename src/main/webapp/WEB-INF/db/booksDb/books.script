INSERT INTO COMMENT VALUES(1422,'Nyakkoev Dmitry','2015-01-15','\u041a\u043d\u0438\u0433\u0430 \u043f\u043e\u043d\u0440\u0430\u0432\u0438\u043b\u0430\u0441\u044c: \u0434\u043e\u0441\u0442\u0430\u0442\u043e\u0447\u043d\u043e \u0432\u0441\u0435\u043e\u0431\u044a\u0435\u043c\u043b\u044e\u0449\u0430. \u0412\u0441\u0435 \u043f\u0440\u0438\u043c\u0435\u0440\u044b \u0440\u0430\u0437\u043e\u0431\u0440\u0430\u043d\u044b \u043f\u043e\u0434\u0440\u043e\u0431\u043d\u043e \u0438 \u043e\u0447\u0435\u043d\u044c \u043f\u043e\u043d\u044f\u0442\u043d\u043e, \u0438\u043b\u043b\u044e\u0441\u0442\u0440\u0430\u0446\u0438\u0439 \u0438 \u0441\u0445\u0435\u043c \u0432 \u0434\u043e\u0441\u0442\u0430\u0442\u043a\u0435, \u0442\u043e \u0447\u0442\u043e \u043d\u0443\u0436\u043d\u043e \u043d\u043e\u0432\u0438\u0447\u043a\u0443. \u041c\u043e\u0436\u043d\u043e \u043d\u0430\u0437\u0430\u0442\u044c \u043e\u0442\u043b\u0438\u0447\u043d\u043e\u0439 , \u0435\u0441\u043b\u0438 \u0431\u044b, \u043d\u0435 \u043a\u0440\u0435\u043f\u043b\u0435\u043d\u0438\u0435 \u0441\u0442\u0440\u0430\u043d\u0438\u0446 \u043d\u0430 \u043a\u043b\u0435\u044e. \u041f\u0440\u0438\u0445\u043e\u0434\u0438\u0442\u0441\u044f \u043a\u0443\u043b\u0430\u043a\u043e\u043c \u0440\u0430\u0437\u0433\u043b\u0430\u0436\u0438\u0432\u0430\u0442\u044c \u0448\u043e\u0432, \u043a\u043d\u0438\u0433\u0430 \u043d\u0430\u0440\u043e\u0432\u0438\u0442 \u0441\u0430\u043c\u0430 \u0437\u0430\u043a\u0440\u044b\u0442\u044c\u0441\u044f. \u0425\u043e\u0442\u044f \u0441\u0442\u0440\u0430\u043d\u0438\u0446\u044b \u043d\u0435 \u0432\u044b\u0432\u0430\u043b\u0438\u0432\u0430\u044e\u0442\u0441\u044f.',3305)
INSERT INTO COMMENT VALUES(1423,'Evgenia Kalinina','2015-01-26','\u042f \u043a\u0443\u043f\u0438\u043b\u0430 \u044d\u0442\u0443 \u043a\u043d\u0438\u0433\u0443 \u0432 \u043d\u0430\u0434\u0435\u0436\u0434\u0435 \u043f\u043e\u0441\u0442\u0438\u0447\u044c \u0430\u0437\u044b HTML-\u0432\u0435\u0440\u0441\u0442\u043a\u0438. \u041d\u0430 \u0442\u043e\u0442 \u043c\u043e\u043c\u0435\u043d\u0442 \u044f \u043c\u0430\u043b\u043e \u043f\u043e\u043d\u0438\u043c\u0430\u043b\u0430 \u043a\u0430\u043a \u0432 \u0441\u0442\u0440\u0443\u043a\u0442\u0443\u0440\u0435 \u0441\u0442\u0438\u043b\u0435\u0439 \u0438 \u0430\u0431\u0441\u043e\u043b\u044e\u0442\u043d\u043e \u043d\u0435 \u0440\u0430\u0437\u0431\u0438\u0440\u0430\u043b\u0430\u0441\u044c \u0432 \u0441\u043a\u0440\u0438\u043f\u0442\u0430\u0445. \u042d\u0442\u043e \u0438\u0437\u0434\u0430\u043d\u0438\u0435 \u0431\u044b\u043b\u043e \u0442\u0430\u043a\u0438\u043c \u0443\u0432\u0435\u0441\u0438\u0441\u0442\u044b\u043c, \u043a \u0442\u043e\u043c\u0443 \u0436\u0435 \u043d\u0430 \u043e\u0431\u043b\u043e\u0436\u043a\u0435 \u043a\u0440\u0430\u0441\u043e\u0432\u0430\u043b\u0430\u0441\u044c \u043d\u0430\u0434\u043f\u0438\u0441\u044c "\u041d\u0430\u0438\u0431\u043e\u043b\u0435\u0435 \u043f\u043e\u043b\u043d\u043e\u0435 \u0440\u0443\u043a\u043e\u0432\u043e\u0434\u0441\u0442\u0432\u043e". \u042f \u0442\u0435\u0440\u043f\u0435\u043b\u0438\u0432\u043e \u0438 \u0432\u043d\u0438\u043c\u0430\u0442\u0435\u043b\u044c\u043d\u043e \u0447\u0438\u0442\u0430\u043b\u0430 \u044d\u0442\u0443 \u043a\u043d\u0438\u0433\u0443, \u043d\u043e, \u0443\u0432\u044b, \u0437\u043d\u0430\u043d\u0438\u0439 \u043e\u043d\u0430 \u043c\u043d\u0435 \u043d\u0435 \u043f\u0440\u0438\u0431\u0430\u0432\u0438\u043b\u0430. \u0414\u0443\u043d\u0430\u0435\u0432 \u043f\u0438\u0448\u0435\u0442 \u0441\u043b\u0438\u0448\u043a\u043e\u043c \u0442\u0435\u0445\u043d\u0438\u0447\u043d\u044b\u043c \u044f\u0437\u044b\u043a\u043e\u043c, \u0434\u043b\u044f \u043d\u043e\u0432\u0438\u0447\u043a\u0430 - \u044d\u0442\u043e \u0441\u043b\u0438\u0448\u043a\u043e\u043c \u0441\u043b\u043e\u0436\u043d\u043e, \u043d\u043e \u0432 \u0442\u043e \u0436\u0435 \u0432\u0440\u0435\u043c\u044f \u043f\u0440\u043e\u0434\u0432\u0438\u043d\u0443\u0442\u044b\u0435 \u0441\u043f\u0435\u0446\u0438\u0430\u043b\u0438\u0441\u0442\u044b \u043d\u0435 \u043d\u0443\u0436\u0434\u0430\u044e\u0442\u0441\u044f \u0432 \u0440\u0430\u0437\u0431\u043e\u0440\u0435 \u0441\u0442\u043e\u043b\u044c \u043f\u0440\u043e\u0441\u0442\u043e\u0433\u043e \u043c\u0430\u0442\u0435\u0440\u0438\u0430\u043b\u0430. \u0421\u043b\u0438\u0448\u043a\u043e\u043c \u043c\u043d\u043e\u0433\u043e "\u0432\u043e\u0434\u044b" \u0438 \u0438\u0441\u0442\u043e\u0440\u0438\u0447\u0435\u0441\u043a\u0438\u0445 \u0441\u043f\u0440\u0430\u0432\u043e\u043a, \u0440\u0430\u0441\u0441\u043a\u0430\u0437\u044b \u043e \u043f\u0440\u043e\u0438\u0441\u0445\u043e\u0436\u0434\u0435\u043d\u0438\u0435 \u0434\u0440\u0435\u0432\u043d\u0438\u0445 \u0431\u0440\u0430\u0443\u0437\u0435\u0440\u043e\u0432. \u0418\u043d\u043e\u0433\u0434\u0430 \u0443 \u043c\u0435\u043d\u044f \u0441\u043e\u0437\u0434\u0430\u0432\u0430\u043b\u043e\u0441\u044c \u0432\u043f\u0435\u0447\u0430\u0442\u043b\u0435\u043d\u0438\u0435, \u0441\u043b\u043e\u0432\u043d\u043e \u044f \u043d\u0430 \u0443\u0440\u043e\u043a\u0435 \u0438\u043d\u0444\u043e\u0440\u043c\u0430\u0442\u0438\u043a\u0435 \u0432 \u0448\u043a\u043e\u043b\u0435, \u043a\u043e\u0433\u0434\u0430 \u0435\u0449\u0435 \u0431\u044b\u043b\u0438 \u043a\u043e\u043c\u043f\u044c\u044e\u0442\u0435\u0440\u044b \u042d\u0412\u041c. \u0412 \u043a\u043d\u0438\u0433\u0435 \u043c\u043d\u043e\u0433\u043e \u043f\u0440\u0438\u043c\u0435\u0440\u043e\u0432 \u043a\u043e\u0434\u0430 \u0438 \u0441\u043a\u0440\u0438\u043d\u0448\u043e\u0442\u043e\u0432 \u0441\u0430\u0439\u0442\u043e\u0432. \u041d\u043e \u043a\u043e\u0434 \u043d\u0430 \u0441\u0442\u0440\u0430\u043d\u0438\u0446\u0430\u0445 \u043a\u043d\u0438\u0433\u0438 \u0432\u043e\u0441\u043f\u0440\u0438\u043d\u0438\u043c\u0430\u0435\u0442\u0441\u044f \u043e\u0447\u0435\u043d\u044c \u0442\u044f\u0436\u0435\u043b\u043e, \u043e\u0441\u043e\u0431\u0435\u043d\u043d\u043e \u043f\u0440\u0438 \u0442\u043e\u043c, \u0447\u0442\u043e \u0432 \u043b\u044e\u0431\u043e\u043c \u0440\u0435\u0434\u0430\u043a\u0442\u043e\u0440\u0435 \u0435\u0441\u0442\u044c \u0441\u0438\u043d\u0442\u0430\u043a\u0441\u0438\u0447\u0435\u0441\u043a\u0430\u044f \u043f\u043e\u0434\u0441\u0432\u0435\u0442\u043a\u0430, \u0430 \u0437\u0434\u0435\u0441\u044c \u0432\u0441\u0435 \u043d\u0430\u043f\u0435\u0447\u0430\u0442\u0430\u043d\u043e \u0441\u043f\u043b\u043e\u0448\u043d\u044b\u043c \u0447\u0435\u0440\u043d\u044b\u043c \u0442\u0435\u043a\u0441\u0442\u043e\u043c. \u0412 \u0438\u0442\u043e\u0433\u0435 \u043f\u0440\u043e\u0431\u0435\u0433\u0430\u0435\u0448\u044c \u0433\u043b\u0430\u0437\u0430\u043c\u0438 \u043f\u043e \u043a\u043e\u0434\u0443 \u0438 \u0441\u043e\u0432\u0441\u0435\u043c \u0432 \u043d\u0435\u0433\u043e \u043d\u0435 \u0432\u043d\u0438\u043a\u0430\u0435\u0448\u044c. \u0414\u043b\u044f \u0441\u0435\u0431\u044f \u044f \u043f\u043e\u043d\u044f\u043b\u0430, \u0447\u0442\u043e \u0414\u0443\u043d\u0430\u0435\u0432 \u043d\u0435 \u0442\u043e\u0442 "\u0443\u0447\u0438\u0442\u0435\u043b\u044c", \u0443 \u043a\u043e\u0442\u043e\u0440\u043e\u0433\u043e \u043c\u043e\u0436\u043d\u043e \u043b\u0435\u0433\u043a\u043e \u0447\u0435\u043c\u0443-\u0442\u043e \u043d\u0430\u0443\u0447\u0438\u0442\u044c\u0441\u044f. \u0412 \u0442\u043e \u0436\u0435 \u0432\u0440\u0435\u043c\u044f \u044f \u0447\u0438\u0442\u0430\u043b\u0430 \u0442\u0435\u0445\u043d\u0438\u0447\u0435\u0441\u043a\u0443\u044e \u043b\u0438\u0442\u0435\u0440\u0430\u0442\u0443\u0440\u0443 \u043f\u043e\u0434\u043e\u0431\u043d\u043e\u0433\u043e \u043f\u043b\u0430\u043d\u0430 \u0443 \u0434\u0440\u0443\u0433\u0438\u0445 \u0430\u0432\u0442\u043e\u0440\u043e\u0432 \u0438 \u043e\u043d\u0430 \u0448\u043b\u0430 \u043f\u0440\u043e\u0441\u0442\u043e \u043d\u0430 "\u0443\u0440\u0430". \u0418\u0437\u0434\u0430\u043d\u0438\u0435 \u0431\u043e\u043b\u044c\u0448\u043e\u0433\u043e \u0444\u043e\u0440\u043c\u0430\u0442\u0430 \u0432 \u0442\u0432\u0435\u0440\u0434\u043e\u0439 \u043e\u0431\u043b\u043e\u0436\u043a\u0435. \u041b\u0438\u0441\u0442\u044b \u0442\u043e\u043d\u043a\u0438\u0435 \u0433\u0430\u0437\u0435\u0442\u043d\u043e\u0433\u043e \u043f\u043b\u0430\u043d\u0430, \u043d\u043e \u043f\u0440\u0438 \u044d\u0442\u043e\u043c \u0441\u0430\u043c\u0430 \u043a\u043d\u0438\u0433\u0430 \u0434\u043e\u0432\u043e\u043b\u044c\u043d\u043e \u0442\u044f\u0436\u0435\u043b\u0430\u044f. \u041e\u0447\u0435\u043d\u044c \u0442\u043e\u043b\u0441\u0442\u0430\u044f. \u042f \u0435\u0435 \u043d\u043e\u0441\u0438\u043b\u0430 \u0441 \u0441\u043e\u0431\u043e\u0439, \u0447\u0442\u043e\u0431\u044b \u0447\u0438\u0442\u0430\u0442\u044c \u0432 \u0434\u043e\u0440\u043e\u0433\u0435 \u0438 \u0430\u0436 \u0440\u0443\u043a\u0438 \u0431\u043e\u043b\u0435\u043b\u0438. \u041f\u0440\u0438\u043a\u043b\u0430\u0434\u044b\u0432\u0430\u044e \u0444\u043e\u0442\u043e\u0433\u0440\u0430\u0444\u0438\u0438 \u043d\u0435\u0441\u043a\u043e\u043b\u044c\u043a\u0438\u0445 \u0441\u0442\u0440\u0430\u043d\u0438\u0446. \u0421\u043a\u0440\u044b\u0442\u044c',3306)
INSERT INTO COMMENT VALUES(1424,'Kassavetes','2015-01-02','\u041d\u0435\u0441\u043a\u043e\u043b\u044c\u043a\u043e \u0444\u043e\u0442\u043e \u043f\u0440\u0435\u0434\u0441\u0442\u0430\u0432\u043b\u0435\u043d\u043d\u043e\u0433\u043e \u0438\u0437\u0434\u0430\u043d\u0438\u044f \u041f\u043e\u043a\u0430\u0437\u0430\u0442\u044c \u0438\u0437\u043e\u0431\u0440\u0430\u0436\u0435\u043d\u0438\u044f \u0421\u043a\u0440\u044b\u0442\u044c \u0438\u0437\u043e\u0431\u0440\u0430\u0436\u0435\u043d\u0438\u044f',3306)
INSERT INTO COMMENT VALUES(1425,'\u041c\u043e\u0436\u0430\u0439\u0441\u043a\u0430\u044f \u041d\u0430\u0442\u0430\u043b\u044c\u044f \u0412\u0430\u043b\u0435\u0440\u044c\u0435\u0432\u043d\u0430','2015-01-26','\u041a\u043d\u0438\u0433\u0430 \u0441\u0440\u0435\u0434\u043d\u044f\u044f.. \u0421 \u043e\u0434\u043d\u043e\u0439 \u0441\u0442\u043e\u0440\u043e\u043d\u044b \u0443 \u0430\u0432\u0442\u043e\u0440\u0430 \u0435\u0441\u0442\u044c \u0442\u0430\u043b\u0430\u043d\u0442 \u043e\u043f\u0438\u0441\u044b\u0432\u0430\u0442\u044c \u0447\u0442\u043e-\u0442\u043e \u043f\u0440\u043e\u0441\u0442\u043e., \u043d\u043e \u0432\u0441\u0451 \u043f\u043e \u0432\u0435\u0440\u0445\u0430\u043c. \u0421 \u0434\u0440\u0443\u0433\u043e\u0439 \u0441\u0442\u043e\u0440\u043e\u043d\u044b \u0432 \u043a\u043d\u0438\u0433\u0435 \u0443\u0441\u0442\u0430\u0440\u0435\u0432\u0448\u0438\u0435 \u0444\u0443\u043d\u043a\u0446\u0438\u0438 php (\u0432 php 5.4 \u0443\u0436\u0435 \u043f\u0440\u0438\u0437\u043d\u0430\u043d\u0430 \u0443\u0441\u0442\u0440\u0430\u0440\u0435\u0432\u0448\u0435\u0439) \u0431\u0438\u0431\u043b\u0438\u043e\u0442\u0435\u043a\u0438 mysql \u0432\u043c\u0435\u0441\u0442\u043e mysqli \u0438 PDO. \u0418\u0437 \u0438\u0437\u0434\u0430\u043d\u0438\u044f \u0432 \u0438\u0437\u0434\u0430\u043d\u0438\u0435 \u043f\u0443\u0442\u0435\u0448\u0435\u0441\u0442\u0432\u0443\u0435\u0442 \u0440\u0430\u0441\u0441\u043a\u0430\u0437 \u043e \u0448\u0430\u0431\u043b\u043e\u043d\u0438\u0437\u0430\u0442\u043e\u0440\u0435 Smarty, \u0445\u043e\u0442\u044f \u0435\u0441\u0442\u044c \u0431\u043e\u043b\u0435\u0435 \u0438\u043d\u0442\u0435\u0440\u0435\u0441\u043d\u044b\u0435 \u0432\u0430\u0440\u0438\u0430\u043d\u0442\u044b, \u043d\u0430\u043f\u0440\u0438\u043c\u0435\u0440: Twig. \u0427\u0435\u0441\u0442\u043d\u043e \u0433\u043e\u0432\u043e\u0440\u044f, \u043d\u0430\u0434\u043e\u0435\u0434\u0430\u044e\u0442 \u0443\u0436\u0435 \u043a\u043d\u0438\u0433\u0438 \u0441 \u043f\u0435\u0440\u0435\u043f\u0435\u0447\u0430\u0442\u043a\u043e\u0439 \u043e\u0444\u0438\u0446\u0438\u0430\u043b\u044c\u043d\u043e\u0439 \u0434\u043e\u043a\u0443\u043c\u0435\u043d\u0442\u0430\u0446\u0438\u0438 \u0438 \u043e\u0447\u0435\u0432\u0438\u0434\u043d\u044b\u0445 \u0432\u0435\u0449\u0435\u0439. \u0425\u043e\u0442\u0435\u043b\u043e\u0441\u044c \u0431\u044b \u0431\u043e\u043b\u044c\u0448\u0435 \u043c\u0430\u0442\u0435\u0440\u0438\u0430\u043b\u043e\u0432 \u043f\u043e \u0440\u0430\u0437\u0440\u0430\u0431\u043e\u0442\u043a\u0435 \u0438\u043d\u0442\u0435\u0440\u0435\u0441\u043d\u044b\u0445 \u043f\u0440\u043e\u0435\u043a\u0442\u043e\u0432 \u0441 \u043d\u0443\u043b\u044f. \u0414\u043b\u044f \u043f\u043e\u0432\u0435\u0440\u0445\u043e\u0441\u0442\u043d\u043e\u0433\u043e \u043e\u0437\u043d\u0430\u043a\u043e\u043c\u043b\u0435\u043d\u0438\u044f \u0441 php \u0438 mysql \u043f\u043e\u0434\u043e\u0439\u0434\u0451\u0442. \u0421\u043a\u0440\u044b\u0442\u044c',3307)
INSERT INTO COMMENT VALUES(1426,'\u041c\u043e\u0448\u043a\u043e\u0432 \u0415\u0432\u0433\u0435\u043d\u0438\u0439 \u0412\u0430\u0441\u0438\u043b\u044c\u0435\u0432\u0438\u0447','2015-01-11','\u043f\u0440\u0435\u0434\u043b\u0430\u0433\u0430\u044e \u0441\u0442\u0440\u0430\u043d\u0438\u0446\u044b \u043a\u043d\u0438\u0433\u0438 \u041f\u043e\u043a\u0430\u0437\u0430\u0442\u044c \u0438\u0437\u043e\u0431\u0440\u0430\u0436\u0435\u043d\u0438\u044f \u0421\u043a\u0440\u044b\u0442\u044c \u0438\u0437\u043e\u0431\u0440\u0430\u0436\u0435\u043d\u0438\u044f',3309)
INSERT INTO COMMENT VALUES(1427,'\u0414\u0430\u0434\u044b\u043a\u0438\u043d \u041f\u0430\u0432\u0435\u043b','2015-01-20','\u041e\u0442\u043b\u0438\u0447\u043d\u0430\u044f \u043a\u043d\u0438\u0433\u0430! \u0412\u0441\u0451 \u043e\u0431\u044a\u044f\u0441\u043d\u044f\u0435\u0442\u0441\u044f \u0434\u043e\u0441\u0442\u0443\u043f\u043d\u043e \u0438 \u043f\u043e\u043d\u044f\u0442\u043d\u043e. \u041e\u0447\u0435\u043d\u044c \u043c\u043d\u043e\u0433\u043e \u0443\u043f\u0440\u0430\u0436\u043d\u0435\u043d\u0438\u0439. \u0418\u0434\u0435\u0430\u043b\u044c\u043d\u043e \u043f\u043e\u0434\u043e\u0439\u0434\u0451\u0442 \u043a\u0430\u043a \u043d\u043e\u0432\u0438\u0447\u043a\u0430\u043c, \u0442\u0430\u043a \u0438 \u043e\u043f\u044b\u0442\u043d\u044b\u043c \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u0438\u0441\u0442\u0430\u043c, \u0442\u0430\u043a \u043a\u0430\u043a \u0432 \u043a\u043d\u0438\u0433\u0435 \u043e\u0431\u044a\u044f\u0441\u043d\u044f\u0435\u0442\u0441\u044f \u043c\u043d\u043e\u0433\u043e \u0432\u0430\u0436\u043d\u044b\u0445 \u043d\u044e\u0430\u043d\u0441\u043e\u0432. \u0417\u0430 \u043d\u0435\u0434\u0435\u043b\u044e \u043f\u0440\u043e\u0447\u0438\u0442\u0430\u043b \u043f\u043e\u043b\u043a\u043d\u0438\u0433\u0438. \u041a\u043d\u0438\u0433\u0430 \u044f\u0432\u043b\u044f\u0435\u0442\u0441\u044f \u0443\u0447\u0435\u0431\u043d\u0438\u043a\u043e\u043c, \u0430 \u043d\u0435 \u0441\u043f\u0440\u0430\u0432\u043e\u0447\u043d\u0438\u043a\u043e\u043c. \u041e\u043d\u0430 \u043f\u0440\u0435\u0434\u043d\u0430\u0437\u043d\u0430\u0447\u0435\u043d\u0430 \u0434\u043b\u044f \u0442\u043e\u0433\u043e, \u0447\u0442\u043e\u0431\u044b \u043d\u0430 \u043a\u043e\u043d\u043a\u0440\u0435\u0442\u043d\u044b\u0445 \u043f\u0440\u0438\u043c\u0435\u0440\u0430\u0445 \u043f\u043e\u043a\u0430\u0437\u0430\u0442\u044c \u0432\u043e\u0437\u043c\u043e\u0436\u043d\u043e\u0441\u0442\u0438 \u044f\u0437\u044b\u043a\u0430. \u0415\u0441\u043b\u0438 \u0432\u0430\u043c \u043d\u0443\u0436\u0435\u043d \u0441\u043f\u0440\u0430\u0432\u043e\u0447\u043d\u0438\u043a, \u043f\u0440\u0438\u0434\u0451\u0442\u0441\u044f \u043f\u043e\u043a\u0443\u043f\u0430\u0442\u044c \u0434\u0440\u0443\u0433\u0443\u044e \u043a\u043d\u0438\u0433\u0443.',3309)
INSERT INTO COMMENT VALUES(1428,'kuznetsov evgeniy','2015-01-25','\u0425\u043e\u0440\u043e\u0448\u0430\u044f \u043a\u043d\u0438\u0433\u0430. \u0434\u043e\u0432\u043e\u043b\u044c\u043d\u043e \u0441\u043b\u043e\u0436\u043d\u044b\u0439 \u044f\u0437\u044b\u043a. \u0438 \u0447\u0435\u043c \u043f\u0440\u043e\u0449\u0435 \u043e\u043d \u043e\u043f\u0438\u0441\u0430\u043d, \u0442\u0435\u043c \u043f\u043e\u043d\u044f\u0442\u043d\u0435\u0439. \u0432 \u044d\u0442\u043e\u0439 \u043a\u043d\u0438\u0433\u0435 \u0432\u0441\u0435 \u043e\u043f\u0438\u0441\u0430\u043d\u043e \u043f\u0440\u043e\u0441\u0442\u043e \u0438 \u043f\u043e\u043d\u044f\u0442\u043d\u043e. \u0414\u0430\u0436\u0435 \u044f, \u0447\u0435\u043b\u043e\u0432\u0435\u043a \u0434\u0430\u043b\u0435\u043a\u0438\u0439 \u043e\u0442 \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u044f \u0441\u043c\u043e\u0433 \u043d\u0435\u043c\u043d\u043e\u0433\u043e \u0440\u0430\u0437\u043e\u0431\u0440\u0430\u0442\u044c\u0441\u044f \u0432 \u043e\u0441\u043d\u043e\u0432\u0430\u0445. \u0434\u0443\u043c\u0430\u044e \u043c\u043d\u043e\u0433\u0438\u043c \u043a\u0430\u043a \u0440\u0430\u0437 \u0438\u043c\u0435\u043d\u043d\u043e \u0442\u0430\u043a\u0430\u044f \u043a\u043d\u0438\u0433\u0430 \u043f\u043e\u043c\u043e\u0436\u0435\u0442 \u0432 \u043e\u0441\u0432\u043e\u0435\u043d\u0438\u0438 \u043c\u0430\u0442\u0435\u0440\u0438\u0430\u043b\u0430.',3309)
INSERT INTO COMMENT VALUES(1429,'Kassavetes','2015-01-03','\u041d\u0435\u0441\u043a\u043e\u043b\u044c\u043a\u043e \u0444\u043e\u0442\u043e \u043f\u0440\u0435\u0434\u0441\u0442\u0430\u0432\u043b\u0435\u043d\u043d\u043e\u0433\u043e \u0438\u0437\u0434\u0430\u043d\u0438\u044f \u041f\u043e\u043a\u0430\u0437\u0430\u0442\u044c \u0438\u0437\u043e\u0431\u0440\u0430\u0436\u0435\u043d\u0438\u044f \u0421\u043a\u0440\u044b\u0442\u044c \u0438\u0437\u043e\u0431\u0440\u0430\u0436\u0435\u043d\u0438\u044f',3311)
INSERT INTO COMMENT VALUES(1430,'\u0414\u043e\u043b\u0433\u043e\u0440\u0443\u043a\u043e\u0432\u0430 \u041a\u0441\u0435\u043d\u0438\u044f','2015-01-10','\u041e\u0442\u043b\u0438\u0447\u043d\u0430\u044f \u043c\u043e\u0442\u0438\u0432\u0438\u0440\u0443\u044e\u0449\u0430\u044f \u043a\u043d\u0438\u0436\u043a\u0430. \u0420\u0435\u043a\u043e\u043c\u0435\u043d\u0434\u0443\u044e \u043b\u044e\u0434\u044f\u043c, \u043c\u0435\u0447\u0442\u0430\u044e\u0449\u0438\u043c \u043e \u043a\u0430\u0440\u044c\u0435\u0440\u0435 \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u0438\u0441\u0442\u0430, \u043d\u043e \u0441\u0442\u0430\u043b\u043a\u0438\u0432\u0430\u044e\u0449\u0438\u043c\u0441\u044f \u0441 \u043f\u0440\u043e\u0431\u043b\u0435\u043c\u0430\u043c\u0438 \u043e\u0440\u0433\u0430\u043d\u0438\u0437\u0430\u0446\u0438\u0438 \u0440\u0430\u0431\u043e\u0447\u0435\u0433\u043e \u043f\u0440\u043e\u0446\u0435\u0441\u0441\u0430 \u0438 \u043c\u043e\u0442\u0438\u0432\u0430\u0446\u0438\u0438.',3313)
INSERT INTO COMMENT VALUES(1431,'DangerT','2015-01-04','\u041a\u043d\u0438\u0433\u0430 \u0431\u0443\u0434\u0435\u0442 \u043e\u0447\u0435\u043d\u044c \u043f\u043e\u043b\u0435\u0437\u043d\u0430 \u043f\u0440\u043e\u0444\u0435\u0441\u0441\u0438\u043e\u043d\u0430\u043b\u044c\u043d\u044b\u043c \u0440\u0430\u0437\u0440\u0430\u0431\u043e\u0442\u0447\u0438\u043a\u0430\u043c. \u0412 \u043d\u0435\u0439 \u043e\u0447\u0435\u043d\u044c \u043c\u043d\u043e\u0433\u043e \u043d\u0443\u0436\u043d\u043e\u0439 \u0438\u043d\u0444\u043e\u0440\u043c\u0430\u0446\u0438\u0438 \u0434\u043b\u044f \u0442\u0435\u0445, \u043a\u0442\u043e \u0438\u0441\u043f\u044b\u0442\u044b\u0432\u0430\u0435\u0442 \u043d\u0435\u0434\u043e\u0441\u0442\u0430\u0442\u043e\u043a \u043c\u043e\u0442\u0438\u0432\u0430\u0446\u0438\u0438, \u0438\u043b\u0438 \u043a\u0442\u043e \u0437\u0430\u0441\u0442\u0440\u044f\u043b \u0432 \u043a\u0430\u0440\u044c\u0435\u0440\u043d\u043e\u043c \u0440\u0430\u0437\u0432\u0438\u0442\u0438\u0438. \u041e\u0433\u0440\u043e\u043c\u043d\u043e\u0435 \u043a\u043e\u043b\u0438\u0447\u0435\u0441\u0442\u0432\u043e \u043f\u043e\u043b\u0435\u0437\u043d\u044b\u0445 \u0441\u043e\u0432\u0435\u0442\u043e\u0432 \u0431\u0443\u043a\u0432\u0430\u043b\u044c\u043d\u043e \u0434\u043b\u044f \u043a\u0430\u0436\u0434\u043e\u0439 \u0441\u0438\u0442\u0443\u0430\u0446\u0438\u0438 \u0441 \u043a\u043e\u0442\u043e\u0440\u043e\u0439 \u043b\u044e\u0431\u043e\u0439 \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u0438\u0441\u0442 \u043c\u043e\u0436\u0435\u0442 \u0441\u0442\u043e\u043b\u043a\u043d\u0443\u0442\u044c\u0441\u044f \u043d\u0430 \u0440\u0430\u0431\u043e\u0442\u0435. \u0423\u0434\u0435\u043b\u0435\u043d\u043e \u0432\u043d\u0438\u043c\u0430\u043d\u0438\u0435 \u043e\u0442\u043d\u043e\u0448\u0435\u043d\u0438\u044f\u043c \u0441 \u043a\u043e\u043b\u043b\u0435\u043a\u0442\u0438\u0432\u043e\u043c. \u041d\u043e \u0441\u0430\u043c\u043e\u0435 \u0433\u043b\u0430\u0432\u043d\u043e\u0435 - \u0430\u0432\u0442\u043e\u0440 \u0440\u0430\u0441\u0441\u043a\u0430\u0437\u044b\u0432\u0430\u0435\u0442 \u043a\u0430\u043a \u0438\u0434\u0442\u0438 \u043a \u0441\u0432\u043e\u0435\u0439 \u0446\u0435\u043b\u0438 \u0438 \u043d\u0435 \u0437\u0430\u0446\u0438\u043a\u043b\u0438\u0432\u0430\u0442\u044c\u0441\u044f \u043d\u0430 \u0441\u0440\u0435\u0434\u0441\u0442\u0432\u0430\u0445 \u0435\u0435 \u0434\u043e\u0441\u0442\u0438\u0436\u0435\u043d\u0438\u044f. \u041a\u0430\u043a \u043d\u0435 \u0440\u0430\u0441\u043f\u044b\u043b\u044f\u0442\u044c\u0441\u044f \u043d\u0430 \u0432\u0442\u043e\u0440\u043e\u0441\u0442\u0435\u043f\u0435\u043d\u043d\u044b\u0435 \u0437\u0430\u043d\u044f\u0442\u0438\u044f \u0438 \u043d\u0430\u0443\u0447\u0438\u0442\u044c\u0441\u044f \u0446\u0435\u043d\u0438\u0442\u044c \u0441\u0435\u0431\u044f \u0438 \u0441\u0432\u043e\u0435 \u0432\u0440\u0435\u043c\u044f. \u041a\u043d\u0438\u0433\u0430 \u043d\u0430\u043f\u0438\u0441\u0430\u043d\u0430 \u043f\u0440\u043e\u0441\u0442\u044b\u043c \u044f\u0437\u044b\u043a\u043e\u043c, \u043c\u0430\u043d\u0435\u0440\u0430 \u0438\u0437\u043b\u043e\u0436\u0435\u043d\u0438\u044f \u043e\u0447\u0435\u043d\u044c \u043b\u0435\u0433\u043a\u0430\u044f. \u041e\u0447\u0435\u043d\u044c \u043b\u0435\u0433\u043a\u043e \u043f\u043e\u0434\u0434\u0430\u0435\u0442\u0441\u044f \u0447\u0442\u0435\u043d\u0438\u044e - \u0432\u044b \u043f\u0440\u043e\u0447\u0442\u0435\u0442\u0435 \u0435\u0435 \u0437\u0430 \u043d\u0435\u0434\u0435\u043b\u044e \u043c\u0430\u043a\u0441\u0438\u043c\u0443\u043c. \u0421\u043a\u0440\u044b\u0442\u044c',3313)
INSERT INTO COMMENT VALUES(1432,'\u042f\u0445\u043e\u0448 \u0410\u043d\u0430\u0441\u0442\u0430\u0441\u0438\u044f','2015-01-09','\u0418\u0437\u0434\u0430\u043d\u0438\u0435 \u043a\u0430\u0447\u0435\u0441\u0442\u0432\u0435\u043d\u043d\u043e\u0435, \u0431\u0435\u043b\u0430\u044f \u0431\u0443\u043c\u0430\u0433\u0430, \u0447\u0435\u0440\u043d\u043e-\u0431\u0435\u043b\u0430\u044f \u043f\u0435\u0447\u0430\u0442\u044c, \u043c\u043d\u043e\u0433\u043e \u0432\u0441\u0442\u0430\u0432\u043e\u043a, \u0442\u0430\u0431\u043b\u0438\u0446, \u043f\u0440\u0438\u043c\u0435\u0440\u043e\u0432. \u0423\u0434\u043e\u0431\u043d\u0430\u044f \u0434\u043b\u044f \u0447\u0442\u0435\u043d\u0438\u044f. \u041d\u0430 \u043c\u043e\u0439 \u0432\u0437\u0433\u043b\u044f\u0434, \u043f\u043e\u0434\u0445\u043e\u0434\u0438\u0442 \u0431\u043e\u043b\u044c\u0448\u0435 \u0434\u043b\u044f \u0442\u0435\u0445, \u043a\u0442\u043e \u0442\u043e\u043b\u044c\u043a\u043e \u043d\u0430\u0447\u0438\u043d\u0430\u0435\u0442 \u0443\u0432\u043b\u0435\u043a\u0430\u0442\u044c\u0441\u044f \u0430\u043d\u0434\u0440\u043e\u0438\u0434-\u0440\u0430\u0437\u0440\u0430\u0431\u043e\u0442\u043a\u0430\u043c\u0438. \u0414\u043b\u044f \u043e\u043f\u044b\u0442\u043d\u043e\u0433\u043e \u043f\u043e\u043b\u044c\u0437\u043e\u0432\u0430\u0442\u0435\u043b\u044f \u0432\u0440\u044f\u0434 \u043b\u0438 \u0441\u0442\u0430\u043d\u0435\u0442 \u043e\u0442\u043a\u0440\u044b\u0442\u0438\u0435\u043c. \u0426\u0435\u043d\u0430, \u043d\u0430 \u043c\u043e\u0439 \u0432\u0437\u0433\u043b\u044f\u0434, \u0437\u0430\u0432\u044b\u0448\u0435\u043d\u0430. \u041e\u0434\u043d\u0430\u043a\u043e \u044d\u0442\u043e \u043e\u0431\u044a\u044f\u0441\u043d\u044f\u0435\u0442\u0441\u044f \u043d\u0435 \u043f\u043e\u0441\u0442\u043e\u044f\u043d\u043d\u044b\u043c \u0441\u043f\u0440\u043e\u0441\u043e\u043c \u043d\u0430 \u043a\u043d\u0438\u0433\u0438 \u0442\u0430\u043a\u043e\u0439 \u0442\u0435\u043c\u0430\u0442\u0438\u043a\u0438 \u0438 \u043d\u0435\u0431\u043e\u043b\u044c\u0448\u043e\u0435 \u043a\u043e\u043b\u0438\u0447\u0435\u0441\u0442\u0432\u043e \u0442\u0430\u043a\u0438\u0445 \u043a\u043d\u0438\u0433, \u043f\u0440\u0435\u0434\u0441\u0442\u0430\u0432\u043b\u0435\u043d\u043d\u044b\u0445 \u043d\u0430 \u0440\u044b\u043d\u043a\u0435. \u042d\u0442\u043e \u0438\u0437\u0434\u0430\u043d\u0438\u0435 2015\u0433, \u0442.\u0435. \u043d\u0430\u0438\u0431\u043e\u043b\u0435\u0435 \u0441\u043e\u0432\u0440\u0435\u043c\u0435\u043d\u043d\u043e\u0435 \u0438\u0437 \u0442\u043e\u0433\u043e, \u0447\u0442\u043e \u043b\u0438\u0447\u043d\u043e \u043c\u043d\u0435 \u0443\u0434\u0430\u0432\u0430\u043b\u043e\u0441\u044c \u043d\u0430\u0439\u0442\u0438. \u0421\u0441\u044b\u043b\u043a\u0438 \u043f\u043e \u0442\u0435\u043c\u0435: http://www.labirint.ru/books/471175/ http://www.labirint.ru/books/477636/ \u041f\u043e\u043a\u0430\u0437\u0430\u0442\u044c \u0438\u0437\u043e\u0431\u0440\u0430\u0436\u0435\u043d\u0438\u044f \u0421\u043a\u0440\u044b\u0442\u044c \u0438\u0437\u043e\u0431\u0440\u0430\u0436\u0435\u043d\u0438\u044f',3314)
