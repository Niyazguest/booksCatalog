INSERT INTO COMMENT VALUES(1779,'\u041a\u0430\u0442\u0440\u0438\u043d7','2014-01-21','\u041c\u043d\u0435 \u043e\u0447\u0435\u043d\u044c \u043d\u0440\u0430\u0432\u044f\u0442\u0441\u044f \u043a\u043d\u0438\u0433\u0438 \u044d\u0442\u043e\u0433\u043e \u0430\u0432\u0442\u043e\u0440\u0430. \u0418\u0445 \u0432\u0441\u0435 \u043e\u0442\u043b\u0438\u0447\u0430\u0435\u0442 \u0442\u043e, \u0447\u0442\u043e \u043e\u043d\u0438 \u043d\u0430\u043f\u0438\u0441\u0430\u043d\u044b \u043e\u0447\u0435\u043d\u044c \u0434\u043e\u0441\u0442\u0443\u043f\u043d\u044b\u043c \u0438 \u043f\u043e\u043d\u044f\u0442\u043d\u044b\u043c \u044f\u0437\u044b\u043a\u043e\u043c, \u0431\u0435\u0437 \u0438\u0437\u043b\u0438\u0448\u043d\u0438\u0445 \u0441\u043b\u043e\u0436\u043d\u043e\u0441\u0442\u0435\u0439. \u0423 \u0430\u0432\u0442\u043e\u0440\u0430 \u0443\u043d\u0438\u043a\u0430\u043b\u044c\u043d\u0430\u044f \u0441\u043f\u043e\u0441\u043e\u0431\u043d\u043e\u0441\u0442\u044c \u0440\u0430\u0441\u0441\u043a\u0430\u0437\u0430\u0442\u044c \u043f\u0440\u043e\u0441\u0442\u043e \u043e \u0441\u043b\u043e\u0436\u043d\u043e\u043c. \u0417\u0434\u0435\u0441\u044c \u0440\u0430\u0441\u0441\u043c\u0430\u0442\u0440\u0438\u0432\u0430\u0435\u0442\u0441\u044f \u044f\u0437\u044b\u043a \u0421++ \u0434\u043b\u044f \u043f\u0440\u0438\u043c\u0435\u043d\u0435\u043d\u0438\u044f \u0435\u0433\u043e \u0432 C++ Builder, \u0430 \u0435\u0441\u043b\u0438 \u0432\u044b \u0438\u043d\u0442\u0435\u0440\u0435\u0441\u0443\u0435\u0442\u0435\u0441\u044c \u044d\u0442\u043e\u0439 \u043a\u043d\u0438\u0433\u043e\u0439, \u0442\u043e \u0437\u043d\u0430\u0435\u0442\u0435, \u0447\u0442\u043e \u0435\u0441\u0442\u044c \u043d\u0435\u043a\u043e\u0442\u043e\u0440\u044b\u0435 \u0441\u043f\u0435\u0446\u0438\u0444\u0438\u0447\u0435\u0441\u043a\u0438\u0435 \u043e\u0441\u043e\u0431\u0435\u043d\u043d\u043e\u0441\u0442\u0438 \u0438\u0441\u043f\u043e\u043b\u044c\u0437\u043e\u0432\u0430\u043d\u0438\u044f \u044d\u0442\u043e\u0433\u043e \u044f\u0437\u044b\u043a\u0430 \u0434\u043b\u044f C++ Builder. \u0417\u0434\u0435\u0441\u044c \u043e\u0447\u0435\u043d\u044c \u043c\u043d\u043e\u0433\u043e \u0440\u0430\u0437\u043b\u0438\u0447\u043d\u044b\u0445 \u043f\u0440\u0438\u043c\u0435\u0440\u043e\u0432 \u0438 \u0442\u0430\u043a\u0436\u0435 \u0441\u0442\u043e\u0438\u0442 \u043e\u0442\u043c\u0435\u0442\u0438\u0442\u044c, \u0447\u0442\u043e \u0432 \u043a\u043d\u0438\u0433\u0435 \u043d\u0435\u0442 \u043f\u0440\u0438\u0432\u044f\u0437\u043a\u0438 \u043a \u043a\u043e\u043d\u043a\u0440\u0435\u0442\u043d\u044b\u043c \u0432\u0435\u0440\u0441\u0438\u044f\u043c, \u0430 \u0437\u043d\u0430\u0447\u0438\u0442 \u0435\u0435 \u043c\u043e\u0436\u043d\u043e \u0438\u0441\u043f\u043e\u043b\u044c\u0437\u043e\u0432\u0430\u0442\u044c \u043f\u0440\u0438 \u0440\u0430\u0431\u043e\u0442\u0435 \u0441 \u043b\u044e\u0431\u043e\u0439 \u0432\u0435\u0440\u0441\u0438\u0435\u0439. \u042d\u0442\u0443 \u043a\u043d\u0438\u0433\u0443 \u043c\u043e\u0436\u043d\u043e \u0438\u0441\u043f\u043e\u043b\u044c\u0437\u043e\u0432\u0430\u0442\u044c \u043a\u0430\u043a \u043d\u0430\u0447\u0438\u043d\u0430\u044e\u0449\u0438\u043c \u0440\u0430\u0437\u0440\u0430\u0431\u043e\u0442\u0447\u0438\u043a\u0430\u043c \u0434\u043b\u044f \u0437\u043d\u0430\u043a\u043e\u043c\u0441\u0442\u0432\u0430 \u0441 \u0421++, \u0442\u0430\u043a \u0438 \u043f\u0440\u043e\u0444\u0435\u0441\u0441\u0438\u043e\u043d\u0430\u043b\u0430\u043c \u0432 \u043a\u0430\u0447\u0435\u0441\u0442\u0432\u0435 \u0441\u043f\u0440\u0430\u0432\u043e\u0447\u043d\u0438\u043a\u0430. \u041d\u043e \u043d\u0430\u0447\u0438\u043d\u0430\u044e\u0449\u0438\u043c \u043d\u0443\u0436\u043d\u043e \u0438\u043c\u0435\u0442\u044c \u0432 \u0432\u0438\u0434\u0443, \u0447\u0442\u043e \u0437\u0434\u0435\u0441\u044c \u043d\u0435\u0442 \u0437\u043d\u0430\u043a\u043e\u043c\u0441\u0442\u0432\u0430 \u0441\u043e \u0441\u0440\u0435\u0434\u043e\u0439 \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u044f \u0438 \u043d\u0435 \u0440\u0430\u0441\u0441\u043c\u0430\u0442\u0440\u0438\u0432\u0430\u044e\u0442\u0441\u044f \u043a\u043e\u043c\u043f\u043e\u043d\u0435\u043d\u0442\u044b, \u043a\u0430\u043a \u0432 \u0434\u0440\u0443\u0433\u0438\u0445 \u043a\u043d\u0438\u0433\u0430\u0445 \u044d\u0442\u043e\u0433\u043e \u0436\u0435 \u0430\u0432\u0442\u043e\u0440\u0430, \u0437\u0434\u0435\u0441\u044c \u0442\u043e\u043b\u044c\u043a\u043e \u0440\u0430\u0441\u0441\u043c\u0430\u0442\u0440\u0438\u0432\u0430\u0435\u0442\u0441\u044f \u0441\u0430\u043c \u044f\u0437\u044b\u043a. \u0412 \u043e\u0431\u0449\u0435\u043c \u043a\u043d\u0438\u0433\u0430 \u043e\u0447\u0435\u043d\u044c \u043f\u043e\u043b\u0435\u0437\u043d\u0430\u044f \u0438 \u0441\u0442\u043e\u044f\u0449\u0430\u044f! \u0415\u0441\u0442\u044c \u043f\u0440\u0430\u0432\u0434\u0430 \u043d\u0435\u0431\u043e\u043b\u044c\u0448\u0438\u0435 \u043d\u0435\u0434\u043e\u0441\u0442\u0430\u0442\u043a\u0438, \u043a\u0430\u0441\u0430\u044e\u0449\u0438\u0435\u0441\u044f \u0441\u0430\u043c\u043e\u0433\u043e \u0438\u0437\u0434\u0430\u043d\u0438\u044f: \u0448\u0440\u0438\u0444\u0442 \u043c\u0435\u043b\u043a\u043e\u0432\u0430\u0442, \u0431\u0443\u043c\u0430\u0433\u0430 \u0441\u0435\u0440\u0430\u044f \u0438 \u043d\u0435 \u043e\u0447\u0435\u043d\u044c \u0445\u043e\u0440\u043e\u0448\u0435\u0433\u043e \u043a\u0430\u0447\u0435\u0441\u0442\u0432\u0430, \u043d\u043e \u0432\u0441\u0435 \u044d\u0442\u043e \u043c\u043e\u0436\u043d\u043e \u043f\u043e\u043d\u044f\u0442\u044c, \u043f\u043e\u0441\u043a\u043e\u043b\u044c\u043a\u0443 \u043e\u0431\u044a\u0435\u043c \u0438\u043d\u0444\u043e\u0440\u043c\u0430\u0446\u0438\u0438 \u0437\u0434\u0435\u0441\u044c \u043f\u0440\u043e\u0441\u0442\u043e \u043e\u0433\u0440\u043e\u043c\u043d\u044b\u0439! \u0411\u043e\u043b\u0435\u0435 \u043f\u043e\u0434\u0440\u043e\u0431\u043d\u043e\u0435 \u0441\u043e\u0434\u0435\u0440\u0436\u0430\u043d\u0438\u0435 \u0438 \u043f\u0440\u0438\u043c\u0435\u0440\u044b \u0433\u043b\u0430\u0432: \u0421\u043a\u0440\u044b\u0442\u044c',4024)
INSERT INTO COMMENT VALUES(1780,'Harley_Quinn','2011-01-05','\u042d\u0442\u043e\u0442 \u0443\u0447\u0435\u0431\u043d\u0438\u043a \u043e\u0441\u0432\u0435\u0449\u0430\u0435\u0442 \u0432\u0441\u0435 \u0442\u0440\u0443\u0434\u043d\u043e\u0441\u0442\u0438 \u0438 \u0442\u043e\u043d\u043a\u043e\u0441\u0442\u0438, \u043a\u043e\u0442\u043e\u0440\u044b\u0445 \u043a\u0430\u0441\u0430\u0435\u0442\u0441\u044f \u041f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u0435 \u043d\u0430 \u044f\u0437\u044b\u043a\u0435 RUBY. \u0418\u0437\u0434\u0430\u043d\u0438\u0435 \u043d\u0435 \u043e\u0447\u0435\u043d\u044c \u0445\u043e\u0440\u043e\u0448\u0435\u0435, \u043d\u043e \u0434\u0440\u0443\u0433\u043e\u0433\u043e \u044f \u043d\u0435 \u0432\u0438\u0434\u0435\u043b\u0430. \u0410 \u0438\u043d\u0444\u043e\u0440\u043c\u0430\u0446\u0438\u044f, \u043a\u043e\u0442\u043e\u0440\u0443\u044e \u043e\u043d\u0430 \u0441\u043e\u0434\u0435\u0440\u0436\u0438\u0442, \u043e\u0447\u0435\u043d\u044c \u043f\u043e\u043b\u0435\u0437\u043d\u0430\u044f. \u0422\u0430\u043a\u043e\u0433\u043e \u043f\u043e\u0434\u0440\u043e\u0431\u043d\u043e\u0433\u043e \u0443\u0447\u0435\u0431\u043d\u0438\u043a\u0430 \u043f\u043e \u0440\u0443\u0431\u0438 \u044f \u0431\u043e\u043b\u044c\u0448\u0435 \u043d\u0435 \u0432\u0438\u0434\u0435\u043b\u0430. \u0422\u043e\u043b\u0441\u0442\u0430\u044f \u043a\u043d\u0438\u0436\u043a\u0430, \u043c\u044f\u0433\u043a\u0430\u044f \u043e\u0431\u043b\u043e\u0436\u043a\u0430, \u0433\u0430\u0437\u0435\u0442\u043d\u044b\u0435 \u0441\u0442\u0440\u0430\u043d\u0438\u0446\u044b. \u0414\u043e\u0431\u0430\u0432\u043b\u044e \u0441\u043e\u0434\u0435\u0440\u0436\u0430\u043d\u0438\u0435: \u0413\u043b\u0430\u0432\u0430 1. \u041e\u0431\u0437\u043e\u0440 Ruby \u0413\u043b\u0430\u0432\u0430 2. \u0421\u0442\u0440\u043e\u043a\u0438 \u0413\u043b\u0430\u0432\u0430 3. \u0420\u0435\u0433\u0443\u043b\u044f\u0440\u043d\u044b\u0435 \u0432\u044b\u0440\u0430\u0436\u0435\u043d\u0438\u044f \u0413\u043b\u0430\u0432\u0430 4. \u0418\u043d\u0442\u0435\u0440\u043d\u0430\u0446\u0438\u043e\u043d\u0430\u043b\u0438\u0437\u0430\u0446\u0438\u044f \u0432 Ruby \u0413\u043b\u0430\u0432\u0430 5. \u0427\u0438\u0441\u043b\u0435\u043d\u043d\u044b\u0435 \u043c\u0435\u0442\u043e\u0434\u044b \u0413\u043b\u0430\u0432\u0430 6. \u0421\u0438\u043c\u0432\u043e\u043b\u044b \u0438 \u0434\u0438\u0430\u043f\u0430\u0437\u043e\u043d\u044b \u0413\u043b\u0430\u0432\u0430 7. \u0414\u0430\u0442\u0430 \u0438 \u0432\u0440\u0435\u043c\u044f \u0413\u043b\u0430\u0432\u0430 8. \u041c\u0430\u0441\u0441\u0438\u0432\u044b, \u0445\u044d\u0448\u0438 \u0438 \u0434\u0440\u0443\u0433\u0438\u0435 \u043f\u0435\u0440\u0435\u0447\u0438\u0441\u043b\u044f\u0435\u043c\u044b\u0435 \u0441\u0442\u0440\u0443\u043a\u0442\u0443\u0440\u044b \u0413\u043b\u0430\u0432\u0430 9. \u0411\u043e\u043b\u0435\u0435 \u0441\u043b\u043e\u0436\u043d\u044b\u0435 \u0441\u0442\u0440\u0443\u043a\u0442\u0443\u0440\u044b \u0434\u0430\u043d\u043d\u044b\u0445 \u0413\u043b\u0430\u0432\u0430 10. \u0412\u0432\u043e\u0434/\u0432\u044b\u0432\u043e\u0434 \u0438 \u0445\u0440\u0430\u043d\u0435\u043d\u0438\u0435 \u0434\u0430\u043d\u043d\u044b\u0445 \u0413\u043b\u0430\u0432\u0430 11. \u041e\u041e\u041f \u0438 \u0434\u0438\u043d\u0430\u043c\u0438\u0447\u0435\u0441\u043a\u0438\u0435 \u043c\u0435\u0445\u0430\u043d\u0438\u0437\u043c\u044b \u0432 Ruby \u0413\u043b\u0430\u0432\u0430 12. \u0413\u0440\u0430\u0444\u0438\u0447\u0435\u0441\u043a\u0438\u0435 \u0438\u043d\u0442\u0435\u0440\u0444\u0435\u0439\u0441\u044b \u0434\u043b\u044f Ruby \u0413\u043b\u0430\u0432\u0430 13. \u041f\u043e\u0442\u043e\u043a\u0438 \u0432 Ruby \u0413\u043b\u0430\u0432\u0430 14. \u0421\u0446\u0435\u043d\u0430\u0440\u0438\u0438 \u0438 \u0441\u0438\u0441\u0442\u0435\u043c\u043d\u043e\u0435 \u0430\u0434\u043c\u0438\u043d\u0438\u0441\u0442\u0440\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u0435 \u0413\u043b\u0430\u0432\u0430 15. Ruby \u0438 \u0444\u043e\u0440\u043c\u0430\u0442\u044b \u0434\u0430\u043d\u043d\u044b\u0445 \u0413\u043b\u0430\u0432\u0430 16. \u0422\u0435\u0441\u0442\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u0435 \u0438 \u043e\u0442\u043b\u0430\u0434\u043a\u0430 \u0413\u043b\u0430\u0432\u0430 17. \u0421\u043e\u0437\u0434\u0430\u043d\u0438\u0435 \u043f\u0430\u043a\u0435\u0442\u043e\u0432 \u0438 \u0440\u0430\u0441\u043f\u0440\u043e\u0441\u0442\u0440\u0430\u043d\u0435\u043d\u0438\u0435 \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c \u0413\u043b\u0430\u0432\u0430 18. \u0421\u0435\u0442\u0435\u0432\u043e\u0435 \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u0435 \u0413\u043b\u0430\u0432\u0430 19. Ruby \u0438 Web-\u043f\u0440\u0438\u043b\u043e\u0436\u0435\u043d\u0438\u044f \u0413\u043b\u0430\u0432\u0430 20. \u0420\u0430\u0441\u043f\u0440\u0435\u0434\u0435\u043b\u0435\u043d\u043d\u044b\u0439 Ruby \u0413\u043b\u0430\u0432\u0430 21. \u0418\u043d\u0441\u0442\u0440\u0443\u043c\u0435\u043d\u0442\u044b \u0440\u0430\u0437\u0440\u0430\u0431\u043e\u0442\u043a\u0438 \u0434\u043b\u044f Ruby \u0413\u043b\u0430\u0432\u0430 22. \u0421\u043e\u043e\u0431\u0449\u0435\u0441\u0442\u0432\u043e \u043f\u043e\u043b\u044c\u0437\u043e\u0432\u0430\u0442\u0435\u043b\u0435\u0439 Ruby \u0421\u043a\u0440\u044b\u0442\u044c',4026)
INSERT INTO COMMENT VALUES(1781,'\u041a\u043e\u0432\u0430\u043b\u0435\u043d\u043a\u043e \u041f\u0430\u0432\u0435\u043b','2015-01-22','\u041f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u0435 \u043d\u0430 \u044f\u0437\u044b\u043a\u0435 RUBY. \u041f\u0435\u0440\u0432\u043e\u0435, \u0447\u0442\u043e \u043d\u0443\u0436\u043d\u043e \u0441\u043a\u0430\u0437\u0430\u0442\u044c. \u042d\u0442\u0430 \u043a\u043d\u0438\u0433\u0430 \u043d\u0435 \u0443\u0447\u0435\u0431\u043d\u0438\u043a! \u041d\u0430\u0443\u0447\u0438\u0442\u044c\u0441\u044f \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u0438\u0440\u043e\u0432\u0430\u0442\u044c \u043d\u0430 \u044f\u0437\u044b\u043a\u0435 Ruby \u0441 \u043d\u0443\u043b\u044f, \u043d\u0435 \u0443\u043c\u0435\u044f \u043f\u0438\u0441\u0430\u0442\u044c \u043a\u043e\u0434 \u043d\u0430 \u043a\u0430\u043a\u043e\u043c-\u043b\u0438\u0431\u043e \u0434\u0440\u0443\u0433\u043e\u043c \u044f\u0437\u044b\u043a\u0435, \u0432\u044b \u043d\u0435 \u0441\u043c\u043e\u0436\u0435\u0442\u0435! \u0415\u0441\u043b\u0438 \u0436\u0435 \u0432\u044b \u0443\u0436\u0435 \u0443\u043c\u0435\u0435\u0442\u0435 \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u0438\u0440\u043e\u0432\u0430\u0442\u044c, \u043f\u0443\u0441\u0442\u044c \u0434\u0430\u0436\u0435 \u043d\u0430 \u0434\u043e\u0441\u0442\u0430\u0442\u043e\u0447\u043d\u043e \u043d\u0438\u0437\u043a\u043e\u043c \u0443\u0440\u043e\u0432\u043d\u0435, \u0442\u043e \u0441\u0438\u0442\u0443\u0430\u0446\u0438\u044f \u043c\u0435\u043d\u044f\u0435\u0442\u0441\u044f. \u0410\u0432\u0442\u043e\u0440 \u0432 \u043f\u0435\u0440\u0432\u044b\u0445 \u0433\u043b\u0430\u0432\u0430\u0445 \u043e\u0447\u0435\u043d\u044c \u043f\u043e\u0434\u0440\u043e\u0431\u043d\u043e \u043e\u043f\u0438\u0441\u0430\u043b \u043a\u043e\u043d\u0446\u0435\u043f\u0446\u0438\u0438 \u044f\u0437\u044b\u043a\u0430, \u0432\u043e\u0437\u043c\u043e\u0436\u043d\u043e\u0441\u0442\u0438, \u0441\u0438\u043d\u0442\u0430\u043a\u0441\u0438\u0441. \u0414\u043b\u044f \u0447\u0435\u043b\u043e\u0432\u0435\u043a\u0430, \u043a\u043e\u0442\u043e\u0440\u044b\u0439 \u0440\u0430\u043d\u0435\u0435 \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u0438\u0440\u043e\u0432\u0430\u043b, \u044d\u0442\u0438 \u0433\u043b\u0430\u0432\u044b \u0431\u0443\u0434\u0443\u0442 \u043d\u0435\u0432\u0435\u0440\u043e\u044f\u0442\u043d\u043e \u0446\u0435\u043d\u043d\u044b, \u043f\u043e\u0442\u043e\u043c\u0443, \u0447\u0442\u043e \u043f\u0440\u043e\u0447\u0438\u0442\u0430\u0432 \u0438\u0445 \u0443\u0436\u0435 \u043c\u043e\u0436\u043d\u043e \u043f\u0438\u0441\u0430\u0442\u044c \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u044b. \u041f\u043e\u043f\u0443\u0442\u043d\u043e \u043f\u043e\u043b\u044c\u0437\u0443\u044f\u0441\u044c api \u044f\u0437\u044b\u043a\u0430. \u041d\u043e, \u0435\u0441\u043b\u0438 \u0432\u044b \u0445\u043e\u0442\u0438\u0442\u0435 \u0438\u0437\u0443\u0447\u0438\u0442\u044c \u044f\u0437\u044b\u043a \u0434\u043e\u0441\u043a\u043e\u043d\u0430\u043b\u044c\u043d\u043e, \u0447\u0442\u043e\u0431\u044b \u0437\u043d\u0430\u0442\u044c, \u043a\u0430\u043a, \u043a\u043e\u0433\u0434\u0430 \u0438 \u0434\u043b\u044f \u0447\u0435\u0433\u043e \u0438\u0441\u043f\u043e\u043b\u044c\u0437\u0443\u044e\u0442 \u0438 \u043c\u043e\u0433\u0443\u0442 \u0431\u044b\u0442\u044c \u0438\u0441\u043f\u043e\u043b\u044c\u0437\u043e\u0432\u0430\u043d\u044b \u0442\u0435 \u0438\u043b\u0438 \u0438\u043d\u044b\u0435 \u0444\u0443\u043d\u043a\u0446\u0438\u0438, \u0442\u043e \u0441\u0442\u043e\u0438\u0442 \u043f\u043e\u0439\u0442\u0438 \u0434\u0430\u043b\u044c\u0448\u0435 \u0438 \u043f\u0440\u043e\u0447\u0435\u0441\u0442\u044c \u043a\u043d\u0438\u0433\u0443 \u043f\u043e\u043b\u043d\u043e\u0441\u0442\u044c\u044e. \u0412\u044b \u0431\u0443\u0434\u0435\u0442\u0435 \u0434\u043e\u0432\u043e\u043b\u044c\u043d\u044b \u0440\u0435\u0437\u0443\u043b\u044c\u0442\u0430\u0442\u043e\u043c. \u0422\u0435\u043c \u0431\u043e\u043b\u0435\u0435, \u0447\u0442\u043e \u043a\u043d\u0438\u0433\u0430 \u0438\u0437\u043e\u0431\u0438\u043b\u0443\u0435\u0442 \u0441\u0430\u043c\u044b\u043c\u0438 \u0440\u0430\u0437\u043b\u0438\u0447\u043d\u044b\u043c\u0438 \u043f\u0440\u0438\u043c\u0435\u0440\u0430\u043c\u0438 \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u043d\u043e\u0433\u043e \u043a\u043e\u0434\u0430. \u0427\u0442\u043e \u043a\u0430\u0441\u0430\u0435\u0442\u0441\u044f \u043f\u0435\u0440\u0435\u0432\u043e\u0434\u0430, \u043a\u0430\u043a \u0432\u0441\u0435\u0433\u0434\u0430 \u043d\u0430 \u0432\u044b\u0441\u043e\u0442\u0435. \u0421\u043b\u0438\u043d\u043a\u0438\u043d \u0435\u0449\u0435 \u043d\u0438 \u0440\u0430\u0437\u0443 \u043d\u0435 \u0440\u0430\u0437\u043e\u0447\u0430\u0440\u043e\u0432\u0430\u043b. \u041d\u0435 \u043d\u0430\u0448\u0435\u043b \u043d\u0438 \u043e\u0434\u043d\u043e\u0439 \u043e\u043f\u0435\u0447\u0430\u0442\u043a\u0438. \u0410\u0432\u0442\u043e\u0440 \u043f\u0435\u0440\u0435\u0432\u043e\u0434\u0430 \u043f\u043e\u043d\u0438\u043c\u0430\u0435\u0442, \u0447\u0442\u043e \u043e\u043d \u0434\u0435\u043b\u0430\u0435\u0442 \u0438 \u043e \u0447\u0435\u043c \u043f\u0438\u0448\u0435\u0442. \u0427\u0442\u043e \u043a\u0430\u0441\u0430\u0435\u0442\u0441\u044f \u0441\u0430\u043c\u043e\u0439 \u043a\u043d\u0438\u0433\u0438, \u0442\u0443\u0442 \u0432\u0441\u0435 \u0441\u043b\u043e\u0436\u043d\u0435\u0435. \u041a\u043d\u0438\u0433\u0430 \u00ab\u0445\u0440\u0443\u043f\u043a\u0430\u044f\u00bb. \u0421 \u043d\u0435\u0439 \u0441\u0442\u043e\u0438\u0442 \u0431\u044b\u0442\u044c \u043e\u0447\u0435\u043d\u044c \u043e\u0441\u0442\u043e\u0440\u043e\u0436\u043d\u044b\u043c. \u0427\u0435\u0440\u0435\u0437 \u043c\u0435\u0441\u044f\u0446 \u0447\u0442\u0435\u043d\u0438\u044f, \u043d\u0435\u043a\u043e\u0442\u043e\u0440\u044b\u0435 \u0441\u0442\u0440\u0430\u043d\u0438\u0446\u044b \u0441\u0442\u0430\u043b\u0438 \u043e\u0442\u043a\u043b\u0435\u0438\u0432\u0430\u0442\u044c\u0441\u044f (\u043d\u043e \u043d\u0435 \u043e\u0442\u043a\u043b\u0435\u0438\u043b\u0438\u0441\u044c, \u0430 \u043a\u043d\u0438\u0433\u0430 \u0443 \u043c\u0435\u043d\u044f \u0443\u0436\u0435 \u0431\u043e\u043b\u044c\u0448\u0435 \u0433\u043e\u0434\u0430). \u0427\u0438\u0442\u0430\u0439\u0442\u0435 \u0430\u043a\u043a\u0443\u0440\u0430\u0442\u043d\u043e \u0438 \u043d\u0435 \u0440\u0430\u0441\u043a\u0440\u044b\u0432\u0430\u0439\u0442\u0435 \u043a\u043d\u0438\u0433\u0443 \u043f\u043e\u043b\u043d\u043e\u0441\u0442\u044c\u044e, \u0435\u0441\u043b\u0438 \u0445\u043e\u0442\u0438\u0442\u0435 \u0441\u043e\u0445\u0440\u0430\u043d\u0438\u0442\u044c \u0435\u0451 \u0432 \u0446\u0435\u043b\u043e\u0441\u0442\u0438 \u0438 \u0441\u043e\u0445\u0440\u0430\u043d\u043d\u043e\u0441\u0442\u0438 \u0435\u0449\u0435 \u0431\u043b\u0438\u0436\u0430\u0439\u0448\u0438\u0435 \u0433\u043e\u0434\u044b. \u041f\u043e\u0434\u0432\u043e\u0434\u044f \u0438\u0442\u043e\u0433. \u041a\u043d\u0438\u0433\u0430 \u0440\u0435\u043a\u043e\u043c\u0435\u043d\u0434\u0443\u0435\u0442\u0441\u044f \u043a \u043f\u0440\u043e\u0447\u0442\u0435\u043d\u0438\u044e, \u0435\u0441\u043b\u0438 \u0432\u044b \u043f\u043e\u0441\u0442\u0430\u0432\u0438\u043b\u0438 \u043f\u0435\u0440\u0435\u0434 \u0441\u043e\u0431\u043e\u0439 \u0446\u0435\u043b\u044c \u0438\u0437\u0443\u0447\u0438\u0442\u044c \u044f\u0437\u044b\u043a \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u044f Ruby. \u0421\u043a\u0440\u044b\u0442\u044c',4026)
INSERT INTO COMMENT VALUES(1782,'\u0415\u0440\u0435\u043c\u0435\u043d\u043a\u043e \u0412\u044f\u0447\u0435\u0441\u043b\u0430\u0432','2010-01-13','\u041a\u043d\u0438\u0433\u0430 \u0445\u043e\u0440\u043e\u0448\u0430\u044f. \u0416\u0430\u043b\u044c Active Record \u0438 Rails \u0437\u0430\u0442\u0440\u043e\u043d\u0443\u0442\u044b \u0441\u043b\u0430\u0431\u043e(\u043f\u043e 3 \u0441\u0442\u0440\u0430\u043d\u0438\u0446\u044b), \u043d\u043e \u043a\u043d\u0438\u0433\u0430 \u0432\u0435\u0434\u044c \u043f\u0440\u043e Ruby. \u041d\u0430 \u043d\u0430\u0447\u0430\u043b\u043e 2010 \u0433\u043e\u0434\u0430 \u0438\u043d\u0444\u043e\u0440\u043c\u0430\u0446\u0438\u044f \u0432 \u043d\u0435\u0439 \u0432\u0441\u0435 \u0435\u0449\u0435 \u0430\u043a\u0442\u0443\u0430\u043b\u044c\u043d\u0430.',4026)
INSERT INTO COMMENT VALUES(1783,'Panterra','2008-01-19','\u0421\u041e\u0414\u0415\u0420\u0416\u0410\u041d\u0418\u0415: \u041f\u0440\u0435\u0434\u0438\u0441\u043b\u043e\u0432\u0438\u0435. \u0411\u0435\u0433\u043b\u044b\u0439 \u043e\u0431\u0437\u043e\u0440 \u043c\u0438\u043a\u0440\u043e\u043a\u043e\u043d\u0442\u0440\u043e\u043b\u043b\u0435\u0440\u043e\u0432. \u0413\u043b\u0430\u0432\u0430 1. \u041f\u0440\u043e\u0442\u043e\u043a\u043e\u043b RS-232 - \u043e\u0441\u043d\u043e\u0432\u0430 \u0441\u0435\u0442\u0435\u0439 \u043d\u0430 \u0431\u0430\u0437\u0435 \u043c\u0438\u043a\u0440\u043e\u043a\u043e\u043d\u0442\u0440\u043e\u043b\u043b\u0435\u0440\u043e\u0432. \u0413\u043b\u0430\u0432\u0430 2. \u0420\u0435\u0430\u043b\u0438\u0437\u0430\u0446\u0438\u044f \u043f\u0440\u043e\u0442\u043e\u043a\u043e\u043b\u0430 RS-232 \u0441 \u0438\u0441\u043f\u043e\u043b\u044c\u0437\u043e\u0432\u0430\u043d\u0438\u0435\u043c \u043c\u0438\u043a\u0440\u043e\u043a\u043e\u043d\u0442\u0440\u043e\u043b\u043b\u0435\u0440\u0430. \u0413\u043b\u0430\u0432\u0430 3. \u0420\u0435\u0430\u043b\u0438\u0437\u0430\u0446\u0438\u044f \u0444\u0443\u043d\u043a\u0446\u0438\u0439 \u0438\u043d\u0442\u0435\u0440\u0444\u0435\u0439\u0441\u0430 RS-232 \u043d\u0430 \u044f\u0437\u044b\u043a\u0435 BASIC. \u0413\u043b\u0430\u0432\u0430 4. \u0424\u043e\u0440\u043c\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u0435 \u0430\u043f\u043f\u0430\u0440\u0430\u0442\u043d\u043e\u0433\u043e \u043e\u0431\u0435\u0441\u043f\u0435\u0447\u0435\u043d\u0438\u044f RS-232. \u0413\u043b\u0430\u0432\u0430 5. \u0423\u043d\u0438\u0432\u0435\u0440\u0441\u0430\u043b\u044c\u043d\u044b\u0439 \u0441\u0438\u043d\u0445\u0440\u043e\u043d\u043d\u043e-\u0430\u0441\u0438\u043d\u0445\u0440\u043e\u043d\u043d\u044b\u0439 \u043f\u0440\u0438\u0451\u043c\u043e\u043f\u0435\u0440\u0435\u0434\u0430\u0442\u0447\u0438\u043a. \u0413\u043b\u0430\u0432\u0430 6. \u0428\u0438\u043d\u0430 I2C... \u0415\u0449\u0451 \u043e\u0434\u0438\u043d \u043f\u043e\u0441\u043b\u0435\u0434\u043e\u0432\u0430\u0442\u0435\u043b\u044c\u043d\u044b\u0439 \u043f\u043e\u0440\u0442. \u0413\u043b\u0430\u0432\u0430 7. Ethernet. \u0413\u043b\u0430\u0432\u0430 8. \u041c\u0438\u043a\u0440\u043e\u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u043d\u043e\u0435 \u043e\u0431\u0435\u0441\u043f\u0435\u0447\u0435\u043d\u0438\u0435 CS8900A-CQ. \u0413\u043b\u0430\u0432\u0430 9. \u041a\u043e\u043c\u0430\u043d\u0434\u0430 PING \u0438 Easy Ethernet CS8900A. \u0413\u043b\u0430\u0432\u0430 10. \u041f\u0440\u043e\u0442\u043e\u043a\u043e\u043b UDP \u0438 Easy Ethernet CS8900A. \u0413\u043b\u0430\u0432\u0430 11. \u041f\u0440\u043e\u0442\u043e\u043a\u043e\u043b TCP \u0438 Easy Ethernet CS8900A. \u0413\u043b\u0430\u0432\u0430 12. \u0414\u0430\u0432\u0430\u0439\u0442\u0435 \u0432\u044b\u043f\u043e\u043b\u043d\u0438\u043c \u044d\u0442\u043e \u0432\u043d\u043e\u0432\u044c. \u0413\u043b\u0430\u0432\u0430 13. \u041f\u0440\u0438\u043c\u0435\u043d\u0435\u043d\u0438\u0435 Easy Ethernet AVR \u0432 \u0434\u0438\u0430\u043b\u043e\u0433\u043e\u0432\u043e\u043c \u0440\u0435\u0436\u0438\u043c\u0435. \u0413\u043b\u0430\u0432\u0430 14. \u0417\u0430\u043a\u043b\u044e\u0447\u0435\u043d\u0438\u0435. \u041f\u0440\u0435\u0434\u043c\u0435\u0442\u043d\u044b\u0439 \u0443\u043a\u0430\u0437\u0430\u0442\u0435\u043b\u044c. \u0421\u043a\u0440\u044b\u0442\u044c',4029)
