INSERT INTO COMMENT VALUES(1410,'\u041a\u0430\u0440\u043c\u044b\u0448\u0435\u0432 \u041c\u0438\u0445\u0430\u0438\u043b','2015-01-27','\u041e\u0447\u0435\u043d\u044c \u043f\u043e\u043b\u0435\u0437\u043d\u0430\u044f \u043a\u043d\u0438\u0433\u0430, \u0435\u0441\u0442\u044c \u0438 \u043d\u0435\u043c\u043d\u043e\u0433\u043e \u0442\u0435\u043e\u0440\u0438\u0438, \u0438 \u043f\u0440\u0430\u043a\u0442\u0438\u043a\u0430. \u041d\u043e \u0442\u043e\u043c\u0443, \u043a\u0442\u043e \u043f\u0440\u0438\u0434\u0443\u043c\u0430\u043b \u043a\u043e\u043c\u043c\u0435\u043d\u0442\u0430\u0440\u0438\u0438 \u043f\u0435\u0447\u0430\u0442\u0430\u0442\u044c \u0431\u043b\u0435\u0434\u043d\u043e-\u0441\u0435\u0440\u044b\u043c \u0442\u043e\u043d\u043a\u0438\u043c \u0448\u0440\u0438\u0444\u0442\u043e\u043c, \u0430 \u0432\u044b\u0432\u043e\u0434 \u0441\u0440\u0435\u0434\u044b R - \u0441\u0432\u0435\u0442\u043b\u043e-\u0433\u043e\u043b\u0443\u0431\u044b\u043c, \u043d\u0443\u0436\u043d\u043e \u043f\u0435\u0441\u043a\u0430 \u0432 \u0433\u043b\u0430\u0437\u0430 \u0441\u044b\u043f\u0430\u043d\u0443\u0442\u044c - \u043f\u0443\u0441\u0442\u044c \u0442\u043e\u0436\u0435 \u043f\u043e\u043c\u0443\u0447\u0430\u0435\u0442\u0441\u044f.',3284)
INSERT INTO COMMENT VALUES(1411,'\u0411\u043e\u0433\u0430\u0442\u044b\u0440\u0435\u0432\u0430 \u041e\u043b\u044c\u0433\u0430 \u0413\u0435\u043d\u043d\u0430\u0434\u044c\u0435\u0432\u043d\u0430','2015-01-29','\u041e\u0442\u043b\u0438\u0447\u043d\u0430\u044f \u0438\u043d\u0442\u0435\u0440\u043f\u0440\u0435\u0442\u0430\u0446\u0438\u044f \u0437\u043d\u0430\u043c\u0435\u043d\u0438\u0442\u043e\u0439 \u043a\u043d\u0438\u0433\u0438 Design Patterns "\u0431\u0430\u043d\u0434\u044b \u0447\u0435\u0442\u044b\u0440\u0435\u0445" \u0441 \u043f\u0440\u0438\u043c\u0435\u0440\u0430\u043c\u0438 \u043d\u0430 C#, \u043d\u0430\u043f\u0440\u0438\u0441\u0430\u043d\u043d\u0430\u044f \u0440\u0443\u0441\u0441\u043a\u043e\u0433\u043e\u0432\u043e\u0440\u044f\u0449\u0438\u043c \u0430\u0432\u0442\u043e\u0440\u043e\u043c. \u041d\u0435 \u043d\u0443\u0436\u043d\u043e \u0432\u0441\u0435 \u0432\u0440\u0435\u043c\u044f \u0434\u0443\u043c\u0430\u0442\u044c \u043e \u0442\u043e\u043c, \u0430 \u043a\u0430\u043a \u044d\u0442\u043e \u0437\u0432\u0443\u0447\u0430\u043b\u043e \u0432 \u043e\u0440\u0438\u0433\u0438\u043d\u0430\u043b\u0435 \u043d\u0430 \u0430\u043d\u0433\u043b\u0438\u0439\u0441\u043a\u043e\u043c \u0438 \u0434\u0443\u043c\u0430\u0442\u044c \u043e\u0431 \u043e\u0448\u0438\u0431\u043a\u0430\u0445 \u043f\u0435\u0440\u0435\u0432\u043e\u0434\u0430. \u041f\u043e\u043c\u0438\u043c\u043e \u043c\u0430\u0442\u0435\u0440\u0438\u0430\u043b\u0430 Design Patterns \u0438\u0437\u043b\u043e\u0436\u0435\u043d\u044b \u0441\u043e\u0432\u0440\u0435\u043c\u0435\u043d\u043d\u044b\u0435 \u043a\u043e\u043d\u0446\u0435\u043f\u0446\u0438\u0438 \u0432\u043d\u0435\u0434\u0440\u0435\u043d\u0438\u044f \u0437\u0430\u0432\u0438\u0441\u0438\u043c\u043e\u0439\u0442\u0435\u0439 \u0438 \u043f\u0440\u0438\u0446\u0438\u043f\u043e\u0432 SOLID. \u041d\u043e \u0443 \u043a\u043d\u0438\u0433\u0438 \u0435\u0441\u0442\u044c \u0433\u0440\u0435\u0448\u043e\u043a - \u043a\u0430\u0447\u0435\u0441\u0442\u0432\u043e \u043f\u0435\u0447\u0430\u0442\u0438 \u0442\u0435\u043a\u0441\u0442\u0430. \u0422\u0435\u043a\u0441\u0442 \u043d\u0435 \u0447\u0435\u0440\u043d\u044b\u0439, \u0430 \u0441\u0435\u0440\u044b\u0439, \u0431\u0443\u043a\u0432\u044b \u043d\u0435\u0440\u043e\u0432\u043d\u044b\u0435 - \u0431\u0443\u0434\u0442\u043e \u043d\u0430 \u043c\u0430\u0442\u0440\u0438\u0447\u043d\u043e\u043c \u043f\u0440\u0438\u043d\u0442\u0435\u0440\u0435 \u043d\u0430\u043f\u0435\u0447\u0430\u0442\u0430\u043d\u044b. \u0427\u0438\u0442\u0430\u0442\u044c \u043c\u043e\u0436\u043d\u043e, \u043d\u043e \u043a\u0430\u043a \u0442\u043e \u043d\u0430\u043f\u0440\u044f\u0433\u0430\u0435\u0442. \u0421\u043a\u0440\u044b\u0442\u044c',3290)
INSERT INTO COMMENT VALUES(1412,'\u0411\u043e\u044f\u0440\u0438\u043d\u0446\u0435\u0432 \u0421\u0442\u0430\u043d\u0438\u0441\u043b\u0430\u0432','2015-01-09','\u041a\u043d\u0438\u0433\u0430 \u043d\u0435 \u0434\u043b\u044f \u043d\u043e\u0432\u0438\u0447\u043a\u043e\u0432 \u0432 \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u0438 \u0438 \u0438\u043c\u0435\u0435\u0442 \u0441\u043c\u044b\u0441\u043b \u0447\u0438\u0442\u0430\u0442\u044c \u0435\u0435, \u0442\u043e\u043b\u044c\u043a\u043e \u0435\u0441\u043b\u0438 \u0432\u044b \u043f\u0438\u0448\u0435\u0442\u0435 \u043f\u043e\u0434 .NET/C#. \u0415\u0441\u043b\u0438 \u0432\u044b \u043f\u043e\u0434\u0445\u043e\u0434\u0438\u0442\u0435 \u043f\u043e\u0434 \u044d\u0442\u0438 \u0442\u0440\u0435\u0431\u043e\u0432\u0430\u043d\u0438\u044f, \u0442\u043e \u043a\u043d\u0438\u0433\u0430 \u0437\u0434\u043e\u0440\u043e\u0432\u043e \u0440\u0430\u0441\u0448\u0438\u0440\u0438\u0442 \u0432\u0430\u0448 \u0432\u0437\u0433\u043b\u044f\u0434 \u043d\u0430 \u043f\u0430\u0442\u0442\u0435\u0440\u043d\u044b \u0438 \u043f\u0440\u0438\u043d\u0446\u0438\u043f\u044b SOLID \u0432 \u043a\u043e\u043d\u0442\u0435\u043a\u0441\u0442\u0435 \u0432\u043e\u0437\u043c\u043e\u0436\u043d\u043e\u0441\u0442\u0435\u0439 \u044f\u0437\u044b\u043a\u0430 C#. \u0414\u043b\u044f \u0441\u0435\u0431\u044f \u0432\u0437\u044f\u043b \u043e\u0447\u0435\u043d\u044c \u043c\u043d\u043e\u0433\u043e \u0438\u043d\u0442\u0435\u0440\u0435\u0441\u043d\u044b\u0445 \u0438\u0434\u0435\u0439 \u0438 \u0435\u0449\u0435 \u0440\u0430\u0437 \u0437\u0430\u043a\u0440\u0435\u043f\u0438\u043b \u043f\u0430\u0442\u0442\u0435\u0440\u043d\u044b \u043f\u0440\u043e\u0435\u043a\u0442\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u044f \u0438 \u043f\u0440\u0438\u043d\u0446\u0438\u043f\u044b SOLID \u0432 \u043f\u0430\u043c\u044f\u0442\u0438. \u0412 \u043b\u0438\u0441\u0442\u0438\u043d\u0433\u0430\u0445 \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u043d\u043e\u0433\u043e \u043a\u043e\u0434\u0430 - \u043e\u0447\u0435\u043d\u044c \u043c\u043d\u043e\u0433\u043e \u043e\u043f\u0435\u0447\u0430\u0442\u043e\u043a (\u0432 \u043e\u0441\u043d\u043e\u0432\u043d\u043e\u043c \u043e\u0442\u0441\u0443\u0442\u0441\u0442\u0432\u0438\u0435 \u043f\u0440\u043e\u0431\u0435\u043b\u043e\u0432).',3290)
INSERT INTO COMMENT VALUES(1413,'Kassavetes','2015-01-17','\u0424\u043e\u0442\u043e \u0441 \u043e\u0433\u043b\u0430\u0432\u043b\u0435\u043d\u0438\u0435\u043c \u043f\u0440\u0435\u0434\u0441\u0442\u0430\u0432\u043b\u0435\u043d\u043d\u043e\u0433\u043e \u043a \u043f\u0440\u043e\u0434\u0430\u0436\u0435 \u0438\u0437\u0434\u0430\u043d\u0438\u044f. \u041e\u0441\u043e\u0431\u043e \u043f\u0440\u043e\u0448\u0443 \u043e\u0431\u0440\u0430\u0442\u0438\u0442\u044c \u0432\u043d\u0438\u043c\u0430\u043d\u0438\u0435 \u043d\u0430 \u0442\u0438\u0440\u0430\u0436. \u041f\u043e\u043a\u0430\u0437\u0430\u0442\u044c \u0438\u0437\u043e\u0431\u0440\u0430\u0436\u0435\u043d\u0438\u044f \u0421\u043a\u0440\u044b\u0442\u044c \u0438\u0437\u043e\u0431\u0440\u0430\u0436\u0435\u043d\u0438\u044f',3292)
INSERT INTO COMMENT VALUES(1414,'\u0427\u0435\u0440\u043a\u0430\u0441\u043e\u0432 \u0414\u0435\u043d\u0438\u0441','2015-01-30','\u0412 \u0446\u0435\u043b\u043e\u043c \u0431\u0435\u0441\u043f\u043e\u043b\u0435\u0437\u043d\u0430\u044f \u043a\u043d\u0438\u0433\u0430, \u0442.\u043a. \u043a\u043d\u0438\u0433\u0430 \u043d\u0435 \u0434\u0430\u0435\u0442 \u0438\u0441\u0447\u0435\u0440\u043f\u044b\u0432\u0430\u044e\u0449\u0438\u0445 \u043e\u0442\u0432\u0435\u0442\u043e\u0432 \u043a\u0430\u043a \u0440\u0430\u0431\u043e\u0442\u0430\u0442\u044c \u0441 yii2. \u041f\u043e\u044d\u0442\u043e\u043c\u0443 \u043a\u043d\u0438\u0433\u0430 \u043b\u0435\u0436\u0438\u0442 \u043d\u0430 \u0441\u0442\u043e\u043b\u0435 \u043d\u0430 "\u0432\u0441\u044f\u043a\u0438\u0439" \u0441\u043b\u0443\u0447\u0430\u0439, \u0430 \u0447\u0438\u0442\u0430\u044e - \u0434\u043e\u043a\u0443\u043c\u0435\u043d\u0442\u0430\u0446\u0438\u044e \u043a yii2. \u0412 \u043a\u043d\u0438\u0433\u0438 \u0434\u0430\u0435\u0442\u0441\u044f \u043f\u043e\u0434\u0445\u043e\u0434 \u043f\u043e \u0440\u0430\u0437\u0440\u0430\u0431\u043e\u0442\u043a\u0435 \u043d\u0430 \u0434\u0430\u043d\u043d\u043e\u043c \u0444\u0440\u0435\u0439\u043c\u0432\u043e\u0440\u043a\u0435 \u0438 \u043d\u0435 \u0431\u043e\u043b\u0435\u0435.',3296)
INSERT INTO COMMENT VALUES(1415,'\u0417\u0430\u0431\u0443\u0433\u0430 \u041c\u0438\u0445\u0430\u0438\u043b','2015-01-13','\u041a\u043d\u0438\u0433\u0430 \u0432\u043f\u0435\u0447\u0430\u0442\u043b\u044f\u0435\u0442 \u043e\u0431\u044a\u0435\u043c\u043e\u043c. \u041f\u043e\u043a\u0430 \u043d\u0435 \u0447\u0438\u0442\u0430\u043b, \u0432\u044b\u043a\u043b\u0430\u0434\u044b\u0432\u0430\u044e \u043f\u043e\u0434\u0440\u043e\u0431\u043d\u043e\u0435 \u043e\u0433\u043b\u0430\u0432\u043b\u0435\u043d\u0438\u0435 \u041f\u043e\u043a\u0430\u0437\u0430\u0442\u044c \u0438\u0437\u043e\u0431\u0440\u0430\u0436\u0435\u043d\u0438\u044f \u0421\u043a\u0440\u044b\u0442\u044c \u0438\u0437\u043e\u0431\u0440\u0430\u0436\u0435\u043d\u0438\u044f',3297)
INSERT INTO COMMENT VALUES(1416,'\u0414\u0430\u0432\u044b\u0434\u043e\u0432 \u0421\u0435\u0440\u0433\u0435\u0439','2015-01-22','\u0414\u0430\u043d\u043d\u0430\u044f \u043a\u043d\u0438\u0433\u0430 \u043e\u0447\u0435\u043d\u044c \u0443\u0434\u043e\u0431\u043d\u0430\u044f, \u0441\u0430\u043c \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u0438\u0440\u043e\u0432\u0430\u043b \u043d\u0430 Pascal \u0434\u043e \u044d\u0442\u043e\u0433\u043e, \u0438\u0437\u0443\u0447\u0430\u043b \u0441\u0442\u0430\u043d\u0434\u0430\u0440\u0442\u044b, \u0432\u0437\u044f\u043b \u044d\u0442\u0443 \u043a\u043d\u0438\u0433\u0443 - \u043e\u0434\u043d\u0438 "+", \u043e\u0431\u044a\u044f\u0441\u043d\u0435\u043d\u0438\u044f - \u044d\u043b\u0435\u043c\u0435\u043d\u0442\u0430\u0440\u043d\u044b\u0435, \u043d\u0435\u0442 \u0442\u044f\u0436\u0435\u043b\u044b\u0445 \u0438 \u043d\u0435 \u043f\u043e\u043d\u044f\u0442\u043d\u044b\u0445 \u0441\u043b\u043e\u0432 \u0434\u043b\u044f \u043d\u043e\u0432\u0438\u0447\u043a\u043e\u0432, \u0435\u0441\u043b\u0438 \u0436\u0435 \u043e\u043d\u0438 \u0435\u0441\u0442\u044c, \u043e\u043d\u0438 \u0440\u0430\u0441\u043a\u0440\u044b\u0432\u0430\u044e\u0442\u0441\u044f. \u041a\u043d\u0438\u0433\u0430 \u0447\u0438\u0442\u0430\u0435\u0442\u0441\u044f \u0431\u044b\u0441\u0442\u0440\u043e \u0438 \u043b\u0435\u0433\u043a\u043e, \u0434\u0430\u044e\u0442\u0441\u044f \u043e\u0441\u043d\u043e\u0432\u044b \u0438 \u043d\u0435\u043c\u043d\u043e\u0433\u043e \u043f\u0440\u043e\u0434\u0432\u0438\u043d\u0443\u0442\u044b\u0439 \u0443\u0440\u043e\u0432\u0435\u043d\u044c, \u0435\u0441\u043b\u0438 \u0432\u044b \u0442\u043e\u043b\u044c\u043a\u043e \u043d\u0430\u0447\u0438\u043d\u0430\u0435\u0442\u0435 \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u0438\u0440\u043e\u0432\u0430\u0442\u044c \u0438 \u0443 \u0432\u0430\u0441 \u0435\u0441\u0442\u044c \u043d\u0430\u0447\u0430\u043b\u044c\u043d\u0430\u044f \u043d\u0435\u0431\u043e\u043b\u044c\u0448\u0430\u044f \u0431\u0430\u0437\u0430 - \u044d\u0442\u0430 \u043a\u043d\u0438\u0433\u0430 \u0434\u043b\u044f \u0432\u0430\u0441!',3299)
