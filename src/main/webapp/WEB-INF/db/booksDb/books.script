INSERT INTO COMMENT VALUES(1439,'\u041c\u043e\u0448\u043a\u043e\u0432 \u0415\u0432\u0433\u0435\u043d\u0438\u0439 \u0412\u0430\u0441\u0438\u043b\u044c\u0435\u0432\u0438\u0447','2015-01-11','\u043f\u0440\u0435\u0434\u043b\u0430\u0433\u0430\u044e \u0441\u0442\u0440\u0430\u043d\u0438\u0446\u044b \u043a\u043d\u0438\u0433\u0438 \u041f\u043e\u043a\u0430\u0437\u0430\u0442\u044c \u0438\u0437\u043e\u0431\u0440\u0430\u0436\u0435\u043d\u0438\u044f \u0421\u043a\u0440\u044b\u0442\u044c \u0438\u0437\u043e\u0431\u0440\u0430\u0436\u0435\u043d\u0438\u044f',3322)
INSERT INTO COMMENT VALUES(1440,'Cyanide_Regime','2015-01-25','\u042f \u0431\u044b \u0440\u0435\u043a\u043e\u043c\u0435\u043d\u0434\u043e\u0432\u0430\u043b \u0431\u0440\u0430\u0442\u044c\u0441\u044f \u0437\u0430 \u044d\u0442\u0443 \u043a\u043d\u0438\u0433\u0443 \u043f\u043e\u0441\u043b\u0435 \u043f\u0440\u043e\u0447\u0442\u0435\u043d\u0438\u044f \u0437\u043d\u0430\u043c\u0435\u043d\u0438\u0442\u043e\u0439 "\u0418\u0437\u0443\u0447\u0430\u0439 Haskell \u0432\u043e \u0438\u043c\u044f \u0434\u043e\u0431\u0440\u0430!" \u041c\u0438\u0440\u0430\u043d\u0430 \u041b\u0438\u043f\u043e\u0432\u0430\u0447\u0430, \u0433\u0434\u0435 \u043f\u043e\u0434\u0440\u043e\u0431\u043d\u043e \u0438\u0437\u043b\u043e\u0436\u0435\u043d\u044b \u0438\u0434\u0435\u0438 \u0438 \u043e\u0441\u043d\u043e\u0432\u044b \u044f\u0437\u044b\u043a\u0430. \u0410 \u0432\u043e\u0442 \u0434\u043b\u044f \u043f\u0440\u0430\u043a\u0442\u0438\u0447\u0435\u0441\u043a\u043e\u0439 \u0447\u0430\u0441\u0442\u0438 - \u0434\u043e\u0431\u0440\u043e \u043f\u043e\u0436\u0430\u043b\u043e\u0432\u0430\u0442\u044c \u043a \u0421\u0435\u0440\u0430\u043d\u043e. \u041f\u0440\u0435\u0442\u0435\u043d\u0437\u0438\u0439 \u043a \u043c\u0430\u0442\u0435\u0440\u0438\u0430\u043b\u0443 \u043d\u0438\u043a\u0430\u043a\u0438\u0445 - \u043d\u0435\u0442 "\u0432\u043e\u0434\u044b", \u0432\u0441\u0435 \u043f\u0440\u043e\u0441\u0442\u043e \u0438 \u043f\u043e \u0434\u0435\u043b\u0443. \u0415\u0441\u043b\u0438 \u043f\u043e\u0441\u043b\u0435 \u043f\u0440\u043e\u0447\u0442\u0435\u043d\u0438\u044f \u041b\u0438\u043f\u043e\u0432\u0430\u0447\u0430 \u0447\u0438\u0442\u0430\u0442\u0435\u043b\u044c \u0447\u0430\u0441\u0442\u043e \u043e\u0441\u0442\u0430\u0435\u0442\u0441\u044f \u0441 \u0432\u043e\u043f\u0440\u043e\u0441\u043e\u043c "\u0410 \u0447\u0442\u043e \u0436\u0435 \u0434\u0435\u043b\u0430\u0442\u044c \u0434\u0430\u043b\u044c\u0448\u0435?", \u0442\u043e \u0421\u0435\u0440\u0430\u043d\u043e \u0434\u043e\u0432\u043e\u043b\u044c\u043d\u043e \u043e\u0431\u0441\u0442\u043e\u044f\u0442\u0435\u043b\u044c\u043d\u043e \u044d\u0442\u0443 \u043f\u0440\u043e\u0431\u043b\u0435\u043c\u0443 \u0440\u0435\u0448\u0430\u0435\u0442 - \u0432\u0430\u0441 \u043f\u043e\u0437\u043d\u0430\u043a\u043e\u043c\u044f\u0442 \u0441 \u044d\u043a\u043e\u0441\u0438\u0441\u0442\u0435\u043c\u043e\u0439 Haskell, \u043c\u043d\u043e\u0433\u0438\u043c\u0438 \u0431\u0438\u0431\u043b\u0438\u043e\u0442\u0435\u043a\u0430\u043c\u0438 \u0438 \u0438\u043d\u0441\u0442\u0440\u0443\u043c\u0435\u043d\u0442\u0430\u043c\u0438, \u043f\u043e\u0441\u043b\u0435 \u0447\u0435\u0433\u043e \u0432\u044b \u0443\u0436\u0435 \u0441\u043f\u043e\u0441\u043e\u0431\u043d\u044b \u0440\u0435\u0448\u0430\u0442\u044c \u043a\u043e\u043d\u043a\u0440\u0435\u0442\u043d\u044b\u0435 \u043f\u0440\u0430\u043a\u0442\u0438\u0447\u0435\u0441\u043a\u0438\u0435 \u0437\u0430\u0434\u0430\u0447\u0438. \u041a\u0430\u0447\u0435\u0441\u0442\u0432\u043e \u0431\u0443\u043c\u0430\u0433\u0438 \u0438 \u043f\u0435\u0447\u0430\u0442\u0438 \u043e\u0447\u0435\u043d\u044c \u0445\u043e\u0440\u043e\u0448\u0435\u0435, \u043a\u043d\u0438\u0433\u0443 \u043e\u0447\u0435\u043d\u044c \u0443\u0434\u043e\u0431\u043d\u043e \u0447\u0438\u0442\u0430\u0442\u044c \u0434\u0430\u0436\u0435 \u0432 \u043c\u0435\u0442\u0440\u043e. \u0421\u043a\u0440\u044b\u0442\u044c',3330)
