INSERT INTO COMMENT VALUES(1749,'Amaterasy','2012-01-14','\u041e\u0431\u043b\u043e\u0436\u043a\u0430 \u0442\u0432\u0435\u0440\u0434\u0430\u044f.\u041f\u043e \u043d\u0430\u0447\u0430\u043b\u0443 \u0440\u0430\u0437\u043e\u0447\u0430\u0440\u043e\u0432\u0430\u043b\u0430\u0441\u044c, \u0432 \u0442\u043e\u043c, \u0447\u0442\u043e \u043b\u0438\u0441\u0442\u044b \u0436\u0435\u043b\u0442\u044b\u0435, \u043d\u043e \u0438\u0437\u0443\u0447\u0430\u044f \u0441\u043e\u0434\u0435\u0440\u0436\u0430\u043d\u0438\u0435 \u043b\u0438\u0441\u0442\u044b \u043d\u0435 \u043c\u043e\u0440\u0449\u0438\u043b\u0438\u0441\u044c \u043e\u0442 \u043f\u0430\u043b\u044c\u0446\u0435\u0432, \u0442.\u043a. \u043f\u043b\u043e\u0442\u043d\u044b\u0435. \u041e\u0447\u0435\u043d\u044c \u043f\u043e\u0440\u0430\u0434\u043e\u0432\u0430\u043b\u043e, \u0447\u0442\u043e \u043f\u043e\u043c\u0438\u043c\u043e \u0442\u0435\u043e\u0440\u0438\u0438, \u0432 \u043a\u043e\u043d\u0446\u0435 \u043a\u043d\u0438\u0433\u0438 - \u043f\u0440\u0438\u043c\u0435\u0440\u044b \u0438 \u0437\u0430\u0434\u0430\u043d\u0438\u044f, \u043d\u0430 \u043a\u043e\u0442\u043e\u0440\u044b\u0445 \u043c\u043e\u0436\u043d\u043e \u0437\u0430\u043a\u0440\u0435\u043f\u0438\u0442\u044c \u043f\u043e\u043b\u0443\u0447\u0435\u043d\u043d\u044b\u0435 \u0437\u043d\u0430\u043d\u0438\u044f. \u041f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u0435 \u043d\u0430 \u044f\u0437\u044b\u043a\u0435 \u043f\u0440\u043e\u043b\u043e\u0433. \u042d\u0442\u0430 \u043a\u043d\u0438\u0433\u0430 \u0432 \u043f\u043e\u043c\u043e\u0449\u044c \u0441\u0442\u0443\u0434\u0435\u043d\u0442\u0430\u043c \u0441\u043e \u0441\u043f\u0435\u0446\u0438\u0430\u043b\u044c\u043d\u043e\u0441\u0442\u0435\u0439, \u0441\u0432\u044f\u0437\u0430\u043d\u043d\u044b\u0445 \u0441 \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u0435\u043c \u0438 \u0438\u043d\u0444\u043e\u0440\u043c\u0430\u0442\u0438\u043a\u043e\u0439, \u0432 \u0442\u0430\u043a\u0438\u0445 \u0434\u0438\u0441\u0446\u0438\u043f\u043b\u0438\u043d\u0430\u0445, \u043a\u0430\u043a \u0442\u0435\u043e\u0440\u0438\u044f \u0430\u043b\u0433\u043e\u0440\u0438\u0442\u043c\u043e\u0432, \u043c\u0430\u0442.\u043b\u043e\u0433\u0438\u043a\u0430, \u0434\u0438\u0441\u043a\u0440\u0435\u0442\u043d\u0430\u044f \u043c\u0430\u0442\u0435\u043c\u0430\u0442\u0438\u043a\u0430... \u041f\u043e\u043a\u0430\u0437\u0430\u0442\u044c \u0438\u0437\u043e\u0431\u0440\u0430\u0436\u0435\u043d\u0438\u044f \u0421\u043a\u0440\u044b\u0442\u044c \u0438\u0437\u043e\u0431\u0440\u0430\u0436\u0435\u043d\u0438\u044f',3994)
INSERT INTO COMMENT VALUES(1750,'\u0427\u0435\u0440\u0435\u0437\u043e\u0432\u0430 \u0421\u0432\u0435\u0442\u043b\u0430\u043d\u0430','2012-01-03','\u042d\u0442\u0430 \u043a\u043d\u0438\u0433\u0430 \u2013 \u0441\u0432\u043e\u0435\u0433\u043e \u0440\u043e\u0434\u0430 \u043c\u0430\u0441\u0442\u0435\u0440-\u043a\u043b\u0430\u0441\u0441, \u043a\u043e\u0442\u043e\u0440\u044b\u0439 \u0434\u0430\u0451\u0442 \u0432\u0441\u0435\u043c\u0438\u0440\u043d\u043e \u0438\u0437\u0432\u0435\u0441\u0442\u043d\u044b\u0439 \u043c\u0430\u044d\u0441\u0442\u0440\u043e. \u042d\u0442\u043e \u2013 \u043f\u0440\u0430\u043a\u0442\u0438\u0447\u0435\u0441\u043a\u043e\u0435 \u043f\u043e\u0441\u043e\u0431\u0438\u0435 \u0434\u043b\u044f \u0432\u0441\u0435\u0445 \u0442\u0435\u0445 \u043b\u044e\u0434\u0435\u0439, \u043a\u0442\u043e \u0436\u0435\u043b\u0430\u0435\u0442 \u0440\u0430\u0437\u043e\u0431\u0440\u0430\u0442\u044c\u0441\u044f \u0438 \u043f\u043e\u043d\u044f\u0442\u044c, \u0447\u0442\u043e \u0442\u0430\u043a\u043e\u0435 \u043a\u043e\u043c\u043f\u0438\u043b\u044f\u0442\u043e\u0440 \u0438 \u043a\u0430\u043a \u043e\u043d \u0443\u0441\u0442\u0440\u043e\u0435\u043d. \u041f\u043e \u043c\u043d\u0435\u043d\u0438\u044e \u0430\u0432\u0442\u043e\u0440\u0430, \u0431\u0435\u0437 \u044d\u0442\u043e\u0433\u043e \u043d\u0438 \u043e\u0434\u0438\u043d \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u0438\u0441\u0442 \u043d\u0435 \u043c\u043e\u0436\u0435\u0442 \u043d\u0430\u0437\u044b\u0432\u0430\u0442\u044c \u0441\u0435\u0431\u044f \u043a\u0432\u0430\u043b\u0438\u0444\u0438\u0446\u0438\u0440\u043e\u0432\u0430\u043d\u043d\u044b\u043c \u0441\u043f\u0435\u0446\u0438\u0430\u043b\u0438\u0441\u0442\u043e\u043c. \u0414\u043b\u044f \u043e\u0437\u043d\u0430\u043a\u043e\u043c\u043b\u0435\u043d\u0438\u044f \u0441 \u043a\u043d\u0438\u0433\u043e\u0439 \u043f\u0440\u0438\u0432\u043e\u0436\u0443 \u043f\u043e\u043b\u043d\u043e\u0435 \u043e\u0433\u043b\u0430\u0432\u043b\u0435\u043d\u0438\u0435 \u0438 \u0438\u0437\u043e\u0431\u0440\u0430\u0436\u0435\u043d\u0438\u044f \u043d\u0435\u043a\u043e\u0442\u043e\u0440\u044b\u0445 \u0441\u0442\u0440\u0430\u043d\u0438\u0446. \u041f\u043e\u043a\u0430\u0437\u0430\u0442\u044c \u0438\u0437\u043e\u0431\u0440\u0430\u0436\u0435\u043d\u0438\u044f \u0421\u043a\u0440\u044b\u0442\u044c \u0438\u0437\u043e\u0431\u0440\u0430\u0436\u0435\u043d\u0438\u044f',3999)
INSERT INTO COMMENT VALUES(1751,'\u041d\u043e\u0432\u0438\u043a\u043e\u0432 \u041f\u0430\u0432\u0435\u043b','2010-01-09','\u042d\u0442\u0430 \u043a\u043d\u0438\u0433\u0430 \u0430\u0434\u0440\u0435\u0441\u043e\u0432\u0430\u043d\u0430 \u043f\u0440\u0435\u0436\u0434\u0435 \u0432\u0441\u0435\u0433\u043e \u0434\u043b\u044f \u0442\u0435\u0445, \u043a\u0442\u043e \u0445\u043e\u0447\u0435\u0442 \u0443\u0437\u043d\u0430\u0442\u044c \u043f\u0440\u0438\u043d\u0446\u0438\u043f\u044b \u043d\u0430\u043f\u0438\u0441\u0430\u043d\u0438\u044f \u043a\u043e\u043c\u043f\u0438\u043b\u044f\u0442\u043e\u0440\u043e\u0432 \u0438 \u043d\u0435 \u0445\u043e\u0447\u0435\u0442 \u0442\u0440\u0430\u0442\u0438\u0442\u044c \u0432\u0440\u0435\u043c\u044f \u043d\u0430 \u0438\u0437\u0443\u0447\u0435\u043d\u0438\u0435 \u0441\u043b\u043e\u0436\u043d\u044b\u0445 \u0434\u043b\u044f \u043f\u043e\u043d\u0438\u043c\u0430\u043d\u0438\u044f \u0432\u043e\u043f\u0440\u043e\u0441\u043e\u0432 \u0441\u0432\u044f\u0437\u0430\u043d\u043d\u044b\u0445 \u0441 \u044d\u0442\u0438\u043c. \u042d\u0442\u0430 \u043a\u043d\u0438\u0433\u0430 \u043f\u043e\u0434\u043a\u0443\u043f\u0430\u0435\u0442 \u0447\u0438\u0442\u0430\u0442\u0435\u043b\u044f \u043f\u0440\u043e\u0441\u0442\u043e\u0442\u043e\u0439 \u0438\u0437\u043b\u043e\u0436\u0435\u043d\u0438\u044f, \u043a\u0440\u0430\u0442\u043a\u043e\u0441\u0442\u044c\u044e \u0438 \u0434\u043e\u0441\u0442\u0443\u043f\u043d\u043e\u0441\u0442\u044c\u044e \u043c\u0430\u0442\u0435\u0440\u0438\u0430\u043b\u0430. \u0415\u0441\u043b\u0438 \u0432\u044b \u0445\u043e\u0442\u0435\u043b\u0438 \u043f\u043e\u043f\u0440\u043e\u0431\u043e\u0432\u0430\u0442\u044c \u0441\u0435\u0431\u044f \u0432 \u044d\u0442\u043e\u043c \u043d\u0435\u043b\u0435\u0433\u043a\u043e\u043c \u0434\u0435\u043b\u0435 \u043d\u0430\u043f\u0438\u0441\u0430\u043d\u0438\u044f \u043a\u043e\u043c\u043f\u0438\u043b\u044f\u0442\u043e\u0440\u043e\u0432 \u0438 \u0442\u0440\u0430\u043d\u0441\u043b\u044f\u0442\u043e\u0440\u043e\u0432, \u043d\u043e \u043d\u0435 \u0437\u043d\u0430\u043b\u0438 \u0441 \u0447\u0435\u0433\u043e \u043d\u0430\u0447\u0430\u0442\u044c - \u044d\u0442\u0430 \u043a\u043d\u0438\u0433\u0430 \u0434\u043b\u044f \u0432\u0430\u0441. \u041b\u0435\u0433\u043a\u043e \u0447\u0438\u0442\u0430\u0442\u044c, \u043b\u0435\u0433\u043a\u043e \u043f\u043e\u043d\u044f\u0442\u044c, \u043b\u0435\u0433\u043a\u043e \u043f\u043e\u043f\u0440\u043e\u0431\u043e\u0432\u0430\u0442\u044c. \u0423\u0434\u0438\u0432\u0438\u0442\u0435\u043b\u044c\u043d\u043e, \u0447\u0442\u043e \u0442\u0430\u043a\u0438\u0445 \u043a\u043d\u0438\u0433 \u043d\u0435 \u0432\u044b\u043f\u0443\u0441\u043a\u0430\u043b\u0438 \u0434\u043e \u0441\u0438\u0445 \u043f\u043e\u0440. \u0421\u043a\u0440\u044b\u0442\u044c',3999)
INSERT INTO COMMENT VALUES(1752,'\u0427\u0435\u0440\u043a\u0430\u0441\u043e\u0432 \u041a\u043e\u043d\u0441\u0442\u0430\u043d\u0442\u0438\u043d','2014-01-29','\u042d\u0442\u0430 \u043a\u043d\u0438\u0433\u0430 \u2013 \u043f\u0440\u043e\u0441\u0442\u043e \u043a\u043e\u043d\u0441\u043f\u0435\u043a\u0442 \u043b\u0435\u043a\u0446\u0438\u0439. \u041e\u043d\u0430 \u0441\u043b\u0438\u0448\u043a\u043e\u043c \u043c\u0430\u043b\u0430, \u0447\u0442\u043e\u0431\u044b \u043d\u043e\u0440\u043c\u0430\u043b\u044c\u043d\u043e \u043e\u0445\u0432\u0430\u0442\u0438\u0442\u044c \u0442\u0430\u043a\u0443\u044e \u043e\u0431\u0448\u0438\u0440\u043d\u0443\u044e \u0442\u0435\u043c\u0443. \u041c\u043d\u043e\u0433\u0438\u0435 \u0442\u0435\u043c\u044b \u043e\u043f\u0438\u0441\u0430\u043d\u044b \u0432\u0441\u043a\u043e\u043b\u044c\u0437\u044c. \u041a\u043d\u0438\u0433\u0430 \u043d\u0435\u043e\u0431\u043e\u0441\u043d\u043e\u0432\u0430\u043d\u043d\u043e \u0434\u043e\u0440\u043e\u0433\u0430\u044f \u2013 \u0443\u0431\u043e\u0433\u0430\u044f \u0436\u0435\u043b\u0442\u0430\u044f \u0431\u0443\u043c\u0430\u0433\u0430, \u043d\u0435\u0443\u0434\u043e\u0431\u043d\u0430\u044f \u0441\u043a\u043b\u0435\u0439\u043a\u0430 \u0438 \u0434\u043e\u0441\u0442\u0430\u0442\u043e\u0447\u043d\u043e \u0431\u0435\u0441\u043f\u043e\u043b\u0435\u0437\u043d\u0430\u044f \u043f\u043e \u0441\u043e\u0434\u0435\u0440\u0436\u0430\u043d\u0438\u044e. \u0427\u0430\u0441\u0442\u044c \u043a\u043d\u0438\u0433\u0438 \u2013 \u044d\u0442\u043e \u0438\u0441\u0445\u043e\u0434\u043d\u0438\u043a\u0438 \u044d\u0442\u043e\u0433\u043e \u043d\u0438\u043a\u0447\u0435\u043c\u043d\u043e\u0433\u043e \u041e\u0431\u0435\u0440\u043e\u043d\u0430.',3999)
INSERT INTO COMMENT VALUES(1753,'arkasa','2012-01-28','\u041f\u0440\u043e\u0447\u0438\u0442\u0430\u0432 "\u0412\u0432\u0435\u0434\u0435\u043d\u0438\u0435 \u0432 \u0441\u043e\u0432\u0440\u0435\u043c\u0435\u043d\u043d\u044b\u0435 \u0421\u0410\u041f\u0420" \u0432\u044b \u043f\u043e\u043b\u0443\u0447\u0438\u0442\u0435 \u043e\u0431\u0449\u0438\u0435 \u043f\u0440\u0435\u0434\u0441\u0442\u0430\u0432\u043b\u0435\u043d\u0438\u044f \u043e \u0421\u0410\u041f\u0420. \u0412 \u043a\u043d\u0438\u0433\u0435 \u043e\u0441\u0432\u044f\u0449\u0435\u043d\u044b: \u043e\u0441\u043d\u043e\u0432\u044b \u043f\u0440\u043e\u0435\u043a\u0442\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u044f, \u0437\u0430\u0434\u0430\u0447\u0438 \u0438 \u0432\u0438\u0434\u044b \u0421\u0410\u041f\u0420, \u0432\u0438\u0434\u044b \u043c\u043e\u0434\u0435\u043b\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u044f,\u0432\u044b\u0431\u043e\u0440 \u0421\u0410\u041f\u0420 \u0438 \u043c\u043d\u043e\u0433\u043e\u0435 \u0434\u0440\u0443\u0433\u043e\u0435. \u0415\u0441\u043b\u0438 \u0432\u044b \u0442\u043e\u043b\u044c\u043a\u043e \u0437\u043d\u0430\u043a\u043e\u043c\u0438\u0442\u0435\u0441\u044c \u0441 \u0421\u0410\u041f\u0420 - \u044d\u0442\u0430 \u043a\u043d\u0438\u0433\u0430 \u0434\u043b\u044f \u0432\u0430\u0441. \u0427\u0442\u043e\u0431\u044b \u0431\u044b\u043b\u043e \u0431\u043e\u043b\u0435\u0435 \u043f\u043e\u043d\u044f\u0442\u043d\u043e \u043e \u0447\u0435\u043c \u043a\u043d\u0438\u0433\u0430, \u0432\u044b\u043a\u043b\u0430\u0434\u044b\u0432\u0430\u044e \u0444\u043e\u0442\u043e \u0441\u043e\u0434\u0435\u0440\u0436\u0430\u043d\u0438\u044f. \u041f\u043e\u043a\u0430\u0437\u0430\u0442\u044c \u0438\u0437\u043e\u0431\u0440\u0430\u0436\u0435\u043d\u0438\u044f \u0421\u043a\u0440\u044b\u0442\u044c \u0438\u0437\u043e\u0431\u0440\u0430\u0436\u0435\u043d\u0438\u044f',4000)
INSERT INTO COMMENT VALUES(1754,'\u042f\u043b\u0438\u043d\u0430','2011-01-01','\u0421\u0442\u0440\u0430\u043d\u0438\u0446\u044b \u0438\u0437 \u043a\u043d\u0438\u0433\u0438: \u041f\u043e\u043a\u0430\u0437\u0430\u0442\u044c \u0438\u0437\u043e\u0431\u0440\u0430\u0436\u0435\u043d\u0438\u044f \u0421\u043a\u0440\u044b\u0442\u044c \u0438\u0437\u043e\u0431\u0440\u0430\u0436\u0435\u043d\u0438\u044f',4002)
INSERT INTO COMMENT VALUES(1755,'\u0417\u043e\u043b\u043e\u0442\u0430\u044f \u0440\u044b\u0431\u043a\u0430','2011-01-17','\u041a\u043d\u0438\u0433\u0430 \u0441\u0442\u0430\u043d\u0434\u0430\u0440\u0442\u043d\u043e\u0433\u043e \u0440\u0430\u0437\u043c\u0435\u0440\u0430,\u0432 \u043c\u044f\u0433\u043a\u043e\u0439 \u043e\u0431\u043b\u043e\u0436\u043a\u0435,\u043d\u0430 \u0445\u043e\u0440\u043e\u0448\u0435\u0439 \u0431\u0443\u043c\u0430\u0433\u0435.\u0414\u043b\u044f \u043e\u0437\u043d\u0430\u043a\u043e\u043c\u043b\u0435\u043d\u0438\u044f \u043f\u0440\u0438\u043a\u043b\u0430\u0434\u044b\u0432\u0430\u044e \u043f\u043e\u043b\u043d\u043e\u0435 \u043e\u0433\u043b\u0430\u0432\u043b\u0435\u043d\u0438\u0435 \u0438 \u0441\u0442\u0440\u0430\u043d\u0438\u0446\u044b \u0438\u0437 \u043a\u043d\u0438\u0433\u0438. \u041f\u043e\u043a\u0430\u0437\u0430\u0442\u044c \u0438\u0437\u043e\u0431\u0440\u0430\u0436\u0435\u043d\u0438\u044f \u0421\u043a\u0440\u044b\u0442\u044c \u0438\u0437\u043e\u0431\u0440\u0430\u0436\u0435\u043d\u0438\u044f',4003)
INSERT INTO COMMENT VALUES(1756,'* \u041e\u043b\u044c\u0433\u0430 *','2010-01-28','"\u0420\u0443\u043a\u043e\u0432\u043e\u0434\u044f\u0449\u0438\u0435 \u043f\u0440\u0438\u043d\u0446\u0438\u043f\u044b \u0434\u043b\u044f \u043e\u043d\u043b\u0430\u0439\u043d-\u0434\u043e\u0441\u0442\u0438\u0436\u0435\u043d\u0438\u0439" \u0412\u044b \u043a\u043e\u0433\u0434\u0430-\u043b\u0438\u0431\u043e \u0437\u0430\u0434\u0430\u0432\u0430\u043b\u0438\u0441\u044c \u0432\u043e\u043f\u0440\u043e\u0441\u043e\u043c, \u043f\u043e\u0447\u0435\u043c\u0443 \u0432\u0430\u0448\u0438 \u0432\u0435\u0431-\u0441\u0430\u0439\u0442\u044b \u043d\u0435 \u0441\u043e\u0432\u0441\u0435\u043c \u0443\u0441\u043f\u0435\u0448\u043d\u044b \u043f\u043e \u0441\u0440\u0430\u0432\u043d\u0435\u043d\u0438\u044e \u0441 \u0432\u0430\u0448\u0438\u043c\u0438 \u043a\u043e\u043d\u043a\u0443\u0440\u0435\u043d\u0442\u0430\u043c\u0438 \u0438\u043b\u0438 \u0440\u0430\u0432\u043d\u043e\u043f\u0440\u0430\u0432\u043d\u044b\u043c\u0438 \u0443\u0447\u0430\u0441\u0442\u043d\u0438\u043a\u0430\u043c\u0438 (\u043f\u043e\u043b\u044c\u0437\u043e\u0432\u0430\u0442\u0435\u043b\u044f\u043c\u0438) \u0441\u0435\u0442\u0438? \u0417\u0430\u0434\u0430\u0432\u0430\u043b\u0438\u0441\u044c \u043b\u0438 \u0432\u044b, \u0433\u043b\u044f\u0434\u044f \u043d\u0430 \u0434\u0440\u0443\u0433\u0438\u0435 \u0441\u0430\u0439\u0442\u044b, \u0432\u043e\u043f\u0440\u043e\u0441\u043e\u043c: "\u041f\u043e\u0447\u0435\u043c\u0443 \u044f \u043d\u0435 \u0434\u0443\u043c\u0430\u043b \u043e\u0431 \u044d\u0442\u043e\u043c \u0440\u0430\u043d\u044c\u0448\u0435?" \u0412\u044b \u043a\u043e\u0433\u0434\u0430-\u043d\u0438\u0431\u0443\u0434\u044c \u0442\u0440\u0430\u0442\u0438\u043b\u0438 \u0441\u043b\u0438\u0448\u043a\u043e\u043c \u043c\u043d\u043e\u0433\u043e \u0432\u0440\u0435\u043c\u0435\u043d\u0438, \u043f\u044b\u0442\u0430\u044f\u0441\u044c \u043d\u0430\u0439\u0442\u0438 \u0438\u043d\u0442\u0435\u0440\u0435\u0441\u0443\u044e\u0449\u0443\u044e \u0438\u043d\u0444\u043e\u0440\u043c\u0430\u0446\u0438\u044e, \u043a\u043e\u0442\u043e\u0440\u0430\u044f \u0431\u044b\u043b\u0430 \u043f\u043e\u0445\u043e\u0440\u043e\u043d\u0435\u043d\u0430 \u0432 \u043d\u0435\u0434\u0440\u0430\u0445 \u0431\u0435\u0437 \u043d\u0443\u0436\u0434\u044b \u0437\u0430\u043f\u0443\u0442\u0430\u043d\u043d\u043e\u0433\u043e \u0432\u0435\u0431-\u0441\u0430\u0439\u0442\u0430? \u0412\u044b \u043d\u0435 \u043e\u0434\u0438\u043d\u043e\u043a\u0438, \u0438 \u044d\u0442\u0430 \u043a\u043d\u0438\u0433\u0430 \u0441\u0442\u0440\u0435\u043c\u0438\u0442\u0441\u044f \u0438\u0437\u043c\u0435\u043d\u0438\u0442\u044c \u0442\u0430\u043a\u043e\u0435 \u043f\u043e\u043b\u043e\u0436\u0435\u043d\u0438\u0435, \u043e\u0431\u044a\u0435\u0434\u0438\u043d\u0438\u0432 \u043d\u0435\u043a\u043e\u0442\u043e\u0440\u044b\u0445 \u0438\u0437 \u043d\u0430\u0438\u0431\u043e\u043b\u0435\u0435 \u0443\u0432\u0430\u0436\u0430\u0435\u043c\u044b\u0445 \u043f\u0440\u043e\u0435\u043a\u0442\u0438\u0440\u043e\u0432\u0449\u0438\u043a\u043e\u0432 \u0438 \u0440\u0430\u0437\u0440\u0430\u0431\u043e\u0442\u0447\u0438\u043a\u043e\u0432 \u0432\u0441\u0435\u0445 \u043a\u043e\u043d\u0442\u0438\u043d\u0435\u043d\u0442\u043e\u0432, \u043a\u0430\u0436\u0434\u044b\u0439 \u0438\u0437 \u043a\u043e\u0442\u043e\u0440\u044b\u0445 \u0434\u0435\u043b\u0438\u0442\u0441\u044f \u0441\u0432\u043e\u0438\u043c\u0438 \u0437\u043d\u0430\u043d\u0438\u044f\u043c\u0438 \u0438 \u043e\u043f\u044b\u0442\u043e\u043c. \u0421 \u043f\u043e\u043c\u043e\u0449\u044c\u044e \u0441\u0438\u0441\u0442\u0435\u043c\u0430\u0442\u0438\u0437\u0438\u0440\u043e\u0432\u0430\u043d\u043d\u044b\u0445 \u043f\u0440\u0435\u0434\u043c\u0435\u0442\u043e\u043c \u0433\u043b\u0430\u0432 (\u0438\u043d\u0442\u0435\u0440\u0444\u0435\u0439\u0441 \u0438 \u0434\u0438\u0437\u0430\u0439\u043d; \u043c\u0430\u0440\u043a\u0435\u0442\u0438\u043d\u0433 \u0438 \u043a\u043e\u043c\u043c\u0443\u043d\u0438\u043a\u0430\u0446\u0438\u044f; \u0442\u0435\u0445\u043d\u043e\u043b\u043e\u0433\u0438\u044f \u0438 \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u0435; \u0442\u0435\u0445\u043d\u0438\u0447\u0435\u0441\u043a\u0438\u0439 \u0441\u043e\u0432\u0435\u0442; \u0441\u043e\u0434\u0435\u0440\u0436\u0438\u043c\u043e\u0435/\u0443\u043f\u0440\u0430\u0432\u043b\u0435\u043d\u0438\u0435 \u0441\u043e\u0434\u0435\u0440\u0436\u0438\u043c\u044b\u043c; \u043a\u043e\u043c\u043c\u0435\u0440\u0446\u0438\u044f), \u0441\u043e \u0441\u0442\u0440\u0443\u043a\u0442\u0443\u0440\u043e\u0439 "\u0434\u0435\u043b\u0430\u0442\u044c/\u043d\u0435 \u0434\u0435\u043b\u0430\u0442\u044c" ("do/don''t") \u0438 \u043c\u043d\u043e\u0433\u043e\u0447\u0438\u0441\u043b\u0435\u043d\u043d\u044b\u043c\u0438 \u0441\u0443\u0449\u0435\u0441\u0442\u0432\u0443\u044e\u0449\u0438\u043c\u0438 \u043f\u0440\u0438\u043c\u0435\u0440\u0430\u043c\u0438 \u0443\u0441\u043f\u0435\u0448\u043d\u044b\u0445 \u0438 \u043d\u0430\u0433\u0440\u0430\u0436\u0434\u0451\u043d\u043d\u044b\u0445 \u0441\u043f\u0435\u0446\u0438\u0430\u043b\u044c\u043d\u044b\u043c\u0438 \u043f\u0440\u0438\u0437\u0430\u043c\u0438 \u0432\u0435\u0431-\u0441\u0430\u0439\u0442\u043e\u0432, - \u044d\u0442\u0430 \u043a\u043d\u0438\u0433\u0430 \u0438\u043c\u0435\u0435\u0442 \u0432\u0441\u0435 \u0441\u043e\u0432\u0435\u0442\u044b \u0438 \u043f\u0440\u0438\u043c\u0435\u0440\u044b, \u043a\u043e\u0442\u043e\u0440\u044b\u0435 \u0432\u044b \u0441\u043c\u043e\u0436\u0435\u0442\u0435 \u043f\u0440\u0438\u043c\u0435\u043d\u0438\u0442\u044c \u043d\u0430 \u0432\u0430\u0448\u0435\u043c \u043b\u0438\u0447\u043d\u043e\u043c \u0438\u043b\u0438 \u0434\u0435\u043b\u043e\u0432\u043e\u043c \u0432\u0435\u0431-\u0441\u0430\u0439\u0442\u0435 \u0434\u043b\u044f \u0443\u0441\u043f\u0435\u0448\u043d\u043e\u0439 \u043a\u043e\u043d\u043a\u0443\u0440\u0435\u043d\u0446\u0438\u0438, \u043f\u0440\u0438\u0437\u043d\u0430\u043d\u0438\u044f \u0438 \u0443\u0432\u0430\u0436\u0435\u043d\u0438\u044f \u0440\u0430\u0432\u043d\u043e\u043f\u0440\u0430\u0432\u043d\u044b\u0445 \u0443\u0447\u0430\u0441\u0442\u043d\u0438\u043a\u043e\u0432 (\u043f\u043e\u043b\u044c\u0437\u043e\u0432\u0430\u0442\u0435\u043b\u0435\u0439) \u0441\u0435\u0442\u0438. \u0412\u0430\u0448\u0438 \u043f\u043e\u0441\u0435\u0442\u0438\u0442\u0435\u043b\u0438 \u043f\u043e\u0431\u043b\u0430\u0433\u043e\u0434\u0430\u0440\u044f\u0442 \u0432\u0430\u0441. \u0420\u0435\u0434\u0430\u043a\u0442\u043e\u0440\u044b: \u0420\u043e\u0431 \u0424\u043e\u0440\u0434 (Rob Ford) \u0438 \u042e\u043b\u0438\u0443\u0441 \u0412\u0438\u0434\u0435\u043c\u0430\u043d\u043d (Julius Wiedemann). \u0421\u043a\u0440\u044b\u0442\u044c',4004)
INSERT INTO COMMENT VALUES(1757,'\u041f\u0440\u043e\u0441\u0442\u043e\u0439 \u0447\u0438\u0442\u0430\u0442\u0435\u043b\u044c','2010-01-12','\u0414\u0430\u043d\u043d\u044b\u0439 \u0443\u0447\u0435\u0431\u043d\u0438\u043a \u0438\u043c\u0435\u0435\u0442 \u0440\u0435\u043a\u043e\u043c\u0435\u043d\u0434\u0430\u0446\u0438\u044e \u0423\u041c\u041e \u0432\u0443\u0437\u043e\u0432 \u043f\u043e \u0443\u043d\u0438\u0432\u0435\u0440\u0441\u0438\u0442\u0435\u0442\u0441\u043a\u043e\u043c\u0443 \u043f\u043e\u043b\u0438\u0442\u0435\u0445\u043d\u0438\u0447\u0435\u0441\u043a\u043e\u043c\u0443 \u043e\u0431\u0440\u0430\u0437\u043e\u0432\u0430\u043d\u0438\u044e, \u0438 \u044d\u0442\u043e \u044f\u0432\u043b\u044f\u0435\u0442\u0441\u044f \u0445\u043e\u0440\u043e\u0448\u0435\u0439 \u043e\u0446\u0435\u043d\u043a\u043e\u0439 \u0434\u043b\u044f \u043b\u044e\u0431\u043e\u0433\u043e \u0443\u0447\u0435\u0431\u043d\u0438\u043a\u0430. \u0423\u0447\u0435\u0431\u043d\u0438\u043a \u043f\u043e\u0434\u0440\u043e\u0431\u043d\u044b\u0439 \u0441\u043e \u043c\u043d\u043e\u0436\u0435\u0441\u0442\u0432\u043e\u043c \u043f\u0440\u0438\u043c\u0435\u0440\u043e\u0432, \u0442\u0430\u0431\u043b\u0438\u0446 \u0438 \u0440\u0438\u0441\u0443\u043d\u043a\u043e\u0432, \u0441 \u043f\u0440\u0438\u043b\u043e\u0436\u0435\u043d\u0438\u044f\u043c\u0438 \u043b\u0430\u0431\u043e\u0440\u0430\u0442\u043e\u0440\u043d\u044b\u0445.\u0412 \u043a\u043e\u043d\u0446\u0435 \u043a\u0430\u0436\u0434\u043e\u0439 \u0433\u043b\u0430\u0432\u044b \u0435\u0441\u0442\u044c \u0441\u043f\u0438\u0441\u043e\u043a \u043a\u043e\u043d\u0442\u0440\u043e\u043b\u044c\u043d\u044b\u0445 \u0432\u043e\u043f\u0440\u043e\u0441\u043e\u0432, \u0430 \u0442\u0430\u043a\u0436\u0435 \u0441\u043f\u0438\u0441\u043e\u043a \u043b\u0438\u0442\u0435\u0440\u0430\u0442\u0443\u0440\u044b \u043f\u043e \u0442\u0435\u043c\u0435. \u0412 \u0443\u0447\u0435\u0431\u043d\u0438\u043a\u0435 \u0434\u0430\u044e\u0442\u0441\u044f \u043e\u043f\u0440\u0435\u0434\u0435\u043b\u0435\u043d\u0438\u044f \u0441\u0430\u043c\u044b\u043c \u043d\u0435\u043e\u0431\u0445\u043e\u0434\u0438\u043c\u044b\u043c \u043f\u043e\u043d\u044f\u0442\u0438\u044f\u043c (\u043d\u0430\u043f\u0440\u0438\u043c\u0435\u0440, \u043f\u043e\u043c\u0435\u0445\u043e\u0443\u0441\u0442\u043e\u0439\u0447\u0438\u0432\u043e\u0435 \u043a\u043e\u0434\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u0435, \u043b\u0438\u043d\u0435\u0439\u043d\u044b\u0435 \u043a\u043e\u0434\u044b,\u0431\u043b\u043e\u0447\u043d\u044b\u0435 \u043a\u043e\u0434\u044b, \u0434\u0432\u043e\u0438\u0447\u043d\u044b\u0439 \u043f\u043e\u043b\u0438\u043d\u043e\u043c, \u043a\u043e\u0434\u043e\u0432\u043e\u0435 \u0440\u0430\u0441\u0441\u0442\u043e\u044f\u043d\u0438\u0435 (\u0440\u0430\u0441\u0441\u0442\u043e\u044f\u043d\u0438\u0435 \u0425\u044d\u043c\u043c\u0438\u043d\u0433\u0430). \u0416\u0430\u043b\u044c, \u0447\u0442\u043e \u043d\u0435\u0442 \u043e\u0442\u0434\u0435\u043b\u044c\u043d\u043e\u0433\u043e \u043f\u0440\u0438\u043b\u043e\u0436\u0435\u043d\u0438\u044f \u0441 \u0441\u043b\u043e\u0432\u0430\u0440\u0435\u043c \u0442\u0435\u0440\u043c\u0438\u043d\u043e\u0432, \u0438\u0441\u043f\u043e\u043b\u044c\u0437\u0443\u0435\u043c\u044b\u0445 \u0432 \u0443\u0447\u0435\u0431\u043d\u0438\u043a\u0435. \u0421\u043a\u0440\u044b\u0442\u044c',4005)
