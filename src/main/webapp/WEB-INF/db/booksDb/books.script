INSERT INTO COMMENT VALUES(1517,'\u043a\u0435\u0442\u0440\u0430\u0440\u044c \u0410\u043d\u0430\u0441\u0442\u0430\u0441\u0438\u044f','2015-01-16','\u041a\u043e\u043d\u0435\u0447\u043d\u043e \u043e\u043d\u0430 \u043d\u0435 \u043f\u043e\u0434\u0445\u043e\u0434\u0438\u0442 \u043a\u0430\u043a \u0441\u043f\u0440\u0430\u0432\u043e\u0447\u043d\u0438\u043a!!! \u041f\u043e\u0442\u043e\u043c\u0443 \u0447\u0442\u043e \u044d\u0442\u043e \u043e\u0431\u0443\u0447\u0430\u044e\u0449\u0430\u044f \u0441 \u043d\u0443\u043b\u044f \u043a\u043d\u0438\u0433\u0430 \u0438 \u0438\u0437\u0434\u0430\u0442\u0435\u043b\u0438 \u0441 \u043f\u0435\u0440\u0432\u043e\u0439 \u0441\u0442\u0440\u0430\u043d\u0438\u0446\u044b \u043f\u0440\u0435\u0434\u0443\u043f\u0440\u0435\u0436\u0434\u0430\u044e\u0442 \u043e\u0431 \u044d\u0442\u043e\u043c. \u0427\u0438\u0442\u0430\u0439\u0442\u0435 \u0432\u043d\u0438\u043c\u0430\u0442\u0435\u043b\u044c\u043d\u043e \u043e\u043f\u0438\u0441\u0430\u043d\u0438\u0435 \u0438 \u043d\u0435 \u0431\u0443\u0434\u0435\u0442\u0435 \u0440\u0430\u0441\u0441\u0442\u0440\u043e\u0435\u043d\u044b. \u0414\u043b\u044f \u0442\u0435\u0445 \u043a\u0442\u043e \u0436\u0435\u043b\u0430\u0435\u0442 \u0438\u0437\u0443\u0447\u0438\u0442\u044c htm , \u043d\u0435 \u0438\u043c\u0435\u044f \u043e \u043d\u0435\u043c \u043d\u0438\u043a\u0430\u043a\u043e\u0433\u043e \u043f\u043e\u043d\u044f\u0442\u0438\u044f, \u044d\u0442\u0430 \u043a\u043d\u0438\u0433\u0430 \u0441\u0430\u043c\u043e\u0435 \u0442\u043e!!!! \u041b\u0435\u0433\u043a\u043e,\u043f\u043e\u043d\u044f\u0442\u043d\u043e \u0438 \u0441 \u043f\u0440\u0430\u043a\u0442\u0438\u0447\u0435\u0441\u043a\u0438\u043c\u0438 \u0437\u0430\u0434\u0430\u043d\u0438\u044f\u043c\u0438!! \u041e\u0447\u0435\u043d\u044c \u0440\u0435\u043a\u043e\u043c\u0435\u043d\u0434\u0443\u044e.',3439)
INSERT INTO COMMENT VALUES(1518,'Waalli  Yeed E','2014-01-05','\u041a\u043d\u0438\u0433\u0430 \u043e\u0442\u043b\u0438\u0447\u043d\u0430\u044f! \u041f\u043e\u0434\u0430\u0447\u0430 \u043c\u0430\u0442\u0435\u0440\u0438\u0430\u043b\u0430 \u0438 \u0435\u0433\u043e \u0434\u043e\u0441\u0442\u0443\u043f\u043d\u043e\u0441\u0442\u044c \u043e\u0447\u0435\u043d\u044c \u043f\u043e\u043c\u043e\u0433\u0430\u044e\u0442 \u0432 \u0438\u0437\u0443\u0447\u0435\u043d\u0438\u0438, \u0438, \u0433\u043b\u0430\u0432\u043d\u043e\u0435, \u0432 \u041f\u041e\u041d\u0418\u041c\u0410\u041d\u0418\u0418 \u0442\u0435\u043c\u044b! \u041c\u043d\u0435 \u043d\u0440\u0430\u0432\u0438\u0442\u0441\u044f \u0441\u0432\u043e\u0431\u043e\u0434\u043d\u043e\u0435 \u043e\u0444\u043e\u0440\u043c\u043b\u0435\u043d\u0438\u0435 \u043a\u043d\u0438\u0433\u0438("\u043a\u043e\u043c\u0438\u043a\u0441\u043e\u0432\u043e\u0441\u0442\u044c", \u043c\u043d\u043e\u0433\u043e \u0440\u0438\u0441\u0443\u043d\u043a\u043e\u0432) \u0438 \u043c\u0435\u0441\u0442\u0430\u043c\u0438 \u0448\u0443\u0442\u043e\u0447\u043d\u0430\u044f \u043f\u043e\u0434\u0430\u0447\u0430 - \u0442\u0430\u043a \u0434\u0435\u0439\u0441\u0442\u0432\u0438\u0442\u0435\u043b\u044c\u043d\u043e \u043b\u0443\u0447\u0448\u0435 \u0437\u0430\u043f\u043e\u043c\u0438\u043d\u0430\u0435\u0442\u0441\u044f \u0438 \u0447\u0438\u0442\u0430\u0442\u044c \u0438\u043d\u0442\u0435\u0440\u0435\u0441\u043d\u0435\u0435, \u0447\u0435\u043c \u0442\u043e\u043d\u043d\u044b \u043e\u0434\u043d\u043e\u043e\u0431\u0440\u0430\u0437\u043d\u043e\u0433\u043e \u0442\u0435\u043a\u0441\u0442\u0430. \u041f\u043e\u043a\u0430 \u043f\u0440\u043e\u0447\u0438\u0442\u0430\u043b\u0430 \u043f\u043e\u043b\u043e\u0432\u0438\u043d\u0443, \u043d\u043e \u0443\u0436\u0435 \u043d\u0430\u0446\u0435\u043b\u0438\u043b\u0430\u0441\u044c \u043d\u0430 \u0434\u0440\u0443\u0433\u0438\u0435 \u043a\u043d\u0438\u0433\u0438 \u044d\u0442\u043e\u0439 \u0441\u0435\u0440\u0438\u0438, \u043e\u0431\u044f\u0437\u0430\u0442\u0435\u043b