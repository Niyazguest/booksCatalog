INSERT INTO BOOK VALUES(3865,300253,'\u041c\u0430\u0437\u0438\u0434\u0438, \u041c\u0430\u043a\u043a\u0438\u043d\u043b\u0438, \u041a\u0443\u0441\u044d\u0439: \u041c\u0438\u043a\u0440\u043e\u043a\u043e\u043d\u0442\u0440\u043e\u043b\u043b\u0435\u0440\u044b PIC \u0438 \u0432\u0441\u0442\u0440\u043e\u0435\u043d\u043d\u044b\u0435 \u0441\u0438\u0441\u0442\u0435\u043c\u044b. \u041f\u0440\u0438\u043c\u0435\u043d\u0435\u043d\u0438\u0435 \u0430\u0441\u0441\u0435\u043c\u0431\u043b\u0435\u0440\u0430 \u0438 \u0421 \u0434\u043b\u044f PIC18','\u041c\u0430\u0437\u0438\u0434\u0438 \u041c\u0443\u0445\u0430\u043c\u043c\u0435\u0434 \u0410\u043b\u0438, \u041c\u0430\u043a\u043a\u0438\u043d\u043b\u0438 \u0420\u043e\u043b\u0438\u043d \u0414., \u041a\u0443\u0441\u044d\u0439 \u0414\u044d\u043d\u043d\u0438','\u0428\u043f\u0430\u043a \u042e. \u0410.','\u041a\u043e\u0440\u043e\u043d\u0430-\u041f\u0440\u0438\u043d\u0442, 2009 \u0433.',1196.0E0,'978-5-7931-0538-5','784 (\u0413\u0430\u0437\u0435\u0442\u043d\u0430\u044f)','\u0422\u0438\u043f \u043e\u0431\u043b\u043e\u0436\u043a\u0438: \u043e\u0431\u043b - \u043c\u044f\u0433\u043a\u0438\u0439 \u043f\u0435\u0440\u0435\u043f\u043b\u0435\u0442 (\u043a\u0440\u0435\u043f\u043b\u0435\u043d\u0438\u0435 \u0441\u043a\u0440\u0435\u043f\u043a\u043e\u0439 \u0438\u043b\u0438 \u043a\u043b\u0435\u0435\u043c) \u0418\u043b\u043b\u044e\u0441\u0442\u0440\u0430\u0446\u0438\u0438: \u0427\u0435\u0440\u043d\u043e-\u0431\u0435\u043b\u044b\u0435','760 \u0433','240x172x26 \u043c\u043c','\u0410\u043d\u043d\u043e\u0442\u0430\u0446\u0438\u044f \u043a \u043a\u043d\u0438\u0433\u0435 "\u041c\u0438\u043a\u0440\u043e\u043a\u043e\u043d\u0442\u0440\u043e\u043b\u043b\u0435\u0440\u044b PIC \u0438 \u0432\u0441\u0442\u0440\u043e\u0435\u043d\u043d\u044b\u0435 \u0441\u0438\u0441\u0442\u0435\u043c\u044b. \u041f\u0440\u0438\u043c\u0435\u043d\u0435\u043d\u0438\u0435 \u0430\u0441\u0441\u0435\u043c\u0431\u043b\u0435\u0440\u0430 \u0438 \u0421 \u0434\u043b\u044f PIC18" \u041a\u043d\u0438\u0433\u043