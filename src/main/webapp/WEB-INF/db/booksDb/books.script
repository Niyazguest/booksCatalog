INSERT INTO COMMENT VALUES(1664,'\u041f\u0430\u0448\u041a\u0430','2011-01-28','\u0420\u0430\u0437\u043e\u0447\u0430\u0440\u043e\u0432\u0430\u043d \u043f\u043e\u043a\u0443\u043f\u043a\u043e\u0439. \u041c\u043d\u043e\u0433\u043e \u0431\u0435\u0441\u043f\u043e\u043b\u0435\u0437\u043d\u044b\u0445 \u043b\u0438\u0441\u0442\u0438\u043d\u0433\u043e\u0432 \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c, \u0430 \u0441 \u043e\u0441\u043c\u044b\u0441\u043b\u0435\u043d\u043d\u044b\u043c \u043f\u043e\u043b\u0435\u0437\u043d\u044b\u043c \u0442\u0435\u043a\u0441\u0442\u043e\u043c - \u0431\u0435\u0434\u0430. \u041d\u043e \u0434\u043e\u043b\u0436\u0435\u043d \u043f\u0440\u0438\u0437\u043d\u0430\u0442\u044c\u0441\u044f, \u0447\u0442\u043e \u0438 \u043d\u0435 \u0447\u0438\u0442\u0430\u043b \u0432\u043d\u0438\u043c\u0430\u0442\u0435\u043b\u044c\u043d\u043e: \u043f\u043e\u043d\u044f\u043b \u0447\u0442\u043e \u043c\u043d\u0435 \u043d\u0435 \u0438\u043d\u0442\u0435\u0440\u0435\u0441\u043d\u043e, \u0430 \u0431\u043e\u043b\u044c\u0448\u0443\u044e \u0447\u0430\u0441\u0442\u044c \u043d\u0430\u043f\u0438\u0441\u0430\u043d\u043d\u043e\u0433\u043e \u044f \u0434\u0430\u0432\u043d\u043e \u0437\u043d\u0430\u044e.',3909)
