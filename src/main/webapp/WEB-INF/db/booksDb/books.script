INSERT INTO COMMENT VALUES(1552,'\u0424\u0438\u043b\u044c\u043a\u0438\u043d \u0421\u0435\u0440\u0433\u0435\u0439','2013-01-19','\u0425\u043e\u0440\u043e\u0448\u0430\u044f \u043a\u043d\u0438\u0433\u0430, \u043a\u043e\u0442\u043e\u0440\u0430\u044f \u043f\u043e\u043c\u043e\u0436\u0435\u0442 \u0440\u0430\u0437\u043e\u0431\u0440\u0430\u0442\u044c\u0441\u044f \u0441 \u0442\u0430\u043a\u043e\u0439 \u0433\u0440\u043e\u043c\u0430\u0434\u0438\u043d\u043e\u0439, \u043a\u0430\u043a \u0414\u0440\u0443\u043f\u0430\u043b, \u0432\u0441\u0435\u043c \u0436\u0435\u043b\u0430\u044e\u0449\u0438\u043c. \u0415\u0441\u0442\u044c \u043e\u0434\u0438\u043d \u043d\u0435\u0434\u043e\u0441\u0442\u0430\u0442\u043e\u043a: \u043a\u043d\u0438\u0433\u0430 \u043b\u0438\u0448\u044c \u0431\u044b\u043b\u0430 \u043d\u0430\u043f\u0435\u0447\u0430\u0442\u0430\u043d\u0430 \u0432 2013 \u0433\u043e\u0434\u0443, \u0430 \u043d\u0430\u043f\u0438\u0441\u0430\u043d\u0430 \u043e\u043d\u0430 \u0431\u044b\u043b\u0430 \u0432 2011. \u041f\u043e\u044d\u0442\u043e\u043c\u0443 \u043e\u043f\u0438\u0441\u0430\u043d\u0438\u0435 \u043c\u043e\u0434\u0443\u043b\u0435\u0439 \u0441\u0438\u043b\u044c\u043d\u043e \u0443\u0441\u0442\u0430\u0440\u0435\u043b\u043e. \u041e\u0434\u043d\u0430\u043a\u043e \u043d\u0435 \u043f\u043e\u043c\u0435\u0448\u0430\u0435\u0442 \u0440\u0430\u0437\u043e\u0431\u0440\u0430\u0442\u044c\u0441\u044f \u0432 \u0441\u0442\u0440\u0443\u043a\u0442\u0443\u0440\u0435 \u0434\u0430\u043d\u043d\u043e\u0439 CMS \u0438 \u043f\u0440\u0438\u043d\u0446\u0438\u043f\u0435\u0445 \u0435\u0435 \u0440\u0430\u0431\u043e\u0442\u044b.',3495)
