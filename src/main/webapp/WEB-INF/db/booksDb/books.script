INSERT INTO BOOK VALUES(3504,365261,'\u0421\u0435\u0440\u0433\u0435\u0439 \u041c\u0435\u043b\u044c\u043d\u0438\u043a\u043e\u0432: Delphi \u0438 Turbo Pascal \u043d\u0430 \u0437\u0430\u043d\u0438\u043c\u0430\u0442\u0435\u043b\u044c\u043d\u044b\u0445 \u043f\u0440\u0438\u043c\u0435\u0440\u0430\u0445 (+CD)','\u041c\u0435\u043b\u044c\u043d\u0438\u043a\u043e\u0432 \u0421\u0435\u0440\u0433\u0435\u0439 \u0412.','\u041a\u0443\u0437\u044c\u043c\u0438\u043d\u0430 \u0410\u043d\u043d\u0430','BHV, 2012 \u0433.',221.0E0,'978-5-9415-7832-0\u00a0 ','448 (\u0413\u0430\u0437\u0435\u0442\u043d\u0430\u044f)','\u0422\u0438\u043f \u043e\u0431\u043b\u043e\u0436\u043a\u0438: \u043e\u0431\u043b - \u043c\u044f\u0433\u043a\u0438\u0439 \u043f\u0435\u0440\u0435\u043f\u043b\u0435\u0442 (\u043a\u0440\u0435\u043f\u043b\u0435\u043d\u0438\u0435 \u0441\u043a\u0440\u0435\u043f\u043a\u043e\u0439 \u0438\u043b\u0438 \u043a\u043b\u0435\u0435\u043c) \u0418\u043b\u043b\u044e\u0441\u0442\u0440\u0430\u0446\u0438\u0438: \u0427\u0435\u0440\u043d\u043e-\u0431\u0435\u043b\u044b\u0435','416 \u0433','232x165x19 \u043c\u043c','\u0415\u0441\u043b\u0438 \u0432\u044b \u0445\u043e\u0442\u0438\u0442\u0435 \u0432\u0437\u0433\u043b\u044f\u043d\u0443\u0442\u044c \u043d\u0430 Turbo Pascal \u0438 Delphi \u0432 \u043f\u043b\u0430\u043d\u0435 \u0438\u0437\u043e\u0431\u0440\u0435\u0442\u0430\u0442\u0435\u043b\u044c\u0441\u0442\u0432\u0430 \u0438 \u0440\u0435\u0448\u0435\u043d\u0438\u044f \u0438\u043d\u0442\u0435\u0440\u0435\u0441\u043d\u044b\u0445 \u0442\u0432\u043e\u0440\u0447\u0435\u0441\u043a\u0438\u0445 \u0437\u0430\u0434\u0430\u0447, \u0442\u043e \u044d\u0442\u043e \u043a\u043d\u0438\u0433\u0430, \u043a\u043e\u0442\u043e\u0440\u0443\u044e \u0432\u044b \u0436\u0434\u0430\u043b\u0438. \u0421\u043b\u043e\u0432\u0435\u0441\u043d\u044b\u0435 \u0438\u0433\u0440\u044b \u0438 \u0433\u043e\u043b\u043e\u0432\u043e\u043b\u043e\u043c\u043a\u0438, \u0440\u0435\u0448\u0435\u043d\u0438\u0435 \u043e\u043b\u0438\u043c\u043f\u0438\u0430\u0434