INSERT INTO COMMENT VALUES(1606,'\u0428\u043c\u0435\u043b\u0435\u0432\u0430 \u0422\u0430\u0442\u044c\u044f\u043d\u0430','2013-01-29','\u0412\u043e\u0442 \u0443\u0436 \u0442\u043e\u0447\u043d\u043e \u0434\u043b\u044f \u0442\u0435\u0445, \u043a\u0442\u043e \u0441\u043e\u0432\u0441\u0435\u043c \u0441 \u043d\u0443\u043b\u044f \u043d\u0430\u0447\u0438\u043d\u0430\u0435\u0442 \u0443\u0447\u0438\u0442\u044c\u0441\u044f. \u0412\u043e-\u043f\u0435\u0440\u0432\u044b\u0445, \u0438\u043d\u0441\u0442\u0440\u0443\u043a\u0446\u0438\u044f \u044d\u0442\u0430 \u0434\u043b\u044f \u0440\u0443\u0441\u0441\u043a\u043e\u044f\u0437\u044b\u0447\u043d\u043e\u0439 \u0432\u0435\u0440\u0441\u0438\u0438, \u0432\u043e \u0432\u0442\u043e\u0440\u044b\u0445, \u0442\u0430\u043a \u0440\u0430\u0437\u0436\u0435\u0432\u0430\u043d\u043e, \u0447\u0442\u043e \u0447\u0438\u0442\u0430\u0442\u044c \u0441\u043a\u0443\u0447\u043d\u043e. \u0415\u0441\u043b\u0438 \u0435\u0441\u0442\u044c \u0443\u0436\u0435 \u043f\u043e\u043d\u044f\u0442\u0438\u0435 \u043e\u0431 \u044d\u0442\u043e\u0439 \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u0435, \u0442\u043e \u0441 \u0446\u0435\u043b\u044c\u044e \u0441\u0438\u0441\u0442\u0435\u043c\u0430\u0442\u0438\u0437\u0438\u0440\u043e\u0432\u0430\u0442\u044c \u0441\u0432\u043e\u0438 \u0437\u043d\u0430\u043d\u0438\u044f \u0431\u0440\u0430\u0442\u044c \u043a\u043d\u0438\u0433\u0443 \u043d\u0435 \u0442\u043e\u0438\u0442. \u0415\u0441\u043b\u0438 \u0441\u043e\u0432\u0441\u0435\u043c \u0441 \u043d\u0443\u043b\u044f \u0445\u043e\u0447\u0435\u0442\u0441\u044f \u043d\u0430\u0447\u0430\u0442\u044c - \u0432\u0430\u0440\u0438\u0430\u043d\u0442 \u0445\u043e\u0440\u043e\u0448\u0438\u0439.',3849)
INSERT INTO COMMENT VALUES(1607,'\u0410\u043b\u0435\u043a\u0441\u0430\u043d\u0434\u0440 \u0422\u0443\u0437\u043e\u0432','2014-01-04','\u0427\u0438\u0442\u0430\u044e \u0414\u043e\u0443\u0441\u043e\u043d\u0430 \u043d\u0435 \u043e\u0442\u0440\u044b\u0432\u0430\u044f\u0441\u044c. \u041e\u0447\u0435\u043d\u044c \u043b\u0435\u0433\u043a\u0430\u044f \u0434\u043b\u044f \u0438\u0437\u0443\u0447\u0435\u043d\u0438\u044f \u0438 \u043f\u043e\u043d\u0438\u043c\u0430\u043d\u0438\u044f \u043a\u043d\u0438\u0433\u0430. \u0412\u0441\u0435\u043c \u043d\u0430\u0447\u0438\u043d\u0430\u044e\u0449\u0438\u043c \u0438\u0437\u0443\u0447\u0435\u043d\u0438\u0435 \u044f\u0437\u044b\u043a\u0430 \u043e\u0447\u0435\u043d\u044c \u0440\u0435\u043a\u043e\u043c\u0435\u043d\u0434\u0443\u044e. \u0414\u043e \u044d\u0442\u043e\u0433\u043e \u0447\u0438\u0442\u0430\u043b \u041f\u0440\u043e\u0445\u043e\u0440\u0435\u043d\u043a\u0430 - \u0443\u0436\u0430\u0441. \u041b\u0443\u0442\u0446 - \u0441\u043b\u0438\u0448\u043a\u043e\u043c \u0433\u0440\u043e\u043c\u043e\u0437\u0434\u043e\u043a. \u0414\u043b\u044f \u043f\u0435\u0440\u0432\u043e\u0439 \u043a\u043d\u0438\u0433\u0438 \u043f\u043e Python - \u043d\u0435\u0437\u0430\u043c\u0435\u043d\u0438\u043c\u0430. \u041e\u0441\u043e\u0431\u0435\u043d\u043d\u043e \u0435\u0441\u043b\u0438 \u043f\u043e\u0442\u043e\u043c \u043f\u0440\u0435\u043f\u043e\u0434\u0430\u0432\u0430\u0442\u044c \u0434\u0435\u0442\u044f\u043c.',3851)
INSERT INTO COMMENT VALUES(1608,'sergeNet','2015-01-06','\u0415\u0441\u043b\u0438 \u0440\u0435\u0448\u0438\u043b\u0438 \u043f\u043e\u0441\u0432\u044f\u0442\u0438\u0442\u044c \u0441\u0432\u043e\u044e \u0436\u0438\u0437\u043d\u044c \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u044e \u0438 \u0432\u044b\u0431\u0440\u0430\u043b\u0438 \u0432 \u043a\u0430\u0447\u0435\u0441\u0442\u0432\u0435 \u0441\u0432\u043e\u0435\u0433\u043e \u043f\u0435\u0440\u0432\u043e\u0433\u043e \u044f\u0437\u044b\u043a\u0430 Python, \u0442\u043e \u044d\u0442\u0430 \u043a\u043d\u0438\u0433\u0430 \u0434\u043b\u044f \u0432\u0430\u0441. \u041a\u0440\u043e\u043c\u0435 \u0441\u0430\u043c\u043e\u0433\u043e \u044f\u0437\u044b\u043a\u0430 \u0440\u0430\u0441\u0441\u043c\u0430\u0442\u0440\u0438\u0432\u0430\u044e\u0442\u0441\u044f \u043e\u0441\u043d\u043e\u0432\u044b \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u044f, \u043a\u0430\u043a \u0442\u0430\u043a\u043e\u0432\u043e\u0433\u043e. \u0422\u0435\u043c \u0436\u0435, \u043a\u0442\u043e \u0443\u0436\u0435 \u0438\u043c\u0435\u0435\u0442 \u043e\u043f\u044b\u0442 \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u044f \u043d\u0430 \u0434\u0440\u0443\u0433\u0438\u0445 \u044f\u0437\u044b\u043a\u0430\u0445, \u043a\u043d\u0438\u0433\u0430 \u043f\u043e\u043a\u0430\u0436\u0435\u0442\u0441\u044f \u0441\u043b\u0438\u0448\u043a\u043e\u043c \u043f\u043e\u0434\u0440\u043e\u0431\u043d\u043e\u0439, \u0441 \u0447\u0440\u0435\u0437\u043c\u0435\u0440\u043d\u044b\u043c \u0440\u0430\u0437\u0436\u0435\u0432\u044b\u0432\u0430\u043d\u0438\u0435\u043c \u043f\u0440\u0438\u043c\u0438\u0442\u0438\u0432\u043d\u044b\u0445 \u0432\u0435\u0449\u0435\u0439. \u0418\u0437\u0434\u0430\u043d\u0430 \u043a\u043d\u0438\u0433\u0430 \u0432 \u043c\u044f\u0433\u043a\u043e\u043c \u043f\u0435\u0440\u0435\u043f\u043b\u0451\u0442\u0435. \u041e\u0431\u043b\u043e\u0436\u043a\u0430 \u043c\u0430\u0442\u043e\u0432\u0430\u044f. \u041a \u043e\u0433\u0440\u0435\u0445\u0430\u043c \u043e\u0442\u043d\u043e\u0441\u0438\u0442\u0441\u044f, \u0442\u043e\u043b\u0449\u0438\u043d\u0430 \u0441\u0442\u0440\u0430\u043d\u0438\u0446 - \u043f\u0440\u043e\u0441\u0432\u0435\u0447\u0438\u0432\u0430\u0435\u0442 \u0441\u043e\u0434\u0435\u0440\u0436\u0438\u043c\u043e\u0435 \u043d\u0430 \u043e\u0431\u0440\u0430\u0442\u043d\u043e\u043c \u043e\u0431\u043e\u0440\u043e\u0442\u0435 \u043b\u0438\u0441\u0442\u0430.',3851)
