INSERT INTO BOOK VALUES(3432,411119,'\u041c\u0438\u0445\u0430\u0438\u043b \u0410\u0431\u0440\u0430\u043c\u044f\u043d: \u0422\u0435\u0445\u043d\u043e\u043b\u043e\u0433\u0438\u044f LINQ \u043d\u0430 \u043f\u0440\u0438\u043c\u0435\u0440\u0430\u0445. \u041f\u0440\u0430\u043a\u0442\u0438\u043a\u0443\u043c \u0441 \u0438\u0441\u043f-\u0435\u043c \u044d\u043b\u0435\u043a\u0442\u0440\u043e\u043d\u043d\u043e\u0433\u043e \u0437\u0430\u0434\u0430\u0447\u043d\u0438\u043a\u0430 Programming Taskbook for LINQ','\u0410\u0431\u0440\u0430\u043c\u044f\u043d \u041c\u0438\u0445\u0430\u0438\u043b \u042d\u0434\u0443\u0430\u0440\u0434\u043e\u0432\u0438\u0447','\u041c\u043e\u0432\u0447\u0430\u043d \u0414. \u0410.','\u0414\u041c\u041a-\u041f\u0440\u0435\u0441\u0441, 2014 \u0433.',583.0E0,'978-5-94074-981-3','326 (\u041e\u0444\u0441\u0435\u0442)','\u0422\u0438\u043f \u043e\u0431\u043b\u043e\u0436\u043a\u0438: \u043e\u0431\u043b - \u043c\u044f\u0433\u043a\u0438\u0439 \u043f\u0435\u0440\u0435\u043f\u043b\u0435\u0442 (\u043a\u0440\u0435\u043f\u043b\u0435\u043d\u0438\u0435 \u0441\u043a\u0440\u0435\u043f\u043a\u043e\u0439 \u0438\u043b\u0438 \u043a\u043b\u0435\u0435\u043c) \u0418\u043b\u043b\u044e\u0441\u0442\u0440\u0430\u0446\u0438\u0438: \u0411\u0435\u0437 \u0438\u043b\u043b\u044e\u0441\u0442\u0440\u0430\u0446\u0438\u0439','364 \u0433','203x141x18 \u043c\u043c','\u0410\u043d\u043d\u043e\u0442\u0430\u0446\u0438\u044f \u043a \u043a\u043d\u0438\u0433\u0435 "\u0422\u0435\u0445\u043d\u043e\u043b\u043e\u0433\u0438\u044f LINQ \u043d\u0430 \u043f\u0440\u0438\u043c\u0435\u0440\u0430\u0445. \u041f\u0440\u0430\u043a\u0442\u0438\u043a\u0443\u043c \u0441 \u0438\u0441\u043f-\u0435\u043c \u044d\u043b\u0435\u043a\u0442\u0440\u043e\u043d\u043d\u043e\u0433\u043e \u0437\u0430\u0434\u0430\u0447\u043d\u0438\u043a\u0430 Programming Taskbook for LINQ" \u0421 \u043f\u043e\u043c\u043e\u0449\u044c\u044e \u044d\u0442\u043e\u0439 \u043a\u043d\u0438\u0433\u0438 \u0432\u044b \u0441\u043c\u043e\u0436\u0435\u0442\u0435 \u0431\u0