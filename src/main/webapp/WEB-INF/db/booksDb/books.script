INSERT INTO BOOK VALUES(3255,506108,'\u0421\u0430\u0439\u043c\u043e\u043d \u041c\u043e\u043d\u043a: \u041f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u0438\u0440\u0443\u0435\u043c Arduino. \u041e\u0441\u043d\u043e\u0432\u044b \u0440\u0430\u0431\u043e\u0442\u044b \u0441\u043e \u0441\u043a\u0435\u0442\u0447\u0430\u043c\u0438','\u041c\u043e\u043d\u043a \u0421\u0430\u0439\u043c\u043e\u043d','\u0420\u0438\u043c\u0438\u0446\u0430\u043d \u041d. \u0410.','\u041f\u0438\u0442\u0435\u0440, 2016 \u0433.',424.0E0,'978-5-496-01956-9','176 (\u041e\u0444\u0441\u0435\u0442)','\u0422\u0438\u043f \u043e\u0431\u043b\u043e\u0436\u043a\u0438: \u043e\u0431\u043b - \u043c\u044f\u0433\u043a\u0438\u0439 \u043f\u0435\u0440\u0435\u043f\u043b\u0435\u0442 (\u043a\u0440\u0435\u043f\u043b\u0435\u043d\u0438\u0435 \u0441\u043a\u0440\u0435\u043f\u043a\u043e\u0439 \u0438\u043b\u0438 \u043a\u043b\u0435\u0435\u043c) \u0418\u043b\u043b\u044e\u0441\u0442\u0440\u0430\u0446\u0438\u0438: \u0427\u0435\u0440\u043d\u043e-\u0431\u0435\u043b\u044b\u0435','244 \u0433','231x165x9 \u043c\u043c','\u0410\u043d\u043d\u043e\u0442\u0430\u0446\u0438\u044f \u043a \u043a\u043d\u0438\u0433\u0435 "\u041f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u0438\u0440\u0443\u0435\u043c Arduino. \u041e\u0441\u043d\u043e\u0432\u044b \u0440\u0430\u0431\u043e\u0442\u044b \u0441\u043e \u0441\u043a\u0435\u0442\u0447\u0430\u043c\u0438" \u0412\u043f\u0435\u0440\u0432\u044b\u0435 \u043d\u0430 \u0440\u0443\u0441\u0441\u043a\u043e\u043c \u044f\u0437\u044b\u043a\u0435 \u0432\u044b\u0445\u043e\u0434\u0438\u0442 \u043b\u0435\u0433\u0435\u043d\u0434\u0430\u0440\u043d\u044b\u0439 \u0431\u0435\u0441\u0442\u0441\u0435\u043b\u043b\u0435\u0440 \u0421\u0430\u0439\u043c\u043e\u043d\u0430 \u041c\u043e\u043d\u043a\u0430, \u043a\u043e\u0442\u043e\u0440\u044b\u0439 \u043c\u043d\u043e\u0433\u043e \u043b\u0435\u0442 \u0437\u0430\u043d\u0438\u043c\u0430\u0435\u0442 \u043f\u0435\u0440\u0432\u044b\u0435 \u0441\u0442\u0440\u043e\u0447\u043a\u0438 \u0432 \u0440\u0435\u0439\u0442\u0438\u043d\u0433\u0430\u0445 Amazon. \u0425\u043e\u0442\u0438\u0442\u0435 \u0441\u043e\u0437\u0434\u0430\u0442\u044c \u0443\u043c\u043d\u044b\u0439 \u0434\u043e\u043c \u0438\u043b\u0438 \u0437\u0430\u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u0438\u0440\u043e\u0432\u0430\u0442\u044c \u0440\u043e\u0431\u043e\u0442\u0430? \u041d\u0435\u0442 \u043d\u0438\u0447\u0435\u0433\u043e \u043f\u0440\u043e\u0449\u0435. \u0421\u0430\u0439\u043c\u043e\u043d \u041c\u043e\u043d\u043a \u043d\u0435 \u0442\u043e\u043b\u044c\u043a\u043e \u043f\u043e\u043c\u043e\u0436\u0435\u0442 \u0440\u0430\u0437\u043e\u0431\u0440\u0430\u0442\u044c\u0441\u044f \u0441 \u043f\u0440\u043e\u0432\u043e\u043b\u043e\u0447\u043a\u0430\u043c\u0438, \u043a\u043e\u043d\u0442\u0430\u043a\u0442\u0430\u043c\u0438 \u0438 \u0434\u0430\u0442\u0447\u0438\u043a\u0430\u043c\u0438, \u043d\u043e \u0438 \u043f\u043e\u043a\u0430\u0436\u0435\u0442 \u043a\u0430\u043a \u0437\u0430\u0441\u0442\u0430\u0432\u0438\u0442\u044c \u0432\u0441\u0435 \u044d\u0442\u043e \u0445\u0438\u0442\u0440\u043e\u0441\u043f\u043b\u0435\u0442\u0435\u043d\u0438\u0435 \u043f\u0440\u043e\u0432\u043e\u0434\u043e\u0432 \u0438 \u043f\u043b\u0430\u0442 \u0434\u0435\u043b\u0430\u0442\u044c \u0442\u043e, \u0447\u0442\u043e \u0432\u0430\u043c \u043d\u0443\u0436\u043d\u043e. Arduino - \u044d\u0442\u043e \u043d\u0435 \u0442\u0430\u043a \u0441\u043b\u043e\u0436\u043d\u043e \u043a\u0430\u043a \u043a\u0430\u0436\u0435\u0442\u0441\u044f \u0441 \u043f\u0435\u0440\u0432\u043e\u0433\u043e \u0432\u0437\u0433\u043b\u044f\u0434\u0430. \u0412\u044b \u0441\u0440\u0430\u0437\u0443 \u0431\u0443\u0434\u0435\u0442\u0435 \u043f\u043e\u043a\u043e\u0440\u0435\u043d\u044b \u043e\u0442\u043a\u0440\u044b\u0432\u0430\u044e\u0449\u0438\u043c\u0438\u0441\u044f \u0432\u043e\u0437\u043c\u043e\u0436\u043d\u043e\u0441\u0442\u044f\u043c\u0438.','http://img1.labirint.ru/books/506108/big.jpg')
