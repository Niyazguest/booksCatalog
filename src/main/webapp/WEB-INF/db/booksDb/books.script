INSERT INTO COMMENT VALUES(1481,'\u0416\u0438\u043b\u0438\u043d\u0441\u043a\u0430\u044f \u0418\u043d\u043d\u0430','2014-01-11','\u041e\u0447\u0435\u043d\u044c \u043f\u043e\u043d\u0440\u0430\u0432\u0438\u043b\u0430\u0441\u044c \u044d\u0442\u0430 \u043a\u043d\u0438\u0433\u0430. \u041f\u043b\u044e\u0441\u044b: 1) \u0445\u043e\u0440\u043e\u0448\u0438\u0439 \u0438 \u043f\u043e\u043d\u044f\u0442\u043d\u044b\u0439 \u044f\u0437\u044b\u043a 2) \u043e\u0442\u043b\u0438\u0447\u043d\u043e \u043f\u043e\u0434\u0445\u043e\u0434\u0438\u0442 \u0442\u0435\u043c, \u043a\u0442\u043e \u0440\u0435\u0448\u0438\u043b \u0441\u0432\u044f\u0437\u0430\u0442\u044c \u0441\u0435\u0431\u044f \u0441 \u0440\u0430\u0437\u0440\u0430\u0431\u043e\u0442\u043a\u043e\u0439 \u043f\u043e\u0434 \u0440\u0435\u0439\u043b\u0441 3) \u0434\u0430\u0435\u0442 \u0445\u043e\u0440\u043e\u0448\u0443\u044e \u0438\u043d\u0444\u043e\u0440\u043c\u0430\u0446\u0438\u044e \u043f\u043e \u0444\u0440\u0435\u0439\u043c\u0432\u043e\u0440\u043a\u0443 4)\u041a\u043e\u0434, \u043f\u0440\u0435\u0434\u043e\u0441\u0442\u0430\u0432\u043b\u0435\u043d\u043d\u044b\u0439 \u0432 \u0443\u0447\u0435\u0431\u043d\u044b\u0445 \u0446\u0435\u043b\u044f\u0445, \u043d\u0430\u043f\u0438\u0441\u0430\u043d \u043d\u0430 \u043f\u0440\u043e\u0444\u0435\u0441\u0441\u0438\u043e\u043d\u0430\u043b\u044c\u043d\u043e\u043c \u0443\u0440\u043e\u0432\u043d\u0435 \u043c\u0438\u043d\u0443\u0441\u044b: 1) \u043a \u0441\u043e\u0436\u0430\u043b\u0435\u043d\u0438\u044e, \u0430\u0432\u0442\u043e\u0440 \u043d\u0435 \u0432\u0441\u0435\u0433\u0434\u0430 \u0443\u0433\u0430\u0434\u044b\u0432\u0430\u043b \u0441\u043e \u0441\u0442\u0438\u043b\u0435\u043c \u0448\u0440\u0438\u0444\u0442\u0430 \u0438 \u043f\u0435\u0440\u0438\u043e\u0434\u0438\u0447\u0435\u0441\u043a\u0438 \u0431\u0443\u043a\u0432\u044b \u0432 \u043a\u043e\u0434\u0435 "\u0441\u043a\u043b\u0435\u0438\u0432\u0430\u043b\u0438\u0441\u044c"',3393)
INSERT INTO COMMENT VALUES(1482,'\u041a\u043e\u0432\u0430\u043b\u0435\u043d\u043a\u043e \u041f\u0430\u0432\u0435\u043b','2015-01-21','\u041f\u0440\u0435\u043a\u0440\u0430\u0441\u043d\u0430\u044f \u043a\u043d\u0438\u0433\u0430 \u0434\u043b\u044f \u0438\u0437\u0443\u0447\u0435\u043d\u0438\u044f Ruby on Rails. \u0418\u0434\u0435\u0430\u043b\u044c\u043d\u043e \u0434\u043b\u044f \u043d\u043e\u0432\u0438\u0447\u043a\u0430. \u0410\u0432\u0442\u043e\u0440\u044b \u0431\u0435\u0440\u0443\u0442 \u0447\u0438\u0442\u0430\u0442\u0435\u043b\u044f \u0437\u0430 \u0440\u0443\u043a\u0443 \u0438 \u0432\u0435\u0434\u0443\u0442 \u0437\u0430 \u0441\u043e\u0431\u043e\u0439, \u0440\u0430\u0441\u0441\u043a\u0430\u0437\u044b\u0432\u0430\u044f \u043e \u0432\u043e\u0437\u043c\u043e\u0436\u043d\u043e\u0441\u0442\u044f\u0445 \u044d\u0442\u043e\u0433\u043e \u0444\u0440\u0435\u0439\u043c\u0432\u043e\u0440\u043a\u0430. \u041d\u0438\u043a\u043e\u0433\u0434\u0430 \u043d\u0435 \u0432\u0438\u0434\u0435\u043b \u0442\u0430\u043a\u043e\u0433\u043e \u0438\u043d\u0442\u0435\u0440\u0435\u0441\u043d\u043e\u0433\u043e \u0438, \u0433\u043b\u0430\u0432\u043d\u043e\u0435, \u0441\u043e\u0434\u0435\u0440\u0436\u0430\u0442\u0435\u043b\u044c\u043d\u043e-\u043f\u043e\u043b\u0435\u0437\u043d\u043e\u0433\u043e \u0441\u043f\u043e\u0441\u043e\u0431\u0430 \u0434\u043e\u043d\u0435\u0441\u0442\u0438 \u0438\u043d\u0444\u043e\u0440\u043c\u0430\u0446\u0438\u044e \u043d\u0435 \u0442\u043e\u043b\u044c\u043a\u043e \u0432 \u0435\u0451 \u0441\u0443\u0445\u043e\u043c \u0432\u0438\u0434\u0435, \u043d\u043e \u0438 \u0441 \u0438\u0441\u043f\u043e\u043b\u044c\u0437\u043e\u0432\u0430\u043d\u0438\u0435\u043c \u043a\u043e\u0434\u0430, \u043a\u043e\u0442\u043e\u0440\u044b\u0439 \u043c\u043e\u0436\u043d\u043e \u043f\u0435\u0440\u0435\u043f\u0438\u0441\u0430\u0442\u044c \u0438 \u043f\u0440\u043e\u0432\u0435\u0440\u0438\u0442\u044c, \u0447\u0442\u043e \u0432\u0441\u0435 \u0438\u043c\u0435\u043d\u043d\u043e \u0442\u0430\u043a! \u041d\u043e, \u0434\u043b\u044f \u0442\u043e\u0433\u043e, \u0447\u0442\u043e\u0431\u044b \u043f\u043e\u043d\u0438\u043c\u0430\u0442\u044c \u0435\u0441\u043b\u0438 \u043d\u0435 \u0432\u0441\u0435, \u0442\u043e \u0431\u043e\u043b\u044c\u0448\u0443\u044e \u0447\u0430\u0441\u0442\u044c \u0432\u0435\u0449\u0435\u0439, \u043e \u043a\u043e\u0442\u043e\u0440\u044b\u0445 \u0438\u0434\u0435\u0442 \u0440\u0435\u0447\u044c, \u0431\u044b\u043b\u043e \u0431\u044b \u043d\u0435 \u043f\u043b\u043e\u0445\u043e \u0437\u043d\u0430\u0442\u044c \u0441\u043b\u0435\u0434\u0443\u044e\u0449\u0435\u0435: 1) \u0431\u0430\u0437\u043e\u0432\u044b\u0435 \u0437\u043d\u0430\u043d\u0438\u044f html; 2) \u0431\u0430\u0437\u043e\u0432\u044b\u0435 \u0437\u043d\u0430\u043d\u0438\u044f css; 3) \u0431\u0430\u0437\u043e\u0432\u043e\u0435 \u0437\u043d\u0430\u043d\u0438\u0435 \u044f\u0437\u044b\u043a\u0430 Ruby(\u0435\u0441\u043b\u0438 \u0443 \u0432\u0430\u0441 \u043d\u0435 \u043e\u043f\u044b\u0442\u0430 \u0440\u0430\u0431\u043e\u0442\u044b \u0441 \u044f\u0437\u044b\u043a\u0430\u043c\u0438 \u0441\u0432\u0435\u0440\u0445\u0432\u044b\u0441\u043e\u043a\u043e\u0433\u043e \u0443\u0440\u043e\u0432\u043d\u044f). \u0425\u044d\u043b \u0424\u0443\u043b\u0442\u043e\u043d "\u041f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u0435 \u043d\u0430 \u044f\u0437\u044b\u043a\u0435 Ruby" \u0434\u043e\u0441\u0442\u0430\u0442\u043e\u0447\u043d\u043e \u043f\u043e\u043b\u043d\u043e \u043e\u043f\u0438\u0448\u0435\u0442 \u0432\u043e\u0437\u043c\u043e\u0436\u043d\u043e\u0441\u0442\u0438 \u044f\u0437\u044b\u043a\u0430; 4) \u0431\u0430\u0437\u043e\u0432\u043e\u0435 \u043f\u043e\u043d\u0438\u043c\u0430\u043d\u0438\u0435 \u0442\u043e\u0433\u043e, \u0447\u0442\u043e \u0442\u0430\u043a\u043e\u0435 "\u0432\u043d\u0435\u0448\u043d\u0438\u0439" \u0438 "\u043f\u0435\u0440\u0432\u0438\u0447\u043d\u044b\u0439 \u043a\u043b\u044e\u0447". \u0411\u044b\u043b\u043e \u0431\u044b \u043d\u0435 \u043f\u043b\u043e\u0445\u043e \u043f\u043e\u043d\u0438\u043c\u0430\u0442\u044c \u0443\u0441\u0442\u0440\u043e\u0439\u0441\u0442\u0432\u043e \u0440\u0435\u043b\u044f\u0446\u0438\u043e\u043d\u043d\u044b\u0445 \u0411\u0414; 5) \u0437\u043d\u0430\u043d\u0438\u0435 \u043f\u0430\u0442\u0442\u0435\u0440\u043d\u0430 MVC. \u041e\u0442\u0434\u0435\u043b\u044c\u043d\u043e \u043e \u043a\u0430\u0447\u0435\u0441\u0442\u0432\u0435 \u043a\u043d\u0438\u0433\u0438. \u041a\u043d\u0438\u0433\u0430 \u043a\u0430\u0447\u0435\u0441\u0442\u0432\u0435\u043d\u043d\u0430\u044f. (1 \u043e\u043f\u0435\u0447\u0430\u0442\u043a\u0430 \u0431\u044b\u043b\u0430, \u043d\u043e \u043d\u0435 \u043a\u0440\u0438\u0442\u0438\u0447\u043d\u043e) \u041e\u0442\u0434\u0435\u043b\u044c\u043d\u043e \u043e \u043a\u0430\u0447\u0435\u0441\u0442\u0432\u0435 \u043f\u0435\u0440\u0435\u0432\u043e\u0434\u0430. \u041f\u0440\u0435\u043a\u0440\u0430\u0441\u043d\u043e. \u041d\u0435 \u0441\u043b\u0443\u0447\u0430\u043b\u043e\u0441\u044c \u0442\u0430\u043a\u043e\u0433\u043e, \u0441\u043b\u043e\u0432\u043d\u043e \u0430\u0432\u0442\u043e\u0440 \u043f\u0435\u0440\u0435\u0432\u043e\u0434\u0430 \u043d\u0430\u0447\u0438\u043d\u0430\u0435\u0442 \u043d\u0435\u0441\u0442\u0438 \u043e\u0442\u0441\u0435\u0431\u044f\u0442\u0438\u043d\u0443 \u0438\u043b\u0438 \u043d\u0435 \u043f\u043e\u043d\u0438\u043c\u0430\u0435\u0442, \u043e \u0447\u0435\u043c \u043f\u0438\u0448\u0435\u0442. \u0421\u043a\u0440\u044b\u0442\u044c',3393)
INSERT INTO COMMENT VALUES(1483,'Alexey  Budaev','2015-01-17','\u0417\u0430\u043c\u0435\u0447\u0430\u0442\u0435\u043b\u044c\u043d\u0430\u044f \u043a\u043d\u0438\u0433\u0430! \u0414\u043e \u043d\u0435\u0435 \u0438\u0437\u0443\u0447\u0430\u043b Ruby on Rails \u0438\u0441\u043b\u043a\u044e\u0447\u0438\u0442\u0435\u043b\u044c\u043d\u043e \u043f\u043e \u0440\u0435\u0441\u0443\u0440\u0441\u0430\u043c \u0432 \u0441\u0435\u0442\u0438. \u0420\u0443\u0441\u0441\u043a\u043e\u044f\u0437\u044b\u0447\u043d\u043e\u0439 \u043f\u043e\u0434\u0440\u043e\u0431\u043d\u043e\u0439 \u0434\u043e\u043a\u0443\u043c\u0435\u043d\u0442\u0430\u0446\u0438\u0438 \u0438 \u0443\u0440\u043e\u043a\u043e\u0432 \u043c\u0430\u043b\u043e, \u0430 \u0442\u0430\u043a\u0438\u0445, \u0447\u0442\u043e\u0431\u044b \u043e\u0431\u044a\u044f\u0441\u043d\u044f\u043b\u043e\u0441\u044c \u0434\u043e\u0441\u043a\u043e\u043d\u0430\u043b\u044c\u043d\u043e \u0432\u0441\u0435 - \u0442\u0430\u043a \u0438 \u0432\u043e\u0432\u0441\u0435. \u041f\u043e\u044d\u0442\u043e\u043c\u0443 \u043c\u043d\u043e\u0433\u043e\u0435 \u043e\u0441\u0442\u0430\u0432\u0430\u043b\u043e\u0441\u044c \u0437\u0430 \u043a\u0430\u0434\u0440\u043e\u043c \u0438 \u043e\u0431\u0449\u0435\u0433\u043e \u043f\u043e\u043d\u0438\u043c\u0430\u043d\u0438\u044f \u043d\u0435 \u0434\u0430\u043b\u043e. \u041a\u043d\u0438\u0433\u0430 \u0436\u0435 \u043e\u0442\u043a\u0440\u044b\u043b\u0430 \u0433\u043b\u0430\u0437\u0430 \u0438 \u0442\u0435\u043f\u0435\u0440\u044c \u044f \u0437\u043d\u0430\u043a\u043e\u043c \u0438 \u0441 MVC-\u043f\u0430\u0442\u0442\u0435\u0440\u043d\u043e\u043c \u0438 \u0441 \u043f\u0440\u0438\u043d\u0446\u0438\u043f\u0430\u043c\u0438 \u0440\u0430\u0431\u043e\u0442\u044b Rails \u0438 \u0441 \u0432\u043e\u0437\u043c\u043e\u0436\u043d\u043e\u0441\u0442\u044f\u043c\u0438 \u0444\u0440\u0435\u0439\u043c\u0432\u043e\u0440\u043a\u0430. \u041a\u043d\u0438\u0433\u0430 \u0443\u0441\u043f\u0435\u0448\u043d\u043e \u043c\u043e\u0442\u0438\u0432\u0438\u0440\u043e\u0432\u0430\u043b\u0430 \u043c\u0435\u043d\u044f \u043d\u0430 \u043f\u0440\u043e\u0434\u043e\u043b\u0436\u0435\u043d\u0438\u0435 \u043e\u0431\u0443\u0447\u0435\u043d\u0438\u044f \u0432 \u044d\u0442\u043e\u043c \u043d\u0430\u043f\u0440\u0430\u0432\u043b\u0435\u043d\u0438\u0438.',3393)
INSERT INTO COMMENT VALUES(1484,'\u0415\u0440\u043c\u043e\u043b\u044c\u0447\u0438\u043a \u0412\u0430\u0441\u0438\u043b\u0438\u0439','2014-01-21','Java Script \u0432 \u043a\u043d\u0438\u0433\u0435 \u043d\u0435\u0442. \u041f\u0440\u0438\u043b\u0430\u0433\u0430\u0435\u0442\u0441\u044f \u043d\u0430 \u0434\u0438\u0441\u043a\u0435. \u041f\u0440\u043e\u0441\u043c\u043e\u0442\u0440\u0435\u043b \u043a\u043d\u0438\u0433\u0443 \u0437\u0430 \u0432\u0435\u0447\u0435\u0440. \u0414\u043b\u044f \u0441\u043e\u0432\u0441\u0435\u043c \u0443\u0436 \u043d\u043e\u0432\u0438\u0447\u043a\u043e\u0432. \u041a\u043e\u0434 \u0435\u0441\u0442\u044c, \u043d\u043e \u043f\u0440\u0438\u043c\u0435\u0440\u044b \u043a \u043d\u0435\u043c\u0443 \u043d\u0435 \u0432\u0435\u0437\u0434\u0435. \u0423\u0434\u0438\u0432\u043b\u0435\u043d \u0431\u044b\u043b, \u0447\u0442\u043e \u0432 \u043a\u0430\u0447\u0435\u0441\u0442\u0432\u0435 \u0440\u0435\u0434\u0430\u043a\u0442\u043e\u0440\u0430 \u043a\u043e\u0434\u0430 \u043d\u0435 \u0440\u0430\u0441\u0441\u043c\u0430\u0442\u0440\u0438\u0432\u0430\u043b\u0441\u044f Notepad++. \u041d\u0443 \u0438... \u0446\u0435\u043d\u0430.. \u0421\u043e\u0432\u0435\u0442: \u043b\u0443\u0447\u0448\u0435 \u043a\u0443\u043f\u0438\u0442\u0435 \u0442\u0440\u0438 \u043a\u043d\u0438\u0433\u0438 \u043e\u0442\u0434\u0435\u043b\u044c\u043d\u043e \u043f\u043e CSS, HTML, JS.',3394)
INSERT INTO COMMENT VALUES(1485,'\u0421\u0438\u0434\u0435\u043b\u044c\u043d\u0438\u043a\u043e\u0432 \u0412\u0430\u0441\u0438\u043b\u0438\u0439','2015-01-21','\u041a\u043d\u0438\u0433\u0430 \u043d\u0430\u0447\u0430\u043b\u044c\u043d\u043e\u0433\u043e \u0443\u0440\u043e\u0432\u043d\u044f. \u041f\u043e\u0434\u043e\u0439\u0434\u0451\u0442 \u0434\u043b\u044f \u043f\u0435\u0440\u0432\u043e\u043d\u0430\u0447\u0430\u043b\u044c\u043d\u043e\u0433\u043e \u0438\u0437\u0443\u0447\u0435\u043d\u0438\u044f HTML \u0438 CSS \u043b\u0438\u0431\u043e \u0432 \u043a\u0430\u0447\u0435\u0441\u0442\u0432\u0435 \u0441\u043f\u0440\u0430\u0432\u043e\u0447\u043d\u0438\u043a\u0430.',3394)
INSERT INTO COMMENT VALUES(1486,'\u041f\u0430\u0440\u0445\u043e\u043c\u0435\u043d\u043a\u043e \u0420\u0443\u0441\u043b\u0430\u043d','2015-01-01','\u041a\u043d\u0438\u0433\u0430 \u043f\u0440\u0435\u0434\u0441\u0442\u0430\u0432\u043b\u044f\u0435\u0442 \u0438\u0437 \u0441\u0435\u0431\u044f \u0440\u0443\u043a\u043e\u0432\u043e\u0434\u0441\u0442\u0432\u043e (\u0441\u043f\u0440\u0430\u0432\u043e\u0447\u043d\u0438\u043a) \u043f\u043e \u0438\u0441\u043f\u043e\u043b\u044c\u0437\u043e\u0432\u0430\u043d\u0438\u044e \u043a\u0430\u0440\u043a\u0430\u0441\u043e\u0432 \u0442\u0435\u0441\u0442\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u044f \u0438 \u0438\u0441\u043f\u043e\u043b\u043d\u0438\u0442\u0435\u043b\u0435\u0439 \u0442\u0435\u0441\u0442\u043e\u0432. \u041a\u0430\u043a \u0442\u0430\u043a\u043e\u0432\u043e\u0433\u043e \u043c\u0430\u0442\u0435\u0440\u0438\u0430\u043b\u0430 \u043f\u043e\u0441\u0432\u044f\u0449\u0451\u043d\u043d\u043e\u0433\u043e \u043f\u0440\u043e\u0446\u0435\u0441\u0441\u0443 \u0441\u043e\u0441\u0442\u0430\u0432\u043b\u0435\u043d\u0438\u044f \u0442\u0435\u0441\u0442\u043e\u0432 \u043d\u0435 \u0442\u0430\u043a \u0443\u0436 \u0438 \u043c\u043d\u043e\u0433\u043e. \u0412\u043e\u0437\u043c\u043e\u0436\u043d\u043e \u0430\u0432\u0442\u043e\u0440 \u0438 \u043d\u0435 \u0441\u0442\u0430\u0432\u0438\u043b \u043f\u0435\u0440\u0435\u0434 \u0441\u043e\u0431\u043e\u0439 \u0437\u0430\u0434\u0430\u0447\u0443 \u0440\u0430\u0441\u043a\u0440\u044b\u0432\u0430\u0442\u044c \u044d\u0442\u0443 \u0442\u0435\u043c\u0443. \u0422\u0435\u043c \u043d\u0435 \u043c\u0435\u043d\u0435\u0435 \u043a\u043d\u0438\u0436\u043a\u0430 \u0434\u043e\u0432\u043e\u043b\u044c\u043d\u043e \u043f\u043e\u043b\u0435\u0437\u043d\u0430\u044f, \u0447\u0438\u0442\u0430\u0435\u0442\u0441\u044f \u043b\u0435\u0433\u043a\u043e, \u043f\u0440\u0438\u043c\u0435\u0440\u044b \u043f\u0440\u043e\u0441\u0442\u044b \u0438 \u043f\u043e\u043d\u044f\u0442\u043d\u044b.',3400)
INSERT INTO COMMENT VALUES(1487,'\u0418\u0432\u0430\u043d \u0424\u0440\u043e\u043b\u043e\u0432','2014-01-03','1. \u041a\u0430\u0447\u0435\u0441\u0442\u0432\u043e \u043a\u043d\u0438\u0433\u0438. \u0417\u0430 \u0442\u0430\u043a\u0438\u0435 \u0434\u0435\u043d\u044c\u0433\u0438 \u043c\u043e\u0436\u043d\u043e \u0431\u044b\u043b\u043e \u0441\u0434\u0435\u043b\u0430\u0442\u044c \u0442\u0432\u0435\u0440\u0434\u044b\u0439 \u043f\u0435\u0440\u0435\u043f\u043b\u0435\u0442 \u0438 \u0444\u043e\u0440\u043c\u0430\u0442 \u043f\u043e\u0431\u043e\u043b\u044c\u0448\u0435 (\u0442.\u043a. \u0431\u043e\u043b\u0435\u0435 500 \u0441\u0442\u0440\u0430\u043d\u0438\u0446 \u0432 \u0442\u0430\u043a\u043e\u043c \u0444\u043e\u0440\u043c\u0430\u0442\u0435 \u043f\u0435\u0440\u0435\u043c\u0430\u0442\u044b\u0432\u0430\u0442\u044c \u043d\u0435 \u043e\u0447\u0435\u043d\u044c \u0443\u0434\u043e\u0431\u043d\u043e). \u041c\u044f\u0433\u043a\u0430\u044f \u043e\u0431\u043b\u043e\u0436\u043a\u0430 \u043d\u0430\u0447\u0430\u043b\u0430 \u0440\u0430\u0441\u0441\u043b\u0430\u0438\u0432\u0430\u0442\u044c\u0441\u044f \u0432 \u043f\u0435\u0440\u0432\u044b\u0439 \u0436\u0435 \u0434\u0435\u043d\u044c, \u043f\u0440\u0438\u0448\u043b\u043e\u0441\u044c \u043f\u0440\u043e\u043a\u043b\u0435\u0438\u0442\u044c \u0441\u043a\u043e\u0442\u0447\u0435\u043c \u043f\u043e \u043f\u0435\u0440\u0438\u043c\u0435\u0442\u0440\u0443. \u041a \u043a\u0430\u0447\u0435\u0441\u0442\u0432\u0443 \u043f\u0435\u0447\u0430\u0442\u0438 \u043d\u0430\u0440\u0435\u043a\u0430\u043d\u0438\u0439 \u043d\u0435\u0442 -- \u043e\u043d\u043e \u043d\u0430 \u0432\u044b\u0441\u043e\u0442\u0435. 2. \u041a\u0430\u0447\u0435\u0441\u0442\u0432\u043e \u043f\u0440\u0435\u043f\u043e\u0434\u043d\u0435\u0441\u0435\u043d\u0438\u044f \u043c\u0430\u0442\u0435\u0440\u0438\u0430\u043b\u0430. \u041e\u043d\u043e \u043d\u0438\u043a\u0430\u043a\u043e\u0435. \u041f\u043e\u0445\u043e\u0436\u0435 \u043d\u0430 \u0431\u0435\u0441\u0441\u043c\u044b\u0441\u043b\u0435\u043d\u043d\u044b\u0439 \u043d\u0430\u0431\u043e\u0440 \u0444\u0440\u0430\u0437. \u0422\u0435\u043c\u044b \u0441\u043a\u0430\u0447\u0443\u0442 \u043a\u0430\u043a \u043f\u043e\u043f\u0430\u043b\u043e. \u041f\u043e\u043b\u043e\u0432\u0438\u043d\u0430 \u043a\u043d\u0438\u0433\u0438 \u043f\u043e\u0445\u043e\u0436\u0430 \u043d\u0430 \u043a\u043e\u043f\u0438\u043f\u0430\u0441\u0442 \u043e\u043f\u0438\u0441\u0430\u043d\u0438\u044f \u0441\u0442\u0430\u043d\u0434\u0430\u0440\u0442\u043d\u043e\u0439 \u0431\u0438\u0431\u043b\u0438\u043e\u0442\u0435\u043a\u0438. \u041d\u043e \u0441\u0430\u043c\u044b\u0439 \u0431\u043e\u043b\u044c\u0448\u043e\u0439 \u043c\u0438\u043d\u0443\u0441 -- \u043e\u043f\u0435\u0447\u0430\u0442\u043a\u0438. \u0418\u0445 \u0434\u0435\u0439\u0441\u0442\u0432\u0438\u0442\u0435\u043b\u044c\u043d\u043e \u043c\u043d\u043e\u0433\u043e. \u041f\u0440\u0438\u0447\u0435\u043c, \u0431\u041e\u043b\u044c\u0448\u0430\u044f \u0447\u0430\u0441\u0442\u044c, -- \u0432 \u043f\u0440\u0438\u043c\u0435\u0440\u0430\u0445 \u0438\u0441\u0445\u043e\u0434\u043d\u043e\u0433\u043e \u043a\u043e\u0434\u0430, \u0447\u0442\u043e \u043f\u0440\u043e\u0441\u0442\u043e \u043d\u0435\u0434\u043e\u043f\u0443\u0441\u0442\u0438\u043c\u043e \u0432 \u043a\u043d\u0438\u0433\u0430\u0445 \u043f\u043e\u0434\u043e\u0431\u043d\u043e\u0433\u043e \u0441\u043e\u0434\u0435\u0440\u0436\u0430\u043d\u0438\u044f. \u0412 \u0438\u0442\u043e\u0433\u0435 \u043d\u0435 \u0441\u043e\u0432\u0435\u0442\u0443\u044e \u043d\u0438\u043a\u043e\u043c\u0443. \u0414\u0435\u0439\u0442\u0435\u043b\u044b \u0441\u0434\u0443\u043b\u0438\u0441\u044c.. \u041d\u0430\u0447\u0438\u043d\u0430\u044e\u0449\u0438\u0445 \u043a\u043d\u0438\u0433\u0430 \u043e\u0442\u0442\u043e\u043b\u043a\u043d\u0435\u0442 \u0441\u0432\u043e\u0435\u0439 \u0442\u0443\u043f\u043e\u0441\u0442\u044c\u044e \u0438\u0437\u043b\u043e\u0436\u0435\u043d\u0438\u044f \u043c\u0430\u0442\u0435\u0440\u0438\u0430\u043b\u0430, \u0430 \u0437\u043d\u0430\u044e\u0449\u0438\u0445 -- \u0435\u0449\u0435 \u0438 \u043c\u043d\u043e\u0433\u043e\u0447\u0438\u0441\u043b\u0435\u043d\u043d\u044b\u043c\u0438 \u043e\u043f\u0435\u0447\u0430\u0442\u043a\u0430\u043c\u0438. \u0421\u043a\u0440\u044b\u0442\u044c',3402)
