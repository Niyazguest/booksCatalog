INSERT INTO BOOK VALUES(3818,338821,'\u0414\u0436\u043e\u043d\u0441\u043e\u043d, \u0421\u043a\u0438\u0431\u043e, \u042f\u043d\u0433: \u041e\u0441\u043d\u043e\u0432\u044b Microsoft Visual Studio .NET 2003','\u0414\u0436\u043e\u043d\u0441\u043e\u043d \u0411\u0440\u0430\u0439\u0430\u043d, \u0421\u043a\u0438\u0431\u043e \u041a\u0440\u044d\u0439\u0433, \u042f\u043d\u0433 \u041c\u0430\u0440\u043a','','\u0420\u0443\u0441\u0441\u043a\u0430\u044f \u0440\u0435\u0434\u0430\u043a\u0446\u0438\u044f, 2003 \u0433.',318.0E0,'5-7502-0225-9\u00a0 \u0432\u0441\u0435,','464 (\u0413\u0430\u0437\u0435\u0442\u043d\u0430\u044f)','\u0422\u0438\u043f \u043e\u0431\u043b\u043e\u0436\u043a\u0438: 7\u0411\u0446 - \u0442\u0432\u0435\u0440\u0434\u0430\u044f, \u0446\u0435\u043b\u043b\u043e\u0444\u0430\u043d\u0438\u0440\u043e\u0432\u0430\u043d\u043d\u0430\u044f (\u0438\u043b\u0438 \u043b\u0430\u043a\u0438\u0440\u043e\u0432\u0430\u043d\u043d\u0430\u044f) \u0418\u043b\u043b\u044e\u0441\u0442\u0440\u0430\u0446\u0438\u0438: \u0427\u0435\u0440\u043d\u043e-\u0431\u0435\u043b\u044b\u0435','690 \u0433','246x170x28 \u043c\u043c','\u0410\u043d\u043d\u043e\u0442\u0430\u0446\u0438\u044f \u043a \u043a\u043d\u0438\u0433\u0435 "\u041e\u0441\u043d\u043e\u0432\u044b Microsoft Visual Studio .NET 2003" \u0412 \u044d\u0442\u043e\u0439 \u043a\u043d\u0438\u0433\u0435 \u043f\u043e\u0434\u0440\u043e\u0431\u043d\u043e \u043e\u043f\u0438\u0441\u044b\u0432\u0430\u0435\u0442\u0441\u044f \u0430\u0440\u0445\u0438\u0442\u0435\u043a\u0442\u0443\u0440\u0430 Microsoft Visual Studio .NET - \u043a\u043b\u044e\u0447\u0435\u0432\u043e\u0433\u043e \u043f\u0440\u043e\u0434\u0443\u043a\u0442\u0430 \u0432 \u0438\u0441\u0442\u043e\u0440\u0438\u0438 \u0438\u043d\u0441\u0442\u0440\u0443\u043c\u0435\u043d\u0442\u0430\u043b\u044c\u043d\u044b\u0445 \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u043d\u044b\u0445 \u0441\u0440\u0435\u0434\u0441\u0442\u0432, \u043e\u0431\u044a\u0435\u0434\u0438\u043d\u044f\u044e\u0449\u0435\u0433\u043e \u043d\u043