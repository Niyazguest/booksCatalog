INSERT INTO COMMENT VALUES(1554,'Saboteur','2014-01-27','\u041d\u0435\u043f\u043b\u043e\u0445\u0430\u044f \u043a\u043d\u0438\u0433\u0430, \u0441\u043f\u043e\u0441\u043e\u0431\u043d\u0430\u044f \u0441\u0442\u0430\u0442\u044c \u043f\u043e\u0441\u043e\u0431\u043d\u0438\u043a\u043e\u043c \u0432 \u043f\u0435\u0440\u0435\u0445\u043e\u0434\u0435 \u043d\u0430 \u043d\u043e\u0432\u0443\u044e \u0441\u0438\u0441\u0442\u0435\u043c\u0443 7.5\10 \u041e\u0441\u043e\u0431\u0435\u043d\u043d\u043e\u0441\u0442\u0438: \u0411\u043e\u043b\u044c\u0448\u043e\u0435 \u0432\u043d\u0438\u043c\u0430\u043d\u0438\u0435 \u0443\u0434\u0435\u043b\u044f\u0435\u0442\u0441\u044f \u043f\u043e\u043b\u044c\u0437\u043e\u0432\u0430\u0442\u0435\u043b\u044c\u0441\u043a\u043e\u043c\u0443 \u0438\u043d\u0442\u0435\u0440\u0444\u0435\u0439\u0441\u0443 \u0438 \u0432\u0437\u0430\u0438\u043c\u043e\u0434\u0435\u0439\u0441\u0442\u0432\u0438\u044e \u0441 \u043f\u043e\u043b\u044c\u0437\u043e\u0432\u0430\u0442\u0435\u043b\u0435\u043c. \u041e\u043f\u0438\u0441\u0430\u043d\u044b \u043f\u0440\u043e\u0446\u0435\u0441\u0441\u044b \u0440\u0435\u0433\u0438\u0441\u0442\u0440\u0430\u0446\u0438\u0438 \u0440\u0430\u0437\u0440\u0430\u0431\u043e\u0442\u0447\u0438\u043a\u0430 \u0438 \u0440\u0430\u0437\u043c\u0435\u0449\u0435\u043d\u0438\u044f \u043f\u0440\u0438\u043b\u043e\u0436\u0435\u043d\u0438\u044f \u0432 Windows Store, \u043c\u043e\u0434\u0435\u043b\u0438 \u0440\u0430\u0441\u043f\u0440\u043e\u0441\u0442\u0440\u0430\u043d\u0435\u043d\u0438\u044f \u0438 \u043c\u043e\u043d\u0435\u0442\u0438\u0437\u0430\u0446\u0438\u0438 \u043f\u0440\u0438\u043b\u043e\u0436\u0435\u043d\u0438\u0439. \u0420\u0430\u0441\u043a\u0440\u044b\u0442 \u043a\u043e\u043d\u0446\u0435\u043f\u0442 Metro, \u043a\u0430\u043a \u0432 \u0441\u0444\u0435\u0440\u0435 \u0434\u0438\u0437\u0430\u0439\u043d\u0430 (\u043e\u0442\u043a\u0430\u0437 \u0438 \u043f\u043e\u0440\u0438\u0446\u0430\u043d\u0438\u0435 \u0438\u043a\u043e\u043d\u043e\u0433\u0440\u0430\u0444\u0438\u0447\u0435\u0441\u043a\u0438\u0445 \u0438\u043d\u0442\u0435\u0440\u0444\u0435\u0439\u0441\u043e\u0432, \u0443\u043a\u043b\u043e\u043d \u043d\u0430 \u0438\u043d\u0444\u043e\u0440\u043c\u0430\u0442\u0438\u0432\u043d\u043e\u0441\u0442\u044c \u0438\u043d\u0442\u0435\u0440\u0444\u0435\u0439\u0441\u0430, \u0433\u043b\u043e\u0431\u0430\u043b\u0438\u0437\u0430\u0446\u0438\u044f, \u043e\u0442\u0437\u044b\u0432\u0447\u0438\u0432\u043e\u0441\u0442\u044c, etc), \u0442\u0430\u043a \u0438 \u0441 \u0442\u0435\u0445\u043d\u0438\u0447\u0435\u0441\u043a\u043e\u0439 \u0441\u0442\u043e\u0440\u043e\u043d\u044b (\u043f\u043e\u0432\u0441\u0435\u043c\u0435\u0441\u0442\u043d\u0430\u044f \u0443\u043d\u0438\u0444\u0438\u043a\u0430\u0446\u0438\u044f; \u0430\u0441\u0438\u043d\u0445\u0440\u043e\u043d\u043d\u043e\u0441\u0442\u044c \u0438 \u043f\u0430\u0440\u0430\u043b\u043b\u0435\u043b\u0438\u0437\u043c, \u043e\u0431\u0435\u0441\u043f\u0435\u0447\u0438\u0432\u0430\u044e\u0449\u0438\u0435 \u043e\u0442\u0437\u044b\u0432\u0447\u0438\u0432\u043e\u0441\u0442\u044c; etc) \u0420\u0430\u0441\u0441\u043c\u043e\u0442\u0440\u0435\u043d\u0438\u0435 \u043b\u044e\u0431\u043e\u0433\u043e \u043d\u043e\u0432\u0448\u0435\u0441\u0442\u0432\u0430 W8 \u0430\u0432\u0442\u043e\u0440\u044b \u043d\u0430\u0447\u0438\u043d\u0430\u044e\u0442 \u0441 \u0440\u0430\u0441\u043a\u0440\u044b\u0442\u0438\u044f \u043c\u043e\u0442\u0438\u0432\u0430\u0446\u0438\u0438 \u043a \u0442\u0430\u043a\u043e\u043c\u0443 \u0440\u0430\u0441\u0448\u0438\u0440\u0435\u043d\u0438\u044e \u043f\u043b\u0430\u0442\u0444\u043e\u0440\u043c\u044b. \u0421\u043a\u0440\u044b\u0442\u044c',3501)
INSERT INTO COMMENT VALUES(1555,'Vitaminka74','2013-01-23','\u0425\u043e\u0440\u043e\u0448\u0430\u044f \u043a\u043d\u0438\u0433\u0430, \u043f\u043e\u043d\u044f\u0442\u043d\u043e \u043d\u0430\u043f\u0438\u0441\u0430\u043d\u0430, \u043d\u0430 \u043c\u043e\u0439 \u0432\u0437\u0433\u043b\u044f\u0434 \u0431\u0443\u0434\u0435\u0442 \u0438\u043d\u0442\u0435\u0440\u0435\u0441\u043d\u0430 \u043a\u0430\u043a \u043d\u0430\u0447\u0438\u043d\u0430\u044e\u0449\u0438\u043c \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u0438\u0441\u0442\u0430\u043c, \u0442\u0430\u043a \u0438 \u0440\u0430\u0437\u0440\u0430\u0431\u043e\u0442\u0447\u0438\u043a\u043e\u043c \u0441 \u043e\u043f\u044b\u0442\u043e\u043c. \u0412 \u043e\u0431\u0449\u0435\u043c \u043a\u043d\u0438\u0433\u0430 \u0434\u043b\u044f \u0432\u0441\u0435\u0445 \u043a\u0442\u043e \u0445\u043e\u0447\u0435\u0442 \u043f\u0438\u0441\u0430\u0442\u044c \u043f\u043e\u0434 Win8. \u0410 \u0433\u043b\u0430\u0432\u043d\u043e\u0435 \u0442\u043e \u0447\u0442\u043e \u0432\u044b\u0448\u043b\u0430 \u043e\u043d\u0430 \u043e\u0447\u0435\u043d\u044c \u0432\u043e \u0432\u0440\u0435\u043c\u044f. \u0422\u0430\u043a\u0436\u0435 \u0445\u043e\u0447\u0435\u0442\u0441\u044f \u043e\u0442\u043c\u0435\u0442\u0438\u0442\u044c \u043e\u0442\u043b\u0438\u0447\u043d\u043e\u0435 \u043a\u0430\u0447\u0435\u0441\u0442\u0432\u043e \u043f\u0435\u0447\u0430\u0442\u0438 \u0438 \u043e\u0444\u043e\u0440\u043c\u043b\u0435\u043d\u0438\u044f \u043f\u0440\u0438\u043c\u0435\u0440\u043e\u0432 \u043a\u043e\u0434\u0430.',3501)
INSERT INTO COMMENT VALUES(1556,'\u041a\u0438\u0441\u0435\u043b\u0435\u0432\u0430 \u0412\u0430\u043b\u0435\u043d\u0442\u0438\u043d\u0430','2013-01-19','\u041e\u0431\u043b\u043e\u0436\u043a\u0430 \u043c\u044f\u0433\u043a\u0430\u044f, \u0441\u0442\u0440\u0430\u043d\u0438\u0446\u044b \u0438\u0437 \u0433\u0430\u0437\u0435\u0442\u043d\u043e\u0439 \u0431\u0443\u043c\u0430\u0433\u0438, \u043a\u0440\u0435\u043f\u043b\u0435\u043d\u0438\u0435 \u0441\u0442\u0440\u0430\u043d\u0438\u0446 - \u043a\u043b\u0435\u0435\u0432\u043e\u0435. \u0410\u0434\u0440\u0435\u0441\u043e\u0432\u0430\u043d\u0430 \u0441\u0442\u0443\u0434\u0435\u043d\u0442\u0430\u043c \u0438 \u043d\u0430\u0447\u0438\u043d\u0430\u044e\u0449\u0438\u043c \u0434\u043b\u044f \u0431\u044b\u0441\u0442\u0440\u043e\u0433\u043e \u043e\u0437\u043d\u0430\u043a\u043e\u043c\u043b\u0435\u043d\u0438\u044f \u0441 \u044f\u0437\u044b\u043a\u0430\u043c\u0438 \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u044f Java \u0438 C#, \u043f\u043e\u0441\u043b\u0443\u0436\u0438\u0442 \u0442\u0440\u0430\u043c\u043f\u043b\u0438\u043d\u043e\u043c \u0434\u043b\u044f \u0434\u0430\u043b\u044c\u043d\u0435\u0439\u0448\u0435\u0433\u043e \u0437\u0430\u043d\u044f\u0442\u0438\u044f \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u0435\u043c. \u041f\u043e\u043a\u0430\u0437\u0430\u0442\u044c \u0438\u0437\u043e\u0431\u0440\u0430\u0436\u0435\u043d\u0438\u044f \u0421\u043a\u0440\u044b\u0442\u044c \u0438\u0437\u043e\u0431\u0440\u0430\u0436\u0435\u043d\u0438\u044f',3505)
INSERT INTO COMMENT VALUES(1557,'\u0422\u0438\u0442\u043e\u0432 \u0414\u0435\u043d\u0438\u0441','2014-01-17','\u041a\u043d\u0438\u0433\u0430 \u043d\u0430\u043f\u0438\u0441\u0430\u043d\u0430 \u0445\u043e\u0440\u043e\u0448\u0438\u043c, \u043f\u043e\u043d\u044f\u0442\u043d\u044b\u043c \u044f\u0437\u044b\u043a\u043e\u043c. \u0427\u0443\u0432\u0441\u0442\u0432\u0443\u0435\u0442\u0441\u044f \u0447\u0442\u043e \u0430\u0432\u0442\u043e\u0440 \u0441\u0430\u043c \u0440\u0430\u0437\u0431\u0438\u0440\u0430\u0435\u0442\u0441\u044f \u0432 \u0442\u043e\u043c, \u043e \u0447\u0435\u043c \u043f\u0438\u0448\u0435\u0442. \u041d\u0430 \u043a\u0430\u0436\u0434\u0443\u044e \u0431\u0430\u0437\u0443 \u043e\u0442\u0432\u043e\u0434\u0438\u0442\u0441\u044f \u043f\u043e \u0442\u0440\u0438 \u0434\u043d\u044f (\u0437\u0430\u043d\u044f\u0442\u0438\u044f), \u0432 \u043a\u043e\u043d\u0446\u0435 \u043a\u0430\u0436\u0434\u043e\u0433\u043e \u0432\u043f\u043e\u043b\u043d\u0435 \u0432\u043d\u044f\u0442\u043d\u044b\u0435 \u0437\u0430\u0434\u0430\u043d\u0438\u044f \u043d\u0430 \u0441\u0430\u043c\u043e\u043f\u043e\u0434\u0433\u043e\u0442\u043e\u0432\u043a\u0443. \u0418\u043d\u0444\u043e\u0440\u043c\u0430\u0446\u0438\u0438 \u043f\u043e \u043a\u0430\u0436\u0434\u043e\u0439 \u0431\u0430\u0437\u0435 \u0434\u043e\u0441\u0442\u0430\u0442\u043e\u0447\u043d\u043e, \u0447\u0442\u043e\u0431\u044b \u043f\u043e\u043d\u044f\u0442\u044c \u0438\u0434\u0435\u044e \u0438 \u0435\u0441\u043b\u0438 "\u0437\u0430\u0446\u0435\u043f\u0438\u043b\u043e" \u043d\u0430\u0447\u0430\u0442\u044c \u0438\u0437\u0443\u0447\u0430\u0442\u044c \u0431\u043e\u043b\u0435\u0435 \u0433\u043b\u0443\u0431\u043e\u043a\u043e. \u041f\u0440\u043e\u0447\u0438\u0442\u0430\u043b \u043a\u0430\u043a \u0434\u0435\u0442\u0435\u043a\u0442\u0438\u0432. \u0415\u0434\u0438\u043d\u0441\u0442\u0432\u0435\u043d\u043d\u044b\u0439 \u043c\u0438\u043d\u0443\u0441 - \u043c\u044f\u0433\u043a\u0438\u0439 \u043f\u0435\u0440\u0435\u043f\u043b\u0435\u0442 \u0438 \u043d\u0435\u0431\u043e\u043b\u044c\u0448\u043e\u0439 \u0444\u043e\u0440\u043c\u0430\u0442, \u0438\u0437-\u0437\u0430 \u0447\u0435\u0433\u043e \u043d\u0435\u0443\u0434\u043e\u0431\u043d\u043e \u0434\u0435\u0440\u0436\u0430\u0442\u044c \u043a\u043d\u0438\u0433\u0443 \u043e\u0442\u043a\u0440\u044b\u0442\u043e\u0439.',3506)
INSERT INTO COMMENT VALUES(1558,'\u0412\u0430\u0441\u0438\u043b\u044c\u0435\u0432 \u0412\u043b\u0430\u0434\u0438\u043c\u0438\u0440','2014-01-12','\u041f\u043b\u043e\u0445\u0430\u044f \u043a\u043d\u0438\u0433\u0430, \u044f \u0440\u0430\u0437\u043e\u0447\u0430\u0440\u043e\u0432\u0430\u043d. \u041f\u0438\u0448\u0443\u0442 \u0432 \u0438\u043d\u0442\u0435\u0440\u0435\u0441\u043d\u043e\u043c \u0441\u0442\u0438\u043b\u0435 - \u043e\u0431\u0449\u0430\u044e\u0442\u0441\u044f 2 \u0440\u0430\u0437\u0440\u0430\u0431\u043e\u0442\u0447\u0438\u043a\u0430. - \u044f \u0441\u043e\u0441\u0442\u0430\u0432\u0438\u043b \u0442\u0430\u0431\u043b\u0438\u0446\u0443 \u0441 \u0442\u0435\u0441\u0442\u0430\u043c\u0438, \u043f\u043e\u0441\u043c\u043e\u0442\u0440\u0438 (\u0437\u0430\u043d\u0438\u043c\u0430\u0435\u0442 \u0432\u0441\u044e \u0441\u0442\u0440\u0430\u043d\u0438\u0446\u0443). - \u0442\u0443\u0442 \u0435\u0441\u0442\u044c \u043b\u0438\u0448\u043d\u0438\u0435 \u0442\u0435\u0441\u0442\u044b - \u0434\u0430\u0432\u0430\u0439 \u0443\u0434\u0430\u043b\u0438\u043c, - \u043f\u0435\u0447\u0430\u0442\u0430\u044e\u0442 \u0435\u0449\u0435 \u043e\u0434\u043d\u0443 \u0442\u0430\u0431\u043b\u0438\u0446\u0443 (\u0435\u0449\u0435 \u043e\u0434\u043d\u0430 \u0441\u0442\u0440\u0430\u043d\u0438\u0446\u0430). - \u043e\u043e\u043e!!! \u043c\u044b \u0443\u0434\u0430\u043b\u0438\u043b\u0438 \u043d\u0435 \u0432\u0441\u0435, \u0434\u0430\u0432\u0430\u0439 \u0443\u0434\u0430\u043b\u0438\u043c \u0435\u0449\u0435 (\u0435\u0449\u0435 \u043e\u0434\u043d\u0430 \u0441\u0442\u0440\u0430\u043d\u0438\u0446\u0430). \u0418 \u0432\u043e\u0442 \u0442\u0430\u043a \u0432\u0441\u044f \u043a\u043d\u0438\u0433\u0430 - \u043e\u0434\u043d\u043e, \u0434\u0430 \u043f\u043e\u0442\u043e\u043c\u0443. \u0412 \u043d\u0430\u0447\u0430\u043b\u0435 \u043a\u043d\u0438\u0433\u0438 \u043f\u0438\u0448\u0443\u0442 \u043f\u0440\u043e \u044e\u043d\u0438\u0442-\u0442\u0435\u0441\u0442\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u0435 \u0441 Ruby \u0441 \u043a\u0443\u043a\u0443\u043c\u0431\u0435\u0440\u043e\u043c. \u0412 \u0441\u0435\u0440\u0435\u0434\u0438\u043d\u0435 \u043e\u043f\u0438\u0441\u0430\u043d \u0432\u0435\u0441\u044c\u043c\u0430 \u043b\u044e\u0431\u043e\u043f\u044b\u0442\u043d\u044b\u0439 \u0444\u0440\u0435\u0439\u043c\u0432\u043e\u0440\u043a \u0434\u043b\u044f \u0442\u0435\u0441\u0442\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u044f \u0432 Java - FitNesse (\u0445\u043e\u0442\u044f \u043e\u043d \u0438 \u0434\u043b\u044f \u0434\u0440\u0443\u0433\u0438\u0445 \u044f\u0437\u044b\u043a\u043e\u0432 \u0435\u0441\u0442\u044c - \u0432 \u043a\u043d\u0438\u0433\u0435 \u043d\u0435 \u043d\u0430\u043f\u0438\u0441\u0430\u043d\u043e). \u0412 \u043a\u043e\u043d\u0446\u0435 \u043a\u043d\u0438\u0433\u0438 \u0431\u043e\u043b\u0435\u0435 \u043c\u0435\u043d\u0435\u0435 \u0438\u043d\u0442\u0435\u0440\u0435\u0441\u043d\u043e \u043c\u043e\u0436\u043d\u043e \u043f\u043e\u0447\u0438\u0442\u0430\u0442\u044c \u043f\u0440\u043e \u0440\u0430\u0437\u0440\u0430\u0431\u043e\u0442\u043a\u0443 \u0431\u0435\u0437 \u043a\u0443\u0447\u0438 \u043e\u0434\u0438\u043d\u0430\u043a\u043e\u0432\u044b\u0445 \u0442\u0430\u0431\u043b\u0438\u0446. \u0415\u0449\u0435 \u0432 \u043a\u043d\u0438\u0433\u0435 \u043a\u0443\u0447\u0430 \u043e\u043f\u0435\u0447\u0430\u0442\u043e\u043a \u0438 \u043e\u0448\u0438\u0431\u043e\u043a (\u0432\u0438\u0434\u0438\u043c\u043e \u043f\u0435\u0440\u0435\u0432\u043e\u0434\u0447\u0438\u043a \u0432\u0438\u043d\u043e\u0432\u0430\u0442). \u041f\u0435\u0440\u0435\u0432\u043e\u0434 \u043c\u0435\u0440\u0437\u043a\u0438\u0439, \u0445\u0443\u0436\u0435 \u044f \u043d\u0435 \u0432\u0438\u0434\u0435\u043b. \u0421\u043a\u0440\u044b\u0442\u044c',3508)
INSERT INTO COMMENT VALUES(1559,'\u0412\u043b\u0430\u0434\u0438\u043c\u0438\u0440','2015-01-13','\u041a\u043d\u0438\u0433\u0430 \u0432\u044b\u0445\u043e\u0434\u043d\u043e\u0433\u043e \u0434\u043d\u044f. \u041d\u0430\u043f\u0438\u0441\u0430\u043d\u043e \u043f\u0440\u0435\u0434\u0435\u043b\u044c\u043d\u043e \u043f\u0440\u043e\u0441\u0442\u043e \u0438 \u043f\u043e\u043d\u044f\u0442\u043d\u043e. \u0421\u0430\u043c\u043e\u0435 \u0447\u0442\u043e \u043d\u0438 \u043d\u0430 \u0435\u0441\u0442\u044c \u0432\u0432\u0435\u0434\u0435\u043d\u0438\u0435. \u041c\u043d\u0435 \u043a\u0430\u0436\u0435\u0442\u0441\u044f \u043b\u044e\u0434\u044f\u043c \u0438\u043c\u0435\u044e\u0449\u0438\u043c \u043f\u0440\u0435\u0434\u0441\u0442\u0430\u0432\u043b\u0435\u043d\u0438\u0435 \u043e \u043f\u0430\u0440\u0430\u0434\u0438\u0433\u043c\u0435 \u0442\u0435\u0441\u0442\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u044f \u0437\u0430 \u044d\u0442\u0438 \u0434\u0435\u043d\u044c\u0433\u0438 \u043a\u043d\u0438\u0433\u0430 \u0431\u0443\u0434\u0435\u0442 \u043d\u0435 \u0438\u043d\u0442\u0435\u0440\u0435\u0441\u043d\u0430. \u041e\u0447\u0435\u043d\u044c \u043f\u0440\u043e\u0441\u0442\u044b\u0435 \u043f\u0440\u0438\u043c\u0435\u0440\u044b, \u0447\u0438\u0442\u0430\u0435\u0442\u0441\u044f \u043e\u0447\u0435\u043d\u044c \u043b\u0435\u0433\u043a\u043e. \u041a\u043d\u0438\u0433\u0430 \u043f\u043e\u0441\u0432\u044f\u0449\u0435\u043d\u0430 \u0438\u043c\u0435\u043d\u043d\u043e \u043f\u043e\u0434\u0445\u043e\u0434\u0443 \u043a \u0440\u0430\u0437\u0440\u0430\u0431\u043e\u0442\u043a\u0435 \u0447\u0435\u0440\u0435\u0437 \u043f\u0440\u0438\u0435\u043c\u043e\u0447\u043d\u043e\u0435 \u0442\u0435\u0441\u0442\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u0435 ATDD. \u041a\u043d\u0438\u0433\u0430 \u043d\u0435 \u043d\u0430\u0443\u0447\u0438\u0442 \u0432\u0430\u0441 \u043f\u0438\u0441\u0430\u0442\u044c \u0445\u043e\u0440\u043e\u0448\u0438\u0435 \u0438 \u043f\u0440\u0430\u0432\u0438\u043b\u044c\u043d\u044b\u0435 \u0442\u0435\u0441\u0442\u044b unit-\u0442\u0435\u0441\u0442\u044b \u0438 \u043f\u0440. \u041e\u043d\u0430 \u0440\u0430\u0441\u043a\u0440\u044b\u0432\u0430\u0435\u0442 \u0441\u0443\u0442\u044c ATDD \u043d\u0430 \u043f\u0440\u043e\u0441\u0442\u044b\u0445 \u043f\u0440\u0438\u043c\u0435\u0440\u0430\u0445. "ATDD \u0444\u043e\u043a\u0443\u0441\u0438\u0440\u0443\u0435\u0442\u0441\u044f \u043d\u0430 \u0442\u0440\u0435\u0431\u043e\u0432\u0430\u043d\u0438\u044f\u0445, \u0442\u043e \u0435\u0441\u0442\u044c \u0441\u043f\u0435\u0446\u0438\u0444\u0438\u0446\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u0438 \u0444\u0443\u043d\u043a\u0446\u0438\u043e\u043d\u0430\u043b\u044c\u043d\u043e\u0441\u0442\u0438. TDD \u0438\u043c\u0435\u0435\u0442 \u0434\u0435\u043b\u043e \u0441 \u0442\u0435\u0445\u043d\u0438\u0447\u0435\u0441\u043a\u043e\u0439 \u0440\u0435\u0430\u043b\u0438\u0437\u0430\u0446\u0438\u0435\u0439, ATDD - \u0441 \u043a\u0440\u0438\u0442\u0435\u0440\u0438\u044f\u043c\u0438 \u043f\u0440\u0438\u0435\u043c\u043a\u0438 \u0441 \u0442\u043e\u0447\u043a\u0438 \u0437\u0440\u0435\u043d\u0438\u044f \u0431\u0438\u0437\u043d\u0435\u0441\u0430" \u0421\u043a\u0440\u044b\u0442\u044c',3508)
