INSERT INTO BOOK VALUES(3314,473067,'\u0414\u0435\u0439\u0442\u0435\u043b, \u0414\u0435\u0439\u0442\u0435\u043b, \u0414\u0435\u0439\u0442\u0435\u043b: Android \u0434\u043b\u044f \u0440\u0430\u0437\u0440\u0430\u0431\u043e\u0442\u0447\u0438\u043a\u043e\u0432','\u0414\u0435\u0439\u0442\u0435\u043b \u0425\u0430\u0440\u0432\u0438, \u0414\u0435\u0439\u0442\u0435\u043b \u041f\u043e\u043b \u0414\u0436., \u0414\u0435\u0439\u0442\u0435\u043b \u042d\u0431\u0431\u0438','\u0421\u0435\u0440\u0433\u0438\u0435\u043d\u043a\u043e \u042e.','\u041f\u0438\u0442\u0435\u0440, 2015 \u0433.',812.0E0,'978-5-496-01517-2','384 (\u041e\u0444\u0441\u0435\u0442)','\u0422\u0438\u043f \u043e\u0431\u043b\u043e\u0436\u043a\u0438: \u043e\u0431\u043b - \u043c\u044f\u0433\u043a\u0438\u0439 \u043f\u0435\u0440\u0435\u043f\u043b\u0435\u0442 (\u043a\u0440\u0435\u043f\u043b\u0435\u043d\u0438\u0435 \u0441\u043a\u0440\u0435\u043f\u043a\u043e\u0439 \u0438\u043b\u0438 \u043a\u043b\u0435\u0435\u043c) \u041e\u0444\u043e\u0440\u043c\u043b\u0435\u043d\u0438\u0435: \u0427\u0430\u0441\u0442\u0438\u0447\u043d\u0430\u044f \u043b\u0430\u043a\u0438\u0440\u043e\u0432\u043a\u0430 \u0418\u043b\u043b\u044e\u0441\u0442\u0440\u0430\u0446\u0438\u0438: \u0427\u0435\u0440\u043d\u043e-\u0431\u0435\u043b\u044b\u0435','498 \u0433','233x165x16 \u043c\u043c','\u0410\u043d\u043d\u043e\u0442\u0430\u0446\u0438\u044f \u043a \u043a\u043d\u0438\u0433\u0435 "Android \u0434\u043b\u044f \u0440\u0430\u0437\u0440\u0430\u0431\u043e\u0442\u0447\u0438\u043a\u043e\u0432" \u0411\u043e\u043b\u0435\u0435 \u043c\u0438\u043b\u043b\u0438\u043e\u043d\u0430 \u0447\u0435\u043b\u043e\u0432\u0435\u043a \u0432\u043e \u0432\u0441\u0435\u043c \u043c\u0438\u0440\u0435 \u0432\u043e\u0441\u043f\u043e\u043b\u044c\u0437\u043e\u0432\u0430\u043b\u0438\u0441\u044c \u043a\u043d\u0438\u0433\u0430\u043c\u0438 \u0414\u0435\u0439\u0442\u0435\u043b\u043e\u0432, \u0447\u0442\u043e\u0431\u044b \u043e\u0441\u0432\u043e\u0438\u0442\u044c Java, C#, C++, C, \u0432\u0435\u0431-\u043f\