INSERT INTO COMMENT VALUES(1521,'Ii \u0414\u043c\u0438\u0442\u0440\u0438\u0439','2015-01-02','\u041a\u0443\u043f\u0438\u043b \u0434\u0430\u043d\u043d\u0443\u044e \u043a\u043d\u0438\u0433\u0443. \u0422\u043e\u043b\u044c\u043a\u043e \u043d\u0430\u0447\u0430\u043b \u0447\u0438\u0442\u0430\u0442\u044c, \u043d\u043e \u0443\u0436\u0435 \u0431\u043e\u043b\u044c\u0448\u043e\u0435 \u0440\u0430\u0437\u043e\u0447\u0430\u0440\u043e\u0432\u0430\u043d\u0438\u0435 \u0432 \u043f\u043e\u043a\u0443\u043f\u043a\u0435. \u0423\u0436\u0430\u0441\u043d\u043e\u0435 \u043e\u0431\u044a\u044f\u0441\u043d\u0435\u043d\u0438\u0435(\u0432\u043e\u0437\u043c\u043e\u0436\u043d\u043e, \u044d\u0442\u043e \u0441\u043b\u0435\u0434\u0441\u0442\u0432\u0438\u0435 \u0443\u0436\u0430\u0441\u043d\u043e\u0433\u043e \u043f\u0435\u0440\u0435\u0432\u043e\u0434\u0430), \u0443\u0436\u0430\u0441\u043d\u044b\u0435 \u043e\u0448\u0438\u0431\u043a\u0438, \u043a\u043e\u0442\u043e\u0440\u044b\u0435 \u043c\u0435\u0448\u0430\u044e\u0442 \u043f\u043e\u043d\u0438\u043c\u0430\u043d\u0438\u044e, \u0443\u0436\u0430\u0441\u043d\u043e\u0435 \u0444\u043e\u0440\u043c\u0430\u0442\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u0435 \u043a\u043e\u0434\u0430 \u0432 \u043a\u043d\u0438\u0433\u0435.. \u041f\u0440\u043e\u043f\u0430\u043b\u043e \u0436\u0435\u043b\u0430\u043d\u0438\u0435 \u0447\u0438\u0442\u0430\u0442\u044c \u0434\u0430\u043b\u044c\u0448\u0435. \u0414\u043b\u044f \u043b\u044e\u0434\u0435\u0439 \u0437\u043d\u0430\u043a\u043e\u043c\u044b\u0445 \u0441 \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u0435\u043c \u043e\u043d\u0430 \u0441\u043b\u0438\u0448\u043a\u043e\u043c \u043f\u0440\u043e\u0441\u0442\u0430, \u0434\u043b\u044f \u043d\u043e\u0432\u0438\u0447\u043a\u0430 \u0441\u043b\u0438\u0448\u043a\u043e\u043c \u043d\u0435\u043f\u043e\u043d\u044f\u0442\u043d\u0430(\u043f\u043e \u043f\u0440\u0438\u0447\u0438\u043d\u0430\u043c \u043e\u043f\u0438\u0441\u0430\u043d\u043d\u044b\u043c \u0432\u04