INSERT INTO BOOK VALUES(3825,327211,'\u0412\u0438\u0440\u0442, \u0413\u0443\u0442\u043a\u043d\u0435\u0445\u0442: \u0420\u0430\u0437\u0440\u0430\u0431\u043e\u0442\u043a\u0430 \u043e\u043f\u0435\u0440\u0430\u0446\u0438\u043e\u043d\u043d\u043e\u0439 \u0441\u0438\u0441\u0442\u0435\u043c\u044b \u0438 \u043a\u043e\u043c\u043f\u0438\u043b\u044f\u0442\u043e\u0440\u0430. \u041f\u0440\u043e\u0435\u043a\u0442 \u041e\u0431\u0435\u0440\u043e\u043d','\u0412\u0438\u0440\u0442 \u041d\u0438\u043a\u043b\u0430\u0443\u0441, \u0413\u0443\u0442\u043a\u043d\u0435\u0445\u0442 \u042e\u0440\u0433','\u041c\u043e\u0432\u0447\u0430\u043d \u0414. \u0410.','\u0414\u041c\u041a-\u041f\u0440\u0435\u0441\u0441, 2015 \u0433.',1077.0E0,'978-5-94074-672-0\u00a0 ','560 (\u041e\u0444\u0441\u0435\u0442)','\u0422\u0438\u043f \u043e\u0431\u043b\u043e\u0436\u043a\u0438: \u043e\u0431\u043b - \u043c\u044f\u0433\u043a\u0438\u0439 \u043f\u0435\u0440\u0435\u043f\u043b\u0435\u0442 (\u043a\u0440\u0435\u043f\u043b\u0435\u043d\u0438\u0435 \u0441\u043a\u0440\u0435\u043f\u043a\u043e\u0439 \u0438\u043b\u0438 \u043a\u043b\u0435\u0435\u043c) \u0418\u043b\u043b\u044e\u0441\u0442\u0440\u0430\u0446\u0438\u0438: \u0427\u0435\u0440\u043d\u043e-\u0431\u0435\u043b\u044b\u0435','640 \u0433','200x140x30 \u043c\u043c','\u0410\u043d\u043d\u043e\u0442\u0430\u0446\u0438\u044f \u043a \u043a\u043d\u0438\u0433\u0435 "\u0420\u0430\u0437\u0440\u0430\u0431\u043e\u0442\u043a\u0430 \u043e\u043f\u0435\u0440\u0430\u0446\u0438\u043e\u043d\u043d\u043e\u0439 \u0441\u0438\u0441\u0442\u0435\u043c\u044b \u0438 \u043a\u043e\u043c\u043f\u0438\u043b\u044f\u0442\u043e\u0440\u0430. \u041f\u0440\u043e\u0435\u043a\u0442 \u041e\u0431\u0435\u0440\u043e\u043d" "\u041c\u044b \u0445\u043e\u0442\u0435\u043b\u0438 \u043d\u0435 \u0442\u043e\u043b\u044c\u043a\u043e \u0434\u0430\u0442\u044c \u0441\u043e\u0432\u0435\u0442, \u043a\u0430\u043a \u043c\u043e\u0436\u0435\u0442 \u0441\u043e\u0437\u0434\u0430\u0432\u0430\u0442\u044c\u0441\u044f \u0441\u0438\u0441\u0442\u0435\