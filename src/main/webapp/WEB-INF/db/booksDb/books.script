INSERT INTO COMMENT VALUES(1523,'\u041a\u0438\u043b\u0438\u043f\u043a\u043e \u0415\u0432\u0433\u0435\u043d\u0438\u0439','2014-01-06','\u041a\u043d\u0438\u0433\u0430 \u043d\u0435 \u043e\u043f\u0440\u0430\u0432\u0434\u0430\u043b\u0430 \u043c\u043e\u0438 \u043e\u0436\u0438\u0434\u0430\u043d\u0438\u044f! \u0412 \u043a\u043d\u0438\u0433\u0435 \u0441\u043b\u0438\u0448\u043a\u043e\u043c \u043c\u043d\u043e\u0433\u043e \u0438\u043b\u043b\u044e\u0441\u0442\u0440\u0430\u0446\u0438\u0439, \u043f\u0440\u0438\u0447\u0435\u043c \u043e\u043d\u0438 \u0431\u043e\u043b\u044c\u0448\u043e\u0433\u043e \u0440\u0430\u0437\u043c\u0435\u0440\u0430 \u0438 \u0447\u0430\u0441\u0442\u0435\u043d\u044c\u043a\u043e \u043f\u043e\u0432\u0442\u043e\u0440\u044f\u044e\u0442\u0441\u044f \u043d\u0430 \u0441\u043b\u0435\u0434\u0443\u044e\u0449\u0435\u0439 \u0441\u0442\u0440\u0430\u043d\u0438\u0446\u0435, \u0442\u0435\u043a\u0441\u0442\u0430 \u043c\u0430\u043b\u043e\u0432\u0430\u0442\u043e. \u041f\u0440\u0438 \u043f\u043e\u043a\u0443\u043f\u043a\u0435 \u0432\u044b \u0437\u0430\u043f\u043b\u0430\u0442\u0438\u0442\u0435 500 \u0440\u0443\u0431\u043b\u0435\u0439 \u0437\u0430 \u0438\u043b\u043b\u044e\u0441\u0442\u0440\u0430\u0446\u0438\u0438 \u0438 \u043e\u0444\u043e\u0440\u043c\u043b\u0435\u043d\u0438\u0435 \u0438 \u043b\u0438\u0448\u044c 300 \u0440\u0443\u0431. \u0437\u0430 \u0442\u0435\u043a\u0441\u0442. CSS \u0440\u0430\u0441\u0441\u043c\u043e\u0442\u0440\u0435\u043d \u043e\u0447\u0435\u043d\u044c \u043f\u043e\u0432\u0435\u0440\u0445\u043d\u043e\u0441\u0442\u043d\u043e. \u042d\u0442\u0430 \u043a\u043d\u0438\u0433\u0430 \u0434\u043b\u044f \u043d\u043e\u0432\u0438\u0447\u043a\u0430, \u043a\u043e\u0442\u043e\u0440\u043e\u043c\u0443 \u043d\u0443\u0436\u043d\u043e \u043b\u0438\u0448\u044c \u043f\u043e\u043d\u044f\u0442\u044c \u0447\u0442\u043e \u0442\u0430\u043a\u043e\u0435 html \u0438 css. \u0414\u043b\u044f \u0442\u0435\u0445 \u043d\u043e\u0432\u0438\u0447\u043a\u043e\u0432, \u043a\u043e\u0442\u043e\u0440\u044b\u0435 \u0445\u043e\u0442\u044f\u0442 \u043f\u043e\u043b\u0443\u0447\u0438\u0442\u044c \u043e\u0442\u043b\u0438\u0447\u043d\u044b\u0435 \u0437\u043d\u0430\u043d\u0438\u044f \u0438 \u0434\u0435\u043b\u0430\u0442\u044c \u0441\u0430\u0439\u0442\u044b \u0440\u0435\u043a\u043e\u043c\u0435\u043d\u0434\u0443\u044e \u043e\u0442\u0434\u0435\u043b\u044c\u043d\u043e \u043d\u0435 \u0434\u043e\u0440\u043e\u0433\u0443\u044e \u043a\u043d\u0438\u0433\u0443 \u043f\u043e html \u0438 \u0414\u044d\u0432\u0438\u0434 \u041c\u0430\u043a\u0444\u0430\u0440\u043b\u0430\u043d\u0434: \u0411\u043e\u043b\u044c\u0448\u0430\u044f \u043a\u043d\u0438\u0433\u0430 CSS3 - \u041f\u043e \u0446\u0435\u043d\u0435 \u0432\u044b\u0439\u0434\u0435\u0442 \u043f\u0440\u0438\u043c\u0435\u0440\u043d\u043e \u043e\u0434\u0438\u043d\u0430\u043a\u043e\u0432\u043e, \u043d\u043e \u0437\u043d\u0430\u043d\u0438\u0439 \u0432\u044b \u043f\u043e\u043b\u0443\u0447\u0438\u0442\u0435 \u0431\u043e\u043b\u044c\u0448\u0435. \u041e\u0442\u0434\u0435\u043b\u044c\u043d\u043e \u0445\u043e\u0447\u0443 \u0441\u043a\u0430\u0437\u0430\u0442\u044c \u043f\u0440\u043e \u043a\u043d\u0438\u0433\u0443 (\u0414\u044d\u0432\u0438\u0434 \u041c\u0430\u043a\u0444\u0430\u0440\u043b\u0430\u043d\u0434: \u0411\u043e\u043b\u044c\u0448\u0430\u044f \u043a\u043d\u0438\u0433\u0430 CSS3) \u0438\u043c\u0435\u043d\u043d\u043e \u043f\u043e \u044d\u0442\u043e\u0439 \u043a\u043d\u0438\u0433\u0435 \u044f \u0441 \u043d\u0443\u043b\u044f \u043e\u0441\u0432\u043e\u0438\u043b CSS, \u0434\u043e\u0432\u043e\u043b\u0435\u043d \u0434\u0430\u043d\u043d\u043e\u0439 \u043a\u043d\u0438\u0433\u043e\u0439 \u043a\u0430\u043a \u0441\u043b\u043e\u043d, \u043b\u0443\u0447\u0448\u0435 \u0438 \u043d\u0435 \u043d\u0430\u0439\u0442\u0438. \u0423\u0434\u0430\u0447\u0438 \u0432\u0441\u0435\u043c \u0432 \u043e\u0441\u0432\u043e\u0435\u043d\u0438\u0438 html \u0438 css \u0421\u043a\u0440\u044b\u0442\u044c',3443)
