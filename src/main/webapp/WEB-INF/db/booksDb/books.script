INSERT INTO BOOK VALUES(3353,450021,'\u0424\u0440\u0435\u0434 \u0425\u0435\u0431\u0435\u0440\u0442: \u0418\u0437\u0443\u0447\u0430\u0439 Erlang \u0432\u043e \u0438\u043c\u044f \u0434\u043e\u0431\u0440\u0430!','\u0425\u0435\u0431\u0435\u0440\u0442 \u0424\u0440\u0435\u0434','\u041c\u043e\u0432\u0447\u0430\u043d \u0414. \u0410.','\u0414\u041c\u041a-\u041f\u0440\u0435\u0441\u0441, 2015 \u0433.',1604.0E0,'978-5-97060-086-3','688 (\u041e\u0444\u0441\u0435\u0442)','\u0422\u0438\u043f \u043e\u0431\u043b\u043e\u0436\u043a\u0438: \u043e\u0431\u043b - \u043c\u044f\u0433\u043a\u0438\u0439 \u043f\u0435\u0440\u0435\u043f\u043b\u0435\u0442 (\u043a\u0440\u0435\u043f\u043b\u0435\u043d\u0438\u0435 \u0441\u043a\u0440\u0435\u043f\u043a\u043e\u0439 \u0438\u043b\u0438 \u043a\u043b\u0435\u0435\u043c) \u0418\u043b\u043b\u044e\u0441\u0442\u0440\u0430\u0446\u0438\u0438: \u0427\u0435\u0440\u043d\u043e-\u0431\u0435\u043b\u044b\u0435','934 \u0433','218x159x35 \u043c\u043c','Erlang \u044f\u0432\u043b\u044f\u0435\u0442\u0441\u044f \u044f\u0437\u044b\u043a\u043e\u043c \u0432\u044b\u0431\u043e\u0440\u0430 \u0434\u043b\u044f \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u0438\u0441\u0442\u043e\u0432, \u043a\u043e\u0442\u043e\u0440\u044b\u0435 \u0445\u043e\u0442\u044f\u0442 \u043f\u0438\u0441\u0430\u0442\u044c \u043d\u0430\u0434\u0451\u0436\u043d\u044b\u0435 \u043f\u0430\u0440\u0430\u043b\u043b\u0435\u043b\u044c\u043d\u044b\u0435 \u043f\u0440\u0438\u043b\u043e\u0436\u0435\u043d\u0438\u044f, \u043d\u043e \u0435\u0433\u043e \u0441\u0442\u0440\u0430\u043d\u043d\u044b\u0439 \u0441\u0438\u043d\u0442\u0430\u043a\u0441\u0438\u0441 \u0438 \u0444\u0443\u043d\u043a\u0446\u0438\u043e\u043d\u0430\u043b\u044c\u043d\u044b\u0439 \u0434\u0438\u0437\u0430\u0439\u043d \u043c\u043e\u0433\u0443\u0442 \u043e\u0442\u043f\u0443\u0433\u0438\u0432\u0430\u0442\u044c \u043d\u0435\u043f\u043e\u0441\u0432\u044f\u0449\u0451\u043d\u043d\u044b\u0445. \u041a \u0441\u0447\u0430\u0441\u0442\u044c\u044e, \u0432 \u0431\u0438\u0442\u0432\u0435 \u043f\u0440\u043e\u0442\u0438\u0432 \u0441\u0442\u0440\u0430\u0445\u0430 \u043b\u044e\u0434\u0435\u0439 \u043f\u0435\u0440\u0435\u0434 Erlang \u043f\u043e\u044f\u0432\u0438\u043b\u043e\u0441\u044c \u043d\u043e\u0432\u043e\u0435 \u043e\u0440\u0443\u0436\u0438\u0435 - "\u0418\u0437\u0443\u0447\u0430\u0439 Erlang \u0432\u043e \u0438\u043c\u044f \u0434\u043e\u0431\u0440\u0430!" \u041c\u0430\u044d\u0441\u0442\u0440\u043e Erlang, \u0424\u0440\u0435\u0434 \u0425\u0435\u0431\u0435\u0440\u0442, \u043d\u0430\u0447\u0438\u043d\u0430\u0435\u0442 \u0443\u0440\u043e\u043a \u0438 \u0431\u0435\u0437 \u0441\u043f\u0435\u0448\u043a\u0438 \u043f\u043e\u044f\u0441\u043d\u044f\u0435\u0442 \u043e\u0441\u043d\u043e\u0432\u044b \u044f\u0437\u044b\u043a\u0430: \u0432\u044b \u0438\u0437\u0443\u0447\u0438\u0442\u0435 \u043d\u0435\u043e\u0431\u044b\u0447\u043d\u044b\u0439 \u0441\u0438\u043d\u0442\u0430\u043a\u0441\u0438\u0441 Erlang, \u0435\u0433\u043e \u0441\u0442\u0440\u0443\u043a\u0442\u0443\u0440\u044b \u0434\u0430\u043d\u043d\u044b\u0445, \u0435\u0433\u043e \u0441\u0438\u0441\u0442\u0435\u043c\u0443 \u0442\u0438\u043f\u043e\u0432 (\u0432\u0435\u0440\u043d\u0435\u0435 \u0435\u0451 \u043e\u0442\u0441\u0443\u0442\u0441\u0442\u0432\u0438\u0435!), \u0438 \u043e\u0441\u043d\u043e\u0432\u044b \u0442\u0435\u0445\u043d\u0438\u043a\u0438 \u0444\u0443\u043d\u043a\u0446\u0438\u043e\u043d\u0430\u043b\u044c\u043d\u043e\u0433\u043e \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u044f. \u041a\u0430\u043a \u0442\u043e\u043b\u044c\u043a\u043e \u0432\u044b \u0440\u0430\u0437\u0431\u0435\u0440\u0451\u0442\u0435\u0441\u044c \u0441 \u043f\u0440\u043e\u0441\u0442\u044b\u043c\u0438 \u0432\u0435\u0449\u0430\u043c\u0438, \u0432\u044b \u043e\u0441\u0432\u043e\u0438\u0442\u0435 \u043d\u0430\u0441\u0442\u043e\u044f\u0449\u0438\u0435 \u043f\u043e\u043b\u0435\u0437\u043d\u044b\u0435 \u0432\u043e\u0437\u043c\u043e\u0436\u043d\u043e\u0441\u0442\u0438 \u044f\u0437\u044b\u043a\u0430: \u043f\u0430\u0440\u0430\u043b\u043b\u0435\u043b\u044c\u043d\u043e\u0435 \u0438\u0441\u043f\u043e\u043b\u043d\u0435\u043d\u0438\u0435, \u0440\u0430\u0441\u043f\u0440\u0435\u0434\u0435\u043b\u0451\u043d\u043d\u044b\u0435 \u0432\u044b\u0447\u0438\u0441\u043b\u0435\u043d\u0438\u044f, \u0433\u043e\u0440\u044f\u0447\u0430\u044f \u043f\u0435\u0440\u0435\u0437\u0430\u0433\u0440\u0443\u0437\u043a\u0430 \u043a\u043e\u0434\u0430 \u0438 \u043f\u0440\u043e\u0447\u0430\u044f \u0442\u0451\u043c\u043d\u0430\u044f \u043c\u0430\u0433\u0438\u044f, \u043a\u043e\u0442\u043e\u0440\u0430\u044f \u0434\u0435\u043b\u0430\u0435\u0442 Erlang \u0442\u0430\u043a\u043e\u0439 \u0432\u043e\u0441\u0442\u0440\u0435\u0431\u043e\u0432\u0430\u043d\u043d\u043e\u0439 \u0442\u0435\u043c\u043e\u0439 \u0441\u0440\u0435\u0434\u0438 \u0441\u043e\u0432\u0440\u0435\u043c\u0435\u043d\u043d\u044b\u0445 \u043f\u0440\u0430\u043a\u0442\u0438\u0447\u043d\u044b\u0445 \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u0438\u0441\u0442\u043e\u0432. \u0423\u0441\u0435\u044f\u043d\u043d\u0430\u044f \u0431\u0435\u0441\u043f\u0435\u0447\u043d\u044b\u043c\u0438 \u0438\u043b\u043b\u044e\u0441\u0442\u0440\u0430\u0446\u0438\u044f\u043c\u0438 \u0438 \u0441\u043c\u0435\u0441\u044c\u044e \u0440\u0430\u0437\u0432\u043b\u0435\u043a\u0430\u0442\u0435\u043b\u044c\u043d\u044b\u0445 \u0438 \u043f\u0440\u0430\u043a\u0442\u0438\u0447\u0435\u0441\u043a\u0438\u0445 \u043f\u0440\u0438\u043c\u0435\u0440\u043e\u0432 \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c, \u043a\u043d\u0438\u0433\u0430 "\u0418\u0437\u0443\u0447\u0430\u0439 Erlang \u0432\u043e \u0438\u043c\u044f \u0434\u043e\u0431\u0440\u0430!" \u044f\u0432\u043b\u044f\u0435\u0442\u0441\u044f \u043e\u0442\u043b\u0438\u0447\u043d\u044b\u043c \u043f\u0443\u043d\u043a\u0442\u043e\u043c \u043e\u0442\u043f\u0440\u0430\u0432\u043b\u0435\u043d\u0438\u044f \u0432 \u0438\u043d\u043e\u0433\u0434\u0430 \u0431\u0435\u0437\u0443\u043c\u043d\u044b\u0439, \u043d\u043e \u0432\u0441\u04