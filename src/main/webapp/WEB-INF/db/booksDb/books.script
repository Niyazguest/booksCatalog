INSERT INTO BOOK VALUES(3815,338826,'\u041c\u0430\u0439\u043a\u043b \u0422\u0435\u0440\u043d\u0435\u0440: \u041e\u0441\u043d\u043e\u0432\u044b Microsoft Solution Framework','\u0422\u0435\u0440\u043d\u0435\u0440 \u041c\u0430\u0439\u043a\u043b','','\u041f\u0438\u0442\u0435\u0440, 2008 \u0433.',595.0E0,'978-5-7502-0306-2','336 (\u0413\u0430\u0437\u0435\u0442\u043d\u0430\u044f)','\u0422\u0438\u043f \u043e\u0431\u043b\u043e\u0436\u043a\u0438: \u043e\u0431\u043b - \u043c\u044f\u0433\u043a\u0438\u0439 \u043f\u0435\u0440\u0435\u043f\u043b\u0435\u0442 (\u043a\u0440\u0435\u043f\u043b\u0435\u043d\u0438\u0435 \u0441\u043a\u0440\u0435\u043f\u043a\u043e\u0439 \u0438\u043b\u0438 \u043a\u043b\u0435\u0435\u043c) \u041e\u0444\u043e\u0440\u043c\u043b\u0435\u043d\u0438\u0435: \u0427\u0430\u0441\u0442\u0438\u0447\u043d\u0430\u044f \u043b\u0430\u043a\u0438\u0440\u043e\u0432\u043a\u0430 \u0418\u043b\u043b\u044e\u0441\u0442\u0440\u0430\u0446\u0438\u0438: \u0427\u0435\u0440\u043d\u043e-\u0431\u0435\u043b\u044b\u0435','398 \u0433','232x162x18 \u043c\u043c','\u0410\u043d\u043d\u043e\u0442\u0430\u0446\u0438\u044f \u043a \u043a\u043d\u0438\u0433\u0435 "\u041e\u0441\u043d\u043e\u0432\u044b Microsoft Solution Framework" \u042d\u0442\u0430 \u043a\u043d\u0438\u0433\u0430 - \u043f\u0440\u0430\u043a\u0442\u0438\u0447\u0435\u0441\u043a\u043e\u0435 \u0440\u0443\u043a\u043e\u0432\u043e\u0434\u0441\u0442\u0432\u043e \u043f\u043e \u0438\u0441\u043f\u043e\u043b\u044c\u0437\u043e\u0432\u0430\u043d\u0438\u044e Microsoft Solutions Framework (MSF) - \u043c\u0435\u0442\u043e\u0434\u043e\u043b\u043e\u0433\u0438\u0438 \u0441\u043e\u0437\u0434\u0430\u043d\u0438\u044f \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u043d\u044b\u0445 \u0440\u0435\u0448\u0435\u043d\u0438\u0439. \u0417\u0434\u0435\u0441\u044c, \u0441 \u0443\u0447\u0435\u0442\u043e\u043c \u043f\u0440\u0430\u043a\u0442\u0438\u0447\u0435\u0441\u043a\u043e\u0433\u043e \u043e\u043f\u044b\u0442\u0430 \u043a\u043e\u0440\u043f\u043e\u0440\u0430\u0446\u0438\u